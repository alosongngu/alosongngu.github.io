[
    {
        "start": "00:00:06,080",
        "end": "00:00:09,720",
        "en": "EEN NETFLIX ORIGINAL-SERIE",
        "pa": "",
        "vi": "LOẠT PHIM CỦA NETFLIX"
    },
    {
        "start": "00:00:29,840",
        "end": "00:00:31,720",
        "en": "Rosa? Ben je klaar? ",
        "pa": "Rosa? Are you done?",
        "vi": "Rosa? Chuẩn bị xong chưa?"
    },
    {
        "start": "00:00:35,880",
        "end": "00:00:36,720",
        "en": "Rosa? ",
        "pa": "Rosa?",
        "vi": "Rosa?"
    },
    {
        "start": "00:00:52,000",
        "end": "00:00:52,840",
        "en": "Geen drank... ",
        "pa": "No alcohol",
        "vi": "Trong máu cô ấy"
    },
    {
        "start": "00:00:53,200",
        "end": "00:00:54,440",
        "en": "...geen drugs in d'r bloed. ",
        "pa": "or drugs in her blood.",
        "vi": "không có cồn hay ma túy."
    },
    {
        "start": "00:00:55,400",
        "end": "00:00:56,880",
        "en": "Behalve haar eigen medicatie. ",
        "pa": "Except for her medication.",
        "vi": "Ngoại trừ loại thuốc cô ấy uống."
    },
    {
        "start": "00:01:00,760",
        "end": "00:01:03,640",
        "en": "En ze heeft 'n belletje gepleegd vlak voordat ze dit deed. ",
        "pa": "And she made a phone call right before this.",
        "vi": "Và cô ấy có gọi một cuộc điện thoại trước khi sự việc xảy ra."
    },
    {
        "start": "00:01:05,720",
        "end": "00:01:06,560",
        "en": "Naar wie? ",
        "pa": "To whom?",
        "vi": "Gọi cho ai?"
    },
    {
        "start": "00:01:07,040",
        "end": "00:01:07,880",
        "en": "Naar jou. ",
        "pa": "To you.",
        "vi": "Cho ông."
    },
    {
        "start": "00:01:14,600",
        "end": "00:01:16,200",
        "en": "Laat ze maar weer binnen. ",
        "pa": "Let them back in.",
        "vi": "Cho họ vào lại đi."
    },
    {
        "start": "00:02:03,560",
        "end": "00:02:04,400",
        "en": "Eet wat. ",
        "pa": "Eat something.",
        "vi": "Cậu ăn gì đi."
    },
    {
        "start": "00:02:11,000",
        "end": "00:02:13,680",
        "en": "Hé. Heb je 'n beetje kunnen slapen? ",
        "pa": "Hey. Could you get some sleep?",
        "vi": "Này, cậu ngủ được không?"
    },
    {
        "start": "00:02:14,440",
        "end": "00:02:15,400",
        "en": "Gaat het? ",
        "pa": "Are you okay?",
        "vi": "Cậu ổn chứ?"
    },
    {
        "start": "00:02:21,840",
        "end": "00:02:23,280",
        "en": "Mag ik even met Rosa praten? ",
        "pa": "May I speak with Rosa?",
        "vi": "Để tôi nói chuyện với Rosa nhé?"
    },
    {
        "start": "00:02:28,800",
        "end": "00:02:30,080",
        "en": "Hoe gaat 't met je, Rosa? ",
        "pa": "How are you doing, Rosa?",
        "vi": "Cháu thế nào, Rosa?"
    },
    {
        "start": "00:02:31,520",
        "end": "00:02:32,360",
        "en": "Ga zitten. ",
        "pa": "Have a seat.",
        "vi": "Ngồi đi."
    },
    {
        "start": "00:02:37,720",
        "end": "00:02:38,840",
        "en": "Iets te eten? ",
        "pa": "Do you want something to eat?",
        "vi": "Cháu muốn ăn gì không?"
    },
    {
        "start": "00:02:41,640",
        "end": "00:02:42,480",
        "en": "Kopje koffie? ",
        "pa": "Some coffee?",
        "vi": "Cà phê nhé?"
    },
    {
        "start": "00:02:54,960",
        "end": "00:02:56,160",
        "en": "Wil je erover praten? ",
        "pa": "Do you want to talk about it?",
        "vi": "Ta nói về chuyện đó nhé?"
    },
    {
        "start": "00:02:59,280",
        "end": "00:03:01,200",
        "en": "Vertellen wat er volgens jou gebeurde? ",
        "pa": "Say what happened according to you?",
        "vi": "Theo cháu, chuyện gì đã xảy ra?"
    },
    {
        "start": "00:03:04,160",
        "end": "00:03:06,160",
        "en": "Hoezo wat er volgens mij gebeurde? ",
        "pa": "What do you mean, what I think happened?",
        "vi": "\"Theo cháu\" nghĩa là sao?"
    },
    {
        "start": "00:03:06,720",
        "end": "00:03:09,800",
        "en": "Ik vind het belangrijk om te weten hoe jij dit heb ervaren. ",
        "pa": "It's important for me to know how you've experienced it.",
        "vi": "Tôi muốn biết cháu đã trải qua những gì."
    },
    {
        "start": "00:03:16,960",
        "end": "00:03:20,560",
        "en": "We proberen de situatie te overzien. We zijn er voor elkaar, Rosa. ",
        "pa": "We're trying to control the situation. We're here to help each other.",
        "vi": "Chúng tôi đang cố kiểm soát tình hình. Ta phải giúp đỡ nhau."
    },
    {
        "start": "00:03:23,200",
        "end": "00:03:24,720",
        "en": "We zijn nu nog aan het gissen. ",
        "pa": "We still don't have an idea.",
        "vi": "Chúng tôi vẫn không hiểu."
    },
    {
        "start": "00:03:25,520",
        "end": "00:03:27,560",
        "en": "Hester slikte medicijnen die... ",
        "pa": "Hester took medication which...",
        "vi": "Hester đã uống loại thuốc..."
    },
    {
        "start": "00:03:29,400",
        "end": "00:03:31,760",
        "en": "...verkeerd kunnen vallen in een bepaalde combinatie. ",
        "pa": "can be harmful when taken wrong.",
        "vi": "có thể gây nguy hiểm nếu uống sai cách."
    },
    {
        "start": "00:03:32,840",
        "end": "00:03:35,240",
        "en": "Een psychose, een ongeluk. ",
        "pa": "A psychosis, an accident.",
        "vi": "Do rối loạn tâm thần, một vụ tai nạn."
    },
    {
        "start": "00:03:37,120",
        "end": "00:03:38,000",
        "en": "Alweer. ",
        "pa": "Again?",
        "vi": "Lại tai nạn?"
    },
    {
        "start": "00:03:40,040",
        "end": "00:03:41,920",
        "en": "Maak jij je zorgen over je carrière? ",
        "pa": "Are you worried about your career?",
        "vi": "Cháu lo lắng cho sự nghiệp sau này?"
    },
    {
        "start": "00:03:42,800",
        "end": "00:03:43,640",
        "en": "Mijn carrière? ",
        "pa": "-My career?",
        "vi": "Sự nghiệp?"
    },
    {
        "start": "00:03:46,840",
        "end": "00:03:49,280",
        "en": "Alles blijft hier binnen deze muren, Rosa. ",
        "pa": "Everything you say will stay here.",
        "vi": "Tôi sẽ giữ kín những gì cháu nói ở đây."
    },
    {
        "start": "00:03:50,240",
        "end": "00:03:51,200",
        "en": "Fleur. ",
        "pa": "Fleur.",
        "vi": "Fleur."
    },
    {
        "start": "00:03:52,480",
        "end": "00:03:53,320",
        "en": "Fleur. ",
        "pa": "Fleur.",
        "vi": "Fleur."
    },
    {
        "start": "00:03:55,200",
        "end": "00:03:56,040",
        "en": "Wat doe je? ",
        "pa": "What are you doing?",
        "vi": "Cậu làm gì thế?"
    },
    {
        "start": "00:03:56,840",
        "end": "00:03:57,840",
        "en": "Ik heb college. ",
        "pa": "I have a class.",
        "vi": "Tôi có tiết học."
    },
    {
        "start": "00:03:58,520",
        "end": "00:04:00,400",
        "en": "Wat heb jij gezegd dat er gebeurd is? ",
        "pa": "What did you say happened?",
        "vi": "Cậu nói chuyện gì đã xảy ra?"
    },
    {
        "start": "00:04:00,960",
        "end": "00:04:02,160",
        "en": "Wat er gebeurd is? ",
        "pa": "What happened?",
        "vi": "Chuyện gì đã xảy ra?"
    },
    {
        "start": "00:04:03,280",
        "end": "00:04:06,200",
        "en": "Ze is dood. Je was erbij, toch? ",
        "pa": "She's dead. You were there, weren't you?",
        "vi": "Cô ấy đã chết. Cậu có mặt ở đó mà, không phải sao?"
    },
    {
        "start": "00:04:06,920",
        "end": "00:04:07,760",
        "en": "Jezus. ",
        "pa": "Jesus...",
        "vi": "Lạy Chúa..."
    },
    {
        "start": "00:04:09,800",
        "end": "00:04:10,640",
        "en": "Fleur. ",
        "pa": "Fleur.",
        "vi": "Fleur."
    },
    {
        "start": "00:04:16,040",
        "end": "00:04:18,360",
        "en": "Je was er toch zelf ook bij? Je zag toch hoe ze... ",
        "pa": "You were there as well. You saw how she...",
        "vi": "Cậu cũng ở đó mà. Cậu thấy cô ấy..."
    },
    {
        "start": "00:04:18,440",
        "end": "00:04:20,800",
        "en": "Ik weet niet wat ik heb gezien, Rosa. ",
        "pa": "I don't know what I saw, Rosa.",
        "vi": "Tôi chẳng biết mình thấy gì nữa, Rosa."
    },
    {
        "start": "00:04:55,120",
        "end": "00:04:56,720",
        "en": "Nee... ",
        "pa": "No. No...",
        "vi": "Không. Không..."
    },
    {
        "start": "00:04:59,200",
        "end": "00:05:00,200",
        "en": "Fleur. ",
        "pa": "Fleur.",
        "vi": "Fleur."
    },
    {
        "start": "00:05:02,720",
        "end": "00:05:04,200",
        "en": "Niet kijken, niet kijken, niet kijken. ",
        "pa": "Don't look. Don't look.",
        "vi": "Đừng nhìn. Đừng."
    },
    {
        "start": "00:05:08,440",
        "end": "00:05:10,200",
        "en": "Haal haar weg. ",
        "pa": "Get her out of here.",
        "vi": "Đưa em ấy ra khỏi đây."
    },
    {
        "start": "00:05:10,760",
        "end": "00:05:12,320",
        "en": "Fleur. Niet kijken. ",
        "pa": "Fleur, don't look.",
        "vi": "Fleur, đừng nhìn."
    },
    {
        "start": "00:05:12,400",
        "end": "00:05:16,480",
        "en": "Nee, nee. ",
        "pa": "No... No, no.",
        "vi": "Không thể nào... Không, không."
    },
    {
        "start": "00:05:30,520",
        "end": "00:05:32,680",
        "en": "Jongens, ik wil jullie hier weg hebben. Allemaal weg. ",
        "pa": "I want you all out of here!",
        "vi": "Tất cả ra khỏi đây!"
    },
    {
        "start": "00:05:33,120",
        "end": "00:05:34,520",
        "en": "Iedereen hier weg, nu. ",
        "pa": "Everybody out, now.",
        "vi": "Tất cả ra ngoài ngay."
    },
    {
        "start": "00:05:35,000",
        "end": "00:05:36,000",
        "en": "Nee... ",
        "pa": "No...",
        "vi": "Không..."
    },
    {
        "start": "00:05:36,080",
        "end": "00:05:36,920",
        "en": "Kom. ",
        "pa": "Come.",
        "vi": "Đi thôi."
    },
    {
        "start": "00:05:37,000",
        "end": "00:05:37,840",
        "en": "Nee. ",
        "pa": "No.",
        "vi": "Không."
    },
    {
        "start": "00:05:42,680",
        "end": "00:05:44,360",
        "en": "Dit is toch geen toeval meer? ",
        "pa": "This can't be a coincidence.",
        "vi": "Đây không thể nào là trùng hợp."
    },
    {
        "start": "00:05:46,040",
        "end": "00:05:47,040",
        "en": "Arme Fleur. ",
        "pa": "Poor Fleur.",
        "vi": "Tội nghiệp Fleur."
    },
    {
        "start": "00:05:48,160",
        "end": "00:05:52,040",
        "en": "We hadden d'r tegen moeten houden, maar ik zag dit niet aankomen. ",
        "pa": "We should have stopped her, but I didn't see it coming. I think that...",
        "vi": "Đúng ra ta phải ngăn cậu ấy lại, nhưng chính tôi cũng không ngờ.\n Tôi nghĩ..."
    },
    {
        "start": "00:06:02,680",
        "end": "00:06:05,440",
        "en": "Ik vind hem geen type om zoiets te doen. ",
        "pa": "Why would he do something like this?",
        "vi": "Sao anh ấy lại làm chuyện đó?"
    },
    {
        "start": "00:06:05,520",
        "end": "00:06:08,120",
        "en": "Ik heb nooit iets aan hem gemerkt. ",
        "pa": "I've never noticed anything about him.",
        "vi": "Tôi chẳng thấy anh ấy có gì khác lạ."
    },
    {
        "start": "00:06:08,800",
        "end": "00:06:09,640",
        "en": "Jij wel? ",
        "pa": "Did you?",
        "vi": "Còn cậu?"
    },
    {
        "start": "00:06:10,000",
        "end": "00:06:11,000",
        "en": "Sorry? ",
        "pa": "Sorry?",
        "vi": "Cậu nói gì?"
    },
    {
        "start": "00:06:11,600",
        "end": "00:06:14,640",
        "en": "Ik wil even liggen. ",
        "pa": "I'd like to lie down a bit.",
        "vi": "Tôi muốn ngả lưng một chút."
    },
    {
        "start": "00:06:15,200",
        "end": "00:06:17,680",
        "en": "Oké. Wil je dat ik blijf? ",
        "pa": "Okay. Would you like me to stay?",
        "vi": "Được. Có cần tôi ở lại cùng không?"
    },
    {
        "start": "00:06:18,880",
        "end": "00:06:20,760",
        "en": "Nee, thanks. Ik wil even alleen zijn. ",
        "pa": "No, I'd like to be alone.",
        "vi": "Không, tôi muốn ở một mình."
    },
    {
        "start": "00:06:21,080",
        "end": "00:06:23,000",
        "en": "Eh, als je wil, ik ben er gewoon, hè? ",
        "pa": "I'm here for you if you need me.",
        "vi": "Nếu cần thì tôi sẽ giúp."
    },
    {
        "start": "00:06:23,080",
        "end": "00:06:23,920",
        "en": "Ja, fijn. ",
        "pa": "Thank you.",
        "vi": "Cảm ơn."
    },
    {
        "start": "00:07:00,000",
        "end": "00:07:01,400",
        "en": "Hé, pap. Met mij. ",
        "pa": "Hey, Dad, it's me.",
        "vi": "Bố, là con đây."
    },
    {
        "start": "00:07:02,480",
        "end": "00:07:05,840",
        "en": "Eh, je neemt steeds niet op. Ik begrijp het dat je boos bent. ",
        "pa": "You must not answer. I know that you're angry.",
        "vi": "Bố không cần trả lời. Con biết bố đang giận."
    },
    {
        "start": "00:07:07,200",
        "end": "00:07:09,000",
        "en": "Maar bel me even terug, alsjeblieft. ",
        "pa": "But call me back, please.",
        "vi": "Nhưng làm ơn gọi lại cho con."
    },
    {
        "start": "00:07:14,760",
        "end": "00:07:16,960",
        "en": "Hé, pap. Met mij nog een keer. ",
        "pa": "Hey, Dad, it's me again.",
        "vi": "Bố, lại là con đây."
    },
    {
        "start": "00:07:18,040",
        "end": "00:07:19,560",
        "en": "Bel me terug, alsjeblieft. ",
        "pa": "Call me back, please.",
        "vi": "Làm ơn gọi lại cho con."
    },
    {
        "start": "00:08:05,600",
        "end": "00:08:07,360",
        "en": "Rosa. -Hé, man. ",
        "pa": "-Rosa! -Hey, man.",
        "vi": "- Rosa! - Chào anh."
    },
    {
        "start": "00:09:49,280",
        "end": "00:09:50,520",
        "en": "Je bent thuis. ",
        "pa": "You're home.",
        "vi": "Con về rồi à?"
    },
    {
        "start": "00:09:53,320",
        "end": "00:09:54,160",
        "en": "Ja. ",
        "pa": "Yes.",
        "vi": "Vâng."
    },
    {
        "start": "00:09:57,440",
        "end": "00:09:58,440",
        "en": "Hoef je niks? ",
        "pa": "Don't you want some?",
        "vi": "Có muốn ăn không?"
    },
    {
        "start": "00:10:00,480",
        "end": "00:10:01,640",
        "en": "Ik heb al gegeten. ",
        "pa": "I've already eaten.",
        "vi": "Con ăn rồi."
    },
    {
        "start": "00:10:02,360",
        "end": "00:10:03,200",
        "en": "Ben je ziek? ",
        "pa": "Are you ill?",
        "vi": "Con bệnh à?"
    },
    {
        "start": "00:10:03,880",
        "end": "00:10:04,720",
        "en": "Nee. ",
        "pa": "No.",
        "vi": "Không."
    },
    {
        "start": "00:10:05,520",
        "end": "00:10:06,520",
        "en": "Je ziet er ziek uit. ",
        "pa": "You look ill.",
        "vi": "Trông con không khỏe."
    },
    {
        "start": "00:10:09,320",
        "end": "00:10:10,640",
        "en": "Ik ben alleen 'n beetje misselijk. ",
        "pa": "I'm just a bit nauseated.",
        "vi": "Con hơi buồn nôn."
    },
    {
        "start": "00:10:13,520",
        "end": "00:10:14,520",
        "en": "Gaat wel weer over. ",
        "pa": "It'll pass.",
        "vi": "Sẽ hết thôi."
    },
    {
        "start": "00:10:28,320",
        "end": "00:10:29,320",
        "en": "Hoe is het met 'r? ",
        "pa": "How is she doing?",
        "vi": "Mẹ sao rồi ạ?"
    },
    {
        "start": "00:10:30,720",
        "end": "00:10:31,960",
        "en": "Interesseert dat je? ",
        "pa": "Do you really want to know?",
        "vi": "Con thật lòng muốn biết?"
    },
    {
        "start": "00:10:35,080",
        "end": "00:10:35,920",
        "en": "Ik wil d'r zien. ",
        "pa": "I want to see her.",
        "vi": "Con muốn gặp mẹ."
    },
    {
        "start": "00:10:36,480",
        "end": "00:10:37,760",
        "en": "Dat gaat niet gebeuren. ",
        "pa": "That's not going to happen.",
        "vi": "Không được đâu."
    },
    {
        "start": "00:10:37,840",
        "end": "00:10:38,680",
        "en": "Want? ",
        "pa": "Because?",
        "vi": "Lý do?"
    },
    {
        "start": "00:10:40,400",
        "end": "00:10:42,200",
        "en": "Want je moeder is eindelijk stabiel. ",
        "pa": "Your mother is finally stable.",
        "vi": "Mãi mẹ con mới ổn định lại."
    },
    {
        "start": "00:10:43,520",
        "end": "00:10:44,600",
        "en": "Ze heeft rust nodig. ",
        "pa": "She needs calmness.",
        "vi": "Bà ấy cần bình tĩnh."
    },
    {
        "start": "00:10:45,440",
        "end": "00:10:47,920",
        "en": "Je kunt niet zomaar binnen komen vallen wanneer het jou uitkomt. ",
        "pa": "You can't just come here whenever you please.",
        "vi": "Con không thể cứ thích là về nhà được."
    },
    {
        "start": "00:10:48,000",
        "end": "00:10:50,440",
        "en": "Jij kan mij niet verbieden wanneer ik haar mag zien. ",
        "pa": "You can't tell me if I can see her or not.",
        "vi": "Bố không có quyền không cho con gặp mẹ."
    },
    {
        "start": "00:10:55,640",
        "end": "00:10:59,320",
        "en": "Zolang jij je nieuwe vrienden belangrijker vindt dan thuis... ",
        "pa": "As long as your new friends are more important than your home...",
        "vi": "Nếu con vẫn còn coi trọng bạn mới của con hơn cái nhà này..."
    },
    {
