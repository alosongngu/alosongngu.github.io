[
    {
        "start": "00:00:06,040",
        "end": "00:00:09,720",
        "en": "EEN NETFLIX ORIGINAL-SERIE",
        "pa": "",
        "vi": "LOẠT PHIM CỦA NETFLIX"
    },
    {
        "start": "00:00:24,400",
        "end": "00:00:25,240",
        "en": "Ja? ",
        "pa": "Yes?",
        "vi": "Sao?"
    },
    {
        "start": "00:00:27,520",
        "end": "00:00:30,520",
        "en": "Bij haar moeder? Hoelang zijn ze daar gebleven? ",
        "pa": "With her mother? How long were they there?",
        "vi": "Với mẹ con bé? Chúng ở đó bao lâu?"
    },
    {
        "start": "00:00:33,080",
        "end": "00:00:34,920",
        "en": "Bel me als ze weer bij Ares zijn. ",
        "pa": "Call me when they're back at Ares.",
        "vi": "Khi nào chúng về Ares thì gọi tôi."
    },
    {
        "start": "00:01:34,800",
        "end": "00:01:35,640",
        "en": "Ben je wakker? ",
        "pa": "Are you awake?",
        "vi": "Bà dậy chưa?"
    },
    {
        "start": "00:01:39,200",
        "end": "00:01:40,600",
        "en": "Gij zijt bang. ",
        "pa": "You're scared.",
        "vi": "Cậu đang sợ."
    },
    {
        "start": "00:01:41,480",
        "end": "00:01:42,960",
        "en": "Ik kan 't ruiken. ",
        "pa": "I can smell it.",
        "vi": "Ta có thể ngửi thấy."
    },
    {
        "start": "00:01:44,760",
        "end": "00:01:49,200",
        "en": "Er is weer een gekozene gestorven. ",
        "pa": "Another chosen one has died.",
        "vi": "Một người được chọn nữa đã chết."
    },
    {
        "start": "00:01:50,200",
        "end": "00:01:51,160",
        "en": "Een jongen. ",
        "pa": "A boy.",
        "vi": "Một chàng trai."
    },
    {
        "start": "00:01:52,280",
        "end": "00:01:53,320",
        "en": "Tweeëntwintig. ",
        "pa": "Twenty-two years old.",
        "vi": "Hai mươi hai tuổi."
    },
    {
        "start": "00:01:54,120",
        "end": "00:01:55,120",
        "en": "Onschuldig. ",
        "pa": "Innocent.",
        "vi": "Vô tội."
    },
    {
        "start": "00:01:57,800",
        "end": "00:02:00,480",
        "en": "Onschuldig siet gie lange niet meer. ",
        "pa": "You're no longer innocent.",
        "vi": "Cậu thì không còn vô tội nữa."
    },
    {
        "start": "00:02:35,360",
        "end": "00:02:38,880",
        "en": "Gie zijt zo gevoelig, mijne jongen. ",
        "pa": "You're so sensitive, my boy.",
        "vi": "Cậu nhạy cảm quá rồi đấy."
    },
    {
        "start": "00:02:48,320",
        "end": "00:02:50,440",
        "en": "Het kwaad blijft boos. ",
        "pa": "That Roos is angry.",
        "vi": "Cô bé Roos đó đang giận dữ."
    },
    {
        "start": "00:02:51,200",
        "end": "00:02:54,160",
        "en": "Er moge vrouwelijk bloede op de troon. ",
        "pa": "She should be the one on the throne.",
        "vi": "Cô ta mới là người được ngồi trên ngai vàng."
    },
    {
        "start": "00:02:54,240",
        "end": "00:02:55,760",
        "en": "Nieuw bloede. ",
        "pa": "New blood.",
        "vi": "Một dòng máu mới."
    },
    {
        "start": "00:02:56,160",
        "end": "00:02:57,920",
        "en": "Ze is er nog niet klaar voor. ",
        "pa": "She's not ready for that.",
        "vi": "Nó chưa sẵn sàng."
    },
    {
        "start": "00:02:58,040",
        "end": "00:03:00,160",
        "en": "Dat bepale gie niet. ",
        "pa": "You don't decide that.",
        "vi": "Chuyện đó không phải do cậu quyết định."
    },
    {
        "start": "00:03:03,360",
        "end": "00:03:04,600",
        "en": "Maurits. ",
        "pa": "Maurits...",
        "vi": "Maurits..."
    },
    {
        "start": "00:03:06,280",
        "end": "00:03:08,320",
        "en": "Gie moet doen wat ik ha dien... ",
        "pa": "you'll do what you have to do...",
        "vi": "cậu sẽ làm những gì phải làm..."
    },
    {
        "start": "00:03:08,880",
        "end": "00:03:10,360",
        "en": "...om te overleven. ",
        "pa": "to survive.",
        "vi": "để tồn tại."
    },
    {
        "start": "00:03:15,160",
        "end": "00:03:16,120",
        "en": "En dat is? ",
        "pa": "Which is?",
        "vi": "Đó là gì?"
    },
    {
        "start": "00:03:18,960",
        "end": "00:03:21,920",
        "en": "Met je tijd meegaan. ",
        "pa": "Adapt to the times.",
        "vi": "Thích ứng với thời đại."
    },
    {
        "start": "00:03:51,240",
        "end": "00:03:53,800",
        "en": "Denk na, Steenwijk. Wat wil je doen? ",
        "pa": "Think about it, Steenwijk. What do you want to do?",
        "vi": "Nghĩ thử xem, Steenwijk. Cậu muốn làm gì?"
    },
    {
        "start": "00:03:57,960",
        "end": "00:03:59,520",
        "en": "We moeten voorzichtig zijn. ",
        "pa": "We have to be careful.",
        "vi": "Ta phải cẩn thận."
    },
    {
        "start": "00:04:00,640",
        "end": "00:04:02,240",
        "en": "Steenwijk, laat 't aan mij over. ",
        "pa": "Leave it to me, Steenwijk.",
        "vi": "Steenwijk, để tôi."
    },
    {
        "start": "00:04:03,640",
        "end": "00:04:04,480",
        "en": "Rosa. ",
        "pa": "Rosa!",
        "vi": "Rosa!"
    },
    {
        "start": "00:04:14,920",
        "end": "00:04:16,240",
        "en": "Hé, ben je d'r weer? ",
        "pa": "Hey, you're back?",
        "vi": "Về rồi đấy à?"
    },
    {
        "start": "00:04:16,320",
        "end": "00:04:17,160",
        "en": "Ja. ",
        "pa": "Yes.",
        "vi": "Ừ."
    },
    {
        "start": "00:04:17,240",
        "end": "00:04:18,960",
        "en": "Heb jij Jacob gezien? ",
        "pa": "Have you seen Jacob?",
        "vi": "Cậu có thấy Jacob không?"
    },
    {
        "start": "00:04:19,720",
        "end": "00:04:20,560",
        "en": "Nee. ",
        "pa": "No.",
        "vi": "Không."
    },
    {
        "start": "00:04:20,920",
        "end": "00:04:23,320",
        "en": "Al uren niet. Ik dacht dat hij misschien bij jou was. ",
        "pa": "Not for hours. I thought he was with you.",
        "vi": "Mấy tiếng đồng hồ rồi. Tôi tưởng cậu ấy đi với cậu."
    },
    {
        "start": "00:04:24,480",
        "end": "00:04:26,520",
        "en": "Ik ga even naar mijn kamer. -Ja. ",
        "pa": "-I'm going to my room. -Okay.",
        "vi": "- Tôi về phòng đây. - Ừ."
    },
    {
        "start": "00:04:32,040",
        "end": "00:04:32,920",
        "en": "Ze is met mij. ",
        "pa": "She's with me.",
        "vi": "Cậu ấy đi với tôi."
    },
    {
        "start": "00:04:34,360",
        "end": "00:04:35,360",
        "en": "Is dat zo? ",
        "pa": "Is that right?",
        "vi": "Vậy sao?"
    },
    {
        "start": "00:04:36,040",
        "end": "00:04:36,880",
        "en": "Ja. ",
        "pa": "Yes.",
        "vi": "Phải."
    },
    {
        "start": "00:04:40,160",
        "end": "00:04:41,320",
        "en": "Waar is dit allemaal voor? ",
        "pa": "What's this for?",
        "vi": "Bày thế này làm gì?"
    },
    {
        "start": "00:04:44,240",
        "end": "00:04:45,600",
        "en": "Orders van hogere hand. ",
        "pa": "Orders from above.",
        "vi": "Lệnh của cấp trên."
    },
    {
        "start": "00:04:47,480",
        "end": "00:04:50,080",
        "en": "De alumni komen dineren vanavond. ",
        "pa": "The alumni are dining here this evening.",
        "vi": "Các cựu thành viên sẽ ăn tối ở đây."
    },
    {
        "start": "00:04:52,520",
        "end": "00:04:53,480",
        "en": "Welke? ",
        "pa": "Which ones?",
        "vi": "Những ai?"
    },
    {
        "start": "00:04:53,560",
        "end": "00:04:54,400",
        "en": "Iedereen. ",
        "pa": "All of them.",
        "vi": "Tất cả họ."
    },
    {
        "start": "00:04:56,480",
        "end": "00:04:58,440",
        "en": "Ze gaan een nieuwe kandidaat uitkiezen. ",
        "pa": "They will choose a new candidate.",
        "vi": "Họ sẽ chọn một ứng cử viên mới."
    },
    {
        "start": "00:05:02,480",
        "end": "00:05:04,200",
        "en": "Ik hoop niet dat ik morgen aan een balk hang. ",
        "pa": "I hope I'm not hanging from a beam tomorrow.",
        "vi": "Hi vọng ngày mai tôi không treo cổ."
    },
    {
        "start": "00:05:06,200",
        "end": "00:05:08,320",
        "en": "Alsof jij president wordt, lamlul. ",
        "pa": "Like you will be president, idiot.",
        "vi": "Làm như cậu là tân chủ tịch ấy."
    },
    {
        "start": "00:06:24,520",
        "end": "00:06:25,720",
        "en": "Ze is terug. ",
        "pa": "She's back.",
        "vi": "Cậu ta đã quay lại."
    },
    {
        "start": "00:06:28,640",
        "end": "00:06:30,720",
        "en": "En ze doet alsof er niks aan de hand is. ",
        "pa": "And she pretends like nothing is going on.",
        "vi": "Và vờ như không có gì xảy ra."
    },
    {
        "start": "00:06:38,120",
        "end": "00:06:40,000",
        "en": "Je laat het niet zo maar gebeuren. ",
        "pa": "Don't just let that happen.",
        "vi": "Đừng để chuyện đó tiếp diễn."
    },
    {
        "start": "00:06:43,120",
        "end": "00:06:44,320",
        "en": "Ach, arm mens. ",
        "pa": "Poor thing.",
        "vi": "Tội nghiệp cậu."
    },
    {
        "start": "00:06:46,160",
        "end": "00:06:47,440",
        "en": "Gaat het wel? ",
        "pa": "Are you okay?",
        "vi": "Cậu ổn chứ?"
    },
    {
        "start": "00:08:07,800",
        "end": "00:08:08,880",
        "en": "Ik ben er bijna. ",
        "pa": "I'm almost there.",
        "vi": "Sắp đến rồi."
    },
    {
        "start": "00:08:10,760",
        "end": "00:08:11,880",
        "en": "Ik voel het. ",
        "pa": "I can feel it.",
        "vi": "Tớ cảm nhận được."
    },
    {
        "start": "00:08:16,000",
        "end": "00:08:18,320",
        "en": "Ze hebben geprobeerd het van ons weg te houden. ",
        "pa": "They tried to keep it from us.",
        "vi": "Họ đã cố gắng giấu chúng ta."
    },
    {
        "start": "00:08:18,920",
        "end": "00:08:20,200",
        "en": "Maar het riep mij. ",
        "pa": "But it called me.",
        "vi": "Nhưng nó gọi tớ."
    },
    {
        "start": "00:08:20,600",
        "end": "00:08:21,920",
        "en": "Het vroeg mij om hulp. ",
        "pa": "It asked me for help.",
        "vi": "Nó nhờ tớ giúp đỡ."
    },
    {
        "start": "00:08:26,200",
        "end": "00:08:28,520",
        "en": "Vanavond ga ik het bevrijden. ",
        "pa": "I'll set it free this evening.",
        "vi": "Tối nay tớ sẽ thả tự do cho nó."
    },
    {
        "start": "00:08:30,840",
        "end": "00:08:32,080",
        "en": "Je bedoelt Beal? ",
        "pa": "Do you mean Beal?",
        "vi": "Cậu đang nói về Beal?"
    },
    {
        "start": "00:08:41,120",
        "end": "00:08:43,640",
        "en": "Vanavond gaan we je bevrijden, Beal. ",
        "pa": "This evening we will set you free, Beal.",
        "vi": "Beal, tối nay ngươi sẽ được tự do."
    },
    {
        "start": "00:08:46,320",
        "end": "00:08:47,160",
        "en": "Jacob... ",
        "pa": "Jacob...",
        "vi": "Jacob..."
    },
    {
        "start": "00:08:49,040",
        "end": "00:08:50,120",
        "en": "...we gaan naar huis. ",
        "pa": "we're going home.",
        "vi": "ta về nhà thôi."
    },
    {
        "start": "00:08:55,120",
        "end": "00:08:57,280",
        "en": "Hebben ze jou hier gestuurd om mij te halen? ",
        "pa": "Did they send you to get me?",
        "vi": "Họ cử cậu đến đưa tớ đi à?"
    },
    {
        "start": "00:08:57,360",
        "end": "00:08:58,760",
        "en": "Hè? -Nee. ",
        "pa": "-Hm? -No.",
        "vi": "Không."
    },
    {
        "start": "00:08:58,840",
        "end": "00:08:59,800",
        "en": "Hè? ",
        "pa": "What?",
        "vi": "Sao?"
    },
    {
        "start": "00:08:59,880",
        "end": "00:09:03,160",
        "en": "Niemand heeft mij gestuurd. Kom, we gaan hier samen weg, oké? ",
        "pa": "Nobody sent me. Let's leave this place together.",
        "vi": "Không ai cử tớ đến cả. Ta sẽ cùng nhau rời khỏi nơi này."
    },
    {
        "start": "00:09:11,000",
        "end": "00:09:11,840",
        "en": "Jacob. ",
        "pa": "Jacob.",
        "vi": "Jacob."
    },
    {
        "start": "00:09:32,200",
        "end": "00:09:33,800",
        "en": "Jacob. ",
        "pa": "Jacob!",
        "vi": "Jacob!"
    },
    {
        "start": "00:10:26,280",
        "end": "00:10:27,280",
        "en": "Wat doe je? ",
        "pa": "What are you doing?",
        "vi": "Cậu làm gì thế?"
    },
    {
        "start": "00:10:30,880",
        "end": "00:10:31,720",
        "en": "Wat is er? ",
        "pa": "What's wrong?",
        "vi": "Chuyện gì vậy?"
    },
    {
        "start": "00:10:54,200",
        "end": "00:10:55,640",
        "en": "Dank je wel, Leopold. ",
        "pa": "Thank you, Leopold.",
        "vi": "Cảm ơn, Leopold."
    },
    {
        "start": "00:10:56,400",
        "end": "00:10:57,680",
        "en": "Dan zien we je vanavond. ",
        "pa": "We'll see you this evening.",
        "vi": "Hẹn gặp ông tối nay."
    },
    {
        "start": "00:10:58,440",
        "end": "00:10:59,440",
        "en": "Ja. ",
        "pa": "Okay.",
        "vi": "Được."
    },
    {
        "start": "00:10:59,520",
        "end": "00:11:00,360",
        "en": "Dag. ",
        "pa": "Bye.",
        "vi": "Chào ông."
    },
    {
        "start": "00:11:03,000",
        "end": "00:11:04,080",
        "en": "Dat was de laatste. ",
        "pa": "That was the last one.",
        "vi": "Đó là người cuối cùng."
    },
    {
        "start": "00:11:05,360",
        "end": "00:11:06,480",
        "en": "Iedereen komt. ",
        "pa": "Everybody will be there.",
        "vi": "Tất cả sẽ có mặt."
    },
    {
        "start": "00:11:06,920",
        "end": "00:11:10,600",
        "en": "Ze willen allemaal weten wie het wordt. Daar ben ik ook wel benieuwd naar. ",
        "pa": "They all want to know who it'll be. I'm also curious about that.",
        "vi": "Ai cũng muốn biết người đó là ai. Cả tôi cũng tò mò."
    },
    {
        "start": "00:11:11,400",
        "end": "00:11:14,240",
        "en": "Het is cruciaal dat de volgende president de juiste wordt. ",
        "pa": "It's crucial that the next president is the right one.",
        "vi": "Quan trọng là phải chọn đúng người lên làm tân chủ tịch."
    },
    {
        "start": "00:11:15,920",
        "end": "00:11:17,480",
        "en": "Borms heeft 't alleen maar... ",
        "pa": "Borms only made her...",
        "vi": "Borms chỉ khiến bà ấy..."
    },
    {
        "start": "00:11:18,400",
        "end": "00:11:19,560",
        "en": "...bozer gemaakt. ",
        "pa": "angrier.",
        "vi": "tức giận hơn thôi."
    },
    {
        "start": "00:11:21,720",
        "end": "00:11:24,920",
        "en": "Volgens gebruik is Roderick van Hal de volgende kandidaat. ",
        "pa": "According to custom, Roderick van Hal is the next candidate.",
        "vi": "Theo tục lệ, Roderick van Hal là ứng viên tiếp theo."
    },
    {
        "start": "00:11:27,440",
        "end": "00:11:30,160",
        "en": "Toen in '53 de dijk brak. ",
        "pa": "When the dike broke in 1953,",
        "vi": "Hồi con đê bị vỡ năm 1953,"
    },
    {
        "start": "00:11:30,240",
        "end": "00:11:32,320",
        "en": "Achttienhonderd zesendertig doden... ",
        "pa": "1836 deaths,",
        "vi": "1.836 người chết,"
    },
    {
        "start": "00:11:32,400",
        "end": "00:11:35,400",
        "en": "...tienduizenden dieren, honderdduizend huizen... ",
        "pa": "thousands of animals and tens of thousands of houses...",
        "vi": "hàng ngàn con vật và mấy chục ngàn căn nhà..."
    },
    {
        "start": "00:11:36,080",
        "end": "00:11:38,320",
        "en": "...tweehonderdduizend hectare grond. ",
        "pa": "200,000 hectares of land...",
        "vi": "200.000 héc-ta đất..."
    },
    {
        "start": "00:11:39,040",
        "end": "00:11:40,680",
        "en": "Alles en iedereen onder water. ",
        "pa": "everything and everybody underwater.",
        "vi": "mọi người, mọi thứ chìm trong nước."
    },
    {
        "start": "00:11:43,360",
        "end": "00:11:44,520",
        "en": "En weet jij waarom? ",
        "pa": "Do you know why?",
        "vi": "Cậu biết lý do chứ?"
    },
    {
        "start": "00:11:45,760",
        "end": "00:11:46,680",
        "en": "Waarom? ",
        "pa": "Why?",
        "vi": "Là gì?"
    },
    {
        "start": "00:11:49,320",
        "end": "00:11:51,000",
        "en": "Omdat we niet vooruitdachten. ",
        "pa": "Because we didn't think ahead.",
        "vi": "Vì ta không nghĩ xa."
    },
    {
        "start": "00:11:52,360",
        "end": "00:11:54,040",
        "en": "We gingen niet mee met onze tijd. ",
        "pa": "We didn't adapt to our times.",
        "vi": "Ta không thích ứng với thời đại."
    },
    {
        "start": "00:12:00,360",
        "end": "00:12:02,000",
        "en": "Het wordt met de dag onrustiger. ",
        "pa": "It's getting more restless by the day.",
        "vi": "Thứ đó không chịu dừng lại."
    },
    {
        "start": "00:12:03,760",
        "end": "00:12:05,720",
        "en": "Of denk je dat die zelfmoorden toeval zijn? ",
        "pa": "Or do you think those suicides are a coincidence?",
        "vi": "Chứ cậu nghĩ mấy vụ tự tử là tình cờ à?"
    },
    {
        "start": "00:12:07,480",
        "end": "00:12:10,240",
        "en": "Het is mijn taak om het weer te kalmeren voor het uit de hand loopt. ",
        "pa": "It's my job to calm it down before it gets out of control.",
