    {
        "start": "00:11:25,280",
        "end": "00:11:26,200",
        "en": "Kom.",
        "pa": "",
        "vi": "Mau lên."
    },
    {
        "start": "00:11:28,680",
        "end": "00:11:30,200",
        "en": "-Kom nu. Kom. - Josefine!",
        "pa": "Over here. Just come.",
        "vi": "Qua đây với tao đi."
    },
    {
        "start": "00:11:33,400",
        "end": "00:11:35,440",
        "en": "Vi skal køre. Kom nu.",
        "pa": "We have to go, Kee-ko. Come on.",
