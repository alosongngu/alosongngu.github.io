    },
    {
        "start": "00:02:22,960",
        "end": "00:02:23,960",
        "en": "Hjælp!",
        "pa": "Help!",
        "vi": "Cứu tôi với!"
    },
    {
        "start": "00:02:25,920",
        "end": "00:02:27,120",
        "en": "Hjælp!",
        "pa": "Help!",
        "vi": "Cứu tôi!"
    },
    {
