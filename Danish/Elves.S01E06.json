    },
    {
        "start": "00:11:44,160",
        "end": "00:11:45,560",
        "en": "Vi er sammen.",
        "pa": "We're the same.",
        "vi": "Ta mãi ở bên nhau."
    },
    {
        "start": "00:12:33,200",
        "end": "00:12:34,400",
        "en": "Er du okay, Liv?",
        "pa": "Liv, are you okay?",
        "vi": "Cậu ổn chứ, Liv?"
    },
    {
        "start": "00:12:35,240",
        "end": "00:12:36,800",
        "en": "Jos, er du okay?",
        "pa": "Jose, are you okay?",
        "vi": "Jose, em ổn chứ hả?"
    },
    {
        "start": "00:12:43,080",
        "end": "00:12:44,840",
        "en": "-Der er en på bilen. - Hvor er den?",
        "pa": "-One of them is on the car! -Where is it?",
        "vi": "- Một con lên xe rồi. - Ở đâu?"
    },
    {
        "start": "00:12:47,000",
        "end": "00:12:48,360",
        "en": "Den er på siden.",
        "pa": "It's on the side. It's on the si--",
        "vi": "Bên hông xe."
    },
    {
        "start": "00:12:49,640",
        "end": "00:12:51,040",
        "en": "Hvad nu? Møller, gør noget.",
        "pa": "Now what? Møller, what do we do?\n - It's coming!",
        "vi": "Sao giờ? Làm gì đi bác Møller!"
    },
    {
        "start": "00:12:55,040",
        "end": "00:12:56,160",
        "en": "Hold fast!",
        "pa": "Hold on tight.",
        "vi": "Ngồi chắc nhé."
    },
    {
        "start": "00:13:02,400",
        "end": "00:13:03,520",
        "en": "Er den væk?",
        "pa": "Is it gone?",
        "vi": "Nó rơi ra chưa?"
    },
    {
        "start": "00:13:04,120",
        "end": "00:13:06,400",
        "en": "- Jeg tror, jeg fik den af. -Jeg tror det.",
        "pa": "-I think I got it. -I think it's gone.",
        "vi": "- Có vẻ rơi rồi. - Cháu không nghĩ vậy."
    },
    {
        "start": "00:13:10,760",
        "end": "00:13:12,320",
        "en": "Kasper!",
        "pa": "- Pull her in, Kasper.",
        "vi": "Kéo con bé vào đi, Kasper."
    },
    {
        "start": "00:13:12,400",
        "end": "00:13:13,520",
        "en": "Liv!",
        "pa": "Liv! Liv! Liv!",
        "vi": "Liv!"
    },
    {
        "start": "00:13:13,600",
        "end": "00:13:15,240",
        "en": "Liv, hold fast!",
        "pa": "-Hold on! Hold on!",
        "vi": "Liv, đợi đã!"
    },
    {
        "start": "00:13:16,240",
        "end": "00:13:17,280",
        "en": "Hold fast, Liv!",
        "pa": "Hold on, Liv!",
        "vi": "Cố lên, Liv!"
    },
    {
        "start": "00:13:17,840",
        "end": "00:13:19,600",
        "en": "Liv, hold fast.",
        "pa": "- Liv! - Liv, hold on.",
        "vi": "Liv, bám chắc vào."
    },
    {
        "start": "00:13:21,200",
        "end": "00:13:22,800",
        "en": "Kasper. Kasper, her.",
        "pa": "Hold on!\n Kasper. Kasper, here!",
        "vi": "Cố bám chắc vào Liv!\n Anh Kasper. Đây!"
    },
    {
        "start": "00:13:23,960",
        "end": "00:13:25,720",
        "en": "Det er en kvægstav. Brug den.",
        "pa": "-What's that? -A cattle prod. You can use it.",
        "vi": "Cần sốc điện đấy. Dùng nó đi."
    },
    {
        "start": "00:13:29,040",
        "end": "00:13:31,000",
        "en": "- Hold fast! -Liv!",
        "pa": "- Hold on!\n -Liv!\n -Liv.",
        "vi": "- Bám chắc vào! - Liv!"
    },
    {
        "start": "00:13:43,000",
        "end": "00:13:44,440",
        "en": "Liv, er du okay?",
        "pa": "-Liv! Liv, are you okay? - Yeah.",
        "vi": "Liv, cháu ổn chứ?"
    },
    {
        "start": "00:13:44,520",
        "end": "00:13:46,240",
        "en": "- Kig på mig. -Jeg er okay.",
        "pa": "-Look at me. -Yes, I'm okay.",
        "vi": "- Nhìn bác đi. - Vâng, ổn ạ."
    },
    {
        "start": "00:13:46,320",
        "end": "00:13:48,320",
        "en": "Godt. Fik du den af?",
        "pa": "Good. Is it gone?",
        "vi": "Tốt. Cháu đuổi được nó đi chưa?"
    },
    {
        "start": "00:13:48,400",
        "end": "00:13:49,960",
        "en": "- Jeg ved det ikke. -Er den af?",
        "pa": "-I don't know. -Is it?",
        "vi": "- Cháu chịu. - Nó đi chưa?"
    },
    {
        "start": "00:13:50,040",
        "end": "00:13:51,640",
        "en": "-Jeg ved det ikke! -Kig efter den.",
        "pa": "-I have no idea! -Look for it!",
        "vi": "- Không biết. - Kiểm tra đi."
    },
    {
        "start": "00:14:02,040",
        "end": "00:14:03,480",
        "en": "Hold fast!",
        "pa": "- Hold on.\n -Hold on! - Møller!",
        "vi": "Ngồi chắc nhé!"
    },
    {
        "start": "00:14:22,680",
        "end": "00:14:23,680",
        "en": "Jose.",
        "pa": "Jose.",
        "vi": "Jose."
    },
    {
        "start": "00:14:24,360",
        "end": "00:14:26,360",
        "en": "Hallo. Jose.",
        "pa": "Hey. Jose.",
        "vi": "Này. Jose."
    },
    {
        "start": "00:14:27,000",
        "end": "00:14:31,320",
        "en": "Jose. Er du okay? Hallo. Jose. Er du okay?",
        "pa": "Jose. Are you okay?\n Hello. Jose, are you okay?",
        "vi": "Jose. Em có sao không?\n Này. Jose, em ổn chứ?"
    },
    {
        "start": "00:14:32,440",
        "end": "00:14:33,920",
        "en": "Du er okay.",
        "pa": "You're okay. You're okay.",
        "vi": "Giờ em ổn rồi."
    },
    {
        "start": "00:14:56,160",
        "end": "00:14:57,560",
        "en": "Jose. Kom.",
        "pa": "Jose. Let's go!\n Jose. We have to get out of here!\n Jose!",
        "vi": "Jose. Đi thôi."
    },
    {
        "start": "00:15:09,760",
        "end": "00:15:11,720",
        "en": "Kom nu, mand.",
        "pa": "Come on, you stupid thing. Come on. Start!",
        "vi": "Nổ máy đi nào!"
    },
    {
        "start": "00:15:13,920",
        "end": "00:15:15,400",
        "en": "-Sæt over på nul igen. - Ja.",
        "pa": "-Try to put it at zero. -Yes.",
        "vi": "- Về lại số không. - Rồi."
    },
    {
        "start": "00:15:15,480",
        "end": "00:15:18,040",
        "en": "-Tilbage på et. Er den på et? -Ja. Den står på et.",
        "pa": "-And back to one. Is it at one? -Yes. It's on one.",
        "vi": "- Quay lại số một. Được chưa? - Số một rồi."
    },
    {
        "start": "00:15:18,120",
        "end": "00:15:20,400",
        "en": "-Hvorfor starter den ikke? - Træk igennem.",
        "pa": "-It won't start. -Then pull harder.",
        "vi": "- Sao nó không nổ? - Kéo mạnh nữa lên."
    },
    {
        "start": "00:15:20,480",
        "end": "00:15:23,720",
        "en": "-Trækker jeg mere, kan jeg hive den ud. -Så pump igennem.",
        "pa": "I can't! Unless you want the machine to break!\n Well, don't stop. Keep pulling it!",
        "vi": "- Mạnh nữa có mà vỡ máy. - Thế thì kéo nhanh vào!"
    },
    {
        "start": "00:15:23,800",
        "end": "00:15:27,280",
        "en": "-Pumper jeg for meget, drukner jeg den. -Skal vi stå og glo på den?",
        "pa": "If I pump too much, I'll drown it, okay?",
        "vi": "- Nhanh quá là máy ngộp dầu. - Thế ta cứ đứng nhìn nó hả?"
    },
    {
        "start": "00:15:27,360",
        "end": "00:15:29,640",
        "en": "Vi pumper løs, så må vi se, om det dur.",
        "pa": "Then what do you want, to stand and do nothing?\n Okay. You're right. I'll try it again and see if it works.",
        "vi": "Vậy để anh kéo nhanh. Có khi được."
    },
    {
        "start": "00:15:42,560",
        "end": "00:15:45,280",
        "en": "Nej, nej, nej!",
        "pa": "No, no, no, no, no, no, no!",
        "vi": "Không!"
    },
    {
        "start": "00:15:46,120",
        "end": "00:15:47,720",
        "en": "Mor! Far!",
        "pa": "Mom! Dad!",
        "vi": "Mẹ ơi! Bố ơi!"
    },
    {
        "start": "00:15:51,440",
        "end": "00:15:52,520",
        "en": "Kom så!",
        "pa": "- Damn it! it's still not working! -Mads! Hurry up!",
        "vi": "Thôi nào!"
    },
    {
        "start": "00:15:52,600",
        "end": "00:15:54,360",
        "en": "Mads! Skynd dig.",
        "pa": "",
        "vi": "Mads! Mau lên!"
    },
    {
        "start": "00:15:56,800",
        "end": "00:15:57,880",
        "en": "Kom nu!",
        "pa": "Come on! Come on! Come on!",
        "vi": "Nổ đi mà!"
    },
    {
        "start": "00:15:57,960",
        "end": "00:16:01,320",
        "en": "Løb! Kom nu! Løb!",
        "pa": "Run!\n Hurry! Oh my God. Come on!",
        "vi": "Chạy đi!\n Cố lên! Chạy mau đi!"
    },
    {
        "start": "00:16:01,400",
        "end": "00:16:02,880",
        "en": "Mads, jeg åbner nu.",
        "pa": "Mads, I'm opening it now.",
        "vi": "Mads, em mở cổng đây."
    },
    {
        "start": "00:16:02,960",
        "end": "00:16:04,440",
        "en": "Den kører ikke endnu.",
        "pa": "It's not running yet.",
        "vi": "Máy phát vẫn chưa chạy."
    },
    {
        "start": "00:16:05,080",
        "end": "00:16:06,360",
        "en": "Jos. Kom nu.",
        "pa": "Jose, hurry.",
        "vi": "Jose. Cố lên."
    },
    {
        "start": "00:16:12,240",
        "end": "00:16:13,480",
        "en": "Løb!",
        "pa": "Run. Faster!\n -Hurry!",
        "vi": "Chạy đi! Nhanh lên.\n Chạy nhanh đi!"
    },
    {
        "start": "00:16:24,200",
        "end": "00:16:25,280",
        "en": "Møller.",
        "pa": "Møller!",
        "vi": "Møller!"
    },
    {
        "start": "00:16:26,080",
        "end": "00:16:27,000",
        "en": "Kører den?",
        "pa": "Did you do it?\n Good.",
        "vi": "Máy chạy chưa?"
    },
    {
        "start": "00:16:39,640",
        "end": "00:16:40,560",
        "en": "Josefine.",
        "pa": "Jose.",
        "vi": "Josefine."
    },
    {
        "start": "00:17:12,480",
        "end": "00:17:13,520",
        "en": "Vi klarede den.",
        "pa": "We made it.",
        "vi": "Ta làm được rồi."
    },
    {
        "start": "00:17:16,200",
        "end": "00:17:17,600",
        "en": "Lad os få jer hjem.",
        "pa": "Let's get you home.",
        "vi": "Đưa tất cả về thôi."
    },
    {
        "start": "00:18:11,640",
        "end": "00:18:12,840",
        "en": "Så er der afgang.",
        "pa": "It's time to go.",
        "vi": "Sẵn sàng đi chưa?"
    },
    {
        "start": "00:18:14,600",
        "end": "00:18:16,360",
        "en": "Vi ordner det her på vores måde.",
        "pa": "We'll sort this out our way.",
        "vi": "Bọn bác sẽ có cách riêng xử lý."
    },
    {
        "start": "00:18:17,080",
        "end": "00:18:18,800",
        "en": "Tag bare hjem og glem alt om det.",
        "pa": "Go home and forget about it.",
        "vi": "Cháu cứ về và quên hết đi."
    },
    {
        "start": "00:18:20,280",
        "end": "00:18:23,600",
        "en": "Jeg gik bare fra den. Jeg lovede, at jeg ville…",
        "pa": "I abandoned it.\n I said that we'd…",
        "vi": "Cháu đã bỏ nó lại.\n Cháu đã hứa…"
    },
    {
        "start": "00:18:24,600",
        "end": "00:18:26,000",
        "en": "At vi ville være sammen.",
        "pa": "we'd stick together.",
        "vi": "Cháu và nó sẽ bên nhau."
    },
    {
        "start": "00:18:26,880",
        "end": "00:18:28,760",
        "en": "Det var den eller din familie.",
        "pa": "It was that or your family.",
        "vi": "Cháu đã chọn gia đình."
    },
    {
        "start": "00:18:28,840",
        "end": "00:18:30,240",
        "en": "Men ved du hvad?",
        "pa": "But you know what?",
        "vi": "Nhưng biết không?"
    },
    {
        "start": "00:18:31,240",
        "end": "00:18:32,840",
        "en": "Du vil altid have den derinde.",
        "pa": "It'll always be with you. Right there.",
        "vi": "Nó sẽ luôn trong đây."
    },
    {
        "start": "00:18:35,640",
        "end": "00:18:36,640",
        "en": "Glædelig jul.",
        "pa": "Always.",
        "vi": "Giáng sinh vui vẻ."
    },
    {
        "start": "00:18:53,960",
        "end": "00:18:56,880",
        "en": "Hvad skal der nu ske? Her.",
        "pa": "What's gonna happen…\n here?",
        "vi": "Giờ sẽ thế nào?\n Ở đảo này ấy."
    },
    {
        "start": "00:18:58,440",
        "end": "00:19:00,200",
        "en": "Møller og mig får styr på det.",
        "pa": "Møller and I will take care of it.",
        "vi": "Bác Møller và tôi sẽ lo hết."
    },
    {
        "start": "00:19:01,000",
        "end": "00:19:04,400",
        "en": "- Og så bliver alt, som det var før. -Og det er det, du vil?",
        "pa": "It'll be like it was before.\n Is that what you want?",
        "vi": "Và rồi tất cả lại như trước ư?\n Cậu muốn thế à?"
    },
    {
        "start": "00:19:18,640",
        "end": "00:19:20,400",
        "en": "-I skal af sted. - Ja.",
        "pa": "-You gotta leave. -Yeah.",
        "vi": "- Nhà cậu phải đi đi. - Ừ."
    },
    {
        "start": "00:19:21,280",
        "end": "00:19:22,480",
        "en": "Det skal vi.",
        "pa": "We do.",
        "vi": "Đúng thế."
    },
    {
        "start": "00:20:54,000",
        "end": "00:20:56,240",
        "en": "Ved du hvad? Alle julegaverne ligger i huset.",
        "pa": "You know, I left all the gifts at the house.",
        "vi": "Quà Giáng sinh ta để hết ở căn nhà đó."
    },
    {
        "start": "00:20:58,680",
        "end": "00:21:00,280",
        "en": "Det gør ikke noget.",
        "pa": "That's okay.",
        "vi": "Không sao ạ."
    },
    {
        "start": "00:21:01,240",
        "end": "00:21:02,440",
        "en": "Nej, du har ret.",
        "pa": "No, you're right.",
        "vi": "Ừ, con nói đúng."
    },
    {
        "start": "00:21:03,640",
        "end": "00:21:06,280",
        "en": "Det var en lidt barnlig gave, jeg havde købt til dig.",
        "pa": "Maybe it was a bit of a childish gift I got you.",
        "vi": "Quà bố mẹ mua cho con trẻ con lắm."
    },
    {
        "start": "00:21:09,160",
        "end": "00:21:10,040",
        "en": "Hvad var det?",
        "pa": "What was it?",
        "vi": "Là gì vậy mẹ?"
    },
    {
        "start": "00:21:12,240",
        "end": "00:21:14,840",
        "en": "-Vil du gerne vide, hvad det var? Vil du? -Mm. Ja.",
        "pa": "-You wanna know what? Yeah? -Yeah.",
        "vi": "- Con thật sự muốn biết à? Thật sao? - Vâng."
    },
    {
        "start": "00:21:14,920",
        "end": "00:21:17,280",
        "en": "-Er du sikker? Jeg siger det sgu da ikke. - Mm.",
        "pa": "-Are you sure? -Mm-hmm.\n It's a secret.",
        "vi": "Con chắc chứ? Mẹ sẽ không nói đâu!"
    },
    {
        "start": "00:21:24,760",
        "end": "00:21:25,960",
        "en": "Store pige.",
        "pa": "Big girl. ",
        "vi": "Con lớn rồi đấy."
    },
    {
        "start": "00:22:40,000",
        "end": "00:22:42,000",
