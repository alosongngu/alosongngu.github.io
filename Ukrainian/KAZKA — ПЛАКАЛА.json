[
    {
        "start": "00:00:20,480",
        "end": "00:00:25,070",
        "en": "Врятуюсь від слів твоїх, буду ціла (ціла)",
        "pa": "Vryatuyuś vid sliv tvoїkh, budu cila (cila)",
        "vi": "Em sẽ tự cứu mình khỏi những lời cay độc của anh, sẽ lại lành lặn (lành lặn)"
    },
    {
        "start": "00:00:25,070",
        "end": "00:00:28,420",
        "en": "За рік образ не складу ціну я",
        "pa": "Za rik obraz ne skladayu cinu ya",
        "vi": "Một năm qua em không thể định giá nỗi đau này"
    },
    {
        "start": "00:00:28,420",
        "end": "00:00:31,230",
        "en": "Горять, мости горять",
        "pa": "Horyatʹ, mosty horyatʹ",
        "vi": "Cháy rực, những cây cầu đang cháy"
    },
    {
        "start": "00:00:31,230",
        "end": "00:00:33,020",
        "en": "І сліз моря",
        "pa": "I sliz moria",
        "vi": "Và cả biển nước mắt"
    },
    {
        "start": "00:00:33,020",
        "end": "00:00:35,510",
        "en": "Та знай, це гра моя",
        "pa": "Ta znay̆, ce hra moya",
        "vi": "Nhưng hãy biết, đây là trò chơi của em"
    },
    {
        "start": "00:00:35,510",
        "end": "00:00:38,800",
        "en": "Менше слів, більше діла",
        "pa": "Menshe sliv, bilʹshe dila",
        "vi": "Ít lời nói, nhiều hành động hơn"
    },
    {
        "start": "00:00:38,800",
        "end": "00:00:43,310",
        "en": "Врятую від стріл твоїх хоч би тіло (тіло)",
        "pa": "Vryatuyu vid stril tvoїkh khoch by tilo (tilo)",
        "vi": "Em sẽ cứu lấy ít nhất là cơ thể mình khỏi những mũi tên của anh (cơ thể)"
    },
    {
        "start": "00:00:43,310",
        "end": "00:00:46,930",
        "en": "Загнав під лід, не знайду душу я",
        "pa": "Zahnav pid lid, ne znay̆du dushe ya",
        "vi": "Anh đã đẩy em xuống vực băng giá, em không thể tìm thấy tâm hồn mình"
    },
    {
        "start": "00:00:46,930",
        "end": "00:00:49,870",
        "en": "Болить, ой як болить",
        "pa": "Bolitʹ, oy yak bolytʹ",
        "vi": "Đau đớn, thật sự rất đau đớn"
    },
    {
        "start": "00:00:49,870",
        "end": "00:00:51,450",
        "en": "І кров кипить",
        "pa": "I krov kypytʹ",
        "vi": "Và máu đang sôi lên"
    },
    {
        "start": "00:00:51,450",
        "end": "00:00:54,120",
        "en": "Та донька вже не спить",
        "pa": "Ta donʹka vzhe ne spytʹ",
        "vi": "Nhưng con gái đã không còn ngủ nữa"
    },
    {
        "start": "00:00:54,120",
        "end": "00:00:57,220",
        "en": "Менше слів, більше діла",
        "pa": "Menshe sliv, bilʹshe dila",
        "vi": "Ít lời nói, nhiều hành động hơn"
    },
    {
        "start": "00:00:57,220",
        "end": "00:01:01,880",
        "en": "Поплакала і стоп! Фіалка розцвіла",
        "pa": "Poplakala i stop! Fialka rozcvila",
        "vi": "Đã khóc rồi, dừng lại! Hoa violet đã nở"
    },
    {
        "start": "00:01:01,880",
        "end": "00:01:06,590",
        "en": "Засяяв день таємними знаками, о!",
        "pa": "Zasyiav denʹ taïemnymy znakamy, o!",
        "vi": "Ngày mới bừng sáng với những điều bí ẩn, oh!"
    },
    {
        "start": "00:01:06,590",
        "end": "00:01:11,100",
        "en": "І мама молода, й закохана мала",
        "pa": "I mama moloda, y̆ zakokhana mala",
        "vi": "Và người mẹ trẻ, với tình yêu nồng cháy"
    },
    {
        "start": "00:01:11,100",
        "end": "00:01:15,900",
        "en": "На кухні всі однаково плакали",
        "pa": "Na kukhni vsi odnakovo plakaly",
        "vi": "Trong căn bếp ấy, ai cũng khóc"
    },
    {
        "start": "00:01:34,030",
        "end": "00:01:38,960",
        "en": "Зотліла ніч, зранку все стало сіре (сіре)",
        "pa": "Zotlila nič, zranku vse stalo sire (sire)",
        "vi": "Đêm tàn, bình minh đến, mọi thứ trở nên xám xịt (xám xịt)"
    },
    {
        "start": "00:01:38,960",
        "end": "00:01:42,360",
        "en": "Знайти нові кольори мушу я",
        "pa": "Znay̆ty novi kolory mushu ya",
        "vi": "Em phải tìm thấy những màu sắc mới"
    },
    {
        "start": "00:01:42,360",
        "end": "00:01:45,190",
        "en": "Лице умий дощем",
        "pa": "Lyce umiy̆ doshchem",
        "vi": "Rửa mặt bằng nước mưa"
    },
    {
        "start": "00:01:45,190",
        "end": "00:01:46,819",
        "en": "Що треба ще?",
        "pa": "Shcho treba shche?",
        "vi": "Cần gì hơn nữa?"
    },
    {
        "start": "00:01:46,819",
        "end": "00:01:49,380",
        "en": "Дай серцю під плащем",
        "pa": "Day̆ sercyu pid plashchem",
        "vi": "Hãy cho trái tim được sưởi ấm dưới lớp áo"
    },
    {
        "start": "00:01:49,380",
        "end": "00:01:52,680",
        "en": "Менше слів, більше віри (віри)",
        "pa": "Menshe sliv, bilʹshe viry (viry)",
        "vi": "Ít lời nói, nhiều niềm tin hơn (niềm tin)"
    },
