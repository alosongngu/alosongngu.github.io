[
    {
        "start": "00:00:12,920",
        "end": "00:00:16,280",
        "en": "Здається, шо то було так давно",
        "pa": "Zdayetʹsya, shcho to bulo tak davno",
        "vi": "Cứ ngỡ như đã từ rất lâu rồi"
    },
    {
        "start": "00:00:16,280",
        "end": "00:00:19,460",
        "en": "Коли в руках тримаю цей альбом",
        "pa": "Koly v rukakh trymayu tseĭ alʹbom",
        "vi": "Khi lật giở từng trang album cũ"
    },
    {
        "start": "00:00:19,460",
        "end": "00:00:22,739",
        "en": "Нам було абсолютно все одно",
        "pa": "Nam bulo absolyutno vse odno",
        "vi": "Ta chẳng bận tâm điều gì"
    },
    {
        "start": "00:00:22,739",
        "end": "00:00:25,860",
        "en": "Не маючи нічого мати всьо",
        "pa": "Ne mayuchy ničolo maty vsʹo",
        "vi": "Chẳng có gì nhưng lại như có tất cả"
    },
    {
        "start": "00:00:25,860",
        "end": "00:00:29,110",
        "en": "За гроші не купити тільки час",
        "pa": "Za hroši ne kupyty tilʹky čas",
        "vi": "Tiền bạc chẳng thể mua được thời gian"
    },
    {
        "start": "00:00:29,110",
        "end": "00:00:32,159",
        "en": "Він всіх нас методично поділив",
        "pa": "Vin vsich nas metodyčno podilyv",
        "vi": "Thời gian lặng lẽ chia cách chúng ta"
    },
    {
        "start": "00:00:32,159",
        "end": "00:00:35,180",
        "en": "Когось він опустив, когось підняв",
        "pa": "Kohosʹ vin opusnyv, kohosʹ pidnyav",
        "vi": "Kẻ bị lãng quên, người được vinh danh"
    },
    {
        "start": "00:00:35,180",
        "end": "00:00:40,860",
        "en": "А є на кого взагалі забив",
        "pa": "A ye na koho vzahali zabyv",
        "vi": "Còn đâu đó, những kẻ bị lãng quên"
    },
    {
        "start": "00:00:40,860",
        "end": "00:00:47,650",
        "en": "Старі фотографії на стіл розклади",
        "pa": "Stari fotohrafiyi na stil rozklady",
        "vi": "Hãy bày những bức ảnh cũ lên bàn"
    },
    {
        "start": "00:00:47,650",
        "end": "00:00:54,030",
        "en": "Дитячі історії смішні розкажи",
        "pa": "Dytyachi istoriyi smishni rozkazhy",
        "vi": "Kể cho nhau nghe những câu chuyện trẻ con"
    },
    {
        "start": "00:00:54,030",
        "end": "00:01:00,240",
        "en": "І справжнім друзям, не забудь, подзвони",
        "pa": "I spravzhnim druzyam, ne zabudʹ, podzvony",
        "vi": "Và đừng quên gọi điện cho những người bạn thật sự"
    },
    {
        "start": "00:01:00,240",
        "end": "00:01:06,900",
        "en": "Бо, добре чи зле, з тобою завжди вони",
        "pa": "Bo, dobre chy zle, z toboyu zavždy vony",
        "vi": "Vì dù tốt hay xấu, họ vẫn luôn bên bạn"
    },
    {
        "start": "00:01:06,900",
        "end": "00:01:10,080",
        "en": "Дешеве пиво і сухе вино",
        "pa": "Desheve pyvo i sukhe vyno",
        "vi": "Bia rẻ tiền và rượu vang nhạt"
    },
    {
        "start": "00:01:10,080",
        "end": "00:01:13,140",
        "en": "Робили нас щасливими людьми",
        "pa": "Robyly nas shchaslyvymy lyudʹmy",
        "vi": "Từng làm ta hạnh phúc biết bao"
    },
    {
        "start": "00:01:13,140",
        "end": "00:01:16,289",
        "en": "І, ніби чудо, польське радіо",
        "pa": "I, niby chudo, polʹsʹke radio",
        "vi": "Và như một phép màu, radio Ba Lan"
    },
    {
        "start": "00:01:16,289",
        "end": "00:01:19,330",
        "en": "Нам відкривало той незнаний світ",
        "pa": "Nam vidkryvalo toĭ neznanyy svit",
        "vi": "Mở ra cho ta một thế giới mới lạ"
    },
    {
        "start": "00:01:19,330",
        "end": "00:01:22,480",
        "en": "Ми жили ніби то був сон",
        "pa": "My zhyly niby to buv son",
        "vi": "Ta đã sống như trong giấc mơ"
    },
    {
        "start": "00:01:22,480",
        "end": "00:01:25,710",
        "en": "І можна бути вічно молодим",
        "pa": "I mozhna buty vichno molodym",
        "vi": "Và có thể trẻ mãi không già"
    },
    {
        "start": "00:01:25,710",
        "end": "00:01:28,760",
        "en": "А залишився тільки цей альбом",
        "pa": "A zalyshyvsya tilʹky tseĭ alʹbom",
