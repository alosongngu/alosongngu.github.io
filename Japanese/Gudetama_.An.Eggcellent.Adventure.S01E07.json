[
    {
        "start": "00:00:06,000",
        "end": "00:00:11,500",
        "en": "NETFLIX シリーズ",
        "pa": "",
        "vi": "LOẠT PHIM NETFLIX"
    },
    {
        "start": "00:00:13,000",
        "end": "00:00:15,750",
        "en": "（しゃきぴよ）ん～！",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:00:17,166",
        "end": "00:00:18,000",
        "en": "ぴよっ！",
        "pa": "",
        "vi": "Chiếp!"
    },
    {
        "start": "00:00:21,750",
        "end": "00:00:22,583",
        "en": "ぴよ",
        "pa": "GUDETAMA: AN EGGCELLENT ADVENTURE\n Piyo!",
        "vi": "GUDETAMA: CUỘC PHIÊU LƯU CỦA QUẢ TRỨNG LƯỜI\n Chiếp!"
    },
    {
        "start": "00:00:23,916",
        "end": "00:00:24,750",
        "en": "うわっ",
        "pa": "Oh.",
        "vi": ""
    },
    {
        "start": "00:00:26,375",
        "end": "00:00:27,791",
        "en": "ううう…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:00:27,875",
        "end": "00:00:28,750",
        "en": "（ハエの羽音）",
        "pa": "Are you all right, Shakipiyo?",
        "vi": ""
    },
    {
        "start": "00:00:28,833",
        "end": "00:00:30,750",
        "en": "（ハードボイルド）\n どうした しゃきぴよ",
        "pa": "",
        "vi": "Sao vậy, Shakipiyo?"
    },
    {
        "start": "00:00:32,000",
        "end": "00:00:34,458",
        "en": "（しゃきぴよ）\n もうママに会えないよ",
        "pa": "Now we're not ever gonna find our mommy.",
        "vi": "Bọn tớ không thể gặp mẹ rồi."
    },
    {
        "start": "00:00:35,083",
        "end": "00:00:37,583",
        "en": "こんなとこ 絶対 出られないもん",
        "pa": "-We're stuck in here.\n We're never getting out.",
        "vi": "Chắc chắn không thể thoát khỏi nơi này."
    },
    {
        "start": "00:00:41,458",
        "end": "00:00:45,625",
        "en": "オリに閉じ込められたまま\n にわとりになるんだ",
        "pa": "I'm going to grow up and live my whole life in a cage.",
        "vi": "Tớ sẽ lớn lên khi bị nhốt trong lồng."
    },
    {
        "start": "00:00:47,375",
        "end": "00:00:50,458",
        "en": "（ぐでたま）普通\n にわとり そんな感じっしょ～",
        "pa": "Isn't that kind of what happens with most chickens though?",
        "vi": "Bình thường thì gà toàn là thế mà?"
    },
    {
        "start": "00:00:50,541",
        "end": "00:00:52,875",
        "en": "（しゃきぴよ）ハッ… う～ん",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:00:53,875",
        "end": "00:00:57,250",
        "en": "（ハードボイルド）\n しゃきぴよ 一つ聞いていいか？",
        "pa": "Shakipiyo, tell me something.",
        "vi": "Shakipiyo.\n Tôi hỏi bạn điều này."
    },
    {
        "start": "00:00:58,375",
        "end": "00:01:02,750",
        "en": "お前 母ちゃんに会ったら\n 何をしたいんだ？",
        "pa": "What would you do if and when you ever… did find your mom?",
        "vi": "Khi gặp được mẹ, bạn muốn làm gì?"
    },
    {
        "start": "00:01:03,583",
        "end": "00:01:04,416",
        "en": "えっ",
        "pa": "Huh?",
        "vi": "Hả?"
    },
    {
        "start": "00:01:05,916",
        "end": "00:01:08,333",
        "en": "あんまり考えてなかったな",
        "pa": "I really haven't thought about it.",
        "vi": "Tớ chưa nghĩ tới."
    },
    {
        "start": "00:01:09,583",
        "end": "00:01:11,708",
        "en": "会いたいだけだったから…",
        "pa": "I just wanted to see her.",
        "vi": "Tớ chỉ muốn gặp thôi."
    },
    {
        "start": "00:01:13,166",
        "end": "00:01:14,875",
        "en": "けど もし会えたら…",
        "pa": "But if I do ever find her,",
        "vi": "Nhưng nếu gặp được mẹ,"
    },
    {
        "start": "00:01:16,250",
        "end": "00:01:18,916",
        "en": "空の飛び方を\n 教えてもらいたいな",
        "pa": "I'm going to get her to teach me how to fly!",
        "vi": "tớ muốn mẹ dạy cách bay lên bầu trời."
    },
    {
        "start": "00:01:19,625",
        "end": "00:01:20,458",
        "en": "悪くない",
        "pa": "Sounds nice.",
        "vi": "Không tồi."
    },
    {
        "start": "00:01:22,666",
        "end": "00:01:24,000",
        "en": "（しゃきぴよ）だよね！\n （ハードボイルド）フフッ",
        "pa": "-Yeah, doesn't it?",
        "vi": "Được nhỉ?"
    },
    {
        "start": "00:01:24,083",
        "end": "00:01:27,416",
        "en": "それで ママと一緒に\n 大空を飛び回るんだ",
        "pa": "Then Mommy and me will fly all around in the big sky!",
        "vi": "Sau đó, tớ sẽ bay vòng quanh bầu trời xanh với mẹ."
    },
    {
        "start": "00:01:28,000",
        "end": "00:01:29,541",
        "en": "どこまでも自由に！",
        "pa": "-We'll go everywhere together! -I don't think chickens can fly.",
        "vi": "Tự do bay đi bất cứ đâu!"
    },
    {
        "start": "00:01:30,125",
        "end": "00:01:32,583",
        "en": "にわとり 飛べなくね？",
        "pa": "",
        "vi": "Gà không bay được mà."
    },
    {
        "start": "00:01:32,666",
        "end": "00:01:34,708",
        "en": "決めつけちゃダメだよ！",
        "pa": "You don't know that for sure!",
        "vi": "Cậu không thể cứ cho là vậy!"
    },
    {
        "start": "00:01:34,791",
        "end": "00:01:37,041",
        "en": "大事なのは信じる気持ち",
        "pa": "You've just got to believe. That's all.",
        "vi": "Quan trọng là phải tin vào điều đó."
    },
    {
        "start": "00:01:37,125",
        "end": "00:01:38,708",
        "en": "あきめちゃダメなの",
        "pa": "You can't ever give up hope.",
        "vi": "Cậu không được bỏ cuộc!"
    },
    {
        "start": "00:01:39,500",
        "end": "00:01:41,125",
        "en": "（ハードボイルド）\n フッ しゃきぴよ",
        "pa": "That's right.",
        "vi": "Shakipiyo."
    },
    {
        "start": "00:01:42,125",
        "end": "00:01:45,208",
        "en": "今の言葉\n そのまま お前に返すぜ",
        "pa": "And maybe you should try heeding your own advice.",
        "vi": "Tôi sẽ nói điều tương tự với bạn."
    },
    {
        "start": "00:01:45,958",
        "end": "00:01:47,458",
        "en": "まだ早いんじゃないか",
        "pa": "Isn't it a little soon for you to be giving up on ever getting out of here?",
        "vi": "Không phải là quá sớm để cho rằng bạn sẽ không thể rời khỏi đây sao?"
    },
    {
        "start": "00:01:48,041",
        "end": "00:01:50,708",
        "en": "ここから出られないと\n 決めつけるのは",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:53,083",
        "end": "00:01:54,375",
        "en": "（ぐでたま）ん？\n （しゃきぴよ）ハッ！",
        "pa": "-Huh? -Huh?",
        "vi": ""
    },
    {
        "start": "00:01:56,166",
        "end": "00:01:57,791",
        "en": "（マフィアマフィン）\n ボスがいらっしゃる",
        "pa": "The boss is here.",
        "vi": "Chủ nhân tới rồi."
    },
    {
        "start": "00:01:57,875",
        "end": "00:01:58,791",
        "en": "ボス？",
        "pa": "Boss?",
        "vi": "Chủ nhân?"
    },
    {
        "start": "00:02:03,750",
        "end": "00:02:05,916",
        "en": "ハッ！　あ… あ～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:02:06,500",
        "end": "00:02:08,416",
        "en": "（ハードボイルド）\n たまごマフィアのボス",
        "pa": "The Egg Mafia's big boss.",
        "vi": "Chủ nhân của Mafia Trứng."
    },
    {
        "start": "00:02:08,916",
        "end": "00:02:10,958",
        "en": "うわさに聞いたことがある",
        "pa": "I've heard rumors.\n They say that gloopy red sauce",
        "vi": "Tôi đã nghe tin đồn."
    },
    {
        "start": "00:02:11,041",
        "end": "00:02:13,458",
        "en": "消した たまごは数知れず",
        "pa": "is the blood of all the eggs that got in the big boss's way",
        "vi": "Kẻ đó đã làm vô số trứng biến mất."
    },
    {
        "start": "00:02:13,541",
        "end": "00:02:16,083",
        "en": "全身が血塗られた存在だとな",
        "pa": "and were bumped off.",
        "vi": "Cả người kẻ đó vấy máu."
    },
    {
        "start": "00:02:16,916",
        "end": "00:02:19,416",
        "en": "（ぐでたま）あ～ オムレツか～",
        "pa": "Huh?\n It's just an omelet.",
        "vi": "Ồ, trứng ốp lết à."
    },
    {
        "start": "00:02:19,916",
        "end": "00:02:21,791",
        "en": "（マフィアマフィン１）\n ただのオムレツじゃないんだわ～",
        "pa": "- Not just any kind of omelet. -Huh?",
        "vi": "Không đơn giản chỉ là trứng ốp lết đâu."
    },
    {
        "start": "00:02:21,791",
        "end": "00:02:22,625",
        "en": "（マフィアマフィン１）\n ただのオムレツじゃないんだわ～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:02:21,791",
        "end": "00:02:22,625",
        "en": "（ぐでたま）え～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:02:22,708",
        "end": "00:02:26,041",
        "en": "（ボスオムレツ）\n シチリア風オムレツだよ～",
        "pa": "I'm Sicilian style!",
        "vi": "Ta là trứng ốp lết kiểu Ý!"
    },
    {
        "start": "00:02:26,125",
        "end": "00:02:28,333",
        "en": "（マフィアマフィン１）\n たまごマフィアだけにね～",
        "pa": "What'd you expect from the Egg Mafia?",
        "vi": "Quả nhiên là mafia trứng!"
    },
    {
        "start": "00:02:28,416",
        "end": "00:02:30,708",
        "en": "（マフィアマフィン２）刃向かった\n ヤツらは みんな消えたんだわ～",
        "pa": "Anyone who messes with the big boss ends up as part of the omelet.",
        "vi": "Kẻ chống lại chủ nhân đều sẽ biến mất."
    },
    {
        "start": "00:02:30,791",
        "end": "00:02:32,958",
        "en": "ボスの具になってね～",
        "pa": "",
        "vi": "Chúng trở thành một phần của chủ nhân."
    },
    {
        "start": "00:02:33,041",
        "end": "00:02:33,916",
        "en": "ぴよ～！",
        "pa": "",
        "vi": "Chiếp!"
    },
    {
        "start": "00:02:34,416",
        "end": "00:02:36,500",
        "en": "（ボスオムレツ）\n 我らのブツを奪うとは",
        "pa": "You've got some nerve thinking you can come in here and steal our stuff.",
        "vi": "Ăn cắp hàng của bọn ta, cũng cả gan đấy."
    },
    {
        "start": "00:02:36,583",
        "end": "00:02:39,041",
        "en": "いい度胸してるね～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:02:39,625",
        "end": "00:02:41,416",
        "en": "う… 奪ってなんかないよ",
        "pa": "But we didn't steal anything!",
        "vi": "Bọn tôi không lấy gì."
    },
    {
        "start": "00:02:41,500",
        "end": "00:02:42,916",
        "en": "（マフィアマフィン１）\n ウソだね～",
        "pa": "That's a lie!",
        "vi": "Nói dối!"
    },
    {
        "start": "00:02:43,000",
        "end": "00:02:44,833",
        "en": "（ボスオムレツ）\n そういうウソついちゃう？",
        "pa": "- And not even a very good one.",
        "vi": "Thật là một lời nói dối tệ hại."
    },
    {
        "start": "00:02:44,916",
        "end": "00:02:45,750",
        "en": "（しゃきぴよ）う～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:02:45,833",
        "end": "00:02:49,750",
        "en": "我 たまご何個\n 使ってると思ってんの？",
        "pa": "Do you want to know how many eggs I've already put in my omelet?",
        "vi": "Mi có biết ta làm từ bao nhiêu quả trứng không?"
    },
    {
        "start": "00:02:49,833",
        "end": "00:02:51,208",
        "en": "知らんけど",
        "pa": "Not really, no.",
        "vi": "Chịu."
    },
    {
        "start": "00:02:52,875",
        "end": "00:02:53,750",
        "en": "（ボスオムレツ）ん？",
        "pa": "Hm, hm.",
        "vi": "Hả?"
    },
    {
        "start": "00:02:53,833",
        "end": "00:02:54,833",
        "en": "（ハードボイルド）\n 逆に聞かせてもらおうか",
        "pa": "- Let me ask you something. -Huh?",
        "vi": "Để tôi hỏi đã."
    },
    {
        "start": "00:02:54,833",
        "end": "00:02:55,666",
        "en": "（ハードボイルド）\n 逆に聞かせてもらおうか",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:02:54,833",
        "end": "00:02:55,666",
        "en": "（ボスオムレツ）ん？",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:02:55,750",
        "end": "00:02:57,375",
        "en": "盗(と)ったとしたなら",
        "pa": "If we stole your stuff, then why don't we have it?",
        "vi": "Nếu bọn tôi trộm hàng,"
    },
    {
        "start": "00:02:58,291",
        "end": "00:02:59,541",
        "en": "そのブツは\n 今 どこにあるんだ？",
        "pa": "",
        "vi": "thì giờ hàng ở đâu?"
    },
    {
        "start": "00:02:59,541",
        "end": "00:03:00,375",
        "en": "そのブツは\n 今 どこにあるんだ？",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:02:59,541",
        "end": "00:03:00,375",
        "en": "（ぐでたま）あ～",
        "pa": "-Where is it?",
        "vi": ""
    },
    {
        "start": "00:03:00,458",
        "end": "00:03:01,375",
        "en": "確かに",
        "pa": "That's true.",
        "vi": "Đúng vậy."
    },
    {
        "start": "00:03:02,083",
        "end": "00:03:04,541",
        "en": "（ぐでたま）ん～な～？",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:03:05,208",
        "end": "00:03:07,833",
        "en": "あ～あ～　あ～ 何～！",
        "pa": "-What the heck?! Where are you going?",
        "vi": "Cái quái gì vậy?"
    },
    {
        "start": "00:03:07,916",
        "end": "00:03:09,583",
        "en": "お前 どうやって…",
        "pa": "How did you get out?",
        "vi": "Sao mày có thể…"
    },
    {
        "start": "00:03:09,666",
        "end": "00:03:11,791",
        "en": "（ぐでたま）知らんけど～",
        "pa": "-I don't know. - Hey!",
        "vi": "Chả biết."
    },
    {
        "start": "00:03:13,416",
        "end": "00:03:15,083",
        "en": "何かに引っ張られている",
        "pa": "-Hey! -Who or what is it pulling Gudetama?",
        "vi": "Cậu ấy đang bị thứ gì đó kéo."
    },
    {
        "start": "00:03:16,125",
        "end": "00:03:18,416",
        "en": "（ぐでたま）あ～ あれだわ",
        "pa": "Oh.\n I think I know.",
        "vi": "Tớ nghĩ tớ biết."
    },
    {
        "start": "00:03:20,541",
        "end": "00:03:22,166",
        "en": "アイツらだわ～",
        "pa": "That's who it is.",
        "vi": "Là bọn họ…"
    },
    {
        "start": "00:03:22,666",
        "end": "00:03:24,625",
        "en": "追って～",
        "pa": "Well, go on! Get 'em!",
        "vi": "Đuổi theo chúng!"
    },
    {
        "start": "00:03:24,708",
        "end": "00:03:26,000",
        "en": "（マフィアマフィンたち）えっ？",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:03:26,083",
        "end": "00:03:30,250",
        "en": "たどってったら\n ほんとの犯人 見つかるっしょ～",
        "pa": "Follow 'em, and maybe you can find out who really stole our stuff.",
        "vi": "Đi theo chúng thì sẽ tìm ra hung thủ thực sự."
    },
    {
        "start": "00:03:30,833",
        "end": "00:03:31,958",
        "en": "（マフィアマフィン１）でも…",
        "pa": "But… But…",
        "vi": "Nhưng"
    },
    {
        "start": "00:03:32,458",
        "end": "00:03:33,416",
        "en": "（マフィアマフィンたち）怖い",
        "pa": "I'm scared!",
        "vi": "sợ lắm."
    },
    {
        "start": "00:03:33,500",
        "end": "00:03:37,708",
        "en": "（ボスオムレツ）\n アイツだけか～ 見込みあるの",
        "pa": "Okay, you're fired.\n I'll find somebody else to do it.",
        "vi": "Vậy là chỉ có thể trông cậy vào tên kia."
    },
    {
        "start": "00:03:38,333",
        "end": "00:03:40,791",
        "en": "スヤリンチョ～",
        "pa": "Oh, I am bushed.",
        "vi": "Ngủ ngon."
    },
    {
        "start": "00:03:41,541",
        "end": "00:03:43,625",
        "en": "じゃあ こうしよ～",
        "pa": "All right, here's the deal.",
        "vi": "Vậy thì thế này đi."
    },
    {
        "start": "00:03:44,125",
        "end": "00:03:48,208",
        "en": "犯人を突き止めて～\n ブツを奪い返してきたら～",
        "pa": "If you three can find the real thief and get back the stuff,",
        "vi": "Nếu bọn mi tìm ra thủ phạm và lấy lại hàng,"
    },
    {
        "start": "00:03:48,291",
        "end": "00:03:50,791",
        "en": "みんな 無罪放免にしてあげちゃう",
        "pa": "then you're off the hook.\n -I'll let you all go.",
        "vi": "bọn mi sẽ thoát tội."
    },
    {
        "start": "00:03:50,875",
        "end": "00:03:52,083",
        "en": "ぴよっ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:03:52,166",
        "end": "00:03:55,583",
        "en": "でもでも～\n もし逃げたら～",
        "pa": "But if you run away, I'll track you down wherever you go",
        "vi": "Nhưng nếu bọn mi bỏ trốn,"
    },
    {
        "start": "00:03:55,666",
        "end": "00:03:59,125",
        "en": "絶対 見つけて\n 我の具にしちゃうっしょ",
        "pa": "and make you a part of my ever-expanding omelet.",
        "vi": "ta sẽ tìm bằng được và nuốt chửng bọn mi."
    },
    {
        "start": "00:03:59,208",
        "end": "00:04:00,041",
        "en": "ハッ！",
        "pa": "- Mm, nice crispy chicken.",
        "vi": ""
    },
    {
        "start": "00:04:00,125",
        "end": "00:04:02,000",
        "en": "（ボスオムレツ）\n コロッと炒めたチキンと～",
        "pa": "",
        "vi": "- Gà giòn ngon… - Chiếp!"
    },
    {
        "start": "00:04:02,000",
        "end": "00:04:03,041",
        "en": "（ボスオムレツ）\n コロッと炒めたチキンと～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:04:02,000",
        "end": "00:04:03,041",
        "en": "（しゃきぴよ）ぴよっ",
        "pa": "-Hm…",
        "vi": ""
    },
    {
        "start": "00:04:03,125",
        "end": "00:04:05,041",
        "en": "ん～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:04:05,125",
        "end": "00:04:06,375",
        "en": "くっ…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:04:06,458",
        "end": "00:04:09,083",
        "en": "ぐっちゃぐっちゃにして\n 混ぜちゃうんだわ",
        "pa": "I'll even find someplace for you in there.",
        "vi": "Ta sẽ trộn tất cả bọn mi lại với nhau."
    },
    {
        "start": "00:04:09,666",
        "end": "00:04:12,125",
        "en": "イッ！　お勧めはしない",
        "pa": "I wouldn't recommend it.",
        "vi": "Tôi khuyên không nên thế."
    },
    {
        "start": "00:04:12,875",
        "end": "00:04:14,166",
        "en": "腐ってるからな",
        "pa": "I'm starting to rot.",
        "vi": "Tôi thối rồi."
    },
    {
        "start": "00:04:16,958",
        "end": "00:04:17,791",
        "en": "（しゃきぴよ）ぴよっ",
        "pa": "Piyo!",
        "vi": "Chiếp."
    },
    {
        "start": "00:04:19,000",
        "end": "00:04:19,833",
        "en": "ぐでたま！",
        "pa": "Gudetama!",
        "vi": "Gudetama!"
    },
    {
        "start": "00:04:21,708",
        "end": "00:04:23,833",
        "en": "（ハードボイルド）\n ごろごろ ごろごろごろ…",
        "pa": "Rolling. Rolling, rolling, rolling. Rolling.",
        "vi": "Lăn, lăn."
    },
    {
        "start": "00:04:23,916",
        "end": "00:04:27,958",
        "en": "どうなるかな～　オムオム",
        "pa": "What'll happen, I wonder.\n Omu-omu.",
        "vi": "Để xem sẽ thế nào."
    },
    {
        "start": "00:04:31,416",
        "end": "00:04:33,541",
        "en": "（井上(いのうえ)Ｄ）この辺りだよな？",
        "pa": "It was here, right?",
        "vi": "Nó ở quanh đây nhỉ?"
    },
    {
        "start": "00:04:33,625",
        "end": "00:04:36,291",
        "en": "動くたまごを見たって\n 情報があったの",
        "pa": "Where someone said they saw the talking egg?",
        "vi": "Có thông tin thấy một quả trứng chuyển động."
    },
    {
        "start": "00:04:36,375",
        "end": "00:04:38,125",
        "en": "（鈴木(すずき)）そうなんじゃないっすか？",
        "pa": "Yeah, I think so.",
        "vi": "Chắc là vậy."
    },
    {
        "start": "00:04:38,208",
        "end": "00:04:40,500",
        "en": "（雨どいからする音）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:04:40,583",
        "end": "00:04:42,291",
        "en": "（鈴木）ん？\n （井上Ｄ）え え？",
        "pa": "-Huh? - What is that?",
        "vi": "- Hả? - Cái gì?"
    },
    {
        "start": "00:04:43,375",
        "end": "00:04:46,500",
        "en": "（ぐでたま）あ～～～",
        "pa": "Ah!",
        "vi": ""
    },
    {
        "start": "00:04:46,583",
        "end": "00:04:47,416",
        "en": "（しゃきぴよ）ぴよっ",
        "pa": "-Piyo!",
        "vi": "Chiếp!"
    },
    {
        "start": "00:04:48,125",
        "end": "00:04:48,958",
        "en": "（ぐでたま）あ～",
        "pa": "- Oh, hey!",
        "vi": ""
    },
    {
        "start": "00:04:49,041",
        "end": "00:04:51,583",
        "en": "（井上）おい\n 雨どいに何かいるぞ！",
        "pa": "There's something up there in the gutter!",
        "vi": "Này! Có gì đó trong máng xối!"
    },
    {
        "start": "00:04:51,666",
        "end": "00:04:55,416",
        "en": "（ぐでたま）あ～～～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:04:56,333",
        "end": "00:05:00,833",
        "en": "（ぐでたま）あ～～～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:05:00,916",
        "end": "00:05:02,458",
        "en": "（しゃきぴよ）ハアッ ぴよ！",
        "pa": "",
        "vi": "Chiếp?"
    },
    {
        "start": "00:05:02,541",
        "end": "00:05:03,750",
        "en": "（ぐでたま）あ～～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:05:04,375",
        "end": "00:05:06,375",
        "en": "（しゃきぴよ）ぴよ～！",
        "pa": "Piyo!",
        "vi": "Chiếp!"
    },
    {
        "start": "00:05:06,458",
        "end": "00:05:08,291",
        "en": "（においを嗅ぐ音）\n （ハードボイルド）ここは…",
        "pa": "",
        "vi": "Đây là nhà hàng Trung Quốc!"
    },
    {
        "start": "00:05:08,375",
        "end": "00:05:09,750",
        "en": "中華料理店！",
        "pa": "A Chinese restaurant.",
        "vi": ""
    },
    {
        "start": "00:05:12,291",
        "end": "00:05:15,750",
        "en": "（ぐでたまたち）わ～っ！",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:05:16,583",
        "end": "00:05:19,708",
        "en": "（しゃきぴよ）ハア ハア ハア…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:05:19,791",
        "end": "00:05:21,041",
        "en": "何 ここ？",
        "pa": "What is this place?",
        "vi": "Đây là nơi nào?"
    },
    {
        "start": "00:05:24,416",
        "end": "00:05:26,250",
        "en": "油断するな！　くっ",
        "pa": "Stay on your guard!",
        "vi": "Cảnh giác!"
    },
    {
        "start": "00:05:28,291",
        "end": "00:05:30,125",
        "en": "あっ！　そうか",
        "pa": "Just as I thought.",
        "vi": "Ra là vậy."
    },
    {
        "start": "00:05:32,500",
        "end": "00:05:34,125",
        "en": "敵は ヤツらか",
        "pa": "We've found the culprits.",
        "vi": "Chúng là kẻ địch!"
    },
    {
        "start": "00:05:35,333",
        "end": "00:05:38,166",
        "en": "中華３千年の歴史が生んだ…",
        "pa": "Century eggs.\n -An ancient Chinese delicacy.",
        "vi": "Ba ngàn năm lịch sử ẩm thực Trung Quốc"
    },
    {
        "start": "00:05:38,250",
        "end": "00:05:41,875",
        "en": "たまごマフィアよりも\n 黒い闇の存在",
        "pa": "This gang is even darker than the Egg Mafia.",
        "vi": "đã tạo ra những kẻ đen tối hơn cả Mafia Trứng."
    },
    {
        "start": "00:05:43,958",
        "end": "00:05:45,166",
        "en": "（ピータン）んん…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:05:46,250",
        "end": "00:05:49,250",
        "en": "（ピータン）あ… 帰ります",
        "pa": "Oops, I forgot something.",
        "vi": "Tớ sẽ về nhà."
    },
    {
        "start": "00:05:49,958",
        "end": "00:05:50,958",
        "en": "待て待て待て！",
        "pa": "Hey, come back here!",
        "vi": "Dừng lại ngay!"
    },
    {
        "start": "00:05:51,541",
        "end": "00:05:52,666",
        "en": "（ピータン）も～",
        "pa": "-Huh?",
        "vi": ""
    },
    {
        "start": "00:05:52,750",
        "end": "00:05:53,583",
        "en": "（ハードボイルド）えっ",
        "pa": "",
        "vi": "Cái gì?"
    },
    {
        "start": "00:05:53,666",
        "end": "00:05:55,750",
        "en": "（ピータン）明るくしないで～",
        "pa": "Does it have to be so bright in here?",
        "vi": "Đừng để phòng sáng như thế."
    },
    {
        "start": "00:05:56,958",
        "end": "00:06:01,291",
        "en": "ちょっと透けてて\n 恥ずかしいんですけど～",
        "pa": "I'm translucent, so you could see right through me.",
        "vi": "Tớ xấu hổ vì cậu có thể nhìn thấu tớ."
    },
    {
        "start": "00:06:01,375",
        "end": "00:06:03,208",
        "en": "怖くなくね？",
        "pa": "It's embarrassing, you know?\n They're not so scary.",
        "vi": "Chả đáng sợ chút nào."
    },
    {
        "start": "00:06:03,875",
        "end": "00:06:08,750",
        "en": "いや このピータンたちこそが\n ブツを奪った真犯人！",
        "pa": "Oh, don't let 'em fool you.\n These little stinkers are the ones who stole the stuff",
        "vi": "Không, những quả trứng bắc thảo này là thủ phạm trộm hàng thực sự."
    },
    {
        "start": "00:06:08,833",
        "end": "00:06:10,333",
        "en": "見た目に だまされるな！",
        "pa": "and left us to take the fall!\n What stuff?",
        "vi": "Đừng bị lừa bởi vẻ ngoài!"
    },
    {
        "start": "00:06:10,416",
        "end": "00:06:11,916",
        "en": "ブツ？",
        "pa": "",
        "vi": "\"Hàng\"?"
    },
    {
        "start": "00:06:12,000",
        "end": "00:06:15,750",
        "en": "ブツって… あれ？",
        "pa": "Oh.\n Oh, you mean that stuff over there?",
        "vi": "Ý cậu là mấy thứ kia à?"
    },
    {
        "start": "00:06:15,833",
        "end": "00:06:19,041",
        "en": "（ピータン）ハア…\n （ピータン）落ち着くわ～",
        "pa": "-Ah!",
        "vi": "Thật là dễ chịu…"
    },
    {
        "start": "00:06:19,125",
        "end": "00:06:23,458",
        "en": "（ピータン）\n アイルビー… バックしません",
        "pa": "Oh, that's so soothing.\n So relaxing.\n I feel like I'm melting.",
        "vi": "Tớ sẽ không quay lại đâu."
    },
    {
        "start": "00:06:23,541",
        "end": "00:06:27,000",
        "en": "まさか ピータンを作るために\n ブツを？",
        "pa": "So you century eggs like that stuff enough to steal it, huh?",
        "vi": "Các bạn dùng thứ đó để làm trứng bắc thảo ư?"
    },
    {
        "start": "00:06:27,583",
        "end": "00:06:29,208",
        "en": "そうだね～",
        "pa": "It's a desiccant.",
        "vi": "Đúng vậy."
    },
    {
        "start": "00:06:29,291",
        "end": "00:06:30,666",
        "en": "乾燥剤…",
        "pa": "It preserves us.",
        "vi": "Để làm trứng bắc thảo…"
    },
    {
        "start": "00:06:30,750",
        "end": "00:06:32,875",
        "en": "ピータン作りに…",
        "pa": "We don't just like it.",
        "vi": "…chất hút ẩm…"
    },
    {
        "start": "00:06:32,958",
        "end": "00:06:34,166",
        "en": "（ピータンたち）大事",
        "pa": "We love it!",
        "vi": "…rất quan trọng."
    },
    {
        "start": "00:06:34,250",
        "end": "00:06:37,083",
        "en": "ぴよっ　ぐでたま？",
        "pa": "Gudetama?",
        "vi": "Chiếp?"
    },
    {
        "start": "00:06:37,166",
        "end": "00:06:39,541",
        "en": "ハア…いいわ～",
        "pa": "This is nice.",
        "vi": "Gudetama?\n Thích quá."
    },
    {
        "start": "00:06:39,625",
        "end": "00:06:41,500",
        "en": "ハッ　ダメーッ！",
        "pa": "Get out of there!",
        "vi": "Không được!"
    },
    {
        "start": "00:06:42,500",
        "end": "00:06:47,125",
        "en": "（ピータン）40日 入ってたら\n ピータンになれるよ",
        "pa": "Stay in there for forty days, and you can be a century egg too.",
        "vi": "Nếu ở trong đó 40 ngày, cậu có thể biến thành quả trứng bắc thảo."
    },
    {
        "start": "00:06:47,708",
        "end": "00:06:50,833",
        "en": "えっ ゆで時間より長い…",
        "pa": "Jeez!\n And I thought my boiling time was long.",
        "vi": "Hả? Còn lâu hơn thời gian luộc."
    },
    {
        "start": "00:06:51,333",
        "end": "00:06:53,166",
        "en": "ピータンたち",
        "pa": "We don't get out much.",
        "vi": "Trứng bắc thảo bọn tớ…"
    },
    {
        "start": "00:06:53,250",
        "end": "00:06:55,958",
        "en": "（ピータンたち）引きこもり～",
        "pa": "We're shut-ins.",
        "vi": "…tự cô lập với xã hội."
    },
    {
        "start": "00:06:57,708",
        "end": "00:06:59,750",
        "en": "大人になりたくない",
        "pa": "I don't want to grow up.",
        "vi": "Bọn tớ không muốn trưởng thành."
    },
    {
        "start": "00:07:00,333",
        "end": "00:07:02,458",
        "en": "分かるわ～",
        "pa": "-I'm with you there.",
        "vi": "Tớ hiểu."
    },
    {
        "start": "00:07:03,208",
        "end": "00:07:04,041",
        "en": "ん？",
        "pa": "You are?",
        "vi": "Hả?"
    },
    {
        "start": "00:07:05,125",
        "end": "00:07:08,958",
        "en": "責任とか自立とか だりぃ～",
        "pa": "All that responsibility is a drag.",
        "vi": "Trách nhiệm với cả độc lập? Mệt ghê."
    },
    {
        "start": "00:07:09,041",
        "end": "00:07:10,125",
        "en": "えっ？",
        "pa": "It is?",
        "vi": "Cái gì?"
    },
    {
        "start": "00:07:10,916",
        "end": "00:07:15,375",
        "en": "大人になるって\n つまり酸化ってことっしょ？",
        "pa": "And growing up really only means you're oxidizing, you know?",
        "vi": "Trưởng thành tương đương với quá trình ô xy hóa còn gì?"
    },
    {
        "start": "00:07:15,958",
        "end": "00:07:17,416",
        "en": "（ピータンたち）\n そうなの？",
        "pa": "-Yeah? -Yeah?",
        "vi": "Thật sao?"
    },
    {
        "start": "00:07:18,000",
        "end": "00:07:21,750",
        "en": "（ぐでたま）まっ\n 結局は働きたくないんだわ",
        "pa": "In the end, it's all way too much work for me.",
        "vi": "Rốt cục thì tớ không muốn làm việc đâu."
    },
    {
        "start": "00:07:21,833",
        "end": "00:07:22,875",
        "en": "あ～あ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:07:22,958",
        "end": "00:07:24,875",
        "en": "（ピータンたち）働きたくない",
        "pa": "- Yeah. - You said it.",
        "vi": "\"Không muốn làm việc\"?"
    },
    {
        "start": "00:07:24,958",
        "end": "00:07:26,833",
        "en": "すごいわ",
        "pa": "That's just how we all feel!",
        "vi": "Tuyệt vời."
    },
    {
        "start": "00:07:28,041",
        "end": "00:07:31,291",
        "en": "（しゃきぴよ）ちょっと！\n ぴよたち あれを取り返さないと",
        "pa": "- Mm-hmm. -Amazing.\n Well, that's nice.\n But if we don't get that stuff back to the big boss,",
        "vi": "Này! Nếu bọn mình không lấy lại hàng, thì sẽ gặp rắc rối lớn!"
    },
    {
        "start": "00:07:31,375",
        "end": "00:07:33,458",
        "en": "大変なことに なっちゃうんだよ",
        "pa": "we're going to be in big trouble!",
        "vi": ""
    },
    {
        "start": "00:07:34,000",
        "end": "00:07:35,416",
        "en": "（ピータン）そうなの？",
        "pa": "Oh, is that so?",
        "vi": "Vậy à? Thế thì bọn tớ sẽ trả lại."
    },
    {
        "start": "00:07:35,500",
        "end": "00:07:37,333",
        "en": "（ピータン）それなら返す",
        "pa": "-Well, in that case, we'll give it back. -Uh-huh.",
        "vi": ""
    },
    {
        "start": "00:07:37,416",
        "end": "00:07:39,791",
        "en": "（ピータンたち）返す 返す 返す…",
        "pa": "-Yeah. -You could have it.",
        "vi": "- Trả lại. - Trả lại."
    },
    {
        "start": "00:07:39,875",
        "end": "00:07:41,208",
        "en": "（しゃきぴよ･ハードボイルド）\n えっ？",
        "pa": "-What?! -Huh?!",
        "vi": "Cái gì?"
    },
    {
        "start": "00:07:41,916",
        "end": "00:07:43,833",
        "en": "（しゃきぴよ）取り返してきたよ",
        "pa": "We got your stuff back.",
        "vi": "Bọn tôi lấy lại được rồi!"
    },
    {
        "start": "00:07:44,833",
        "end": "00:07:46,875",
        "en": "や～る～",
        "pa": "Nice work!",
        "vi": "Tốt lắm!"
    },
    {
        "start": "00:07:47,833",
        "end": "00:07:49,791",
        "en": "お願いがあるんだけど…",
        "pa": "-Can I ask you a favor?",
        "vi": "Tôi muốn xin một việc."
    },
    {
        "start": "00:07:51,500",
        "end": "00:07:52,625",
        "en": "これ 一つ頂戴！",
        "pa": "-Oh! -Can I have one?",
        "vi": "Cho tôi một cái nhé?"
    },
    {
        "start": "00:07:52,708",
        "end": "00:07:54,791",
        "en": "えっ ふ… 二つ頼む",
        "pa": "Um, can you make it two?",
        "vi": "Hả? Hai cái đi mà."
    },
    {
        "start": "00:07:56,000",
        "end": "00:07:57,333",
        "en": "（ボスオムレツ）まあ いいよ～",
        "pa": "- Sure, help yourselves.",
        "vi": "- Được thôi. - Ôi!"
    },
    {
        "start": "00:07:57,416",
        "end": "00:07:58,250",
        "en": "（ハードボイルド）おっ\n （しゃきぴよ）えっ！",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:07:59,125",
        "end": "00:08:00,041",
        "en": "やった～",
        "pa": "Really?\n -Yay! -Ah!",
        "vi": "Hoan hô!"
    },
    {
        "start": "00:08:00,125",
        "end": "00:08:02,875",
        "en": "（ぐでたま）あ ああ あ～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:08:06,666",
        "end": "00:08:08,666",
        "en": "（たまごマフィア）あ～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:08:09,666",
        "end": "00:08:12,166",
        "en": "これで腐らずに済むぜ",
        "pa": "With this, I won't rot anymore.",
        "vi": "Giờ tôi sẽ không thối nữa."
    },
    {
        "start": "00:08:12,791",
        "end": "00:08:16,750",
        "en": "（ぐでたま）\n しょうゆ… しょうゆ…",
        "pa": "Soy sauce!\n Soy sauce!",
        "vi": "Xì dầu."
    },
    {
        "start": "00:08:17,375",
        "end": "00:08:18,416",
        "en": "（ボスオムレツ）\n そんなことないと思うけど～",
        "pa": "-I don't think that stuff works that way. -Huh?",
        "vi": "Ta nghĩ không có chuyện đó đâu."
    },
    {
        "start": "00:08:18,416",
        "end": "00:08:19,250",
        "en": "（ボスオムレツ）\n そんなことないと思うけど～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:08:18,416",
        "end": "00:08:19,250",
        "en": "（ハードボイルド）\n え？",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:08:19,916",
        "end": "00:08:22,666",
        "en": "そのブツは乾燥剤で～",
        "pa": "It's for absorbing moisture.",
        "vi": "Đó là chất hút ẩm."
    },
    {
        "start": "00:08:23,250",
        "end": "00:08:25,375",
        "en": "のりとか\n 乾いてなきゃいけないものを",
        "pa": "It's used to keep things dry,",
        "vi": "Chúng bảo vệ những thứ khô như rong biển khỏi bị ẩm."
    },
    {
        "start": "00:08:25,458",
        "end": "00:08:27,666",
        "en": "湿気から守るためのもので～",
        "pa": "like roasted seaweed and aspirin and stuff.",
        "vi": ""
    },
    {
        "start": "00:08:27,750",
        "end": "00:08:28,791",
        "en": "何！",
        "pa": "What?!",
        "vi": "Gì cơ?"
    },
    {
        "start": "00:08:29,708",
        "end": "00:08:32,666",
        "en": "腐るのは とめられないんだわ～",
        "pa": "If you think it'll keep you from rotting, it won't.",
        "vi": "Nó không ngăn mi bị thối rữa đâu."
    },
    {
        "start": "00:08:32,750",
        "end": "00:08:35,291",
        "en": "ちょっと大げさにウソついて～",
        "pa": "That was just a line we came up with.",
        "vi": "Bọn tao chém gió một tí."
    },
    {
        "start": "00:08:35,375",
        "end": "00:08:37,625",
        "en": "寿命が延びるって言って\n 売ってんだわ～",
        "pa": "We'd sell more if people thought it would make them live longer.",
        "vi": "Nói chúng giúp kéo dài tuổi thọ để bán."
    },
    {
        "start": "00:08:37,708",
        "end": "00:08:41,000",
        "en": "マジか～ ハア…",
        "pa": "No way!",
        "vi": "Đùa tôi à?"
    },
    {
        "start": "00:08:42,208",
        "end": "00:08:44,333",
        "en": "（ぐでたま）あ～ しょうゆ～",
        "pa": "Soy sauce!",
        "vi": "Xì dầu…"
    },
    {
        "start": "00:08:44,416",
        "end": "00:08:47,458",
        "en": "（しゃきぴよ）あっ よかった～",
        "pa": "",
        "vi": "Ôi, tốt quá!"
    },
    {
        "start": "00:08:47,541",
        "end": "00:08:48,500",
        "en": "（マフィアマフィンたち）ん？",
        "pa": "-Look! It's better already! - Huh?!",
        "vi": "Cái gì?"
    },
    {
        "start": "00:08:48,583",
        "end": "00:08:50,541",
        "en": "何で効いてるん？",
        "pa": "You mean it's actually working?",
        "vi": "Tại sao nó lại có tác dụng?"
    },
    {
        "start": "00:08:50,625",
        "end": "00:08:51,958",
        "en": "え… フッ",
        "pa": "Hm. They do say sickness is mostly in the mind.",
        "vi": ""
    },
    {
        "start": "00:08:52,041",
        "end": "00:08:54,416",
        "en": "病は気からと言うからな",
        "pa": "",
        "vi": "Người ta nói bệnh từ tâm mà ra mà."
    },
    {
        "start": "00:08:54,500",
        "end": "00:08:57,375",
        "en": "そういう問題？",
        "pa": "You learn something every day.",
        "vi": "Đơn giản vậy sao?"
    },
    {
        "start": "00:08:59,250",
        "end": "00:09:00,083",
        "en": "（ハエの羽音）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:09:00,166",
        "end": "00:09:03,458",
        "en": "（ハードボイルド）\n じゃあな ぐでたま しゃきぴよ",
        "pa": "Well, see you around, Gudetama, Shakipiyo.",
        "vi": "Hẹn gặp lại, Gudetama. Shakipiyo."
    },
    {
        "start": "00:09:04,666",
        "end": "00:09:06,875",
        "en": "一夜の いい思い出になったぜ",
        "pa": "It has truly been a night to remember.\n Huh.",
        "vi": "Đây sẽ là một đêm đáng nhớ."
    },
    {
        "start": "00:09:10,583",
        "end": "00:09:14,083",
        "en": "ハードボイルドさんには\n 効いてないみたいだね",
        "pa": "You know, I'm not so sure that stuff worked so well on Hard-boiled.",
        "vi": "Có vẻ không có tác dụng với Trứng Luộc."
    },
    {
        "start": "00:09:14,166",
        "end": "00:09:15,083",
        "en": "（ぐでたま）うん",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:09:15,583",
        "end": "00:09:17,125",
        "en": "（鈴木）あっ\n （ぐでたま）あ～？",
        "pa": "-Huh?",
        "vi": ""
    },
    {
        "start": "00:09:17,208",
        "end": "00:09:18,750",
        "en": "（鈴木）やっといた～",
        "pa": "There it is.",
        "vi": "Cuối cùng cũng tìm thấy."
    },
    {
        "start": "00:09:19,375",
        "end": "00:09:21,166",
        "en": "（ぐでたま）え… え～\n （しゃきぴよ）ハッ！",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:09:21,250",
        "end": "00:09:22,291",
        "en": "ぐでたま！",
        "pa": "Gudetama!",
        "vi": "Gudetama!"
    },
    {
        "start": "00:09:23,291",
        "end": "00:09:24,875",
        "en": "（ぐでたま）あ～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:09:24,958",
        "end": "00:09:27,375",
        "en": "（鈴木）\n この子です 例の動くたまご",
        "pa": "This is that egg, the one that moves and talks.",
        "vi": "Đây rồi. Quả trứng biết cử động."
    },
    {
        "start": "00:09:28,291",
        "end": "00:09:32,333",
        "en": "え？　これは\n ただの生卵じゃないんだな？",
        "pa": "Uh… It's not…\n Wait. It's not just a raw egg?",
        "vi": "Gì cơ? Đây chỉ là một quả trứng sống bình thường mà?"
    },
    {
        "start": "00:09:32,833",
        "end": "00:09:33,666",
        "en": "はい",
        "pa": "-Look at it.",
        "vi": "Vâng."
    },
    {
        "start": "00:09:35,125",
        "end": "00:09:36,125",
        "en": "よし",
        "pa": "Oh… Sh…",
        "vi": "Rồi."
    },
    {
        "start": "00:09:37,416",
        "end": "00:09:38,791",
        "en": "（ぐでたま）あ～ あ…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:09:43,083",
        "end": "00:09:44,958",
        "en": "（鈴木）やっと帰れる～",
        "pa": "Sh…\n Now can I finally go home?",
        "vi": "Cuối cùng cũng có thể về nhà."
    },
    {
        "start": "00:09:45,041",
        "end": "00:09:46,708",
        "en": "（井上Ｄ）会社 戻るぞ～",
        "pa": "Uh, let's go straight to the office.",
        "vi": "Về công ty thôi."
    },
    {
        "start": "00:09:48,041",
        "end": "00:09:49,208",
        "en": "え～",
        "pa": "Ugh.",
        "vi": "Hả?"
    },
    {
        "start": "00:09:49,958",
        "end": "00:09:51,541",
        "en": "（タクシーがとまる音）\n （井上Ｄ）ほら！",
        "pa": "Come on! Come on!",
        "vi": "Nào!"
    },
    {
        "start": "00:09:51,625",
        "end": "00:09:52,458",
        "en": "（ドアを開ける音）",
        "pa": "-No, no, wait! Gudetama!",
        "vi": ""
    },
    {
        "start": "00:09:52,541",
        "end": "00:09:55,000",
        "en": "ぐでたま ダメ！",
        "pa": "",
        "vi": "Gudetama! Không!"
    },
    {
        "start": "00:09:56,041",
        "end": "00:09:57,500",
        "en": "ハア ハア ハア…",
        "pa": "Gudetama, come back!",
        "vi": ""
    },
    {
        "start": "00:09:57,583",
        "end": "00:10:00,458",
        "en": "ぐでたまっ　ダメ～！",
        "pa": "Come back!",
        "vi": "Gudetama! Không!"
    },
    {
        "start": "00:10:00,541",
        "end": "00:10:06,083",
        "en": "ハア ハア ハア…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:10:07,791",
        "end": "00:10:11,125",
        "en": "ハア ハア ハア…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:10:12,083",
        "end": "00:10:14,791",
        "en": "あっ あれ？　ない！",
        "pa": "Oh no! Where is it?",
        "vi": "Ôi không, mất rồi!"
    },
    {
        "start": "00:10:14,875",
        "end": "00:10:16,708",
        "en": "ママのＱＲコード",
        "pa": "The secret code to find Mommy!",
        "vi": "Mã QR của mẹ!"
    },
    {
        "start": "00:10:17,583",
        "end": "00:10:18,833",
        "en": "もしかして…",
        "pa": "I wonder if I…",
        "vi": "Chả lẽ…"
    },
    {
        "start": "00:10:20,041",
        "end": "00:10:20,750",
        "en": "♪～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:10:20,750",
        "end": "00:10:23,583",
        "en": "♪～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:10:20,750",
        "end": "00:10:23,583",
        "en": "（しゃきぴよ）\n あの時に 落としたのかも！",
        "pa": "I must've dropped it somewhere along the way!",
        "vi": "lúc đó mình đánh rơi nó rồi!"
    },
    {
        "start": "00:10:24,375",
        "end": "00:10:25,541",
        "en": "急がなきゃ！",
        "pa": "I've gotta go back! ",
        "vi": "Mình phải nhanh lên!"
    },
    {
        "start": "00:10:26,083",
