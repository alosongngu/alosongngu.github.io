[
    {
        "start": "00:00:06,000",
        "end": "00:00:10,958",
        "en": "NETFLIX シリーズ",
        "pa": "",
        "vi": "LOẠT PHIM NETFLIX"
    },
    {
        "start": "00:00:19,208",
        "end": "00:00:20,875",
        "en": "（しゃきぴよ）早く出なきゃ",
        "pa": "OKI SUSHI\n GUDETAMA: AN EGGCELLENT ADVENTURE\n -We've got to get out of here!",
        "vi": "OKI SUSHI\n GUDETAMA: CUỘC PHIÊU LƯU CỦA QUẢ TRỨNG LƯỜI\n Phải đi nhanh thôi!"
    },
    {
        "start": "00:00:20,958",
        "end": "00:00:21,958",
        "en": "（ぐでたま）え～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:00:23,125",
        "end": "00:00:24,541",
        "en": "あ～ そうなん？",
        "pa": "-Do we really--",
        "vi": "Thế à?"
    },
    {
        "start": "00:00:24,625",
        "end": "00:00:26,250",
        "en": "（ぐでたま）わっ\n （しゃきぴよ）わあー！",
        "pa": "",
        "vi": "Ôi!"
    },
    {
        "start": "00:00:26,750",
        "end": "00:00:28,250",
        "en": "当たり前でしょ",
        "pa": "- Ah! -We have our quest!",
        "vi": "Đương nhiên rồi!"
    },
    {
        "start": "00:00:28,333",
        "end": "00:00:31,250",
        "en": "ぴよたち兄弟で\n 絶対にママに会うんだから",
        "pa": "We're siblings on a quest to find our mommy, remember?",
        "vi": "Vì bọn mình là anh em, nhất định phải tìm mẹ!"
    },
    {
        "start": "00:00:31,333",
        "end": "00:00:32,291",
        "en": "（ぐでたま）わっ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:00:32,375",
        "end": "00:00:33,208",
        "en": "（しゃきぴよ）あっ！",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:00:33,291",
        "end": "00:00:35,041",
        "en": "（ぐでたま）マジか～",
        "pa": "Seriously?",
        "vi": "Đùa mình à?"
    },
    {
        "start": "00:00:36,291",
        "end": "00:00:37,583",
        "en": "あっ ぐでたま！",
        "pa": "Gudetama!",
        "vi": "Gudetama!"
    },
    {
        "start": "00:00:37,666",
        "end": "00:00:41,125",
        "en": "あ～～～～",
        "pa": "Whoa!",
        "vi": ""
    },
    {
        "start": "00:00:41,208",
        "end": "00:00:42,333",
        "en": "スルスル",
        "pa": "",
        "vi": "Trượt."
    },
    {
        "start": "00:00:43,708",
        "end": "00:00:44,708",
        "en": "（寿司(すし)職人）あ？",
        "pa": "Hm?",
        "vi": "Hả?"
    },
    {
        "start": "00:00:47,583",
        "end": "00:00:48,666",
        "en": "何だ？",
        "pa": "How'd you get down there, huh?",
        "vi": "Cái gì đây?"
    },
    {
        "start": "00:00:49,416",
        "end": "00:00:50,583",
        "en": "だりぃ",
        "pa": "What a drag.",
        "vi": "Mệt ghê."
    },
    {
        "start": "00:00:52,333",
        "end": "00:00:54,333",
        "en": "俺も だりぃ",
        "pa": "Yeah, that's the way I feel exactly.",
        "vi": "Tôi cũng mệt ghê."
    },
    {
        "start": "00:00:55,833",
        "end": "00:00:57,875",
        "en": "（大将）おい！　ぼやぼやすんな",
        "pa": "- Hey!",
        "vi": "Này! Không được lơ đễnh!"
    },
    {
        "start": "00:00:58,375",
        "end": "00:00:59,500",
        "en": "へ… へい！",
        "pa": "-Get to work. -Uh, yes, sir.",
        "vi": "Vâng!"
    },
    {
        "start": "00:01:02,458",
        "end": "00:01:03,583",
        "en": "（ぐでたま）ん～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:05,416",
        "end": "00:01:10,333",
        "en": "あ～ 食べられる運命っぽい～",
        "pa": "Oh, well.\n I guess I was destined to be someone's lunch.",
        "vi": "Có vẻ là vận mệnh bị ăn rồi."
    },
    {
        "start": "00:01:12,250",
        "end": "00:01:15,458",
        "en": "ダメ～！\n ママに会いにいくんだから",
        "pa": "Oh no!\n Our quest can't be over already!",
        "vi": "Không được! Bọn mình phải cùng nhau đi gặp mẹ!"
    },
    {
        "start": "00:01:16,000",
        "end": "00:01:18,125",
        "en": "ハア ハア ハア…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:18,833",
        "end": "00:01:22,416",
        "en": "流されるだけの人生…",
        "pa": "Living life by going with the flow.",
        "vi": "Cuộc đời chỉ toàn là trôi đi."
    },
    {
        "start": "00:01:24,500",
        "end": "00:01:25,833",
        "en": "見んなよ",
        "pa": "Living life by going with the flow.",
        "vi": "Nhìn gì mà nhìn."
    },
    {
        "start": "00:01:26,750",
        "end": "00:01:29,875",
        "en": "（男の子）お母さん\n あのたまご しゃべった",
        "pa": "Did you hear that?\n That egg just spoke to me.",
        "vi": "Mẹ! Quả trứng đó vừa nói chuyện."
    },
    {
        "start": "00:01:30,458",
        "end": "00:01:32,708",
        "en": "（母親）何言ってんの",
        "pa": "Of course it did, sweetie.",
        "vi": "Con nói gì vậy?"
    },
    {
        "start": "00:01:33,958",
        "end": "00:01:36,541",
        "en": "（いくらたち）個性って何？",
        "pa": "What is meant by individuality?",
        "vi": "Cá tính là gì?"
    },
    {
        "start": "00:01:36,625",
        "end": "00:01:39,583",
        "en": "（ぐでたま）え～\n いきなり何なん？",
        "pa": "Huh? Uh, what? What's that?",
        "vi": "Hả? Tự nhiên lại hỏi."
    },
    {
        "start": "00:01:40,083",
        "end": "00:01:43,041",
        "en": "個性って何？",
        "pa": "What is individuality?",
        "vi": "Cá tính là gì?"
    },
    {
        "start": "00:01:43,125",
        "end": "00:01:46,750",
        "en": "そんなの 味の違いじゃね？",
        "pa": "I think it's the things that set us apart from each other.",
        "vi": "Có thể là sự khác biệt về hương vị?"
    },
    {
        "start": "00:01:47,333",
        "end": "00:01:48,166",
        "en": "味",
        "pa": "Like taste!",
        "vi": "Hương vị!"
    },
    {
        "start": "00:01:48,250",
        "end": "00:01:49,208",
        "en": "形",
        "pa": "Shape!",
        "vi": "Hình dạng!"
    },
    {
        "start": "00:01:49,291",
        "end": "00:01:50,916",
        "en": "（いくら）色\n （いくら）食感",
        "pa": "-Color! -Texture!",
        "vi": "- Màu sắc! - Cảm giác khi ăn!"
    },
    {
        "start": "00:01:51,000",
        "end": "00:01:54,791",
        "en": "うちら みんな同じなんだわ～",
        "pa": "But we're exactly the same. We have no individuality.",
        "vi": "Bọn tớ đều giống nhau."
    },
    {
        "start": "00:01:55,625",
        "end": "00:01:59,416",
        "en": "（たまご寿司）個性か　個性ね…",
        "pa": "Individuality?",
        "vi": "Cá tính à? Cá tính thì…"
    },
    {
        "start": "00:01:59,500",
        "end": "00:02:01,041",
        "en": "（いくらたち）誰？",
        "pa": "Interesting.\n Who are you?",
        "vi": "Ai đấy?"
    },
    {
        "start": "00:02:01,875",
        "end": "00:02:04,541",
        "en": "（たまご寿司）\n きみたちの… 大先輩？",
        "pa": "Someone who's been around longer than you have.",
        "vi": "Đại tiền bối của các bạn."
    },
    {
        "start": "00:02:05,041",
        "end": "00:02:06,875",
        "en": "（いくらたち）どういうこと？",
        "pa": "Really? How much longer?",
        "vi": "Nghĩa là sao?"
    },
    {
        "start": "00:02:06,958",
        "end": "00:02:10,333",
        "en": "（たまご寿司）店 開いた時から\n 回りっぱなし",
        "pa": "I've been on this belt since the place opened.",
        "vi": "Từ lúc quán mở cửa, tôi đã trên băng chuyền suốt."
    },
    {
        "start": "00:02:10,416",
        "end": "00:02:14,458",
        "en": "マジか～　長老じゃね？",
        "pa": "That's long.",
        "vi": "Thật sao? Vậy là trưởng lão rồi."
    },
    {
        "start": "00:02:14,541",
        "end": "00:02:18,375",
        "en": "えっ それって\n 人気(にんき)ないってことじゃね？",
        "pa": "You're like the village elder.\n So… I guess that means you're not a very popular dish?",
        "vi": "Hả? Thế nghĩa là không ai muốn ăn còn gì."
    },
    {
        "start": "00:02:19,458",
        "end": "00:02:22,208",
        "en": "もう カサカサじゃよ",
        "pa": "Not now.\n I'm stale and dried out.",
        "vi": "Tôi đã bị khô cong rồi."
    },
    {
        "start": "00:02:22,291",
        "end": "00:02:23,791",
        "en": "（いくらたち）わあ…",
        "pa": "Aw.",
        "vi": ""
    },
    {
        "start": "00:02:24,375",
        "end": "00:02:26,791",
        "en": "潤いたいのう",
        "pa": "Oh, to be moist and tender again.",
        "vi": "Tôi muốn mềm ẩm…"
    },
    {
        "start": "00:02:26,875",
        "end": "00:02:28,250",
        "en": "切な～",
        "pa": "TMI.",
        "vi": "Buồn ghê."
    },
    {
        "start": "00:02:29,625",
        "end": "00:02:33,875",
        "en": "（しゃきぴよ）\n んしょ んしょ んしょ あ～！",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:02:33,958",
        "end": "00:02:34,791",
        "en": "うっ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:02:36,958",
        "end": "00:02:40,208",
        "en": "んしょ んしょ んしょ んしょ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:02:40,291",
        "end": "00:02:41,291",
        "en": "んしょ～",
        "pa": "Huh?",
        "vi": ""
    },
    {
        "start": "00:02:45,291",
        "end": "00:02:48,416",
        "en": "フフッ　ん～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:02:48,500",
        "end": "00:02:51,125",
        "en": "“たまご天国”…",
        "pa": "Uh…\n \"Egg Heaven?\"",
        "vi": "\"Thiên Đường Trứng?\""
    },
    {
        "start": "00:02:51,208",
        "end": "00:02:52,916",
        "en": "（卵を割る音）\n （しゃきぴよ）ハッ！",
        "pa": "EGG HEAVEN MENU FRIED EGG, OMELET…",
        "vi": "TRỨNG LÒNG ĐÀO RÁN TRỨNG CUỘN…"
    },
    {
        "start": "00:02:56,583",
        "end": "00:02:59,125",
        "en": "これ 地獄だよね？",
        "pa": "That's more like Egg Hell.",
        "vi": "Đây là Địa Ngục mà!"
    },
    {
        "start": "00:03:00,166",
        "end": "00:03:02,791",
        "en": "ぴよは ひよこだから\n 大丈夫だけど…",
        "pa": "Luckily, I'm a chick, so I'm safe. But still…",
        "vi": "Mình là gà con nên mình an toàn, nhưng…"
    },
    {
        "start": "00:03:02,875",
        "end": "00:03:03,833",
        "en": "（包丁でたたき切る音）\n （しゃきぴよ）ん？",
        "pa": "-Hm?",
        "vi": ""
    },
    {
        "start": "00:03:07,958",
        "end": "00:03:09,666",
        "en": "あ～！",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:03:09,750",
        "end": "00:03:11,958",
        "en": "（たまご寿司）教えてやろう",
        "pa": "Here is a lesson.",
        "vi": "Để tôi chỉ dạy cho."
    },
    {
        "start": "00:03:12,041",
        "end": "00:03:15,750",
        "en": "個性とはな\n 自分では無いと思っていても",
        "pa": "Listen carefully.\n Individuality is something you don't even realize you have,",
        "vi": "Các bạn có thể nghĩ các bạn không có cá tính,"
    },
    {
        "start": "00:03:15,833",
        "end": "00:03:19,791",
        "en": "本当は\n 生まれ持っているものなんじゃよ",
        "pa": "because it is something that you are born with.",
        "vi": "nhưng thực ra từ khi sinh ra các bạn đã có nó rồi."
    },
    {
        "start": "00:03:20,375",
        "end": "00:03:22,291",
        "en": "（いくら）えっ 何か…",
        "pa": "Oh, wise words.",
        "vi": "Ồ? Như thể…"
    },
    {
        "start": "00:03:22,375",
        "end": "00:03:24,416",
        "en": "（いくらたち）長老の名言！",
        "pa": "Thank you, wise village elder!",
        "vi": "Danh ngôn của trưởng lão!"
    },
    {
        "start": "00:03:24,500",
        "end": "00:03:26,375",
        "en": "そうなんか？",
        "pa": "It wasn't that wise.",
        "vi": "Vậy à?"
    },
    {
        "start": "00:03:27,416",
        "end": "00:03:30,708",
        "en": "長老の個性って何？",
        "pa": "What's your individuality, wise one?",
        "vi": "Cá tính của trưởng lão là gì?"
    },
    {
        "start": "00:03:31,291",
        "end": "00:03:33,333",
        "en": "カサカサ感？",
        "pa": "My dried-out skin?",
        "vi": "Khô cong queo?"
    },
    {
        "start": "00:03:35,208",
        "end": "00:03:37,291",
        "en": "あと… のり？",
        "pa": "My seaweed belt?",
        "vi": "Với cả rong biển?"
    },
    {
        "start": "00:03:38,958",
        "end": "00:03:40,958",
        "en": "それ キツくないん？",
        "pa": "Doesn't look very comfortable.",
        "vi": "Rong biển có quấn chặt quá không?"
    },
    {
        "start": "00:03:41,541",
        "end": "00:03:44,208",
        "en": "（いくらたち）確かに～",
        "pa": "No, it sure doesn't.",
        "vi": "Đúng vậy."
    },
    {
        "start": "00:03:44,291",
        "end": "00:03:49,041",
        "en": "（たまご寿司）\n んん ん ん ん～ ハア…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:03:49,875",
        "end": "00:03:51,583",
        "en": "自由が欲しい",
        "pa": "I yearn to be free.",
        "vi": "Tôi muốn tự do."
    },
    {
        "start": "00:03:52,625",
        "end": "00:03:56,375",
        "en": "無理っしょ　がんじがらめだし",
        "pa": "Face it. You're bound to that seaweed belt,",
        "vi": "Không thể.\n Bạn bị trói buộc."
    },
    {
        "start": "00:03:56,458",
        "end": "00:04:00,833",
        "en": "のりっていう世間の風潮に\n 縛られてんだわ",
        "pa": "just like you are to the current of the times.",
        "vi": "Bạn bị trói buộc bởi xu hướng rong biển của xã hội."
    },
    {
        "start": "00:04:01,625",
        "end": "00:04:04,291",
        "en": "（しゃきぴよ）\n んしょ んしょ ぴよっ",
        "pa": "",
        "vi": "Chiếp!"
    },
    {
        "start": "00:04:08,166",
        "end": "00:04:10,791",
        "en": "あれに乗れば\n 助けにいけるよね",
        "pa": "If I can get onto that thing, I can save Gudetama!",
        "vi": "Nếu lên được đó, mình có thể đi cứu Gudetama."
    },
    {
        "start": "00:04:13,833",
        "end": "00:04:14,958",
        "en": "ハッ！",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:04:19,666",
        "end": "00:04:21,125",
        "en": "ママ？",
        "pa": "Mommy?",
        "vi": "Mẹ?"
    },
    {
        "start": "00:04:22,416",
        "end": "00:04:24,291",
        "en": "ママなの？　ママ～！",
        "pa": "Mommy, Mommy, it's me!",
        "vi": "Mẹ là mẹ con à? Mẹ ơi!"
    },
    {
        "start": "00:04:26,208",
        "end": "00:04:28,375",
        "en": "（寿司職人）\n 何で ひよこがいんだ？",
        "pa": "What's this chick doing in here?",
        "vi": "Sao lại có gà con ở đây?"
    },
    {
        "start": "00:04:28,958",
        "end": "00:04:32,541",
        "en": "（しゃきぴよ）ねえ ぴよだよ ぴよ\n そのジョリジョリ！",
        "pa": "I remember your scritchy-scratchy face.\n It's me, Shakipiyo!",
        "vi": "Nè, con đây! Piyo! Con biết bộ lông xù xù đó!"
    },
    {
        "start": "00:04:35,666",
        "end": "00:04:36,833",
        "en": "んっ…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:04:38,958",
        "end": "00:04:39,791",
        "en": "あっ",
        "pa": "Oh.",
        "vi": ""
    },
    {
        "start": "00:04:41,166",
        "end": "00:04:44,208",
        "en": "（いくら）それで結局 個性って…",
        "pa": "Won't you tell us more about individuality?",
        "vi": "Vậy rốt cuộc thì cá tính…"
    },
    {
        "start": "00:04:44,291",
        "end": "00:04:45,708",
        "en": "（いくらたち）何？",
        "pa": "Please! Please!",
        "vi": "Là gì?"
    },
    {
        "start": "00:04:46,208",
        "end": "00:04:50,125",
        "en": "（たまご寿司）たまごは たまご\n 個性なんてないっしょ",
        "pa": "There's no such thing.\n An egg is just an egg.",
        "vi": "Một quả trứng là một quả trứng. Không có cá tính đâu."
    },
    {
        "start": "00:04:51,250",
        "end": "00:04:54,291",
        "en": "（ぐでたま）言ってること\n さっきと違わね？",
        "pa": "Weren't you saying just the opposite a minute ago?",
        "vi": "Lúc trước bạn nói khác mà?"
    },
    {
        "start": "00:04:54,375",
        "end": "00:04:57,916",
        "en": "だって しょせん\n たまごは たまごっしょ",
        "pa": "I was wrong.\n One egg is just like another.",
        "vi": "Rốt cuộc một quả trứng cũng chỉ là một quả trứng."
    },
    {
        "start": "00:04:58,000",
        "end": "00:04:59,375",
        "en": "（プリン）プリ～ン",
        "pa": "-Pudding! -Oh!",
        "vi": "Pudding!"
    },
    {
        "start": "00:04:59,458",
        "end": "00:05:00,291",
        "en": "お～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:05:00,375",
        "end": "00:05:02,458",
        "en": "めっちゃ個性的",
        "pa": "-Now that's individuality.",
        "vi": "Rất cá tính đấy."
    },
    {
        "start": "00:05:02,541",
        "end": "00:05:05,166",
        "en": "原宿(はらじゅく)系じゃね？",
        "pa": "-Jiggle, jiggle, jiggle… -Probably Harajuku style?",
        "vi": "Phong cách Harajuku à?"
    },
    {
        "start": "00:05:05,875",
        "end": "00:05:09,958",
        "en": "（プリン）揺らさないで～\n クリームが乱れる～",
        "pa": "Wobble, wobble.\n Whoa! Careful. Don't knock my toppings off.",
        "vi": "Đừng lắc tớ nữa. Làm hỏng kem của tớ mất."
    },
    {
        "start": "00:05:11,125",
        "end": "00:05:12,708",
        "en": "（いくら）ねえねえ",
        "pa": "Hey, Pudding.",
        "vi": "Nè."
    },
    {
        "start": "00:05:12,791",
        "end": "00:05:16,291",
        "en": "（いくらたち）どうやって\n そんな個性的になったの？",
        "pa": "Where did you get all your individuality?",
        "vi": "Làm thế nào mà cậu trở nên cá tính thế?"
    },
    {
        "start": "00:05:17,041",
        "end": "00:05:18,500",
        "en": "え？",
        "pa": "We're dying to know.\n My what?",
        "vi": "Cái gì?"
    },
    {
        "start": "00:05:18,583",
        "end": "00:05:21,625",
        "en": "（たまごたち）ぷり ぷり ぷり",
        "pa": "",
        "vi": "Pudding!"
    },
    {
        "start": "00:05:21,708",
        "end": "00:05:25,250",
        "en": "（たまごたち）\n ぶりーん ぷりーん ぷりーん",
        "pa": "Oh.",
        "vi": "Pudding."
    },
    {
        "start": "00:05:25,333",
        "end": "00:05:29,083",
        "en": "（プリンたち）\n プリーン プリーン プリーン",
        "pa": "Pudding.\n Pudding.",
        "vi": "Pudding."
    },
    {
        "start": "00:05:29,666",
        "end": "00:05:30,958",
        "en": "（いくらたち）へえ～",
        "pa": "Pudding. Pudding.\n Oh!",
        "vi": "Ồ."
    },
    {
        "start": "00:05:31,458",
        "end": "00:05:33,916",
        "en": "（いくら）個性的な たまごは…",
        "pa": "Each egg is an individual.",
        "vi": "Những quả trứng cá tính"
    },
    {
        "start": "00:05:34,000",
        "end": "00:05:36,000",
        "en": "（いくら）生まれから違うわ～",
        "pa": "They're all unique from the very start.",
        "vi": "khác biệt ngay từ khi sinh ra."
    },
    {
        "start": "00:05:36,583",
        "end": "00:05:39,291",
        "en": "そういう きみたちは？",
        "pa": "Okay, so what's your story?",
        "vi": "Còn các cậu thì sao?"
    },
    {
        "start": "00:05:39,375",
        "end": "00:05:42,791",
        "en": "（いくらたち）\n うちらなんて普通なんだわ～",
        "pa": "Oh, there's nothing much to tell.\n We're pretty normal.",
        "vi": "Bọn tớ bình thường lắm."
    },
    {
        "start": "00:05:44,125",
        "end": "00:05:47,583",
        "en": "（いくら）生まれたのは\n ベーリング海で～",
        "pa": "We were born way, way out in the Bering Sea.",
        "vi": "Bọn tớ sinh ra ở biển Bering."
    },
    {
        "start": "00:05:47,666",
        "end": "00:05:52,291",
        "en": "（いくら）\n カニ漁で一獲千金を狙う男たちに～",
        "pa": "There are men out there who fish for crabs.",
        "vi": "Những người bắt cua để làm giàu nhanh chóng"
    },
    {
        "start": "00:05:53,250",
        "end": "00:05:55,250",
        "en": "（いくら）ママが捕られて～",
        "pa": "There's a lot of money in crabs.\n They captured our mother.",
        "vi": "đã bắt mẹ chúng tớ."
    },
    {
        "start": "00:05:55,791",
        "end": "00:05:57,583",
        "en": "（いくら）かっさばかれて～",
        "pa": "Then she got filleted.",
        "vi": "Rồi mổ mẹ ra."
    },
    {
        "start": "00:05:57,666",
        "end": "00:05:59,125",
        "en": "（いくらたち）ママ～",
        "pa": "Mommy!",
        "vi": "Mẹ ơi!"
    },
    {
        "start": "00:05:59,916",
        "end": "00:06:03,541",
        "en": "それで日本に送られたんだよね～",
        "pa": "Then we all got put in a boat and shipped to Japan.",
        "vi": "Sau đó chúng tớ được chuyển đến Nhật Bản."
    },
    {
        "start": "00:06:04,250",
        "end": "00:06:06,958",
        "en": "いや めっちゃ個性的じゃね？",
        "pa": "Wow, you have lots of individuality.",
        "vi": "Cực kỳ cá tính còn gì."
    },
    {
        "start": "00:06:07,041",
        "end": "00:06:09,833",
        "en": "個性 一番あるんじゃね？",
        "pa": "Probably more individuality than anyone here.",
        "vi": "Các cậu độc đáo nhất ở đây còn gì."
    },
    {
        "start": "00:06:10,416",
        "end": "00:06:13,333",
        "en": "え？　そうなん？",
        "pa": "Huh?\n Really? We do?",
        "vi": "Cái gì? Thật à?"
    },
    {
        "start": "00:06:14,041",
        "end": "00:06:16,958",
        "en": "あっ じゃあね～",
        "pa": "Uh-oh!\n Gotta go.",
        "vi": "Chào nhé."
    },
    {
        "start": "00:06:18,375",
        "end": "00:06:19,375",
        "en": "あ…",
        "pa": "Oh…",
        "vi": ""
    },
    {
        "start": "00:06:19,875",
        "end": "00:06:21,125",
        "en": "おっ先～",
        "pa": "See ya 'round.",
        "vi": "Đi trước nhé."
    },
    {
        "start": "00:06:21,708",
        "end": "00:06:24,583",
        "en": "これは そろそろ\n うちらの番じゃな",
        "pa": "Looks like our turn will be coming up soon.",
        "vi": "Sắp đến lượt tôi rồi."
    },
    {
        "start": "00:06:24,666",
        "end": "00:06:28,958",
        "en": "（うずらのつぶやき）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:06:29,041",
        "end": "00:06:31,583",
        "en": "（茶碗蒸し）息苦しい～",
        "pa": "Whew, it's steamy in here.",
        "vi": "Khó thở ghê."
    },
    {
        "start": "00:06:31,666",
        "end": "00:06:34,375",
        "en": "（たまご豆腐）\n 選ばれなかったら へこむわ…",
        "pa": "I'm gonna be depressed if no one chooses me.",
        "vi": "Nếu không ai chọn thì tớ chán nản lắm."
    },
    {
        "start": "00:06:36,541",
        "end": "00:06:39,791",
        "en": "食べてもらえないのも\n また運命",
        "pa": "Not being eaten is another type of fate, I suppose.",
        "vi": "Không bị ăn là một số phận khác."
    },
    {
        "start": "00:06:39,875",
        "end": "00:06:44,375",
        "en": "覚悟しておくのじゃ\n ここからは長い道のりに…",
        "pa": "We must be prepared for a long road ahead that may never end.",
        "vi": "Bạn phải chuẩn bị cho con đường dài phía trước."
    },
    {
        "start": "00:06:44,458",
        "end": "00:06:46,958",
        "en": "（女性）おいしそう\n （女性）たまご豆腐 大好き",
        "pa": "-Ah, this looks delicious!",
        "vi": "- Trông ngon ghê! - Con thích đậu phụ trứng lắm!"
    },
    {
        "start": "00:06:47,041",
        "end": "00:06:49,125",
        "en": "（男性）おっ うずらあんのか！",
        "pa": "Oh! They have quail eggs!",
        "vi": "Có cả trứng cút kìa!"
    },
    {
        "start": "00:06:49,791",
        "end": "00:06:50,750",
        "en": "あ～",
        "pa": "I give up.",
        "vi": ""
    },
    {
        "start": "00:06:51,916",
        "end": "00:06:53,083",
        "en": "はいはい",
        "pa": "",
        "vi": "Đành vậy."
    },
    {
        "start": "00:06:54,333",
        "end": "00:06:57,666",
        "en": "心まで渇いちゃってるっしょ",
        "pa": "A little soy sauce and you're good as new.",
        "vi": "Trái tim bạn cũng khô héo rồi phải không?"
    },
    {
        "start": "00:06:58,250",
        "end": "00:07:02,750",
        "en": "（しゃきぴよ）ん～～～ あっ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:07:02,833",
        "end": "00:07:03,666",
        "en": "んっ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:07:04,750",
        "end": "00:07:05,916",
        "en": "やった～！",
        "pa": "Hooray!",
        "vi": "Mình làm được rồi!"
    },
    {
        "start": "00:07:08,250",
        "end": "00:07:09,375",
        "en": "ハッ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:07:10,541",
        "end": "00:07:11,458",
        "en": "ママ？",
        "pa": "Mommy?\n I WAS BORN HERE",
        "vi": "TÔI ĐÃ SINH RA Ở ĐÂY\n Mẹ?"
    },
    {
        "start": "00:07:16,666",
        "end": "00:07:18,000",
        "en": "この暗号…",
        "pa": "A secret code!",
        "vi": "Mã này"
    },
    {
        "start": "00:07:18,500",
        "end": "00:07:20,791",
        "en": "ママのいる所が\n 書いてあるんだ！",
        "pa": "This will lead us to where Mommy is!",
        "vi": "cho biết mẹ đang ở đâu!"
    },
    {
        "start": "00:07:22,333",
        "end": "00:07:23,166",
        "en": "ハッ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:07:26,541",
        "end": "00:07:28,416",
        "en": "あっ たまご来る",
        "pa": "Look! The egg's coming around again!",
        "vi": "Trứng đến rồi!"
    },
    {
        "start": "00:07:29,125",
        "end": "00:07:32,083",
        "en": "（たまご寿司）\n ついに お迎えが来たようじゃ",
        "pa": "Looks like my time has finally come.",
        "vi": "Có vẻ cũng đã đến lúc tôi phải đi rồi."
    },
    {
        "start": "00:07:33,083",
        "end": "00:07:35,708",
        "en": "お前も諦めるでないぞ",
        "pa": "Your turn will come too, my friend. Don't worry.",
        "vi": "Bạn cũng đừng bỏ cuộc nhé."
    },
    {
        "start": "00:07:38,791",
        "end": "00:07:39,708",
        "en": "えっ…",
        "pa": "-Hm?",
        "vi": "Hả?"
    },
    {
        "start": "00:07:39,791",
        "end": "00:07:42,333",
        "en": "あ～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:07:42,833",
        "end": "00:07:45,791",
        "en": "うっ う う う う…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:07:45,875",
        "end": "00:07:50,250",
        "en": "た… 耐えろ 耐えろ　耐え～るう～",
        "pa": "You can do this.\n Oh. Oh. Oh!",
        "vi": "Gắng lên nào!"
    },
    {
        "start": "00:07:50,916",
        "end": "00:07:53,208",
        "en": "自由も不自由",
        "pa": "Freedom's more work than I thought it would be.",
        "vi": "Tự do cũng bất tiện thật."
    },
    {
        "start": "00:07:54,583",
        "end": "00:07:55,791",
        "en": "さっきのだ",
        "pa": "-This is that egg. -Mm.",
        "vi": "- Là quả trứng lúc nãy. - Ở đây có cả trứng sống nhỉ?"
    },
    {
        "start": "00:07:55,875",
        "end": "00:07:58,125",
        "en": "（母親）ふう～ん\n 生卵なんてあるのねえ",
        "pa": "Weird they serve raw eggs here.",
        "vi": ""
    },
    {
        "start": "00:08:00,166",
        "end": "00:08:01,000",
        "en": "ぴよっ",
        "pa": "Piyo!",
        "vi": "Chiếp!"
    },
    {
        "start": "00:08:03,000",
        "end": "00:08:05,041",
        "en": "釜玉うどんにしよーっと",
        "pa": "I'm putting you on my udon, talking egg.",
        "vi": "Con sẽ ăn nó với mỳ udon."
    },
    {
        "start": "00:08:14,541",
        "end": "00:08:15,625",
        "en": "（ぐでたま）あ… あ…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:08:16,416",
        "end": "00:08:17,500",
        "en": "やめてください",
        "pa": "Do away with those.",
        "vi": "Thôi đi."
    },
    {
        "start": "00:08:20,875",
        "end": "00:08:25,875",
        "en": "（泣き声）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:08:25,958",
        "end": "00:08:27,625",
        "en": "（母親）すみません",
        "pa": "I'm awfully sorry.",
        "vi": "Tôi xin lỗi."
    },
    {
        "start": "00:08:27,708",
        "end": "00:08:28,583",
        "en": "今だ！",
        "pa": "I gotcha! Phew.",
        "vi": "Đến lúc rồi!"
    },
    {
        "start": "00:08:28,666",
        "end": "00:08:29,500",
        "en": "（母親）すみません",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:08:29,583",
        "end": "00:08:30,750",
        "en": "（男の子の泣き声）",
        "pa": "- Oh, what's wrong?",
        "vi": ""
    },
    {
        "start": "00:08:30,833",
        "end": "00:08:32,250",
        "en": "（しゃきぴよ）ハア ハア ハア…",
        "pa": "-Sweetie, I can't know what's wrong.",
        "vi": ""
    },
    {
        "start": "00:08:32,958",
        "end": "00:08:36,000",
        "en": "（しゃきぴよ）やっ\n （ぐでたま）ぬるっ あ～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:08:37,791",
        "end": "00:08:41,291",
        "en": "（男の子）\n 今 たまごが逃げてった～",
        "pa": "Where'd it go? The talking egg ran away!",
        "vi": "Quả trứng vừa chạy mất rồi!"
    },
    {
        "start": "00:08:43,916",
        "end": "00:08:45,416",
        "en": "（ぐでたま）ハア…",
        "pa": "Aw, poor Shakipiyo. They're looking a little fried.",
        "vi": "OKI SUSHI MÓN ĐẶC BIỆT HÔM NAY"
    },
    {
        "start": "00:08:45,500",
        "end": "00:08:48,583",
        "en": "しゃきぴよ 揚げられちゃったか～",
        "pa": "",
        "vi": "Shakipiyo bị chiên giòn mất rồi."
    },
    {
        "start": "00:08:49,458",
        "end": "00:08:51,250",
        "en": "それ ぴよじゃないよ！",
        "pa": "That's not me. I'm right here.",
        "vi": "Đó không phải tớ!"
    },
    {
        "start": "00:08:51,750",
        "end": "00:08:53,416",
        "en": "今 助けたでしょ",
        "pa": "-Whoa. That was exhausting.",
        "vi": "Tớ vừa cứu cậu đó."
    },
    {
        "start": "00:08:54,166",
        "end": "00:08:56,791",
        "en": "あれから大変だったんだから～",
        "pa": "-And scary too.",
        "vi": "Lúc nãy tớ gặp rắc rối đấy."
    },
    {
        "start": "00:08:56,875",
        "end": "00:08:59,375",
        "en": "もう会えないかと\n 思っちゃったし～",
        "pa": "It's tough out here.\n I thought I was never going to see you again.",
        "vi": "Cứ tưởng không thể gặp cậu nữa."
    },
    {
        "start": "00:08:59,458",
        "end": "00:09:01,625",
        "en": "やっぱ しょうゆだわ～",
        "pa": "Isn't soy sauce the best?",
        "vi": "Không có gì bằng xì dầu."
    },
    {
        "start": "00:09:03,000",
        "end": "00:09:04,208",
        "en": "（しゃきぴよ）聞いて！",
        "pa": "-Listen to me!",
        "vi": "Nghe này!"
    },
    {
        "start": "00:09:04,750",
        "end": "00:09:08,500",
        "en": "それより さっき\n お箸 パシッってしたでしょ",
        "pa": "I saw you just now.\n You knocked that boy's chopsticks away.",
        "vi": "Lúc nãy cậu hất chiếc đũa của cậu nhóc đó còn gì."
    },
    {
        "start": "00:09:08,583",
        "end": "00:09:12,041",
        "en": "やっぱり ほんとは\n 食べられたくないんだよ",
        "pa": "You don't want to be eaten.",
        "vi": "Quả nhiên thật ra cậu không muốn bị ăn."
    },
    {
        "start": "00:09:12,125",
        "end": "00:09:13,666",
        "en": "ママに会いたいんだよ",
        "pa": "You want to come with me and find Mommy, don't you?",
        "vi": "Cậu cũng muốn gặp mẹ!"
    },
    {
        "start": "00:09:15,083",
        "end": "00:09:18,375",
        "en": "ん～ そうなんか？",
        "pa": "Well… I don't know.",
        "vi": "Là vậy sao?"
    },
    {
        "start": "00:09:19,500",
        "end": "00:09:21,208",
        "en": "（しゃきぴよ）さっ 行くよ",
        "pa": "Yeah. Let's go.",
        "vi": "Giờ thì đi thôi!"
    },
    {
        "start": "00:09:21,291",
        "end": "00:09:23,000",
        "en": "どっから？",
        "pa": "Where'd that come from?",
        "vi": "Ở đâu ra thế?"
    },
    {
        "start": "00:09:24,250",
        "end": "00:09:25,875",
        "en": "（井上Ｄ）あの～ すいません",
        "pa": "Uh, excuse me.",
        "vi": "Xin lỗi."
    },
    {
        "start": "00:09:26,875",
        "end": "00:09:28,625",
        "en": "こういう者なんですが",
        "pa": "-I produce a television show. - Oh.",
        "vi": "Tôi xin tự giới thiệu."
    },
    {
        "start": "00:09:28,708",
        "end": "00:09:29,541",
        "en": "（母親）え？",
        "pa": "",
        "vi": "Dạ?"
    },
    {
        "start": "00:09:29,625",
        "end": "00:09:31,708",
        "en": "（井上Ｄ）未知なる生物を追う\n テレビ番組を",
        "pa": "It's about hunting and capturing unknown creatures.",
        "vi": "Tôi đang sản xuất một chương trình tìm kiếm những sinh vật lạ."
    },
    {
        "start": "00:09:31,791",
        "end": "00:09:33,541",
        "en": "作ろうとしている者なんですが",
        "pa": "Huh?",
        "vi": ""
    },
    {
        "start": "00:09:34,166",
        "end": "00:09:40,250",
