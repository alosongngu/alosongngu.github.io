[
    {
        "start": "00:00:06,000",
        "end": "00:00:06,750",
        "en": "NETFLIX シリーズ",
        "pa": "",
        "vi": "LOẠT PHIM NETFLIX"
    },
    {
        "start": "00:00:06,750",
        "end": "00:00:11,541",
        "en": "NETFLIX シリーズ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:00:06,750",
        "end": "00:00:11,541",
        "en": "（波の音）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:00:11,541",
        "end": "00:00:12,916",
        "en": "（波の音）",
        "pa": "GUDETAMA: AN EGGCELLENT ADVENTURE",
        "vi": "GUDETAMA: CUỘC PHIÊU LƯU CỦA QUẢ TRỨNG LƯỜI"
    },
    {
        "start": "00:00:20,583",
        "end": "00:00:22,416",
        "en": "（半グレ１）おっせえな～",
        "pa": "Where are these bums? They're late.",
        "vi": "Đến muộn vãi."
    },
    {
        "start": "00:00:22,916",
        "end": "00:00:23,791",
        "en": "（半グレ２）んあ～",
        "pa": "Oh, don't you sound tough?",
        "vi": ""
    },
    {
        "start": "00:00:23,875",
        "end": "00:00:27,666",
        "en": "あの人の前で\n そんな態度 ダメなんだわ～",
        "pa": "I wouldn't be so eager for them to show up if I was you.",
        "vi": "Mày không được có thái độ như thế trước mặt kẻ đó đâu."
    },
    {
        "start": "00:00:28,958",
        "end": "00:00:31,666",
        "en": "（半グレ１）\n だって 早くアレがねえと…",
        "pa": "Yeah, well, let's just hope they bring the goods,",
        "vi": "Nhưng nếu không lấy hàng sớm, ta sẽ…"
    },
    {
        "start": "00:00:33,041",
        "end": "00:00:34,541",
        "en": "ヤベ～っしょ～",
        "pa": "or we're in big trouble.",
        "vi": "chết dở đấy."
    },
    {
        "start": "00:00:34,625",
        "end": "00:00:36,916",
        "en": "（半グレ３）ヤツらは闇…",
        "pa": "-These guys are mobsters. -Oh!",
        "vi": "Bọn nó là dân xã hội đen."
    },
    {
        "start": "00:00:38,375",
        "end": "00:00:40,916",
        "en": "そんな態度を見せれば",
        "pa": "Huh?\n Cop an attitude with them.",
        "vi": "Mày mà cứ có thái độ như thế,"
    },
    {
        "start": "00:00:42,000",
        "end": "00:00:43,000",
        "en": "消されるぞ",
        "pa": "-And they'll rub you out.",
        "vi": "thì mày đăng xuất đấy."
    },
    {
        "start": "00:00:43,000",
        "end": "00:00:43,833",
        "en": "消されるぞ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:00:43,000",
        "end": "00:00:43,833",
        "en": "（たまごリムジンの\n 走行音）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:00:43,916",
        "end": "00:00:44,750",
        "en": "（半グレ２）あっ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:00:48,625",
        "end": "00:00:49,833",
        "en": "（半グレ２）来た！\n （半グレ１）え？",
        "pa": "-They're here. -Huh?",
        "vi": "Đến rồi."
    },
    {
        "start": "00:00:53,875",
        "end": "00:00:58,458",
        "en": "（うずらの煮たまごたちの声）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:00,500",
        "end": "00:01:02,875",
        "en": "（煮たまご）さっさと済ますぞ",
        "pa": "Let's get this over with.",
        "vi": "Làm nhanh nào."
    },
    {
        "start": "00:01:02,958",
        "end": "00:01:06,166",
        "en": "（うずらの煮たまごたちの小声）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:08,541",
        "end": "00:01:10,708",
        "en": "（うずらの煮たまごたちの小声）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:15,250",
        "end": "00:01:17,416",
        "en": "（半グレたち）おお～",
        "pa": "Whoa!",
        "vi": ""
    },
    {
        "start": "00:01:19,166",
        "end": "00:01:20,333",
        "en": "（半グレ１）んあ…\n （煮たまご）おうっ",
        "pa": "-Oh!",
        "vi": ""
    },
    {
        "start": "00:01:20,916",
        "end": "00:01:22,666",
        "en": "出すもん出してからだ",
        "pa": "-Ow! -You give me what I need first.",
        "vi": "Xì dầu trao cháo múc."
    },
    {
        "start": "00:01:23,250",
        "end": "00:01:25,666",
        "en": "（半グレ２）\n 持ってきました しょうゆなら",
        "pa": "- That hurt! - The soy sauce!\n It's all right there.",
        "vi": "Chúng tôi mang xì dầu tới rồi đây!"
    },
    {
        "start": "00:01:25,750",
        "end": "00:01:26,416",
        "en": "（しゃきぴよ）あれだよー！",
        "pa": "-Gudetama, look!",
        "vi": "Kia rồi!"
    },
    {
        "start": "00:01:26,416",
        "end": "00:01:27,500",
        "en": "（しゃきぴよ）あれだよー！",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:26,416",
        "end": "00:01:27,500",
        "en": "（ぐでたまの鼻歌）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:27,500",
        "end": "00:01:28,000",
        "en": "（ぐでたまの鼻歌）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:28,000",
        "end": "00:01:29,166",
        "en": "（ぐでたまの鼻歌）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:28,000",
        "end": "00:01:29,166",
        "en": "（しゃきぴよ）あれさえあれば",
        "pa": "I bet that's the magical stuff we need to get to keep you from rotting,",
        "vi": "Chỉ cần có thứ kia, cậu sẽ không bị thối nữa."
    },
    {
        "start": "00:01:29,166",
        "end": "00:01:29,250",
        "en": "（ぐでたまの鼻歌）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:29,250",
        "end": "00:01:32,250",
        "en": "（ぐでたまの鼻歌）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:29,250",
        "end": "00:01:32,250",
        "en": "ぐでたまが腐るの\n とめられるんだよ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:32,250",
        "end": "00:01:33,375",
        "en": "（ぐでたまの鼻歌）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:33,458",
        "end": "00:01:35,208",
        "en": "グレたまが言ってたでしょ",
        "pa": "like what Guretama said.",
        "vi": "Guretama nói rồi mà."
    },
    {
        "start": "00:01:35,958",
        "end": "00:01:38,666",
        "en": "（グレたま）\n 食いもんが悪くなんのをとめる",
        "pa": "There's this magical stuff out there. They say it keeps food from going bad.",
        "vi": "Ngoài kia có thứ hàng phép thuật. Nó ngăn thức ăn không bị hỏng."
    },
    {
        "start": "00:01:38,750",
        "end": "00:01:40,708",
        "en": "魔法のブツが\n あるっちゅうわけや",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:41,291",
        "end": "00:01:42,500",
        "en": "それ欲しい！",
        "pa": "-That's just what we need.",
        "vi": "Bọn tớ muốn có nó!"
    },
    {
        "start": "00:01:42,583",
        "end": "00:01:45,541",
        "en": "でもよ 売りさばいてんのは…",
        "pa": "The thing is the only one selling the stuff are really bad eggs",
        "vi": "Nhưng những kẻ bán nó là"
    },
    {
        "start": "00:01:46,125",
        "end": "00:01:49,833",
        "en": "闇の世界の 危険な連中なんや",
        "pa": "from the criminal underworld!",
        "vi": "đám xã hội đen nguy hiểm."
    },
    {
        "start": "00:01:49,916",
        "end": "00:01:53,208",
        "en": "（ぐでたま）ハア…\n “危険な連中”か…",
        "pa": "Bad people, huh?",
        "vi": "Đám xã hội đen nguy hiểm à?"
    },
    {
        "start": "00:01:53,291",
        "end": "00:01:54,958",
        "en": "しょうゆ あんの？",
        "pa": "Where's my soy sauce?",
        "vi": "Cậu có xì dầu không?"
    },
    {
        "start": "00:01:55,541",
        "end": "00:01:56,833",
        "en": "ないけど…",
        "pa": "You know we haven't got any.",
        "vi": "Không, nhưng…"
    },
    {
        "start": "00:01:57,333",
        "end": "00:01:58,166",
        "en": "（ぐでたま）よいしょ",
        "pa": "- Soy sauce. -But don't worry.",
        "vi": ""
    },
    {
        "start": "00:01:58,250",
        "end": "00:01:59,500",
        "en": "ぴよ 取ってくる！",
        "pa": "I'll get it for you, 'cause we can't see Mommy if…",
        "vi": "tớ sẽ đi lấy về."
    },
    {
        "start": "00:02:00,250",
        "end": "00:02:01,875",
        "en": "腐ったら ママに会えないし",
        "pa": "",
        "vi": "Nếu bị thối, cậu sẽ không gặp mẹ được."
    },
    {
        "start": "00:02:01,958",
        "end": "00:02:03,416",
        "en": "（ぐでたま）ハア～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:02:03,500",
        "end": "00:02:07,500",
        "en": "それに ぐでたま\n いつかは料理されたいんでしょ？",
        "pa": "Well, if you've gone rotten.\n And I know you want to be cooked one day.",
        "vi": "Hơn nữa, Gudetama muốn ngày nào đó được nấu thành món ăn còn gì?"
    },
    {
        "start": "00:02:07,583",
        "end": "00:02:09,916",
        "en": "あ～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:02:11,166",
        "end": "00:02:12,083",
        "en": "ここで待ってて",
        "pa": "So you just wait right here.",
        "vi": "Chờ ở đây nhé!"
    },
    {
        "start": "00:02:12,166",
        "end": "00:02:13,000",
        "en": "うん",
        "pa": "Meh.",
        "vi": ""
    },
    {
        "start": "00:02:13,666",
        "end": "00:02:14,958",
        "en": "（ハードボイルド）待つんだ",
        "pa": "Hold it.",
        "vi": "Khoan đã."
    },
    {
        "start": "00:02:17,750",
        "end": "00:02:19,833",
        "en": "（ハードボイルド）\n ヤツに近づけば…",
        "pa": "You don't wanna mess with those guys.",
        "vi": "Nếu bạn đến gần chúng,"
    },
    {
        "start": "00:02:20,500",
        "end": "00:02:23,500",
        "en": "ヤケドするぜ　フッ…",
        "pa": "They'll eat you alive.",
        "vi": "bạn sẽ bị bỏng đấy."
    },
    {
        "start": "00:02:23,583",
        "end": "00:02:26,458",
        "en": "（しゃきぴよ）\n ヤケド！　熱いのヤダ！",
        "pa": "-Hm. -Oh no!\n I don't wanna be eaten alive!",
        "vi": "Bỏng? Tớ ghét nóng!"
    },
    {
        "start": "00:02:27,041",
        "end": "00:02:31,166",
        "en": "いや そういう\n 文字どおりの意味じゃないんだわ",
        "pa": "No, I didn't mean it literally.",
        "vi": "Không, không phải nghĩa đen của từ đó đâu."
    },
    {
        "start": "00:02:31,250",
        "end": "00:02:33,208",
        "en": "顔 違くね？",
        "pa": "It's an expression.\n Did your face just change?",
        "vi": "Mặt trông khác thế."
    },
    {
        "start": "00:02:33,791",
        "end": "00:02:34,625",
        "en": "あっ！",
        "pa": "-No!",
        "vi": ""
    },
    {
        "start": "00:02:35,291",
        "end": "00:02:36,125",
        "en": "フッ…",
        "pa": "See?",
        "vi": ""
    },
    {
        "start": "00:02:36,208",
        "end": "00:02:38,916",
        "en": "っていうか誰？",
        "pa": "-Wait. Just who are you?",
        "vi": "Mà cậu là ai?"
    },
    {
        "start": "00:02:39,500",
        "end": "00:02:40,666",
        "en": "ハードボイルド",
        "pa": "They call me Hard-boiled",
        "vi": "Trứng Luộc."
    },
    {
        "start": "00:02:41,541",
        "end": "00:02:44,083",
        "en": "15分という ゆで時間を経て",
        "pa": "because I was boiled for 15 minutes straight.",
        "vi": "Sau 15 phút luộc,"
    },
    {
        "start": "00:02:44,166",
        "end": "00:02:47,791",
        "en": "身も心も\n ハードになっちまった卵さ",
        "pa": "My body and my soul ended up being as hard as a rock.",
        "vi": "tôi là một quả trứng có cơ thể và linh hồn cứng rắn."
    },
    {
        "start": "00:02:47,875",
        "end": "00:02:48,708",
        "en": "フッ",
        "pa": "Huh.",
        "vi": ""
    },
    {
        "start": "00:02:48,791",
        "end": "00:02:50,041",
        "en": "カッチカチ",
        "pa": "You are hard.",
        "vi": "Rắn chắc đấy."
    },
    {
        "start": "00:02:51,458",
        "end": "00:02:54,125",
        "en": "（ハードボイルド）\n とにかく ヤツには近づくな",
        "pa": "Anyway, you wanna steer clear of those guys.",
        "vi": "Tóm lại thì tránh xa chúng ra."
    },
    {
        "start": "00:02:54,750",
        "end": "00:02:57,125",
        "en": "ヤツは黒き煮たまご",
        "pa": "The tall one is Seasoned-Egg,\n one of the big bosses of the Egg Mafia",
        "vi": "Đó là trứng trà. Trứng đang khóc thấy kẻ đó là phải nín bặt."
    },
    {
        "start": "00:02:57,208",
        "end": "00:03:00,250",
        "en": "泣くたまごも黙る\n たまごマフィアの幹部だ",
        "pa": "and not somebody you wanna cross.",
        "vi": "Một trong những kẻ cầm đầu Mafia Trứng."
    },
    {
        "start": "00:03:01,583",
        "end": "00:03:03,958",
        "en": "た… たまごマフィア？",
        "pa": "-The… The Egg Mafia?",
        "vi": "Mafia Trứng?"
    },
    {
        "start": "00:03:04,041",
        "end": "00:03:07,458",
        "en": "近づけば\n ヤケドするどころじゃ済まない",
        "pa": "That's right.\n You'd be lucky to get eaten alive because they'll fry you first.",
        "vi": "Bạn đến gần chúng thì không chỉ đơn giản là bị bỏng đâu."
    },
    {
        "start": "00:03:08,291",
        "end": "00:03:09,291",
        "en": "炭になるぜ",
        "pa": "",
        "vi": "- Bị nướng thành than đấy. - Nướng than?"
    },
    {
        "start": "00:03:09,375",
        "end": "00:03:10,875",
        "en": "炭火焼き！",
        "pa": "Too scary!",
        "vi": ""
    },
    {
        "start": "00:03:10,958",
        "end": "00:03:12,666",
        "en": "（車のドアが閉まる音）\n （ハードボイルド）ん？",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:03:12,750",
        "end": "00:03:15,000",
        "en": "（しゃきぴよ）\n あっ 行っちゃった！",
        "pa": "They're leaving.",
        "vi": "Họ đi mất rồi!"
    },
    {
        "start": "00:03:15,083",
        "end": "00:03:16,708",
        "en": "ハア ハア ハア…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:03:17,708",
        "end": "00:03:18,541",
        "en": "あ…",
        "pa": "Hold it, you.",
        "vi": ""
    },
    {
        "start": "00:03:20,041",
        "end": "00:03:21,041",
        "en": "何だ これ　あっ！",
        "pa": "",
        "vi": "Cái quái gì vậy?"
    },
    {
        "start": "00:03:21,125",
        "end": "00:03:24,583",
        "en": "あ… あ あ～～",
        "pa": "-What the heck? -Hm?",
        "vi": ""
    },
    {
        "start": "00:03:25,166",
        "end": "00:03:26,000",
        "en": "（ハードボイルド）ん？",
        "pa": "Hm?",
        "vi": ""
    },
    {
        "start": "00:03:26,083",
        "end": "00:03:28,083",
        "en": "（においを嗅ぐ音）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:03:28,583",
        "end": "00:03:30,583",
        "en": "（ぐでたま）あ～",
        "pa": "-Huh.",
        "vi": ""
    },
    {
        "start": "00:03:31,250",
        "end": "00:03:33,208",
        "en": "（息を吸って吐く音）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:03:33,291",
        "end": "00:03:34,333",
        "en": "何か におうな",
        "pa": "Something smells.",
        "vi": "Có mùi gì đó."
    },
    {
        "start": "00:03:35,583",
        "end": "00:03:36,833",
        "en": "（においを嗅ぐ音）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:03:37,416",
        "end": "00:03:38,875",
        "en": "（ぐでたま）あ～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:03:38,958",
        "end": "00:03:40,833",
        "en": "（においを嗅ぐ音）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:03:40,916",
        "end": "00:03:41,750",
        "en": "うんうん",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:03:41,833",
        "end": "00:03:44,291",
        "en": "（においを嗅ぐ音）",
        "pa": "-Hm.",
        "vi": ""
    },
    {
        "start": "00:03:44,375",
        "end": "00:03:47,958",
        "en": "（ぐでたま）ん？　何なん？",
        "pa": "Hm, hm, hm.\n -What are you doing?",
        "vi": ""
    },
    {
        "start": "00:03:48,791",
        "end": "00:03:49,875",
        "en": "あーっ",
        "pa": "-Oh.",
        "vi": "Gì vậy?"
    },
    {
        "start": "00:03:51,041",
        "end": "00:03:52,250",
        "en": "大変！",
        "pa": "Oh no!",
        "vi": "Nguy rồi!"
    },
    {
        "start": "00:03:52,958",
        "end": "00:03:55,333",
        "en": "もっと腐ってるー！",
        "pa": "Your rotting's gotten even worse!",
        "vi": "Cậu đang thối hơn rồi!"
    },
    {
        "start": "00:03:55,416",
        "end": "00:04:00,083",
        "en": "（においを嗅ぐ音）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:04:00,166",
        "end": "00:04:02,125",
        "en": "いつまで嗅ぐん？",
        "pa": "You really gotta do that?",
        "vi": "Cậu định ngửi đến bao giờ?"
    },
    {
        "start": "00:04:02,208",
        "end": "00:04:04,833",
        "en": "フッ… クセになるぜ",
        "pa": "You know, that smell, it kind of grows on you.",
        "vi": "Đang thối hơn rồi."
    },
    {
        "start": "00:04:07,791",
        "end": "00:04:12,208",
        "en": "（しゃきぴよ）ぐでたまはね\n 本当は ずっと冷蔵庫にいれば",
        "pa": "You know, if you'd just stayed in that refrigerator, Gudetama,",
        "vi": "Nếu Gudetama cứ ở trong tủ lạnh,"
    },
    {
        "start": "00:04:12,708",
        "end": "00:04:15,375",
        "en": "今頃 料理になれてたかもしれない",
        "pa": "you could've been cooked into a nice egg dish by now.",
        "vi": "có thể bây giờ đã được nấu thành món ăn rồi."
    },
    {
        "start": "00:04:17,000",
        "end": "00:04:19,666",
        "en": "でも ぴよが\n ママに会いにいこうって",
        "pa": "But instead, I made you come out of there with me to search for Mommy.",
        "vi": "Nhưng tớ đã lỡ rủ cậu ấy đi tìm mẹ, nên…"
    },
    {
        "start": "00:04:19,750",
        "end": "00:04:21,166",
        "en": "誘っちゃったから…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:04:21,250",
        "end": "00:04:22,083",
        "en": "（ハードボイルド）うん…",
        "pa": "Hm.",
        "vi": ""
    },
    {
        "start": "00:04:22,166",
        "end": "00:04:23,458",
        "en": "それな～",
        "pa": "Yeah, you did.",
        "vi": "Có lẽ vậy."
    },
    {
        "start": "00:04:23,541",
        "end": "00:04:24,458",
        "en": "だよね？",
        "pa": "I'm sorry.\n Well, you got no choice then.",
        "vi": "Đúng mà nhỉ?"
    },
    {
        "start": "00:04:25,750",
        "end": "00:04:26,916",
        "en": "（ハードボイルド）\n そういうことなら",
        "pa": "",
        "vi": "Nếu như vậy,"
    },
    {
        "start": "00:04:27,750",
        "end": "00:04:29,625",
        "en": "ブツを手に入れるしかないな",
        "pa": "You're just gonna have to go get the stuff.",
        "vi": "chỉ còn cách lấy được hàng."
    },
    {
        "start": "00:04:30,541",
        "end": "00:04:32,000",
        "en": "方法は一つ",
        "pa": "There's only one way.",
        "vi": "Chỉ có một cách thôi."
    },
    {
        "start": "00:04:32,083",
        "end": "00:04:33,208",
        "en": "どうするの？",
        "pa": "-What's that?",
        "vi": "Làm như nào?"
    },
    {
        "start": "00:04:33,791",
        "end": "00:04:36,083",
        "en": "狙ったハートと同じさ",
        "pa": "The same way you win someone's heart.",
        "vi": "Giống như cách bạn có được trái tim người khác."
    },
    {
        "start": "00:04:36,708",
        "end": "00:04:40,000",
        "en": "欲しければ… 奪うんだ",
        "pa": "If you really want it,\n you steal it.",
        "vi": "Một khi bạn muốn, bạn sẽ đánh cắp nó."
    },
    {
        "start": "00:04:41,916",
        "end": "00:04:44,500",
        "en": "（しゃきぴよ）ここが\n たまごマフィアのアジト？",
        "pa": "Which one's the Egg Mafia's secret hideout?",
        "vi": "Đây là sào huyệt của Mafia Trứng?"
    },
    {
        "start": "00:04:45,375",
        "end": "00:04:48,875",
        "en": "（ハードボイルド）目印は\n イケてるマスコットキャラだ",
        "pa": "The one with the super cool mascot character in front of it.",
        "vi": "Linh vật sành điệu là nơi đánh dấu vị trí."
    },
    {
        "start": "00:04:48,958",
        "end": "00:04:50,541",
        "en": "（しゃきぴよ）イケてる？",
        "pa": "-What's a mascot character?",
        "vi": "\"Sành điệu?\""
    },
    {
        "start": "00:04:51,916",
        "end": "00:04:52,875",
        "en": "この中だ",
        "pa": "- Sh!\n It's right there.",
        "vi": "Ở trong đó."
    },
    {
        "start": "00:04:54,166",
        "end": "00:04:55,958",
        "en": "（ぐれたま）イケてな～",
        "pa": "You call that super cool?",
        "vi": "Chả sành điệu tí nào."
    },
    {
        "start": "00:04:57,666",
        "end": "00:04:59,750",
        "en": "（ハードボイルド）\n ついてこい ごろごろごろ",
        "pa": "Follow me.\n Rolling, rolling…",
        "vi": "Đi theo tôi. Lăn, lăn…"
    },
    {
        "start": "00:05:01,000",
        "end": "00:05:02,833",
        "en": "ごろ ごろ ごろごろごろ",
        "pa": "Rolling, rolling, rolling, rolling, rolling, rolling…",
        "vi": "Lăn, lăn…"
    },
    {
        "start": "00:05:05,750",
        "end": "00:05:06,583",
        "en": "えっ",
        "pa": "Huh?",
        "vi": ""
    },
    {
        "start": "00:05:07,083",
        "end": "00:05:07,916",
        "en": "（しゃきぴよ）何？\n （ぐでたま）わっ",
        "pa": "",
        "vi": "Gì vậy?"
    },
    {
        "start": "00:05:12,041",
        "end": "00:05:12,916",
        "en": "行くぞ",
        "pa": "Let's go.",
        "vi": "Đi thôi."
    },
    {
        "start": "00:05:15,583",
        "end": "00:05:17,541",
        "en": "だ… 大丈夫かな…",
        "pa": "You sure this is a good idea?",
        "vi": "Bọn mình sẽ ổn chứ?"
    },
    {
        "start": "00:05:18,125",
        "end": "00:05:19,333",
        "en": "用心しろ",
        "pa": "Probably not.",
        "vi": "Hãy cảnh giác."
    },
    {
        "start": "00:05:20,000",
        "end": "00:05:22,916",
        "en": "この先が中枢だ　たまごの…",
        "pa": "We're going right into the center,",
        "vi": "Phía trước chính là sào huyệt rồi."
    },
    {
        "start": "00:05:24,583",
        "end": "00:05:26,083",
        "en": "闇社会のな",
        "pa": "into the yolk\n of the egg underworld.",
        "vi": "Xã hội đen của trứng."
    },
    {
        "start": "00:05:28,083",
        "end": "00:05:28,708",
        "en": "ぴよっ！",
        "pa": "-No!",
        "vi": ""
    },
    {
        "start": "00:05:29,666",
        "end": "00:05:30,333",
        "en": "闇…",
        "pa": "The underworld?",
        "vi": "Tối đen."
    },
    {
        "start": "00:05:30,416",
        "end": "00:05:33,500",
        "en": "（ハードボイルド）\n フッ… ひよこは目立つ",
        "pa": "You gotta blend in.",
        "vi": "Gà con sẽ gây chú ý."
    },
    {
        "start": "00:05:33,583",
        "end": "00:05:34,458",
        "en": "（エレベーターの到着音）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:05:36,000",
        "end": "00:05:42,958",
        "en": "（バンドの演奏）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:05:46,583",
        "end": "00:05:50,708",
        "en": "たまごが先か",
        "pa": "♪ What came first? I'd like to know ♪",
        "vi": "Trứng ra đời trước ư?"
    },
    {
        "start": "00:05:50,791",
        "end": "00:05:55,083",
        "en": "にわとりか",
        "pa": "♪ The chicken or the egg? ♪",
        "vi": "Hay gà ra đời trước?"
    },
    {
        "start": "00:05:55,166",
        "end": "00:06:01,708",
        "en": "見えない答えを追うのは\n 　　　　　　　　　　やめて",
        "pa": "♪ Don't go looking For the answer tonight ♪",
        "vi": "Đừng kiếm tìm câu trả lời không bao giờ có\n Nhé bạn ơi"
    },
    {
        "start": "00:06:02,916",
        "end": "00:06:08,000",
        "en": "今宵(こよい)だけは ぐでぐで",
        "pa": "♪ Stay lazy ♪\n ♪ I'm talking through the night ♪",
        "vi": "Chỉ đêm nay thôi, làm biếng đi nào"
    },
    {
        "start": "00:06:08,083",
        "end": "00:06:11,166",
        "en": "ぐでればいい",
        "pa": "♪ Through the day ♪\n ♪ Though you may think it's crazy ♪",
        "vi": "Cứ làm biếng đi"
    },
    {
        "start": "00:06:11,250",
        "end": "00:06:16,458",
        "en": "夜も朝も お昼も",
        "pa": "♪ Maybe take it slow ♪\n ♪ Go with the flow ♪",
        "vi": "Dù đêm hay ngày\n Dù là ban trưa"
    },
    {
        "start": "00:06:16,541",
        "end": "00:06:21,000",
        "en": "いつも ぐでぐで",
        "pa": "♪ It's time to keep it lazy ♪",
        "vi": "Lúc nào cũng làm biếng"
    },
    {
        "start": "00:06:26,708",
        "end": "00:06:29,625",
        "en": "（演奏の終了）\n （拍手）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:06:29,708",
        "end": "00:06:31,541",
        "en": "（ぐでたま）ああ～\n （ハードボイルド）ん？",
        "pa": "- Oh.",
        "vi": ""
    },
    {
        "start": "00:06:31,625",
        "end": "00:06:32,458",
        "en": "（しゃきぴよ）ん？",
        "pa": "Huh?\n Hey, where is my drink?",
        "vi": ""
    },
    {
        "start": "00:06:40,750",
        "end": "00:06:42,583",
        "en": "（しゃきぴよ）\n ぐでたまが たくさんいる",
        "pa": "Wow, there's a lot of you here, Gudetama.",
        "vi": "Có rất nhiều trứng lười."
    },
    {
        "start": "00:06:42,666",
        "end": "00:06:43,666",
        "en": "（ぐでたま）あっ",
        "pa": "- Can't reach it. -Huh?",
        "vi": ""
    },
    {
        "start": "00:06:44,750",
        "end": "00:06:45,833",
        "en": "（ぐでたま）えいっ\n （しゃきぴよ）あっ！",
        "pa": "Oh!",
        "vi": ""
    },
    {
        "start": "00:06:47,250",
        "end": "00:06:49,625",
        "en": "（ぐでたま）ああ～ ふう…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:06:50,375",
        "end": "00:06:51,583",
        "en": "カラザ！",
        "pa": "-Chalaza!",
        "vi": "Dây treo của cậu!"
    },
    {
        "start": "00:06:53,166",
        "end": "00:06:54,500",
        "en": "ダメ～！",
        "pa": "",
        "vi": "Không được!"
    },
    {
        "start": "00:06:54,583",
        "end": "00:06:57,125",
        "en": "（ぐでたま）あっ あ～",
        "pa": "-No! -Ah!\n Hey!",
        "vi": "Không!"
    },
    {
        "start": "00:06:59,458",
        "end": "00:07:01,666",
        "en": "ヤツらの 根城はＶＩＰルーム",
        "pa": "-What we want is in the inner sanctum.",
        "vi": "Căn cứ của chúng là phòng VIP."
    },
    {
        "start": "00:07:02,208",
        "end": "00:07:04,041",
        "en": "あの扉の先だ",
        "pa": "Just beyond that door.",
        "vi": "Phía sau cánh cửa đó."
    },
    {
        "start": "00:07:05,666",
        "end": "00:07:09,875",
        "en": "Ｌサイズ…\n 正味60グラムのグレたまか…",
        "pa": "That one Gudetama guarding the door is L-sized.\n Has a net weight of 60 grams.",
        "vi": "Một quả trứng lười cỡ L với khối lượng tịnh là 60 gram."
    },
    {
        "start": "00:07:10,458",
        "end": "00:07:11,541",
        "en": "どうするの？",
        "pa": "-What do we do?",
        "vi": "Nên làm gì đây?"
    },
    {
        "start": "00:07:14,750",
        "end": "00:07:15,583",
        "en": "あそこだ",
        "pa": "Oh well.\n Hm.\n -Up there. -Huh?",
        "vi": "Đằng kia."
    },
    {
        "start": "00:07:20,708",
        "end": "00:07:21,958",
        "en": "ちゃんと来てる？",
        "pa": "-Come on. Come on.",
        "vi": "Đi theo tớ không đấy?"
    },
    {
        "start": "00:07:22,791",
        "end": "00:07:24,041",
        "en": "（ぐでたま）あ～",
        "pa": "Rolling. Rolling, rolling. Rolling…",
        "vi": ""
    },
    {
        "start": "00:07:24,791",
        "end": "00:07:27,833",
        "en": "あ～ ああああ～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:07:32,125",
        "end": "00:07:34,125",
        "en": "（ハードボイルド）ごろごろごろ…",
        "pa": "Rolling, rolling, rolling…",
        "vi": "Lăn, lăn, lăn."
    },
    {
        "start": "00:07:36,875",
        "end": "00:07:37,708",
        "en": "あっ！",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:07:39,208",
        "end": "00:07:40,291",
        "en": "（しゃきぴよが においを嗅ぐ音）",
        "pa": "-Hm.",
        "vi": ""
    },
    {
        "start": "00:07:40,791",
        "end": "00:07:41,708",
        "en": "ん？",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:07:41,791",
        "end": "00:07:43,041",
        "en": "下を見ろ",
        "pa": "-Hm? -Well, take a look?",
        "vi": "Nhìn xuống đi."
    },
    {
        "start": "00:07:43,125",
        "end": "00:07:43,958",
        "en": "ぴよっ",
        "pa": "-Ah.",
        "vi": "Chiếp!"
    },
    {
        "start": "00:07:46,958",
        "end": "00:07:48,208",
        "en": "（ハードボイルド）\n あれを手に入れれば",
        "pa": "If we can get our hands on that stuff, none of us will rot.",
        "vi": "Có trong tay thứ đó thì sẽ không bị thối rữa nữa."
    },
    {
        "start": "00:07:48,291",
        "end": "00:07:50,041",
        "en": "腐らずに済むわけだ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:07:50,750",
        "end": "00:07:52,875",
        "en": "（しゃきぴよ）\n どうやって手に入れるの",
        "pa": "-But how are we gonna get it? -Oh. I smell soy sauce.",
        "vi": "Làm sao để có được nó?"
    },
    {
        "start": "00:07:52,958",
        "end": "00:07:55,041",
        "en": "（ぐでたま）しょうゆ～\n （しゃきぴよ）ダメっ！",
        "pa": "",
        "vi": "- Xì dầu! - Không được!"
    },
    {
        "start": "00:07:55,125",
        "end": "00:07:57,166",
        "en": "どうやって手に入れるか",
        "pa": "-\"How will we get it?\" you ask.",
        "vi": "Làm thế nào để lấy à?"
    },
    {
        "start": "00:07:57,708",
        "end": "00:08:01,333",
        "en": "その答えなら\n お前の手の中にあるぜ",
        "pa": "Well, the answer is right there in your fuzzy hand.",
        "vi": "Câu trả lời chính là thứ trong tay bạn đấy."
    },
    {
        "start": "00:08:02,833",
        "end": "00:08:03,916",
        "en": "カラザ？",
        "pa": "- Swim. -The chalaza?",
        "vi": "Dây treo?"
    },
    {
        "start": "00:08:04,458",
        "end": "00:08:07,000",
        "en": "料理には邪魔だが\n 栄養満点",
        "pa": "Did you know it's jam-packed with nutrients",
        "vi": "Khi nấu ăn, nó gây phiền phức. Nhưng nhiều dinh dưỡng."
    },
    {
        "start": "00:08:07,708",
        "end": "00:08:09,083",
        "en": "今は使える",
        "pa": "and useful for us?",
        "vi": "Bây giờ nó sẽ có ích. Dùng nó để thả Gudetama xuống,"
    },
    {
        "start": "00:08:09,166",
        "end": "00:08:11,666",
        "en": "そいつで ぐでたまを\n 下に降ろして",
        "pa": "-Soy sauce. -Just lower Gudetama down.",
        "vi": ""
    },
    {
        "start": "00:08:12,291",
        "end": "00:08:14,500",
        "en": "こっそりブツを奪うんだ",
        "pa": "We'll steal the stuff when no one's looking.",
        "vi": "lén lút đánh cắp hàng."
    },
    {
        "start": "00:08:14,583",
        "end": "00:08:15,750",
        "en": "（しゃきぴよ）えっ",
        "pa": "What? But…",
        "vi": "Cái gì?"
    },
    {
        "start": "00:08:15,833",
        "end": "00:08:18,083",
        "en": "そ… そんなの危なすぎるよ",
        "pa": "But what if we get caught?",
        "vi": "Như thế quá nguy hiểm!"
    },
    {
        "start": "00:08:19,375",
        "end": "00:08:20,375",
        "en": "そうか",
        "pa": "You serious?",
        "vi": "Vậy à."
    },
    {
        "start": "00:08:21,000",
        "end": "00:08:24,791",
        "en": "だがな 起きてもいない\n 未来に おじけづいて",
        "pa": "Listen, kid.\n If you're gonna live your whole life worrying",
        "vi": "Nhưng nếu không làm gì vì sợ điều chưa xảy ra trong tương lai,"
    },
    {
        "start": "00:08:24,875",
        "end": "00:08:29,125",
        "en": "一歩も踏み出さないなら\n 腐るしかないぜ",
        "pa": "about what-ifs and what-might-have-beens,\n you might as well rot right now.",
        "vi": ""
    },
    {
        "start": "00:08:29,208",
        "end": "00:08:30,041",
        "en": "ハッ！",
        "pa": "",
        "vi": "thì chỉ có thể bị thối rữa."
    },
    {
        "start": "00:08:30,541",
        "end": "00:08:33,791",
        "en": "（ぐでたま）やっちゃうっしょ\n 行っちゃうっしょ～",
        "pa": "Let's do it. I'm good to go.",
        "vi": "Làm luôn thôi. Đi luôn thôi."
    },
    {
        "start": "00:08:34,458",
        "end": "00:08:38,208",
        "en": "まっ 腐るのは自然の摂理",
        "pa": "Not that I think there's anything wrong with rotting.",
        "vi": "Thối rữa là quy luật tự nhiên."
    },
    {
        "start": "00:08:38,291",
        "end": "00:08:41,833",
        "en": "運命だと受け入れるのも\n なしじゃない",
        "pa": "Look, it's a perfectly natural thing, right?\n Happens to everyone.",
        "vi": "Chấp nhận số phận luôn là một lựa chọn."
    },
    {
        "start": "00:08:43,000",
        "end": "00:08:45,708",
        "en": "そ… そんなこと言うけど…",
        "pa": "Well, that's easy for you to say.",
        "vi": "Cậu nói thế, nhưng…"
    },
    {
        "start": "00:08:46,750",
        "end": "00:08:47,583",
        "en": "ん？",
        "pa": "You're not…\n Hm?",
        "vi": ""
    },
    {
        "start": "00:08:48,083",
        "end": "00:08:49,291",
        "en": "（においを嗅ぐ音）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:08:49,791",
        "end": "00:08:50,625",
        "en": "あ？",
        "pa": "Huh?",
        "vi": "Hả?"
    },
    {
        "start": "00:08:50,708",
        "end": "00:08:52,666",
        "en": "う… ううん 何でもない",
        "pa": "Uh, nothing. Never mind.",
        "vi": "Không, không có gì."
    },
    {
        "start": "00:08:53,250",
        "end": "00:08:54,083",
        "en": "何だ",
        "pa": "What? What is it?",
        "vi": "Là gì vậy?"
    },
    {
        "start": "00:08:54,708",
        "end": "00:08:55,916",
        "en": "じゃあ あの…",
        "pa": "Well, it's just that I'm thinking that maybe",
        "vi": "À thì tớ nghĩ"
    },
    {
        "start": "00:08:56,750",
        "end": "00:08:59,250",
        "en": "ハードボイルドさんも\n 腐りかけてると思う",
        "pa": "",
        "vi": "- cậu cũng bắt đầu thối rữa rồi. - Gì cơ?"
    },
    {
        "start": "00:08:59,333",
        "end": "00:09:00,166",
        "en": "えっ！",
        "pa": "you're starting to rot too!",
        "vi": ""
    },
    {
        "start": "00:09:01,208",
        "end": "00:09:04,000",
        "en": "フッ 何をバカなことを",
        "pa": "I'm thinking that maybe you're cuckoo.",
        "vi": "Nói vớ vẩn gì thế."
    },
    {
        "start": "00:09:04,583",
        "end": "00:09:08,458",
        "en": "だって さっき背中にぶつかった時\n におったもん",
        "pa": "But you see, when I bumped into you earlier,",
        "vi": "Vì nãy tớ va vào lưng cậu, tớ thấy có mùi lắm."
    },
    {
        "start": "00:09:09,291",
        "end": "00:09:10,625",
        "en": "硫黄の香り",
        "pa": "you smelled\n like sulfur!",
        "vi": "Cậu có mùi lưu huỳnh!"
    },
    {
        "start": "00:09:11,791",
        "end": "00:09:13,166",
        "en": "（ぐでたま）くんくん\n （ハードボイルド）ん？",
        "pa": "Huh?\n -Huh?",
        "vi": ""
    },
    {
        "start": "00:09:13,250",
        "end": "00:09:14,125",
        "en": "（ぐでたま）おえっ",
        "pa": "Ew!",
        "vi": "Eo ơi."
    },
    {
        "start": "00:09:14,208",
        "end": "00:09:17,041",
        "en": "（ハードボイルド）\n ん！　そんなわけ…",
        "pa": "What are you talking about?",
        "vi": "Cái gì? Không thể nào."
    },
    {
        "start": "00:09:17,541",
        "end": "00:09:19,375",
        "en": "くんくん くんくん",
        "pa": "",
        "vi": "Ngửi, ngửi…"
    },
    {
        "start": "00:09:19,458",
        "end": "00:09:20,875",
        "en": "んっ おえ～",
        "pa": "Ugh!",
        "vi": "Ọe!"
    },
    {
        "start": "00:09:20,958",
        "end": "00:09:23,208",
        "en": "（ぐでたま）\n いつ ゆでられたん？",
        "pa": "How long ago were you boiled?",
        "vi": "Cậu bị luộc khi nào?"
    },
    {
        "start": "00:09:23,791",
        "end": "00:09:25,041",
        "en": "５日前",
        "pa": "Um… five days.",
        "vi": "Năm ngày trước."
    },
    {
        "start": "00:09:25,125",
        "end": "00:09:27,625",
        "en": "ゆで卵って 腐っても",
        "pa": "With hard-boiled eggs,",
        "vi": "Trứng luộc dù bị thối thì nhìn mắt thường không biết được nhỉ."
    },
    {
        "start": "00:09:27,708",
        "end": "00:09:29,916",
        "en": "見た目じゃ\n 分からないんだね",
        "pa": "I guess you can't always tell they're rotten just by looking at them.",
        "vi": ""
    },
    {
        "start": "00:09:30,000",
        "end": "00:09:34,000",
        "en": "マジか～　手は一つだ",
        "pa": "Are you serious?\n Well, okay. In that case…",
        "vi": "Thật sao? Vậy là không còn lựa chọn nào khác."
    },
    {
        "start": "00:09:40,333",
        "end": "00:09:45,416",
        "en": "（ぐでたま）\n しょうゆ～　しょうゆ～　うわっ",
        "pa": "Oh, soy sauce.",
        "vi": "Xì dầu…"
    },
    {
        "start": "00:09:46,000",
        "end": "00:09:49,291",
        "en": "二袋だ！　こっちの分も\n 持ってくるんだぞ！",
        "pa": "Don't forget!\n Grab a bag of that stuff for me too!",
        "vi": "Hai túi! Nhớ lấy cả cho tôi nhé!"
    },
    {
        "start": "00:09:49,916",
        "end": "00:09:53,583",
        "en": "（ぐでたま）しょうゆ…\n んま んま んま んま…",
        "pa": "Soy sauce…\n -Me, me, me, me, me-- -Huh? Huh?",
        "vi": "Xì dầu… Ôi!"
    },
    {
        "start": "00:09:54,625",
        "end": "00:09:55,833",
        "en": "（しゃきぴよ）あぶなっ",
        "pa": "-That's it!\n That was close.",
        "vi": "Nguy hiểm!"
    },
    {
        "start": "00:10:00,458",
        "end": "00:10:02,458",
        "en": "（ハードボイルド）\n よーし いいぞ",
        "pa": "I could've sworn I saw something.\n Yeah, there you go. There you go.",
        "vi": "Đúng rồi."
    },
    {
        "start": "00:10:04,583",
        "end": "00:10:05,416",
        "en": "（電源が落ちる音）\n （ピータン）ハッ！",
        "pa": "-Ah!",
        "vi": ""
    },
    {
        "start": "00:10:05,500",
        "end": "00:10:06,333",
        "en": "（ぐでてま）な～",
        "pa": "-Gudetama!",
        "vi": ""
    },
    {
        "start": "00:10:06,416",
        "end": "00:10:07,541",
        "en": "（しゃきぴよ）ぐでたま！",
        "pa": "",
        "vi": "Gudetama!"
    },
    {
        "start": "00:10:07,625",
        "end": "00:10:11,833",
        "en": "（一同の うろたえる声）",
        "pa": "- Whoa!",
        "vi": "Gì vậy?"
    },
    {
        "start": "00:10:11,916",
        "end": "00:10:13,208",
        "en": "（ハードボイルド）ハッ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:10:21,666",
        "end": "00:10:24,500",
        "en": "（マフィアマフィン）\n あっ ブツが！",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:10:24,583",
        "end": "00:10:26,375",
        "en": "（しゃきぴよ）あっ… うわ",
        "pa": "The stuff is gone!\n Ooh!",
        "vi": "Mất hàng rồi!"
    },
    {
        "start": "00:10:28,208",
        "end": "00:10:30,625",
        "en": "ぴよたち 何もしてないよ",
        "pa": "It wasn't us. We didn't take your stuff. Honest!",
        "vi": "Bọn tớ chẳng làm gì cả."
    },
    {
        "start": "00:10:30,708",
        "end": "00:10:34,750",
        "en": "（マフィンマフィア）そんな言い訳\n ボスに通用しないんだわ～",
        "pa": "Yeah, yeah, yeah. Tell it to the boss.",
        "vi": "Chủ nhân không tin mấy lời bao biện đó đâu."
    },
    {
        "start": "00:10:34,833",
        "end": "00:10:35,875",
        "en": "（しゃきぴよ）あっ 待ってよ！",
        "pa": "No, wait! Wait!",
        "vi": "Đợi đã!"
    },
    {
        "start": "00:10:37,583",
        "end": "00:10:41,125",
        "en": "どうしよう\n このままじゃママに…",
        "pa": "What do we do?\n We're never going to find Mommy now.",
        "vi": "Làm thế nào đây? Thế này thì sẽ không bao giờ gặp lại mẹ."
    },
    {
        "start": "00:10:41,916",
        "end": "00:10:43,750",
        "en": "（ぐれたま）あ～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:10:45,666",
        "end": "00:10:47,416",
        "en": "しょうゆ 欲しい～",
        "pa": "All I really want is some soy sauce. ",
        "vi": "Thèm xì dầu quá."
    },
    {
        "start": "00:10:48,916",
        "end": "00:10:53,916",
        "en": "♪～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:11:36,083",
        "end": "00:11:41,083",
        "en": "～♪",
        "pa": "",
        "vi": "Biên dịch: Nhimseo "
    }
]