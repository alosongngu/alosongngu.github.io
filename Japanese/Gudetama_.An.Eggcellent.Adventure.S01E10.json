[
    {
        "start": "00:00:06,041",
        "end": "00:00:11,291",
        "en": "NETFLIX シリーズ",
        "pa": "",
        "vi": "LOẠT PHIM NETFLIX"
    },
    {
        "start": "00:00:13,000",
        "end": "00:00:18,125",
        "en": "（しゃきぴよ）ぴよーっ！\n （ぐでたま）あ～",
        "pa": "",
        "vi": "Chiếp!"
    },
    {
        "start": "00:00:18,208",
        "end": "00:00:19,208",
        "en": "えいっ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:00:20,208",
        "end": "00:00:25,458",
        "en": "（しゃきぴよ）うわ～\n （ぐでたま）あ～",
        "pa": "Whoa!\n GUDETAMA: AN EGGCELLENT ADVENTURE",
        "vi": "NÔNG TRẠI WILSON\n GUDETAMA: CUỘC PHIÊU LƯU CỦA QUẢ TRỨNG LƯỜI"
    },
    {
        "start": "00:00:25,541",
        "end": "00:00:26,458",
        "en": "ぺちゃ",
        "pa": "WILSON FARMS",
        "vi": "Pẹp."
    },
    {
        "start": "00:00:27,833",
        "end": "00:00:28,666",
        "en": "（しゃきぴよ）着いた！",
        "pa": "We're here.",
        "vi": "Đến rồi."
    },
    {
        "start": "00:00:30,000",
        "end": "00:00:32,541",
        "en": "（にわとりの鳴き声）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:00:32,625",
        "end": "00:00:36,291",
        "en": "いよいよ会えるね\n ぴよたちのママに",
        "pa": "We finally made it.",
        "vi": "Cuối cùng bọn mình cũng sắp được gặp mẹ!"
    },
    {
        "start": "00:00:37,083",
        "end": "00:00:39,333",
        "en": "立ち直り早いな～",
        "pa": "We're gonna meet Mommy!\n You recover quickly, don't you?",
        "vi": "Cậu hồi phục nhanh ghê."
    },
    {
        "start": "00:00:46,208",
        "end": "00:00:47,541",
        "en": "ママ～",
        "pa": "-Mommy? -Huh?",
        "vi": "Mẹ!"
    },
    {
        "start": "00:00:48,250",
        "end": "00:00:49,083",
        "en": "えっ",
        "pa": "Huh?",
        "vi": "Hả?"
    },
    {
        "start": "00:00:49,166",
        "end": "00:00:54,333",
        "en": "（にわとりの鳴き声）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:00:54,416",
        "end": "00:00:56,500",
        "en": "多いな～",
        "pa": "That's a lot of chickens.",
        "vi": "Nhiều ghê."
    },
    {
        "start": "00:00:56,583",
        "end": "00:00:58,125",
        "en": "この中にいるんだ",
        "pa": "Mommy's here somewhere.",
        "vi": "Mẹ ở trong số gà này!"
    },
    {
        "start": "00:00:59,583",
        "end": "00:01:01,666",
        "en": "（にわとり）コケーッ！\n （たまごが割れる音）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:01,750",
        "end": "00:01:03,666",
        "en": "（たまご）あ～ うっ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:04,750",
        "end": "00:01:06,416",
        "en": "（たまご）未来が見えない",
        "pa": "The future looks dark.",
        "vi": "Chả thể nhìn thấy tương lai."
    },
    {
        "start": "00:01:07,166",
        "end": "00:01:08,500",
        "en": "（たまご）だりぃ～",
        "pa": "What a drag.",
        "vi": "Mệt ghê."
    },
    {
        "start": "00:01:09,000",
        "end": "00:01:11,750",
        "en": "あの… ママを探してるんです",
        "pa": "Excuse me. We're here to find our mommy.",
        "vi": "Cháu đang đi tìm mẹ."
    },
    {
        "start": "00:01:12,458",
        "end": "00:01:15,250",
        "en": "いますか？　名前はジュリア",
        "pa": "Is she here? Her name's Julia.",
        "vi": "Mẹ cháu có ở đây không? Tên mẹ là Julia."
    },
    {
        "start": "00:01:15,333",
        "end": "00:01:16,250",
        "en": "（にわとりたち）コケッ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:16,750",
        "end": "00:01:19,000",
        "en": "えっ どういうこと？",
        "pa": "Huh? You know her?",
        "vi": "Hả? Có chuyện gì vậy?"
    },
    {
        "start": "00:01:20,625",
        "end": "00:01:22,208",
        "en": "ジュリアさ～ん",
        "pa": "-Oh, Julia!",
        "vi": "Cô Julia!"
    },
    {
        "start": "00:01:22,875",
        "end": "00:01:24,125",
        "en": "（にわとりたち）コケッ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:24,208",
        "end": "00:01:26,041",
        "en": "（しゃきぴよ）ジュリアさん\n （にわとり）コケッ",
        "pa": "-Julia!",
        "vi": "Cô Julia!"
    },
    {
        "start": "00:01:26,125",
        "end": "00:01:27,875",
        "en": "（しゃきぴよ）ジュリアさん！\n （にわとり）コケッ",
        "pa": "-Julia!",
        "vi": "Cô Julia!"
    },
    {
        "start": "00:01:28,416",
        "end": "00:01:30,500",
        "en": "（しゃきぴよ）どういうこと？",
        "pa": "What is going on?",
        "vi": "Có chuyện gì vậy?"
    },
    {
        "start": "00:01:30,583",
        "end": "00:01:34,875",
        "en": "これもしかして\n 全員 ジュリアなんじゃね？",
        "pa": "Oh, come on, chickens.\n You can't all be Julia.",
        "vi": "Có lẽ nào ai ở đây cũng đều là \"Julia\"."
    },
    {
        "start": "00:01:35,583",
        "end": "00:01:38,583",
        "en": "（たまご）それ 正解っぽい～",
        "pa": "Well, as a matter of fact, they are, sort of.",
        "vi": "Có thể đúng đấy."
    },
    {
        "start": "00:01:38,666",
        "end": "00:01:40,541",
        "en": "（たまご）ジュリアっていうのは…",
        "pa": "You see, Julia is not really someone's name.",
        "vi": "\"Julia\" không chỉ là…"
    },
    {
        "start": "00:01:40,625",
        "end": "00:01:43,083",
        "en": "（たまご）名前じゃなくて…",
        "pa": "",
        "vi": "…một cái tên riêng."
    },
    {
        "start": "00:01:43,666",
        "end": "00:01:47,125",
        "en": "（たまご）\n 品種の名前… なんだわ～",
        "pa": "It's the name of a breed of chicken.",
        "vi": "Đó là tên của giống gà đó."
    },
    {
        "start": "00:01:48,333",
        "end": "00:01:49,416",
        "en": "えいっ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:50,000",
        "end": "00:01:50,916",
        "en": "（しゃきぴよ）そうなの？",
        "pa": "Is that true?",
        "vi": "Thật à?"
    },
    {
        "start": "00:01:51,000",
        "end": "00:01:53,833",
        "en": "（ぐでたま）\n それ 見つけらんなくね？",
        "pa": "Good luck finding Mommy, then.",
        "vi": "Thế thì không tìm ra được đâu."
    },
    {
        "start": "00:01:53,916",
        "end": "00:01:55,708",
        "en": "（しゃきぴよ）\n そんなことないもん",
        "pa": "We'll still find her!",
        "vi": "Không đúng!"
    },
    {
        "start": "00:01:58,333",
        "end": "00:01:59,166",
        "en": "ママ？",
        "pa": "Mommy?",
        "vi": "Mẹ?"
    },
    {
        "start": "00:01:59,250",
        "end": "00:02:00,291",
        "en": "（にわとり）違うわ",
        "pa": "Uh-uh.",
        "vi": "Nhầm rồi."
    },
    {
        "start": "00:02:01,125",
        "end": "00:02:01,958",
        "en": "ママ？",
        "pa": "-Mommy? -I'm a rooster!",
        "vi": "Mẹ?"
    },
    {
        "start": "00:02:02,041",
        "end": "00:02:03,250",
        "en": "俺 オス",
        "pa": "",
        "vi": "Tôi là gà trống."
    },
    {
        "start": "00:02:04,666",
        "end": "00:02:06,625",
        "en": "（しゃきぴよ）ママ！\n （にわとり）誰？",
        "pa": "-Mommy? -Do I know you?",
        "vi": "Mẹ?\n Ai đấy?"
    },
    {
        "start": "00:02:07,166",
        "end": "00:02:09,083",
        "en": "コケッ コケー！",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:02:10,208",
        "end": "00:02:12,583",
        "en": "（ぐでたま）\n ここには いないっしょ",
        "pa": "-She's not here.",
        "vi": "Mẹ không ở đây rồi."
    },
    {
        "start": "00:02:13,375",
        "end": "00:02:15,458",
        "en": "隣の小屋じゃね？",
        "pa": "Huh?\n Try the shed next door.",
        "vi": "Chắc là ở cái chuồng bên cạnh."
    },
    {
        "start": "00:02:16,416",
        "end": "00:02:17,416",
        "en": "（しゃきぴよ）ぴよっ\n （ぐでたま）ん？",
        "pa": "-Oh! Right! -Huh?",
        "vi": "Chiếp?"
    },
    {
        "start": "00:02:18,208",
        "end": "00:02:19,083",
        "en": "（しゃきぴよ）んしょ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:02:21,791",
        "end": "00:02:23,458",
        "en": "（しゃきぴよ）ママ～！",
        "pa": "Mommy!",
        "vi": "Mẹ!"
    },
    {
        "start": "00:02:29,250",
        "end": "00:02:30,083",
        "en": "ん？",
        "pa": "- Huh? - Ah, do those eggs catch your eye?",
        "vi": "Hả?"
    },
    {
        "start": "00:02:30,625",
        "end": "00:02:33,083",
        "en": "（おばあちゃん）\n あれが気になるの？",
        "pa": "",
        "vi": "Các cháu hứng thú với chúng à?"
    },
    {
        "start": "00:02:33,166",
        "end": "00:02:34,250",
        "en": "（しゃきぴよ）あっ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:02:34,916",
        "end": "00:02:36,791",
        "en": "変わった たまごでしょ",
        "pa": "Aren't they strange looking though?",
        "vi": "Những quả trứng khác thường nhỉ?"
    },
    {
        "start": "00:02:38,333",
        "end": "00:02:40,333",
        "en": "中身は普通だけど",
        "pa": "They look normal when their shells are off,",
        "vi": "Bên trong thì bình thường,"
    },
    {
        "start": "00:02:40,416",
        "end": "00:02:44,041",
        "en": "見た目があれで\n 出荷はできないの",
        "pa": "but because of their peculiar color,",
        "vi": "nhưng vì trông như thế,"
    },
    {
        "start": "00:02:44,125",
        "end": "00:02:45,500",
        "en": "（ぐでたま）あ～",
        "pa": "I can't ship them or sell them to anyone.\n Ah!",
        "vi": "nên không thể giao chúng đi."
    },
    {
        "start": "00:02:48,000",
        "end": "00:02:49,708",
        "en": "（ぐでたま）あ～～～",
        "pa": "Ah!",
        "vi": ""
    },
    {
        "start": "00:02:50,375",
        "end": "00:02:51,416",
        "en": "ん？",
        "pa": "Whoa.",
        "vi": "Hả?"
    },
    {
        "start": "00:02:53,583",
        "end": "00:02:58,625",
        "en": "あれと～ おんなじのから\n 生まれたっぽいわ",
        "pa": "Hey, you know,\n the egg I came out of looked just like those eggs.",
        "vi": "Có vẻ như cháu được sinh ra từ trứng giống thế."
    },
    {
        "start": "00:02:58,708",
        "end": "00:03:00,583",
        "en": "えっ ぐでたまが？",
        "pa": "Huh? You sure about that?",
        "vi": "Gì cơ? Cậu á?"
    },
    {
        "start": "00:03:02,083",
        "end": "00:03:05,291",
        "en": "それって 最近のことよね？",
        "pa": "This would've happened just a few days ago, right?",
        "vi": "Mới gần đây phải không?"
    },
    {
        "start": "00:03:06,375",
        "end": "00:03:11,375",
        "en": "この間 私が間違って\n 出荷するケースに入れちゃったのよ",
        "pa": "Because the other day, I accidentally\n put some of these eggs into an outgoing shipment.",
        "vi": "Hôm trước bà bị nhầm, bà lỡ cho nó vào kiện hàng gửi đi đấy."
    },
    {
        "start": "00:03:12,333",
        "end": "00:03:14,750",
        "en": "ぴよたち\n お寿司(すし)屋さんから来たの",
        "pa": "We were both born in a sushi restaurant.",
        "vi": "Bọn cháu đến từ nhà hàng sushi."
    },
    {
        "start": "00:03:15,541",
        "end": "00:03:17,750",
        "en": "ん？　そうなの",
        "pa": "Hm. You were, were you?",
        "vi": "Ồ, thế à?"
    },
    {
        "start": "00:03:18,333",
        "end": "00:03:21,666",
        "en": "うん　ママを探すために\n 帰ってきたの",
        "pa": "Yup! And now we've come back to find our mommy!",
        "vi": "Vâng! Bọn cháu quay lại tìm mẹ!"
    },
    {
        "start": "00:03:22,250",
        "end": "00:03:24,875",
        "en": "あら そうなの",
        "pa": "Oh, have you now?",
        "vi": "À, ra là vậy."
    },
    {
        "start": "00:03:26,083",
        "end": "00:03:30,125",
        "en": "でも いっぱいいすぎて\n 誰がママだか…",
        "pa": "But there are so many. I can't tell which one's her.",
        "vi": "Nhưng có quá nhiều gà, cháu không thể biết được ai là mẹ."
    },
    {
        "start": "00:03:32,708",
        "end": "00:03:34,541",
        "en": "（おばあちゃん）\n 分かるわよ",
        "pa": "Oh, that's easy.",
        "vi": "Biết được mà."
    },
    {
        "start": "00:03:34,625",
        "end": "00:03:35,458",
        "en": "（しゃきぴよ）えっ",
        "pa": "Huh?",
        "vi": "Hả?"
    },
    {
        "start": "00:03:38,750",
        "end": "00:03:41,666",
        "en": "あんたを生んだのは\n 〝カナ〞",
        "pa": "There's only one chicken it could be.\n It's Kana.",
        "vi": "Kana là cô gà sinh ra cháu."
    },
    {
        "start": "00:03:44,166",
        "end": "00:03:48,916",
        "en": "これを生むのは\n あの子だけだからね",
        "pa": "She's the only one who lays",
        "vi": "Nó là con gà duy nhất"
    },
    {
        "start": "00:03:49,000",
        "end": "00:03:50,916",
        "en": "そうなん？",
        "pa": "these curious eggs.\n Is that right?",
        "vi": "đẻ ra những quả trứng này.\n Vậy à?"
    },
    {
        "start": "00:03:51,000",
        "end": "00:03:55,958",
        "en": "えっ ぐでたまと ぴよのママは\n 違うってこと？",
        "pa": "But if Gudetama's shell and mine are different colors…",
        "vi": "Hả? Tức là Gudetama và cháu có mẹ khác nhau?"
    },
    {
        "start": "00:03:56,041",
        "end": "00:03:57,250",
        "en": "（おばあちゃん）そうよ",
        "pa": "You have different mommies.",
        "vi": "Đúng vậy."
    },
    {
        "start": "00:03:58,458",
        "end": "00:03:59,791",
        "en": "そうなんだ",
        "pa": "I see.",
        "vi": "Vậy à."
    },
    {
        "start": "00:03:59,875",
        "end": "00:04:03,291",
        "en": "まっ もともと似てないしね～",
        "pa": "There never was much of a family resemblance.",
        "vi": "Thì dù sao chúng ta cũng chả giống nhau."
    },
    {
        "start": "00:04:04,541",
        "end": "00:04:06,416",
        "en": "あっ これ",
        "pa": "Oh! Look!",
        "vi": "À! Cái này!"
    },
    {
        "start": "00:04:07,166",
        "end": "00:04:09,708",
        "en": "これ見たら\n ぴよのママのこと 分かる？",
        "pa": "Can you find out where my mommy is with this secret code?",
        "vi": "Bà xem cái này thì biết mẹ cháu ở đâu không?"
    },
    {
        "start": "00:04:14,916",
        "end": "00:04:17,250",
        "en": "（ぐでたま）そうやって読むん？",
        "pa": "You can read it just like that?",
        "vi": "Đọc như thế sao?"
    },
    {
        "start": "00:04:21,250",
        "end": "00:04:22,208",
        "en": "分かった？",
        "pa": "I WAS BORN HERE\n Does it say?",
        "vi": "TÔI ĐÃ SINH RA Ở ĐÂY\n Bà biết không?"
    },
    {
        "start": "00:04:23,083",
        "end": "00:04:24,000",
        "en": "（おばあちゃん）うん",
        "pa": "Mm-hmm.",
        "vi": "Có."
    },
    {
        "start": "00:04:24,500",
        "end": "00:04:25,375",
        "en": "ほんと？",
        "pa": "-Really? -Huh?",
        "vi": "Thật ạ?"
    },
    {
        "start": "00:04:26,666",
        "end": "00:04:27,666",
        "en": "んしょ",
        "pa": "Oh boy! Hm.",
        "vi": ""
    },
    {
        "start": "00:04:28,541",
        "end": "00:04:30,750",
        "en": "（おばあちゃん）\n あんたのママは もう…",
        "pa": "I'm afraid your mommy's…",
        "vi": "Mẹ cháu đã…"
    },
    {
        "start": "00:04:31,416",
        "end": "00:04:32,250",
        "en": "（しゃきぴよ）ハッ…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:04:33,208",
        "end": "00:04:34,041",
        "en": "ハア…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:04:35,291",
        "end": "00:04:36,958",
        "en": "残念だけど",
        "pa": "It's sad to hear. I know.",
        "vi": "Thật đáng tiếc, nhưng…"
    },
    {
        "start": "00:04:38,541",
        "end": "00:04:42,708",
        "en": "でも いいたまごを\n たくさん生んでくれた",
        "pa": "But if it's any consolation,\n she was a loving and loveable chicken.",
        "vi": "mẹ cháu là một cô gà đáng yêu,\n đã đẻ rất nhiều trứng chất lượng tốt."
    },
    {
        "start": "00:04:43,666",
        "end": "00:04:45,416",
        "en": "かわいい子だったのよ",
        "pa": "And she laid many great eggs.",
        "vi": ""
    },
    {
        "start": "00:04:45,500",
        "end": "00:04:52,458",
        "en": "（しゃきぴよのすすり泣く声）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:04:54,916",
        "end": "00:04:58,541",
        "en": "ぴよっ ハア ハア ハア…",
        "pa": "",
        "vi": "Chiếp!"
    },
    {
        "start": "00:04:58,625",
        "end": "00:04:59,458",
        "en": "あ…",
        "pa": "Oh.",
        "vi": ""
    },
    {
        "start": "00:05:04,791",
        "end": "00:05:11,750",
        "en": "（号泣する声）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:05:14,583",
        "end": "00:05:15,750",
        "en": "あんた…",
        "pa": "Now, then…",
        "vi": "Cháu…"
    },
    {
        "start": "00:05:20,041",
        "end": "00:05:22,416",
        "en": "お母さんに会いにいく？",
        "pa": "would you like to see your mother?",
        "vi": "muốn gặp mẹ không?"
    },
    {
        "start": "00:05:24,416",
        "end": "00:05:25,625",
        "en": "（おじいちゃん）これでよろしく",
        "pa": "Now, watch those eggs.",
        "vi": "Nhờ anh đấy."
    },
    {
        "start": "00:05:25,708",
        "end": "00:05:28,000",
        "en": "（業者）はい\n 確かにお預かりしました",
        "pa": "Yeah, I'll be careful with them. Don't worry.",
        "vi": "Vâng, tôi xin được nhận."
    },
    {
        "start": "00:05:28,083",
        "end": "00:05:28,916",
        "en": "気を付けてね",
        "pa": "-You take care. -Thanks. I will.",
        "vi": "Lái xe an toàn."
    },
    {
        "start": "00:05:29,000",
        "end": "00:05:30,625",
        "en": "（業者）はい\n ありがとうございます",
        "pa": "",
        "vi": "Vâng, cảm ơn ông."
    },
    {
        "start": "00:05:30,708",
        "end": "00:05:37,000",
        "en": "（にわとりの鳴き声）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:05:37,958",
        "end": "00:05:40,000",
        "en": "（おばあちゃん）\n おかしいわね",
        "pa": "Well, this is very odd.",
        "vi": "Lạ thật. Lúc nào nó cũng ở đây mà."
    },
    {
        "start": "00:05:40,083",
        "end": "00:05:42,500",
        "en": "いつも この辺りに\n いるんだけどね",
        "pa": "I always find your mother right around here.",
        "vi": ""
    },
    {
        "start": "00:05:49,375",
        "end": "00:05:51,625",
        "en": "じいちゃん カナ 見なかった？",
        "pa": "Where's Kana?\n Have you seen Kana?",
        "vi": "Ông có thấy Kana không?"
    },
    {
        "start": "00:05:51,708",
        "end": "00:05:55,416",
        "en": "（おじいちゃん）\n えっ 出荷したよ たった今",
        "pa": "Yeah, I just shipped her out. She's on the truck.",
        "vi": "Hả? Tôi giao nó đi rồi. Vừa xong."
    },
    {
        "start": "00:05:55,500",
        "end": "00:05:56,375",
        "en": "えっ！",
        "pa": "-Oh!",
        "vi": "Gì cơ?"
    },
    {
        "start": "00:05:59,333",
        "end": "00:06:01,875",
        "en": "あんた 会いたいわよね？",
        "pa": "And just when you were finally about to meet her.",
        "vi": "Cháu muốn gặp đúng không?"
    },
    {
        "start": "00:06:02,708",
        "end": "00:06:05,333",
        "en": "ここに来てか～",
        "pa": "What a strange turn of events.",
        "vi": "Một biến cố lại tới."
    },
    {
        "start": "00:06:07,583",
        "end": "00:06:08,750",
        "en": "（おばあちゃん）待って！",
        "pa": "Wait!",
        "vi": "Chờ đã!"
    },
    {
        "start": "00:06:08,833",
        "end": "00:06:11,375",
        "en": "（ぐでたま）\n 電話かければよくね？",
        "pa": "Stop!\n I don't suppose you could just call him on the phone?",
        "vi": "Sao bà không gọi điện?"
    },
    {
        "start": "00:06:13,458",
        "end": "00:06:14,458",
        "en": "（ぐでたま）ん？",
        "pa": "Huh?",
        "vi": ""
    },
    {
        "start": "00:06:15,375",
        "end": "00:06:16,208",
        "en": "（おばあちゃん）ん？",
        "pa": "Huh?",
        "vi": ""
    },
    {
        "start": "00:06:19,000",
        "end": "00:06:19,958",
        "en": "（業者）ん？",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:06:24,375",
        "end": "00:06:26,083",
        "en": "ぐでたまー！",
        "pa": "Gudetama!",
        "vi": "Gudetama!"
    },
    {
        "start": "00:06:26,166",
        "end": "00:06:28,625",
        "en": "ママに会えるよ～",
        "pa": "Now you can see your mommy!",
        "vi": "Cậu sẽ được gặp mẹ!"
    },
    {
        "start": "00:06:29,500",
        "end": "00:06:31,041",
        "en": "しゃきぴよ…",
        "pa": "Shakipiyo.",
        "vi": "Shakipiyo."
    },
    {
        "start": "00:06:43,000",
        "end": "00:06:45,125",
        "en": "（カナ）\n おかえりなさい コケッ",
        "pa": "Welcome back to the farm!",
        "vi": "Mừng con trở về!"
    },
    {
        "start": "00:06:45,666",
        "end": "00:06:46,541",
        "en": "（ぐでたま）お…",
        "pa": "Huh?",
        "vi": ""
    },
    {
        "start": "00:06:49,041",
        "end": "00:06:51,833",
        "en": "よかったね ぐでたま",
        "pa": "I'm happy for you, Gudetama.",
        "vi": "Tớ mừng cho cậu, Gudetama."
    },
    {
        "start": "00:06:53,291",
        "end": "00:06:55,666",
        "en": "どうやってコケまで？",
        "pa": "How in the… …did you get all the way here?",
        "vi": "Làm sao con đến được đây?"
    },
    {
        "start": "00:06:56,458",
        "end": "00:06:57,875",
        "en": "（ぐでたま）え？\n （カナ）コケッ",
        "pa": "-Well…",
        "vi": "Hả?"
    },
    {
        "start": "00:06:57,958",
        "end": "00:07:01,125",
        "en": "コケッ コケ～ コケッ コケ～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:07:01,208",
        "end": "00:07:02,916",
        "en": "あれ～",
        "pa": "Uh-huh…",
        "vi": "Ôi trời…"
    },
    {
        "start": "00:07:03,500",
        "end": "00:07:05,250",
        "en": "最初は…",
        "pa": "It all started like this…",
        "vi": "Ngay từ đầu…"
    },
    {
        "start": "00:07:06,708",
        "end": "00:07:09,666",
        "en": "決めた！　ぴよが連れてく",
        "pa": "All right, then, I'll take you myself!",
        "vi": "Tớ quyết định rồi. Tớ sẽ đưa cậu theo."
    },
    {
        "start": "00:07:10,333",
        "end": "00:07:12,041",
        "en": "無理やり？",
        "pa": "I was sort of kidnapped.",
        "vi": "…con bị buộc phải làm vậy."
    },
    {
        "start": "00:07:12,125",
        "end": "00:07:14,875",
        "en": "（しゃきぴよ）\n そう ぴよが誘ったの",
        "pa": "That's right!",
        "vi": "Phải! Cháu đã rủ cậu ấy."
    },
    {
        "start": "00:07:14,958",
        "end": "00:07:16,875",
        "en": "コケッ コケッ コケ～",
        "pa": "-I made Gudetama come.",
        "vi": ""
    },
    {
        "start": "00:07:16,958",
        "end": "00:07:18,833",
        "en": "聞いてるん？",
        "pa": "Are you even listening?",
        "vi": "Mẹ có nghe không đấy?"
    },
    {
        "start": "00:07:20,791",
        "end": "00:07:23,208",
        "en": "思い出 いっぱい？",
        "pa": "Yes, I'm listening. What happened then?",
        "vi": "Có nhiều kỷ niệm?"
    },
    {
        "start": "00:07:24,750",
        "end": "00:07:30,166",
        "en": "思い出っていうか\n たまごいっぱいかねえ",
        "pa": "Then we had many adventures on the road.",
        "vi": "Nói đến kỷ niệm thì\n gặp rất nhiều trứng."
    },
    {
        "start": "00:07:31,250",
        "end": "00:07:34,208",
        "en": "（いくらたち）個性って何？",
        "pa": "I met lots of different kinds of eggs.\n What is individuality?",
        "vi": "Cá tính là gì?"
    },
    {
        "start": "00:07:34,291",
        "end": "00:07:37,291",
        "en": "（グレたま）グレたまさんよ～",
        "pa": "I am Guretama the Almighty! That's who!",
        "vi": "Tao là anh Guretama!"
    },
    {
        "start": "00:07:38,125",
        "end": "00:07:40,833",
        "en": "（ハードボイルド）\n 狙ったハートと同じさ",
        "pa": "If you really want it, you steal it.",
        "vi": "Giống như cách bạn có được trái tim người khác."
    },
    {
        "start": "00:07:40,916",
        "end": "00:07:44,083",
        "en": "そん中だと 普通かな～",
        "pa": "Compared to them, I'm just an ordinary egg.",
        "vi": "So với họ thì con khá tầm thường."
    },
    {
        "start": "00:07:44,166",
        "end": "00:07:45,875",
        "en": "（プリンの歌声）",
        "pa": "♪ Ah ♪",
        "vi": ""
    },
    {
        "start": "00:07:45,958",
        "end": "00:07:47,958",
        "en": "（ぐでたま）生のまんまだし",
        "pa": "I'm nothing special.",
        "vi": "Con vẫn là trứng sống."
    },
    {
        "start": "00:07:48,791",
        "end": "00:07:51,666",
        "en": "塩すら ふられてないし",
        "pa": "I'm just a raw egg who's never even been sprinkled with salt.",
        "vi": "Được rắc muối lên người thôi cũng chưa từng."
    },
    {
        "start": "00:07:54,208",
        "end": "00:07:56,625",
        "en": "普通じゃないわよ　コケッ",
        "pa": "-Oh no, you're not ordinary.",
        "vi": "Con không tầm thường."
    },
    {
        "start": "00:07:56,708",
        "end": "00:07:57,541",
        "en": "（ぐでたま）あ～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:07:57,625",
        "end": "00:08:01,916",
        "en": "だって ここまで\n 戻ってきてくれたんですもの",
        "pa": "After all,\n you found a way to come all the way here to see me.",
        "vi": "Bởi vì con đã quay về được tận đây mà."
    },
    {
        "start": "00:08:03,083",
        "end": "00:08:03,916",
        "en": "コケッ",
        "pa": "-Huh? -Didn't you?",
        "vi": ""
    },
    {
        "start": "00:08:05,458",
        "end": "00:08:09,875",
        "en": "あなたは そのままで\n 十分 特別なたまごよ",
        "pa": "You don't need a dash of salt to make you special.\n You're special just the way you are.",
        "vi": "Con cứ giữ nguyên như vậy là đã quá đủ đặc biệt rồi."
    },
    {
        "start": "00:08:11,416",
        "end": "00:08:13,333",
        "en": "そうなん？",
        "pa": "You think so?",
        "vi": "Vậy sao?"
    },
    {
        "start": "00:08:14,625",
        "end": "00:08:16,125",
        "en": "でも ぐでたま…",
        "pa": "But, Gudetama,",
        "vi": "Nhưng, Gudetama,"
    },
    {
        "start": "00:08:17,375",
        "end": "00:08:19,958",
        "en": "ほんとは\n 料理になりたいんじゃない？",
        "pa": "you still want to end up being eaten, right?",
        "vi": "cậu muốn được nấu thành món ăn phải không?"
    },
    {
        "start": "00:08:20,708",
        "end": "00:08:24,041",
        "en": "腐るよりは\n 食べられたいんじゃないの？",
        "pa": "You don't want to rot.\n Better eaten than rotten. Remember?",
        "vi": "Cậu muốn bị ăn hơn là bị thối rữa phải không?"
    },
    {
        "start": "00:08:25,166",
        "end": "00:08:28,708",
        "en": "それなら\n すてきな食べられ方があるわよ",
        "pa": "Well, in that case, I know a wonderful way you can be eaten.",
        "vi": "Vậy thì có một cách tuyệt vời để được ăn đấy."
    },
    {
        "start": "00:08:28,791",
        "end": "00:08:31,250",
        "en": "え～ 何なん？",
        "pa": "And what way is that?",
        "vi": "Là gì vậy?"
    },
    {
        "start": "00:08:31,333",
        "end": "00:08:32,541",
        "en": "それは…",
        "pa": "They call it…",
        "vi": "Đó là…"
    },
    {
        "start": "00:08:33,208",
        "end": "00:08:36,250",
        "en": "（おばあちゃん）\n キングオブたまご料理",
        "pa": "The greatest… The king of them all…",
        "vi": "Vua của các món trứng."
    },
    {
        "start": "00:08:37,250",
        "end": "00:08:38,375",
        "en": "はい？",
        "pa": "What?",
        "vi": "Cái gì?"
    },
    {
        "start": "00:08:38,958",
        "end": "00:08:41,291",
        "en": "それ 前に聞いたやつだ",
        "pa": "Oh, we heard about that one before!",
        "vi": "Chúng cháu đã nghe nói tới món đó!"
    },
    {
        "start": "00:08:41,875",
        "end": "00:08:43,958",
        "en": "たまご料理の王様でしょ？",
        "pa": "The king of all egg dishes!",
        "vi": "Món trứng tuyệt nhất phải không?"
    },
    {
        "start": "00:08:44,041",
        "end": "00:08:46,875",
        "en": "私は そう思うわよ",
        "pa": "Exactly right.\n Nothing else comes close.",
        "vi": "Bà cũng nghĩ vậy đấy."
    },
    {
        "start": "00:08:48,041",
        "end": "00:08:51,333",
        "en": "てか それ… 何なん？",
        "pa": "So what is it?\n What is this dish?",
        "vi": "Vậy món đó là món gì?"
    },
    {
        "start": "00:09:02,041",
        "end": "00:09:03,708",
        "en": "（ぐでたま）あ～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:09:04,750",
        "end": "00:09:05,750",
        "en": "あ～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:09:07,291",
        "end": "00:09:08,666",
        "en": "ああ…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:09:10,791",
        "end": "00:09:13,000",
        "en": "お～ お尻 熱っ",
        "pa": "That's hot on my butt!",
        "vi": "Nóng mông quá!"
    },
    {
        "start": "00:09:13,083",
        "end": "00:09:18,000",
        "en": "だから しょうゆかけたほうが\n うまいって",
        "pa": "No soy sauce?\n A little soy sauce makes it so much better.",
        "vi": "Tôi nói rồi.\n Ăn với xì dầu thì ngon hơn nhiều."
    },
    {
        "start": "00:09:18,541",
        "end": "00:09:19,875",
        "en": "いいのよ",
        "pa": "It needs nothing else.",
        "vi": "Không cần đâu,"
    },
    {
        "start": "00:09:21,416",
        "end": "00:09:23,500",
        "en": "そのまんまで",
        "pa": "-I like it fine, just as it is.",
        "vi": "cứ để nguyên như vậy."
    },
    {
        "start": "00:09:24,541",
        "end": "00:09:27,458",
        "en": "しょうゆ かけないんか～",
        "pa": "No soy sauce?",
        "vi": "Không rưới xì dầu sao?"
    },
    {
        "start": "00:09:27,541",
        "end": "00:09:28,375",
        "en": "おなか 熱っ",
        "pa": "Now it's hot on my tummy.",
        "vi": "Nóng bụng quá."
    },
    {
        "start": "00:09:29,708",
        "end": "00:09:30,875",
        "en": "（カナ）ぐでたま",
        "pa": "Gudetama?\n Go on.",
        "vi": "Gudetama."
    },
    {
        "start": "00:09:36,291",
        "end": "00:09:42,541",
        "en": "ぐでたま…\n ぴよと旅に出て よかった？",
        "pa": "Gudetama,\n are you glad that you came",
        "vi": "Gudetama, cậu có vui khi đi chuyến phiêu lưu này cùng tớ không?"
    },
    {
        "start": "00:09:43,541",
        "end": "00:09:48,583",
        "en": "やってみりゃ\n 意外と何とかなったわ～",
        "pa": "on our quest?\n I took a chance going with you,\n but it went surprisingly well.",
        "vi": "Tớ làm thử, không ngờ lại thành công."
    },
    {
        "start": "00:09:50,708",
        "end": "00:09:53,250",
        "en": "しゃきぴよの おかげ",
        "pa": "It's only because of you I'm here now.",
        "vi": "Tất cả là nhờ Shakipiyo."
    },
    {
        "start": "00:09:53,833",
        "end": "00:09:56,458",
        "en": "ちょっと今\n しゃべってるんですけど",
        "pa": "Thanks to you, I made it all the way to this moment.",
        "vi": "Ơ, cháu vẫn đang nói mà."
    },
    {
        "start": "00:09:57,458",
        "end": "00:09:59,125",
        "en": "（食べる音）\n （しゃきぴよ）あっ",
        "pa": "-Oh!",
        "vi": ""
    },
    {
        "start": "00:10:05,041",
        "end": "00:10:06,291",
        "en": "ん～",
        "pa": "Mm.",
        "vi": ""
    },
    {
        "start": "00:10:07,458",
        "end": "00:10:14,416",
        "en": "あ～～～",
        "pa": "Hey.",
        "vi": ""
    },
    {
        "start": "00:10:19,958",
        "end": "00:10:23,708",
        "en": "（ひよこたちの鳴き声）",
        "pa": "Piyo! Piyo! Piyo! Piyo! Piyo!",
        "vi": ""
    },
    {
        "start": "00:10:23,791",
        "end": "00:10:24,833",
        "en": "（おばあちゃん）ん？",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:10:25,750",
        "end": "00:10:27,958",
        "en": "ん～ ん…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:10:29,875",
        "end": "00:10:31,625",
        "en": "（しゃきぴよ）それ ぴよの？",
        "pa": "Piyo! Piyo! Piyo! Piyo!\n Is that my secret code?",
        "vi": "Cái đó là của cháu ạ?"
    },
    {
        "start": "00:10:31,708",
        "end": "00:10:32,541",
        "en": "（おばあちゃん）そうよ",
        "pa": "Yeah.",
        "vi": "Đúng vậy."
    },
    {
        "start": "00:10:34,333",
        "end": "00:10:37,625",
        "en": "ん～ ちょっと気になってねえ",
        "pa": "Oh, now I'm not so sure.",
        "vi": "Bà đang thắc mắc một chút."
    },
    {
        "start": "00:10:38,291",
        "end": "00:10:41,291",
        "en": "（おじいちゃん）\n おい これ使うんだ",
        "pa": "Here. You need a scanner for that.",
        "vi": "Này, bà phải dùng cái này."
    },
    {
        "start": "00:10:42,166",
        "end": "00:10:43,458",
        "en": "（おばあちゃん）そうよねえ",
        "pa": "You're probably right.",
        "vi": "Phải rồi."
    },
