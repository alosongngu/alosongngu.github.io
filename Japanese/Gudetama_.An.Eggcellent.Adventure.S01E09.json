[
    {
        "start": "00:00:06,000",
        "end": "00:00:11,333",
        "en": "NETFLIX シリーズ",
        "pa": "",
        "vi": "LOẠT PHIM NETFLIX"
    },
    {
        "start": "00:00:13,000",
        "end": "00:00:18,375",
        "en": "（バイクの走行音）",
        "pa": "GUDETAMA: AN EGGCELLENT ADVENTURE",
        "vi": "GUDETAMA: CUỘC PHIÊU LƯU CỦA QUẢ TRỨNG LƯỜI"
    },
    {
        "start": "00:00:33,458",
        "end": "00:00:34,833",
        "en": "（しゃきぴよ）あっ…\n （ぐでたま）あ～",
        "pa": "",
        "vi": "NHÀ HÀNG THE MAVERICK"
    },
    {
        "start": "00:00:34,916",
        "end": "00:00:37,000",
        "en": "ハッ！　着いた",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:00:37,500",
        "end": "00:00:38,583",
        "en": "フッ",
        "pa": "-We've arrived.",
        "vi": "Đến rồi."
    },
    {
        "start": "00:00:49,250",
        "end": "00:00:53,708",
        "en": "（井上(いのうえ)Ｄ）ぐでたま\n また どこかで会おうな",
        "pa": "Gudetama,\n I, uh, hope that we meet again.",
        "vi": "Gudetama, hẹn ngày nào đó gặp lại nhé."
    },
    {
        "start": "00:01:01,833",
        "end": "00:01:06,291",
        "en": "（バイクが走り去る音）",
        "pa": "See you.",
        "vi": "RẤT TIẾC CHÚNG TÔI ĐÃ ĐÓNG CỬA"
    },
    {
        "start": "00:01:32,208",
        "end": "00:01:35,333",
        "en": "（料理長）え～\n さっきの電話の人からだねえ",
        "pa": "I'm guessing this is from that person who phoned me earlier.",
        "vi": "Cái này hẳn là từ người đã gọi cho mình lúc nãy."
    },
    {
        "start": "00:01:37,333",
        "end": "00:01:38,750",
        "en": "あっ",
        "pa": "-Ah!",
        "vi": ""
    },
    {
        "start": "00:01:39,500",
        "end": "00:01:42,625",
        "en": "かわいい たまごちゃん",
        "pa": "Look at you.\n What a cute little egg!",
        "vi": "Quả trứng đáng yêu làm sao!"
    },
    {
        "start": "00:01:43,916",
        "end": "00:01:44,958",
        "en": "（タイマーの音）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:45,041",
        "end": "00:01:47,000",
        "en": "あっ ちょっと待っててね",
        "pa": "Oh.\n You stay right here.",
        "vi": "Đợi ta chút nhé."
    },
    {
        "start": "00:01:48,083",
        "end": "00:01:49,791",
        "en": "（ぐでたま）あ～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:54,083",
        "end": "00:01:56,416",
        "en": "（しゃきぴよ）ママ どこ？",
        "pa": "Mommy? Are you here?",
        "vi": "Mẹ đang ở đâu?"
    },
    {
        "start": "00:01:57,750",
        "end": "00:02:00,916",
        "en": "普通 キッチンじゃね～？",
        "pa": "Wouldn't she be in the kitchen?",
        "vi": "Thường thì sẽ ở trong bếp mà?"
    },
    {
        "start": "00:02:01,000",
        "end": "00:02:01,875",
        "en": "（包丁で切る音）",
        "pa": "You're right!",
        "vi": ""
    },
    {
        "start": "00:02:01,958",
        "end": "00:02:04,125",
        "en": "あっ 急がなきゃ",
        "pa": "So we have to hurry!",
        "vi": "Ta phải nhanh lên!"
    },
    {
        "start": "00:02:05,625",
        "end": "00:02:09,416",
        "en": "ハア ハア ハア…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:02:10,333",
        "end": "00:02:13,166",
        "en": "あっ！　ハア ハア ハア…",
        "pa": "CHICKEN AND EGG BOWL A MATCH MADE IN HEAVEN\n BUTTER CHICKEN CURRY (SPICY) WITH TENDER YOUNG CHICKEN MEAT",
        "vi": "SỰ KẾT HỢP TUYỆT VỜI GIỮA TRỨNG VÀ THỊT GÀ\n CÀ RI BƠ GÀ (CAY) VỚI THỊT MỀM CỦA GÀ TƠ"
    },
    {
        "start": "00:02:17,166",
        "end": "00:02:18,000",
        "en": "あっ！",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:02:18,583",
        "end": "00:02:20,833",
        "en": "（ジュリア）さよなら しゃきぴよ",
        "pa": "Goodbye, Shakipiyo.",
        "vi": "Tạm biệt, Shakipiyo."
    },
    {
        "start": "00:02:23,208",
        "end": "00:02:26,083",
        "en": "もしかして もう…",
        "pa": "Could it be we're too late?",
        "vi": "Có lẽ nào bọn mình tới quá muộn?"
    },
    {
        "start": "00:02:26,166",
        "end": "00:02:28,541",
        "en": "（しゃきぴよ）ハア ハア ハア…\n （ぐでたま）え え～",
        "pa": "-It can't be.\n Whoa, whoa, whoa! You might wanna watch where you're going.",
        "vi": ""
    },
    {
        "start": "00:02:28,625",
        "end": "00:02:31,083",
        "en": "前 見た方がよくね？",
        "pa": "",
        "vi": "Đi mà không nhìn đường gì hết vậy?"
    },
    {
        "start": "00:02:31,166",
        "end": "00:02:32,791",
        "en": "（しゃきぴよ）えっ あ…",
        "pa": "Whoa! Whoa!",
        "vi": ""
    },
    {
        "start": "00:02:33,333",
        "end": "00:02:36,208",
        "en": "（しゃきぴよ）ぴよ～\n （ぐでたま）あ～",
        "pa": "",
        "vi": "Chiếp!"
    },
    {
        "start": "00:02:36,291",
        "end": "00:02:37,958",
        "en": "（しゃきぴよ）あ… ハッ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:02:40,458",
        "end": "00:02:41,583",
        "en": "わあ～！",
        "pa": "IT'S DELICIOUS, CLUCK! PLEASE EAT, CLUCK!",
        "vi": "NGON LẮM ĐẤY, CỤC TÁC! HÃY ĂN NHÉ, CỤC TÁC!"
    },
    {
        "start": "00:02:48,083",
        "end": "00:02:50,958",
        "en": "（しゃきぴよ）ママ～ ぴよだよ～",
        "pa": "Mommy, it's me.\n Where are you?",
        "vi": "Mẹ ơi? Là con, Piyo đây."
    },
    {
        "start": "00:02:52,416",
        "end": "00:02:55,166",
        "en": "二人でママに\n 会いにきたんだよ",
        "pa": "Gudetama and I came a long way to see you.",
        "vi": "Bọn con đến gặp mẹ rồi đây."
    },
    {
        "start": "00:02:56,000",
        "end": "00:02:57,166",
        "en": "どこ？",
        "pa": "Are you here?",
        "vi": "Mẹ ở đâu?"
    },
    {
        "start": "00:02:57,708",
        "end": "00:02:59,416",
        "en": "（調理する音）",
        "pa": "Meh.",
        "vi": ""
    },
    {
        "start": "00:03:01,541",
        "end": "00:03:02,583",
        "en": "えっ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:03:03,416",
        "end": "00:03:04,250",
        "en": "ママ？",
        "pa": "Mommy?",
        "vi": "Mẹ?"
    },
    {
        "start": "00:03:07,208",
        "end": "00:03:11,666",
        "en": "（しゃきぴよの泣き声）",
        "pa": "-Uh…",
        "vi": ""
    },
    {
        "start": "00:03:11,750",
        "end": "00:03:13,083",
        "en": "驚かしちゃったかな",
        "pa": "Oh.\n -I'm sorry. I startled you, didn't I?",
        "vi": "Ta làm cháu giật mình à?"
    },
    {
        "start": "00:03:13,958",
        "end": "00:03:16,958",
        "en": "これ 裏の畑で採れた\n 新鮮なカブ",
        "pa": "These are just some fresh radishes I was going to cook up.",
        "vi": "Đây là củ cải tươi từ khu vườn phía sau."
    },
    {
        "start": "00:03:18,583",
        "end": "00:03:19,916",
        "en": "（しゃきぴよ）え…\n （ぐでたあ）え～",
        "pa": "",
        "vi": "Cái gì?"
    },
    {
        "start": "00:03:20,416",
        "end": "00:03:23,250",
        "en": "よかった～",
        "pa": "Oh, they're radishes!\n Hooray!",
        "vi": "May quá!"
    },
    {
        "start": "00:03:26,416",
        "end": "00:03:30,750",
        "en": "（料理長）お～\n さっきの たまごちゃんだね～",
        "pa": "You cute little egg.\n -Now what should I do with you.",
        "vi": "Ồ, là quả trứng lúc nãy nè."
    },
    {
        "start": "00:03:31,333",
        "end": "00:03:32,541",
        "en": "（しゃきぴよ）ママ？",
        "pa": "Mommy?",
        "vi": "Mẹ?"
    },
    {
        "start": "00:03:33,708",
        "end": "00:03:34,583",
        "en": "ママ",
        "pa": "Mommy?",
        "vi": "Mẹ?"
    },
    {
        "start": "00:03:36,500",
        "end": "00:03:39,583",
        "en": "（料理長）\n よ～し よし よし よし…",
        "pa": "- There. How's that?",
        "vi": "Ngoan!"
    },
    {
        "start": "00:03:40,875",
        "end": "00:03:42,833",
        "en": "おお～？",
        "pa": "You like that?",
        "vi": ""
    },
    {
        "start": "00:03:42,916",
        "end": "00:03:46,791",
        "en": "あの… ぴよたちのママ\n 知りませんか？",
        "pa": "Sir?\n You wouldn't happen to have seen our mommy anywhere?",
        "vi": "Cho hỏi ông có biết mẹ cháu không?"
    },
    {
        "start": "00:03:47,541",
        "end": "00:03:49,583",
        "en": "ジュリアっていう名前です",
        "pa": "Her name is Julia.",
        "vi": "Tên mẹ là Julia."
    },
    {
        "start": "00:03:51,291",
        "end": "00:03:53,958",
        "en": "いるとしたら 隣の養鶏場だね",
        "pa": "She's, uh, at the farm next door if she's still here.",
        "vi": "Nếu ở đây thì sẽ ở trang trại gia cầm bên cạnh."
    },
    {
        "start": "00:03:55,000",
        "end": "00:03:56,958",
        "en": "ありがとうございます！",
        "pa": "Thanks a lot! That's a big help!",
        "vi": "Cháu cảm ơn ạ! Đi thôi."
    },
    {
        "start": "00:03:57,041",
        "end": "00:03:58,833",
        "en": "行こ！　あっ…",
        "pa": "Let's go!",
        "vi": ""
    },
    {
        "start": "00:03:58,916",
        "end": "00:04:01,416",
        "en": "（料理長）\n きみは どんな料理がいい？",
        "pa": "So what would you like to be made into, huh?",
        "vi": "Cháu muốn được làm thành món gì?"
    },
    {
        "start": "00:04:01,500",
        "end": "00:04:03,458",
        "en": "お出しと砂糖を加えて",
        "pa": "How about a sweet and savory rolled omelette",
        "vi": "Trộn với nước dashi và đường,"
    },
    {
        "start": "00:04:03,541",
        "end": "00:04:07,375",
        "en": "薄～く焼いてクルクルしちゃう\n だし巻きたまご？",
        "pa": "with a little bit of dashi and a pinch of sugar?",
        "vi": "tráng thật mỏng rồi cuộn vào,"
    },
    {
        "start": "00:04:08,000",
        "end": "00:04:10,416",
        "en": "それ 面倒だわ～",
        "pa": "I don't know. Sounds like a lot of work.",
        "vi": "trứng cuộn dashi.\n Nghe rắc rối quá."
    },
    {
        "start": "00:04:11,041",
        "end": "00:04:12,375",
        "en": "（料理長）そ～だ",
        "pa": "I've got it!",
        "vi": "Đúng rồi!"
    },
    {
        "start": "00:04:12,458",
        "end": "00:04:16,208",
        "en": "白身と黄身を別々に泡立てた\n 口の中で溶けちゃうくらい⸺",
        "pa": "How about I separate your yolk from your egg white",
        "vi": "Hay là tách lòng đỏ và lòng trắng,"
    },
    {
        "start": "00:04:16,291",
        "end": "00:04:19,125",
        "en": "フワフワトロトロの\n オムレツとか どう？",
        "pa": "and then whip you into a fluffy, mouth-watering egg soufflé?",
        "vi": "rồi làm thành món trứng tráng mềm mịn, tan chảy trong miệng?"
    },
    {
        "start": "00:04:19,208",
        "end": "00:04:22,541",
        "en": "え～ 分けんの？",
        "pa": "-You're going to separate me from me? - That's right.",
        "vi": "Gì cơ? Tách ra à?"
    },
    {
        "start": "00:04:23,041",
        "end": "00:04:27,291",
        "en": "（しゃきぴよ）今ならまだ\n 料理してもらえるんだよね…",
        "pa": "It's still not too late for Gudetama to be cooked.",
        "vi": "Giờ vẫn chưa quá muộn để Gudetama được nấu thành món ăn."
    },
    {
        "start": "00:04:28,916",
        "end": "00:04:29,750",
        "en": "（ぐでたま）あ～",
        "pa": "Ah!",
        "vi": ""
    },
    {
        "start": "00:04:29,833",
        "end": "00:04:32,416",
        "en": "（しゃきぴよ）\n ここで料理になる方が",
        "pa": "In fact, Gudetama might be happier being cooked than staying with me.",
        "vi": "Có lẽ Gudetama sẽ hạnh phúc hơn khi được nấu thành món ăn ở đây."
    },
    {
        "start": "00:04:32,500",
        "end": "00:04:35,250",
        "en": "ぐでたまは幸せなのかも",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:04:37,833",
        "end": "00:04:43,291",
        "en": "（ぐでたま）あ～ ああ～ あ～",
        "pa": "Ah!\n Ah!",
        "vi": ""
    },
    {
        "start": "00:04:43,916",
        "end": "00:04:45,708",
        "en": "やっぱり いいわ",
        "pa": "Ah!\n Actually, I changed my mind.",
        "vi": "Thôi được rồi."
    },
    {
        "start": "00:04:47,375",
        "end": "00:04:48,208",
        "en": "えっ！",
        "pa": "What?!",
        "vi": "Cái gì?"
    },
    {
        "start": "00:04:49,750",
        "end": "00:04:52,208",
        "en": "探しにいくか～",
        "pa": "I'm coming with you to find her.",
        "vi": "Đi tìm mẹ nào."
    },
    {
        "start": "00:04:53,666",
        "end": "00:04:54,541",
        "en": "いいの？",
        "pa": "You are?",
        "vi": "Cậu chắc chứ?"
    },
    {
        "start": "00:04:55,500",
        "end": "00:05:00,208",
        "en": "クライマックス\n やっぱ そこっしょ～",
        "pa": "This is the big finale of our epic quest, right?",
        "vi": "Đó mới là cao trào mà."
    },
    {
        "start": "00:05:01,375",
        "end": "00:05:02,208",
        "en": "あっ…",
        "pa": "Ah… Ooh…",
        "vi": "Ồ…"
    },
    {
        "start": "00:05:03,791",
        "end": "00:05:04,708",
        "en": "うん！",
        "pa": "Yeah!",
        "vi": "Ừ!"
    },
    {
        "start": "00:05:11,250",
        "end": "00:05:12,791",
        "en": "養鶏所は あそこね",
        "pa": "The chicken farm's right over there.",
        "vi": "Trang trại gia cầm ở đằng kia."
    },
    {
        "start": "00:05:16,208",
        "end": "00:05:18,500",
        "en": "面倒だから 自分らで行って",
        "pa": "I'd take you there, but I'm too lazy.",
        "vi": "Ta lười lắm, hai đứa tự đi đi."
    },
    {
        "start": "00:05:19,750",
        "end": "00:05:21,333",
        "en": "ありがとう",
        "pa": "-Thanks again!",
        "vi": "Cảm ơn ạ!"
    },
    {
        "start": "00:05:22,625",
        "end": "00:05:24,000",
        "en": "（しゃきぴよ）ハア ハア ハア…\n （ぐでたま）あああ…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:05:24,083",
        "end": "00:05:26,958",
        "en": "（しゃきぴよ）\n 本当にママに会えるんだね",
        "pa": "This is it.\n -We're gonna finally see our mommy.",
        "vi": "Bọn mình sắp gặp được mẹ thật rồi."
    },
    {
        "start": "00:05:27,750",
        "end": "00:05:29,958",
        "en": "（ぐでたま）そうね～",
        "pa": "That's the idea.",
        "vi": "Ừ."
    },
    {
        "start": "00:05:31,583",
        "end": "00:05:33,875",
        "en": "このパンツ 似合ってるかな",
        "pa": "What do you think? Do these pants look all right?",
        "vi": "Quần này có hợp với tớ không?"
    },
    {
        "start": "00:05:34,375",
        "end": "00:05:35,833",
        "en": "今さら～？",
        "pa": "You're asking now?",
        "vi": "Giờ lại đi hỏi?"
    },
    {
        "start": "00:05:36,416",
        "end": "00:05:37,250",
        "en": "んしょ",
        "pa": "Uh…",
        "vi": ""
    },
    {
        "start": "00:05:38,458",
        "end": "00:05:42,250",
        "en": "ママ 気に入ってくれるといいな",
        "pa": "I only hope Mommy likes them. That's all.\n Oh…",
        "vi": "Hy vọng mẹ thích chúng."
    },
    {
        "start": "00:05:51,958",
        "end": "00:05:53,666",
        "en": "ぴよお～！",
        "pa": "",
        "vi": "Chiếp!"
    },
    {
        "start": "00:05:53,750",
        "end": "00:05:55,625",
        "en": "（ぐでたま）あ～",
        "pa": "We'll be okay! It's not much further!",
        "vi": ""
    },
    {
        "start": "00:05:55,708",
        "end": "00:05:57,750",
        "en": "だ… 大丈夫だからね",
        "pa": "",
        "vi": "Sẽ ổn cả thôi."
    },
    {
        "start": "00:05:58,250",
        "end": "00:05:59,958",
        "en": "ほら すぐそこだし",
        "pa": "-Ah! -Oh. Look! It's right over there!",
        "vi": "Thấy chứ? Ở ngay đằng kia."
    },
    {
        "start": "00:06:01,166",
        "end": "00:06:02,500",
        "en": "んんん…",
        "pa": "Ah!",
        "vi": ""
    },
    {
        "start": "00:06:02,583",
        "end": "00:06:05,750",
        "en": "あっ 何！\n 急に暗くなったけど！",
        "pa": "What happened? Who turned off the moon?",
        "vi": "Gì vậy? Đột nhiên tối đen rồi!"
    },
    {
        "start": "00:06:05,833",
        "end": "00:06:09,541",
        "en": "何 何 何！\n どうなってるの～",
        "pa": "How'd it get so dark? I can't see a thing!",
        "vi": "Cái gì thế? Chuyện gì thế này?"
