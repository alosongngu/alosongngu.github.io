[
    {
        "start": "00:00:06,000",
        "end": "00:00:10,958",
        "en": "NETFLIX シリーズ",
        "pa": "GUDETAMA: AN EGGCELLENT ADVENTURE",
        "vi": "LOẠT PHIM NETFLIX\n GUDETAMA: CUỘC PHIÊU LƯU CỦA QUẢ TRỨNG LƯỜI"
    },
    {
        "start": "00:00:38,125",
        "end": "00:00:39,416",
        "en": "（ぐでたま）ふあ～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:00:40,458",
        "end": "00:00:43,083",
        "en": "ん ふう～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:00:44,916",
        "end": "00:00:46,500",
        "en": "だりぃ～",
        "pa": "Great.",
        "vi": "Mệt ghê."
    },
    {
        "start": "00:00:51,125",
        "end": "00:00:52,875",
        "en": "あ～～",
        "pa": "Ah!",
        "vi": ""
    },
    {
        "start": "00:00:53,458",
        "end": "00:00:54,416",
        "en": "ん？",
        "pa": "Whoa.",
        "vi": "Hả?"
    },
    {
        "start": "00:00:56,000",
        "end": "00:00:56,833",
        "en": "あっ",
        "pa": "Ugh.",
        "vi": ""
    },
    {
        "start": "00:00:58,125",
        "end": "00:01:00,208",
        "en": "よっ　あ～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:01,333",
        "end": "00:01:02,541",
        "en": "あ～あ～",
        "pa": "Oh!",
        "vi": ""
    },
    {
        "start": "00:01:03,375",
        "end": "00:01:04,208",
        "en": "ううっ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:06,958",
        "end": "00:01:08,333",
        "en": "あ…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:10,125",
        "end": "00:01:12,791",
        "en": "流されるだけの人生…",
        "pa": "I suppose I'm just living life by going with the flow.",
        "vi": "Cuộc đời chỉ toàn là trôi đi."
    },
    {
        "start": "00:01:12,875",
        "end": "00:01:14,541",
        "en": "（卵殻が割れる音）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:14,625",
        "end": "00:01:15,750",
        "en": "あ？",
        "pa": "-Huh?",
        "vi": "Hả?"
    },
    {
        "start": "00:01:16,416",
        "end": "00:01:17,250",
        "en": "（しゃきぴよ）ぴよっ",
        "pa": "Piyo!",
        "vi": "Chiếp!"
    },
    {
        "start": "00:01:21,125",
        "end": "00:01:21,958",
        "en": "（しゃきぴよ）ぴよ",
        "pa": "Huh?",
        "vi": "Chiếp!"
    },
    {
        "start": "00:01:26,791",
        "end": "00:01:28,166",
        "en": "ぴよー！",
        "pa": "Piyo!",
        "vi": "Chiếp!"
    },
    {
        "start": "00:01:30,666",
        "end": "00:01:32,541",
        "en": "生まれた～！",
        "pa": "I'm born! I'm born!",
        "vi": "Tớ ra đời rồi!"
    },
    {
        "start": "00:01:33,291",
        "end": "00:01:36,083",
        "en": "え？　有精卵なん？",
        "pa": "Oh, a fertilized egg.",
        "vi": "Hả? Trứng được thụ tinh?"
    },
    {
        "start": "00:01:39,750",
        "end": "00:01:42,666",
        "en": "誰か いるの？",
        "pa": "What was that?\n Is someone there?",
        "vi": "Có ai ở đó à?"
    },
    {
        "start": "00:01:44,000",
        "end": "00:01:45,208",
        "en": "（ぐでたま）よいしょ",
        "pa": "Here we go.",
        "vi": "Cố nào."
    },
    {
        "start": "00:01:45,875",
        "end": "00:01:47,166",
        "en": "ぷりぷり",
        "pa": "Jiggle, jiggle.",
        "vi": "Núng nính, núng nính."
    },
    {
        "start": "00:01:47,250",
        "end": "00:01:48,083",
        "en": "あっ！",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:48,708",
        "end": "00:01:49,791",
        "en": "ママ？",
        "pa": "-Mommy?",
        "vi": "Mẹ?"
    },
    {
        "start": "00:01:49,875",
        "end": "00:01:50,916",
        "en": "（ぐでたま）はあ…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:51,416",
        "end": "00:01:53,125",
        "en": "ぷりぷり　ぷりっ",
        "pa": "Jiggle.",
        "vi": "Núng nính, núng nính."
    },
    {
        "start": "00:01:53,208",
        "end": "00:01:54,250",
        "en": "ママなの？",
        "pa": "-Are you my mommy? - Shake.",
        "vi": "Mẹ là mẹ con à?"
    },
    {
        "start": "00:01:54,333",
        "end": "00:01:56,166",
        "en": "（ぐでたま）ぷりっ ぷりっ",
        "pa": "- Shake.",
        "vi": ""
    },
    {
        "start": "00:01:57,875",
        "end": "00:01:59,833",
        "en": "（しゃきぴよ）ママー！\n （ぐでたま）ふぐっ…",
        "pa": "Mommy!",
        "vi": "Mẹ!"
    },
    {
        "start": "00:01:59,916",
        "end": "00:02:01,125",
        "en": "違います",
        "pa": "-I'm not your mommy. -Huh?",
        "vi": "Không phải."
    },
    {
        "start": "00:02:01,208",
        "end": "00:02:02,125",
        "en": "ハッ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:02:02,208",
        "end": "00:02:05,916",
        "en": "初めて見たのを\n 親だと思っちゃうん？",
        "pa": "You think I'm your mommy 'cause I'm the first person you saw?",
        "vi": "Cậu nghĩ người đầu tiên cậu thấy là \"mẹ\" à?"
    },
    {
        "start": "00:02:06,000",
        "end": "00:02:08,458",
        "en": "刷り込み 半端ないわ～",
        "pa": "Wow, subconscious imprinting.",
        "vi": "Tập tính in vết mạnh mẽ ghê."
    },
    {
        "start": "00:02:09,041",
        "end": "00:02:09,958",
        "en": "だねっ",
        "pa": "That's right.",
        "vi": "Cậu nói đúng."
    },
    {
        "start": "00:02:10,041",
        "end": "00:02:12,458",
        "en": "何かママだって思っちゃった～",
        "pa": "I did think you were my mommy. Yeah.",
        "vi": "Tớ đã tưởng cậu là mẹ tớ."
    },
    {
        "start": "00:02:12,541",
        "end": "00:02:16,125",
        "en": "ママなら\n むしろ あっちっしょ",
        "pa": "Your mommy looks more like that there.",
        "vi": "Nếu là mẹ thì đằng kia giống hơn."
    },
    {
        "start": "00:02:17,416",
        "end": "00:02:18,708",
        "en": "あっ！",
        "pa": "",
        "vi": "Ôi!"
    },
    {
        "start": "00:02:20,625",
        "end": "00:02:21,458",
        "en": "ママ？",
        "pa": "Mommy?",
        "vi": "Mẹ?"
    },
    {
        "start": "00:02:21,541",
        "end": "00:02:23,541",
        "en": "ママなの？　ママ～！",
        "pa": "Mommy! Mommy! Mommy!",
        "vi": "Mẹ là mẹ con? Mẹ ơi!"
    },
    {
        "start": "00:02:24,125",
        "end": "00:02:26,583",
        "en": "え～ マジ？",
        "pa": "Seriously?",
        "vi": "Cái gì? Đùa à?"
    },
    {
        "start": "00:02:26,666",
        "end": "00:02:29,250",
        "en": "何だか覚えてる気がするな～",
        "pa": "Yeah, it's all coming back to me now.",
        "vi": "Có cảm giác tớ đã nhớ ra…"
    },
    {
        "start": "00:02:30,208",
        "end": "00:02:32,500",
        "en": "ママの あのジョリジョリ感…",
        "pa": "Mommy's smiling, scritchy-scratchy face.",
        "vi": "bộ lông xù xù của mẹ."
    },
    {
        "start": "00:02:33,458",
        "end": "00:02:38,125",
        "en": "（ぐでたま）ていうか～\n 指したの その隣な",
        "pa": "Actually, I was pointing to that one there.",
        "vi": "Thật ra, tớ chỉ vào chỗ bên cạnh cơ."
    },
    {
        "start": "00:02:39,083",
        "end": "00:02:39,916",
        "en": "（しゃきぴよ）ハッ",
        "pa": "Mommy! Mommy! Mommy!",
        "vi": ""
    },
    {
        "start": "00:02:40,000",
        "end": "00:02:42,291",
        "en": "ママなの？　ママ～！",
        "pa": "",
        "vi": "Mẹ là mẹ con à? Mẹ ơi!"
    },
    {
        "start": "00:02:43,166",
        "end": "00:02:46,791",
        "en": "やっぱ刷り込み 半端ないな～",
        "pa": "Man, I'll give you one thing. You're persistent.",
        "vi": "Quả nhiên tập tính in vết mạnh mẽ ghê."
    },
    {
        "start": "00:02:47,416",
        "end": "00:02:49,583",
        "en": "覚えてる気がするな",
        "pa": "Yeah, it's all coming back to me now.",
        "vi": "Có cảm giác tớ đã nhớ ra"
    },
    {
        "start": "00:02:50,208",
        "end": "00:02:52,416",
        "en": "あの 目が笑ってない感じ",
        "pa": "That blank look in her eyes.",
        "vi": "ánh mắt vô hồn đó."
    },
    {
        "start": "00:02:53,583",
        "end": "00:02:54,916",
        "en": "会いたいな",
        "pa": "I really miss her.",
        "vi": "Tớ muốn gặp ghê."
    },
    {
        "start": "00:02:56,291",
        "end": "00:02:57,291",
        "en": "ママ…",
        "pa": "Mommy.",
        "vi": "Mẹ."
    },
    {
        "start": "00:02:58,375",
        "end": "00:02:59,208",
        "en": "あっ",
        "pa": "",
        "vi": "A!"
    },
    {
        "start": "00:02:59,833",
        "end": "00:03:01,666",
        "en": "ぴよたち 兄弟だよね？",
        "pa": "Wait. We're… We're siblings, right?",
        "vi": "Bọn mình là anh em ruột nhỉ?"
    },
    {
        "start": "00:03:02,500",
        "end": "00:03:04,291",
        "en": "同じケースにいたんだし",
        "pa": "-We came out of the same carton.",
        "vi": "Mình ở cùng một khay giấy."
    },
    {
        "start": "00:03:05,083",
        "end": "00:03:07,000",
        "en": "ママを探しにいこうよ",
        "pa": "-We'll go look for our mommy together.",
        "vi": "Cùng đi tìm mẹ đi."
    },
    {
        "start": "00:03:07,583",
        "end": "00:03:09,583",
        "en": "せっかく生まれたんだから",
        "pa": "Come on, newborn sibling.",
        "vi": "Bọn mình được ra đời rồi mà!"
    },
    {
        "start": "00:03:09,666",
        "end": "00:03:12,958",
        "en": "ぐでたまは たまごなんで\n 生まれてもないです",
        "pa": "The name's Gudetama, and I was never really born.",
        "vi": "Gudetama chỉ là một quả trứng, nên chưa được ra đời."
    },
    {
        "start": "00:03:13,041",
        "end": "00:03:14,458",
        "en": "そんなことないよ",
        "pa": "Of course you were born.",
        "vi": "Không đúng!"
    },
    {
        "start": "00:03:15,416",
        "end": "00:03:18,625",
        "en": "っていうか\n 名前 “ぐでたま”っていうんだね",
        "pa": "Gudetama, huh?\n Well, if we're making introductions,",
        "vi": "Vậy tên cậu là Gudetama nhỉ?"
    },
    {
        "start": "00:03:19,375",
        "end": "00:03:21,125",
        "en": "ぴよの名前は しゃきぴよ",
        "pa": "I'm Shakipiyo. Nice to meet you!",
        "vi": "Tên tớ là Shakipiyo!"
    },
    {
        "start": "00:03:21,750",
        "end": "00:03:22,791",
        "en": "ん？",
        "pa": "Ugh!",
        "vi": "Hả?"
    },
    {
        "start": "00:03:22,875",
        "end": "00:03:25,250",
        "en": "（ぐでたま）\n そういうの いいです～",
        "pa": "No need for introductions.",
        "vi": "Không cần giới thiệu đâu."
    },
    {
        "start": "00:03:26,125",
        "end": "00:03:28,750",
        "en": "ちょっ！　起きようよ",
        "pa": "Hey! Wake up! Wake up! Rise and shine!",
        "vi": "Này! Dậy đi!"
    },
    {
        "start": "00:03:28,833",
        "end": "00:03:31,375",
        "en": "おはようございま…",
        "pa": "All right. Are you a rooster already?",
        "vi": "Chào buổi…"
    },
    {
        "start": "00:03:32,083",
        "end": "00:03:33,166",
        "en": "せん",
        "pa": "",
        "vi": "Thôi bỏ đi."
    },
    {
        "start": "00:03:33,750",
        "end": "00:03:36,500",
        "en": "もお～　黄身かっぽじって聞いて！",
        "pa": "Hey, hey, hey! You're going to want to hear this, so listen up.",
        "vi": "Này, nghe tớ nói này!"
    },
    {
        "start": "00:03:37,875",
        "end": "00:03:40,583",
        "en": "１年間で１人の人間に\n 食べられる たまごの数は",
        "pa": "Mexico is the number one egg-eating country in the world",
        "vi": "Số trứng bình quân mỗi người ở Mexico ăn hàng năm là 372 quả, cao nhất thế giới."
    },
    {
        "start": "00:03:40,666",
        "end": "00:03:42,958",
        "en": "世界１位がメキシコで372個",
        "pa": "at 372 eggs per person per year.",
        "vi": ""
    },
    {
        "start": "00:03:43,041",
        "end": "00:03:46,708",
        "en": "２位が日本で338個\n ３位がロシア 306個",
        "pa": "Then comes Japan at 338 and Russia at 306.",
        "vi": "Xếp thứ hai là Nhật với 338 quả, xếp thứ ba là Nga với 306 quả."
    },
    {
        "start": "00:03:46,791",
        "end": "00:03:49,875",
        "en": "（ぐでたま）\n 情報の海 あふれてね～？",
        "pa": "So much information in a tiny brain.",
        "vi": "Cậu đang đắm chìm trong biển thông tin."
    },
    {
        "start": "00:03:49,958",
        "end": "00:03:50,875",
        "en": "（しゃきぴよ）うわ～",
        "pa": "",
        "vi": "Trời!"
    },
    {
        "start": "00:03:51,625",
        "end": "00:03:55,500",
        "en": "これだけ食べられているのに\n ぴよたち生きているんだから！",
        "pa": "So many eggs were eaten, but not us. There must be a reason for that.",
        "vi": "Tận từng đó quả trứng bị ăn, mà bọn mình vẫn còn sống!"
    },
    {
        "start": "00:03:56,208",
        "end": "00:03:57,958",
        "en": "ママに会いにいかなきゃ！",
        "pa": "We've got to find Mommy!",
        "vi": "Bọn mình phải đi tìm mẹ!"
    },
    {
        "start": "00:03:58,708",
        "end": "00:04:00,625",
        "en": "スヤリンチョ",
        "pa": "Okay, good night.",
        "vi": "Ngủ ngon."
    },
    {
        "start": "00:04:01,458",
        "end": "00:04:03,208",
        "en": "寝ないでー！",
        "pa": "Hey, hey, wake up!",
        "vi": "Đừng đi ngủ!"
    },
    {
        "start": "00:04:04,083",
        "end": "00:04:05,083",
        "en": "ハア…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:04:05,166",
        "end": "00:04:08,875",
        "en": "何か 疲れてきちゃった",
        "pa": "Oh, boy.\n Now I'm tired.",
        "vi": "Tự nhiên tớ thấy mệt rồi."
    },
    {
        "start": "00:04:10,041",
        "end": "00:04:13,625",
        "en": "疲れてんのは\n こっちなんだわ",
        "pa": "You and me both. Let's chill out for a while.",
        "vi": "Tớ mới là người mệt mà."
    },
    {
        "start": "00:04:14,791",
        "end": "00:04:17,416",
        "en": "ねえねえ 想像してみようよ",
        "pa": "Think of it! The quest to find Mommy.",
        "vi": "Nè, cậu thử tưởng tượng đi,"
    },
    {
        "start": "00:04:18,541",
        "end": "00:04:20,000",
        "en": "ママを探す旅",
        "pa": "An epic adventure.",
        "vi": "chuyến đi tìm mẹ."
    },
    {
        "start": "00:04:20,750",
        "end": "00:04:22,583",
        "en": "２人の大冒険だよ",
        "pa": "Can't you just picture it in your mind?",
        "vi": "Cuộc phiêu lưu tuyệt vời của bọn mình."
    },
    {
        "start": "00:04:22,666",
        "end": "00:04:24,000",
        "en": "え～",
        "pa": "Uh-huh.",
        "vi": "Hả?"
    },
    {
        "start": "00:04:28,583",
        "end": "00:04:30,750",
        "en": "わっ！　びっくりした～",
        "pa": "Ah! Whoa. That was scary.",
        "vi": "Ối! Làm tớ giật mình!"
    },
    {
        "start": "00:04:31,958",
        "end": "00:04:34,041",
        "en": "ぐでたまも想像してみてよ",
        "pa": "-Come on. You try imagining it too.",
        "vi": "Gudetama cũng tưởng tượng thử đi!"
    },
    {
        "start": "00:04:34,958",
        "end": "00:04:37,083",
        "en": "ママ探しのクライマックス",
        "pa": "Picture us together on our epic quest.",
        "vi": "Cao trào trong chuyến đi tìm mẹ!"
    },
    {
        "start": "00:04:38,750",
        "end": "00:04:40,041",
        "en": "え～",
        "pa": "",
        "vi": "Hả?"
    },
    {
        "start": "00:04:40,125",
        "end": "00:04:42,791",
        "en": "（しゃきぴよ）旅の最後まで\n 絶対 一緒だよ",
        "pa": "Think of all the exciting adventures",
        "vi": "Bọn mình sẽ gắn bó đến cuối chuyến đi!"
    },
    {
        "start": "00:04:42,875",
        "end": "00:04:44,083",
        "en": "きみと ぴよは",
        "pa": "the future has in store for us.",
        "vi": "Cậu và tớ!"
    },
    {
        "start": "00:04:44,166",
        "end": "00:04:47,500",
        "en": "（ぐでたま）ひよこって\n 成長 早いわね～",
        "pa": "Whoa, I didn't know chicks grew up so fast.",
        "vi": "Gà con lớn nhanh ghê."
    },
    {
        "start": "00:04:47,583",
        "end": "00:04:48,375",
        "en": "（にわとり）コケー！",
        "pa": "-Wait, wait!",
        "vi": ""
    },
    {
        "start": "00:04:48,458",
        "end": "00:04:51,458",
        "en": "ちょっと！　変な想像しないで",
        "pa": "Don't think that far into the future.",
        "vi": "Từ từ, đừng tưởng tượng ra cảnh kỳ quái!"
    },
    {
        "start": "00:04:52,500",
        "end": "00:04:56,583",
        "en": "ぴよは ぴよのままで\n ママのいる場所まで行くの",
        "pa": "-I'm not gonna grow up yet.",
        "vi": "Tớ sẽ vẫn y như bây giờ, và tớ sẽ tìm đến chỗ mẹ ở!"
    },
    {
        "start": "00:04:56,666",
        "end": "00:04:58,791",
        "en": "ママのいる場所？",
        "pa": "Not till I find out where Mommy is.\n You really sure you want to?",
        "vi": "Chỗ mẹ ở?"
    },
    {
        "start": "00:05:02,541",
        "end": "00:05:04,291",
        "en": "（ぐでたま）あ～\n （しゃきぴよ）あそこだ！",
        "pa": "Oh! There she is!",
        "vi": "- A. - Mẹ kia rồi!"
    },
    {
        "start": "00:05:04,375",
        "end": "00:05:06,166",
        "en": "（油で揚げる音）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:05:04,375",
        "end": "00:05:06,166",
        "en": "（しゃきぴよ）ママー！",
        "pa": "Mommy!",
        "vi": "Mẹ ơi!"
    },
    {
        "start": "00:05:06,166",
        "end": "00:05:06,958",
        "en": "（油で揚げる音）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:05:06,958",
        "end": "00:05:08,833",
        "en": "（油で揚げる音）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:05:06,958",
        "end": "00:05:08,833",
        "en": "（しゃきぴよ）\n フライドチキン屋さん？",
        "pa": "In a fried chicken place?!",
        "vi": "Quán gà rán à?"
    },
    {
        "start": "00:05:08,916",
        "end": "00:05:10,833",
        "en": "そんなことないから！",
        "pa": "How dare you even suggest that?!",
        "vi": "Mẹ không ở đó đâu!"
    },
    {
        "start": "00:05:13,666",
        "end": "00:05:16,833",
        "en": "んま んま んま んま んま…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:05:16,916",
        "end": "00:05:18,750",
        "en": "一人で行ってきてちょ",
        "pa": "Good luck with the quest.",
        "vi": "Cậu đi một mình đi."
    },
    {
        "start": "00:05:19,333",
        "end": "00:05:22,291",
        "en": "もお～　このままでいいの？",
        "pa": "Aw! You're not gonna do anything at all?",
        "vi": "Trời ạ! Cậu cứ như này cũng thấy được sao?"
    },
    {
        "start": "00:05:22,875",
        "end": "00:05:24,083",
        "en": "いいっしょ～",
        "pa": "Yeah, no.",
        "vi": "Được mà."
    },
    {
        "start": "00:05:24,166",
        "end": "00:05:25,833",
        "en": "殻を破ろうよ",
        "pa": "You need to break out of your shell.",
        "vi": "Phá vỡ vỏ chui ra đi!"
    },
    {
        "start": "00:05:25,916",
        "end": "00:05:27,166",
        "en": "破れたし～",
        "pa": "Already did.",
        "vi": "Phá vỡ rồi mà."
    },
    {
        "start": "00:05:28,083",
        "end": "00:05:29,125",
        "en": "このチキン野郎！",
        "pa": "You're… You're chicken!",
        "vi": "Đồ gà mờ!"
    },
    {
        "start": "00:05:29,208",
        "end": "00:05:31,291",
        "en": "たまごなんですけど～",
        "pa": "Actually, I'm only an egg.",
        "vi": "Tớ là một quả trứng mà."
    },
    {
        "start": "00:05:31,875",
        "end": "00:05:33,250",
        "en": "もお～",
        "pa": "Ugh!",
        "vi": "Nào!"
    },
    {
        "start": "00:05:33,333",
        "end": "00:05:36,416",
        "en": "何かないの？　未来の夢とか",
        "pa": "Don't you have any dreams or ambitions?",
        "vi": "Cậu không có mong muốn hay ước mơ nào sao?"
    },
    {
        "start": "00:05:37,166",
        "end": "00:05:39,500",
        "en": "え～",
        "pa": "Nah.",
        "vi": ""
    },
    {
        "start": "00:05:39,583",
        "end": "00:05:41,000",
        "en": "現状維持？",
        "pa": "What's the point, really?",
        "vi": "Giữ nguyên tình hình?"
    },
    {
        "start": "00:05:41,083",
        "end": "00:05:42,875",
        "en": "安定志向",
        "pa": "Life is hard.",
        "vi": "Duy trì ổn định."
    },
    {
        "start": "00:05:42,958",
        "end": "00:05:45,416",
        "en": "お先 真っ暗",
        "pa": "And the future is bleak.",
        "vi": "Tương lai tăm tối."
    },
    {
        "start": "00:05:45,500",
        "end": "00:05:47,875",
        "en": "ここにいても食べられるだけだよ",
        "pa": "But if you stay here, you'll just get eaten!",
        "vi": "Nếu ở lại đây, cậu sẽ bị ăn mất đấy!"
    },
    {
        "start": "00:05:48,958",
        "end": "00:05:51,416",
        "en": "（ぐでたま）\n どうせ食べられるっしょ",
        "pa": "Yeah, you're probably right.",
        "vi": "Đằng nào chả bị ăn."
    },
    {
        "start": "00:05:52,458",
        "end": "00:05:53,500",
        "en": "ハア…",
        "pa": "Poached, maybe.",
        "vi": ""
    },
    {
        "start": "00:05:53,583",
        "end": "00:05:57,708",
        "en": "ゆでるもよし 焼くもよし\n 生もよし",
        "pa": "Or scrambled.\n Or even raw.",
        "vi": "Luộc cũng được,\n rán hay ăn sống cũng được."
    },
    {
        "start": "00:05:58,583",
        "end": "00:05:59,916",
        "en": "そんなんじゃダメ",
        "pa": "You're okay with that?",
        "vi": "Như thế không ổn đâu."
    },
    {
        "start": "00:06:00,958",
        "end": "00:06:03,208",
        "en": "（ぐでたま）あしたから本気 出す",
        "pa": "Let me sleep on it.",
        "vi": "Từ ngày mai tớ sẽ cố gắng hơn."
    },
    {
        "start": "00:06:04,916",
        "end": "00:06:07,916",
        "en": "決めた！　ぴよが連れてく！",
        "pa": "All right, then!\n I'll take you myself!",
        "vi": "Tớ quyết định rồi. Tớ sẽ đưa cậu theo."
    },
    {
        "start": "00:06:08,541",
        "end": "00:06:09,416",
        "en": "え～",
        "pa": "You'll what?",
        "vi": "Hả?"
    },
    {
        "start": "00:06:10,541",
        "end": "00:06:13,750",
        "en": "（ぐでたま）あっ あっ あ～\n （しゃきぴよ）ん… んん んっ",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:06:14,250",
        "end": "00:06:15,083",
        "en": "ぴよっ！",
        "pa": "There.",
        "vi": "Chiếp!"
    },
    {
        "start": "00:06:15,875",
        "end": "00:06:18,541",
        "en": "行くよ ママ探しの旅へ！",
        "pa": "And now, on with our quest,",
        "vi": "Lên đường đi tìm mẹ nào!"
    },
    {
        "start": "00:06:18,625",
        "end": "00:06:19,166",
        "en": "（ぐでたま）ハア…",
        "pa": "ending in a heartwarming reunion episode when we find our mommy.",
        "vi": ""
    },
    {
        "start": "00:06:19,250",
        "end": "00:06:22,375",
        "en": "２人で すっごく感動的な最終回を\n 目指そう！",
        "pa": "",
        "vi": "Bọn mình hãy hướng đến một kết thúc cực kỳ cảm động nào!"
    },
    {
        "start": "00:06:22,458",
        "end": "00:06:23,250",
        "en": "うわ～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:06:23,958",
        "end": "00:06:24,750",
        "en": "（しゃきぴよ）ん？",
        "pa": "",
        "vi": "Hả?"
    },
    {
        "start": "00:06:24,833",
        "end": "00:06:26,208",
        "en": "（ぐでたま）いや～",
        "pa": "That might not be quite as easy as you think.",
        "vi": ""
    },
    {
        "start": "00:06:26,291",
        "end": "00:06:29,791",
        "en": "思ったとおりには\n ならない気がするな～",
        "pa": "",
        "vi": "Không dễ như cậu nghĩ đâu."
    },
    {
        "start": "00:06:29,875",
        "end": "00:06:30,750",
        "en": "どうして？",
        "pa": "Why is that?",
        "vi": "Tại sao?"
    },
    {
        "start": "00:06:31,625",
        "end": "00:06:34,041",
        "en": "だって たまごってだけで…",
        "pa": "Well, yeah, okay, we're eggs,",
        "vi": "Thì bọn mình là trứng,"
    },
    {
        "start": "00:06:34,666",
        "end": "00:06:37,958",
        "en": "にわとりの たまごか\n 分かんないっしょ",
        "pa": "but how do we even know we're chicken eggs?",
        "vi": "nhưng chắc gì đã là trứng gà?"
    },
    {
        "start": "00:06:38,791",
        "end": "00:06:40,625",
        "en": "な～に言ってんだか",
        "pa": "How can we be sure?\n Oh, don't be ridiculous.",
        "vi": "Cậu nói gì vậy?"
    },
    {
        "start": "00:06:41,291",
        "end": "00:06:43,541",
        "en": "にわとりに決まってるでしょ",
        "pa": "Of course we're chicken eggs, Gudetama.",
        "vi": "Tất nhiên bọn mình là từ gà đẻ ra!"
    },
    {
        "start": "00:06:43,625",
        "end": "00:06:45,333",
        "en": "そうなんか？",
        "pa": "Oh, really?",
        "vi": "Vậy à?"
    },
    {
        "start": "00:06:54,166",
        "end": "00:06:57,083",
        "en": "にわとりじゃなかったら\n 何なの？",
        "pa": "If we're not chicken eggs, then what are we?",
        "vi": "Nếu không phải là gà thì bọn mình là gì?"
    },
    {
        "start": "00:06:57,166",
        "end": "00:06:57,750",
        "en": "え～",
        "pa": "",
        "vi": "Gì thế?"
    },
    {
        "start": "00:06:57,833",
        "end": "00:06:59,375",
        "en": "（人々の話し声）\n （寿司(すし)職人）いらっしゃいませ！",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:06:59,458",
        "end": "00:07:00,583",
        "en": "（ぐでたま）ん？",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:06:59,458",
        "end": "00:07:00,583",
        "en": "（人々の話し声）",
        "pa": "- Welcome. Welcome.",
        "vi": "Hoan nghênh!"
    },
    {
        "start": "00:07:00,583",
        "end": "00:07:01,458",
        "en": "（人々の話し声）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:07:01,541",
        "end": "00:07:03,083",
        "en": "何か来る",
        "pa": "-Something's coming.",
        "vi": "Có gì đó đang đến!"
    },
    {
        "start": "00:07:05,000",
        "end": "00:07:08,333",
        "en": "大丈夫だからね\n 一緒にママに会いにいくんだよ",
        "pa": "- Don't worry. It'll be okay.\n We're going to go find our mommy.",
        "vi": "Sẽ ổn thôi. Bọn mình sẽ cùng nhau đi tìm mẹ."
    },
    {
        "start": "00:07:10,958",
        "end": "00:07:11,875",
        "en": "（扉を開ける音）\n （しゃきぴよ）ん！",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:07:12,500",
        "end": "00:07:15,625",
        "en": "よいしょ　ぴよっ　ぴよ～！",
        "pa": "Come on. Here we go.",
        "vi": "Đi thôi! Chiếp, chiếp!"
    },
    {
        "start": "00:07:15,708",
        "end": "00:07:17,583",
        "en": "（ぐでたま）あ～！\n （しゃきぴよ）わあ～",
        "pa": "Piyo! Piyo!",
        "vi": ""
    },
    {
        "start": "00:07:18,541",
        "end": "00:07:19,958",
        "en": "（ぐでたま）ぐで～",
        "pa": "",
        "vi": "Núng nính."
    },
    {
        "start": "00:07:21,250",
        "end": "00:07:24,000",
        "en": "（寿司職人）誰だ？\n 生卵 落としたヤツは",
        "pa": "Okay, okay. Who was it that dropped this egg?",
        "vi": "Ai làm đổ trứng sống vậy?"
    },
    {
        "start": "00:07:25,250",
        "end": "00:07:27,541",
        "en": "ん… ハッ！　ぐでたま！",
        "pa": "Ah!",
        "vi": "Gudetama!"
    },
    {
        "start": "00:07:28,083",
        "end": "00:07:29,208",
        "en": "ハア ハア…",
        "pa": "Gudetama!",
        "vi": ""
    },
    {
        "start": "00:07:29,875",
        "end": "00:07:30,833",
        "en": "よいしょ",
        "pa": "",
        "vi": "Đi thôi!"
    },
    {
        "start": "00:07:31,541",
        "end": "00:07:33,208",
        "en": "ここどこ～？",
        "pa": "-What happened? Where am I? -I don't know.",
        "vi": "Đây là đâu?"
    },
    {
        "start": "00:07:33,291",
        "end": "00:07:36,208",
        "en": "（しゃきぴよ）分かんない\n 分かんないけど きっと…",
        "pa": "But don't worry. You're okay.",
        "vi": "Tớ không biết! Tớ không biết, nhưng…"
    },
    {
        "start": "00:07:36,291",
        "end": "00:07:37,583",
        "en": "あれは～？",
        "pa": "Who are they?",
        "vi": "Đó là gì?"
    },
    {
        "start": "00:07:38,333",
        "end": "00:07:39,125",
        "en": "（しゃきぴよ）えっ",
        "pa": "Uh…",
        "vi": "Hả?"
    },
    {
        "start": "00:07:39,208",
        "end": "00:07:41,916",
        "en": "（人々の話し声）",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:07:42,000",
        "end": "00:07:43,250",
        "en": "（寿司職人）いらっしゃいませ～",
        "pa": "",
        "vi": "- Hoan nghênh! - Hoan nghênh!"
    },
    {
        "start": "00:07:43,875",
        "end": "00:07:45,291",
        "en": "（寿司職人）いらっしゃいませ～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:07:46,583",
        "end": "00:07:48,208",
        "en": "ま… まさか！",
        "pa": "No way. It can't be.",
        "vi": "Chẳng lẽ…"
    },
    {
        "start": "00:07:49,208",
        "end": "00:07:50,875",
        "en": "まさか ここって…",
        "pa": "Is this what I think it is?",
        "vi": "Chẳng lẽ nơi đây là…"
    },
    {
        "start": "00:07:53,958",
        "end": "00:07:54,875",
        "en": "あ～",
        "pa": "",
        "vi": "Ôi."
    },
    {
        "start": "00:07:54,958",
        "end": "00:07:55,791",
        "en": "ああ…",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:07:57,833",
        "end": "00:08:00,791",
        "en": "見つかったら食べられちゃう",
        "pa": "If someone finds us, we're going to get eaten. ",
        "vi": "Nếu ai đó phát hiện ra là bọn mình bị ăn mất đấy!"
    },
    {
        "start": "00:08:01,500",
        "end": "00:08:02,375",
        "en": "（ぐでたま）え～",
        "pa": "",
        "vi": "Thế à?"
    },
    {
        "start": "00:08:10,541",
        "end": "00:08:15,541",
        "en": "♪～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:08:46,750",
        "end": "00:08:51,750",
        "en": "～♪",
        "pa": "",
        "vi": "Biên dịch: Nhimseo "
    }
]