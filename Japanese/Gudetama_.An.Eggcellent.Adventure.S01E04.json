    {
        "start": "00:02:33,458",
        "end": "00:02:36,208",
        "en": "たった３階～！",
        "pa": "Let's see.\n There's only 40 more!",
        "vi": "Chỉ đi được ba tầng?"
    },
    {
        "start": "00:02:36,916",
        "end": "00:02:38,791",
        "en": "ふあ～あ～",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:02:42,333",
        "end": "00:02:44,708",
        "en": "（マヨネーズ）まよってんの？",
        "pa": "Hey, buddy, you lost?",
        "vi": "Cậu bị lạc à?"
