[
    {
        "start": "00:00:24,960",
        "end": "00:00:26,160",
        "en": "Tak ada.",
        "pa": "No?",
        "vi": "Không có?"
    },
    {
        "start": "00:00:32,560",
        "end": "00:00:34,720",
        "en": "Dapat! Ara yang jadi!",
        "pa": "Gotcha! You're it!",
        "vi": "Tìm thấy rồi! Tới lượt em đi bắt đấy!"
    },
    {
        "start": "00:00:37,400",
        "end": "00:00:40,600",
        "en": "Tak kena. Kejarlah!",
        "pa": "Missed! You can't catch me!",
        "vi": "Trượt rồi nhé! Đừng hòng bắt được anh!"
    },
    {
        "start": "00:00:45,480",
        "end": "00:00:46,840",
        "en": "Nak pergi mana?",
        "pa": "Where are you going?",
        "vi": "Xem anh chạy đi đâu?"
    },
    {
        "start": "00:00:48,000",
        "end": "00:00:49,600",
        "en": "Jangan lari!",
        "pa": "Brother, stop!",
        "vi": "Anh à, dừng lại đi!"
    },
    {
        "start": "00:00:51,680",
        "end": "00:00:53,280",
        "en": "Jangan main di sini.",
        "pa": "Not in here!",
        "vi": "Đừng đùa trong đây!"
    },
    {
        "start": "00:00:53,360",
        "end": "00:00:55,320",
        "en": "- Ara jadi! - Aris jadi!",
        "pa": "Oh no, I can't get you.",
        "vi": "Ôi không, em bắt anh không được."
    },
    {
        "start": "00:00:58,640",
        "end": "00:01:01,160",
        "en": "Aris, Ara! Jangan lari-lari.",
        "pa": "Aris, Ara. Stop running around!",
        "vi": "Aris, Ara. Đừng chạy nữa!"
    },
    {
        "start": "00:01:01,240",
        "end": "00:01:02,960",
        "en": "Tepilah, Aki.",
        "pa": "Aki, move aside!",
        "vi": "Ông à, tránh sang một bên!"
    },
    {
        "start": "00:01:03,040",
        "end": "00:01:06,240",
        "en": "Aris. Ara! Dengar tak Aki cakap apa?",
        "pa": "Aris, Ara! Did you hear what I said?",
        "vi": "Aris, Ara! Hai đứa có nghe ông nói không?"
    },
    {
        "start": "00:01:06,320",
        "end": "00:01:07,560",
        "en": "Apa, Aki?",
        "pa": "What, Aki?",
        "vi": "Gì vậy ông?"
    },
    {
        "start": "00:01:07,640",
        "end": "00:01:10,080",
        "en": "Jangan lari-lari. Nanti jatuh.",
        "pa": "Don't run around. You'll fall.",
        "vi": "Đừng chạy lung tung. Hai cháu sẽ ngã cho xem."
    },
    {
        "start": "00:01:18,080",
        "end": "00:01:19,480",
        "en": "Cepatlah!",
        "pa": "Come on!",
        "vi": "Thôi nào!"
    },
    {
        "start": "00:01:21,280",
        "end": "00:01:24,760",
        "en": "Jangan lari di situ. Nanti pecah pasu Aki.",
        "pa": "Get away from there. You'll break my vase!",
        "vi": "Tránh xa chỗ đó ra. Vỡ cái bình của ông mất!"
    },
    {
        "start": "00:01:26,560",
        "end": "00:01:29,800",
        "en": "Yalah. Kami tak lari.",
        "pa": "All right. We won't run.",
        "vi": "Được rồi. Chúng cháu sẽ không chạy nữa."
    },
    {
        "start": "00:01:34,600",
        "end": "00:01:36,480",
        "en": "Jaga, Abang.",
        "pa": "-Watch out, brother. -What are you going to do?",
        "vi": "- Anh cẩn thận đấy. - Em sẽ làm gì nào?"
    },
    {
        "start": "00:01:36,560",
        "end": "00:01:37,440",
        "en": "Kenapa, Ara?",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:37,520",
        "end": "00:01:40,400",
        "en": "Ara kejar sampai dapat.",
        "pa": "I'm going to get you!",
        "vi": "Em sẽ bắt được anh!"
    },
    {
        "start": "00:01:40,480",
        "end": "00:01:43,600",
        "en": "- Siaplah Abang. - Ara bukan boleh tangkap Abang pun.",
        "pa": "-Watch it, Aris. -You can't catch me, Ara.",
        "vi": "- Coi chừng đấy. - Đừng hòng bắt được anh, Ara."
    },
    {
        "start": "00:01:43,680",
        "end": "00:01:44,720",
        "en": "Abang!",
        "pa": "Brother!",
        "vi": "Anh!"
    },
    {
        "start": "00:01:45,720",
        "end": "00:01:47,560",
        "en": "Tak dapat.",
        "pa": "Come get me!",
        "vi": "Đến bắt anh đi!"
    },
    {
        "start": "00:01:48,880",
        "end": "00:01:50,560",
        "en": "Abang!",
        "pa": "Brother!",
        "vi": "Anh!"
    },
    {
        "start": "00:01:50,640",
        "end": "00:01:52,840",
        "en": "- Geramnya! - Tak kena! Tak dapat!",
        "pa": "-Just you wait! -You can't catch me!",
        "vi": "- Cứ đợi đấy! - Đừng hòng bắt được anh!"
    },
    {
        "start": "00:01:52,920",
        "end": "00:01:54,040",
        "en": "- Tak dapat. - Aduh.",
        "pa": "You can't catch me!",
        "vi": "Đừng hòng bắt được anh!"
    },
    {
        "start": "00:01:55,160",
        "end": "00:01:57,600",
        "en": "- Sini Abang, sini! - Kejarlah!",
        "pa": "-Aris, come here. -Try and catch me!",
        "vi": "- Anh đến đây xem. - Thử bắt anh xem!"
    },
    {
        "start": "00:01:58,360",
        "end": "00:01:59,960",
        "en": "Cepatlah Ara.",
        "pa": "Come on, Ara!",
        "vi": "Nào, Ara!"
    },
    {
        "start": "00:02:04,160",
        "end": "00:02:05,960",
        "en": "Pasu aku!",
        "pa": "My vase!",
        "vi": "Cái bình của ông!"
    },
    {
        "start": "00:02:08,880",
        "end": "00:02:10,639",
        "en": "Bukankah Aki dah cakap tadi.",
        "pa": "I told you not to run just now.",
        "vi": "Ông đã bảo đừng chạy rồi mà."
    },
    {
        "start": "00:02:10,720",
        "end": "00:02:13,520",
        "en": "Jangan lari-lari. Tengok sekarang apa dah jadi.",
        "pa": "Look what you did.",
        "vi": "Giờ xem các cháu làm gì này."
    },
    {
        "start": "00:02:14,120",
        "end": "00:02:18,400",
        "en": "Habis pasu kesayangan Aki!",
        "pa": "My favorite vase is broken!",
        "vi": "Cái bình ông thích nhất đã vỡ rồi!"
    },
    {
        "start": "00:02:19,120",
        "end": "00:02:22,400",
        "en": "Aki, kami tak sengaja.",
        "pa": "We didn't mean to, Aki…",
        "vi": "Chúng cháu không cố ý mà ông…"
    },
    {
        "start": "00:02:22,480",
        "end": "00:02:26,360",
        "en": "Minta maaf. Janji tak buat lagi.",
        "pa": "We're sorry. We don't do it again.",
        "vi": "Chúng cháu xin lỗi ạ. Chúng cháu sẽ không làm thế nữa."
    },
    {
        "start": "00:02:27,400",
        "end": "00:02:32,200",
        "en": "Tak guna nak janji. Dah pecah!",
        "pa": "It's no use. It's broken!",
        "vi": "Vô ích thôi. Cái bình vỡ rồi!"
    },
    {
        "start": "00:02:34,040",
        "end": "00:02:36,200",
        "en": "Aki marah?",
        "pa": "Are you mad?",
        "vi": "Ông giận rồi ạ?"
    },
    {
        "start": "00:02:36,280",
        "end": "00:02:39,480",
        "en": "Nasi sudah menjadi bubur.",
        "pa": "It's no use crying over spilled milk.",
        "vi": "Sữa đổ hết rồi ngồi khóc cũng như không."
    },
    {
        "start": "00:02:41,840",
        "end": "00:02:44,840",
        "en": "- Abang, pergilah. - Ara pergilah. Ara yang buat.",
        "pa": "-Brother, you go. -You go. You did this.",
        "vi": "- Anh, đi đi. - Em đi đi. Em làm mà."
    },
    {
        "start": "00:02:44,920",
        "end": "00:02:47,440",
        "en": "- Abanglah! - Abang tak buat apa-apa pun.",
        "pa": "-You did it! -I didn't do anything.",
        "vi": "- Anh làm thì có! - Anh có làm gì đâu."
    },
    {
        "start": "00:02:49,360",
        "end": "00:02:50,200",
        "en": "Aki.",
        "pa": "Aki, here.",
        "vi": "Ông ơi, đây ạ."
    },
    {
        "start": "00:02:50,280",
        "end": "00:02:51,320",
        "en": "- Ya? - Nah.",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:02:51,400",
        "end": "00:02:55,280",
        "en": "- Nak buat apa? - Kita gam balik.",
        "pa": "What's that for?",
        "vi": "Cái đó để làm gì?"
    },
    {
        "start": "00:02:55,360",
        "end": "00:02:57,440",
        "en": "Tak ada maknanya.",
        "pa": "We'll glue it back together.\n It's useless.",
        "vi": "Để dán cái bình lại ạ.\n Vô ích thôi."
    },
    {
        "start": "00:02:58,600",
        "end": "00:03:01,600",
        "en": "Itu betul. Nasi dah jadi bubur.",
        "pa": "Yeah, the milk has been spilled!",
        "vi": "Đúng ạ, vì sữa đã đổ hết rồi!"
    },
    {
        "start": "00:03:03,920",
        "end": "00:03:05,680",
        "en": "Kau faham maksud peribahasa itu?",
        "pa": "You know the meaning of that idiom?",
        "vi": "Cháu hiểu ý nghĩa của câu đó à?"
    },
    {
        "start": "00:03:06,960",
        "end": "00:03:07,840",
        "en": "Tak.",
        "pa": "No.",
        "vi": "Không ạ."
    },
    {
        "start": "00:03:10,280",
        "end": "00:03:11,320",
        "en": "Dengar sini.",
        "pa": "Listen here.",
        "vi": "Nghe này."
    },
    {
        "start": "00:03:12,160",
        "end": "00:03:13,320",
        "en": "Cerita?",
        "pa": "A story?",
        "vi": "Nghe kể chuyện ạ?"
    },
    {
        "start": "00:03:16,040",
        "end": "00:03:18,160",
        "en": "Ceritalah!",
        "pa": "Tell us!",
        "vi": "Kể đi ông!"
    },
    {
        "start": "00:03:19,080",
        "end": "00:03:22,120",
        "en": "Pada zaman dahulu…",
        "pa": "Once upon a time…",
        "vi": "Ngày xửa ngày xưa…"
    },
    {
        "start": "00:03:23,000",
        "end": "00:03:25,120",
        "en": "- Hai, kawan-kawan. - Hai, Itik.",
        "pa": "-Hi, guys! -Hi, Itik!",
        "vi": "- Chào các cậu! - Chào Itik!"
    },
    {
        "start": "00:03:25,960",
        "end": "00:03:27,920",
        "en": "- Nah. - Itu apa?",
        "pa": "-Here. -What is that?",
        "vi": "- Đây. - Cái gì thế?"
    },
    {
        "start": "00:03:28,000",
        "end": "00:03:32,320",
        "en": "\"Kenduri besar!\"",
        "pa": "\"A big… feast!\"",
        "vi": "\"Một bữa tiệc… lớn!\""
    },
    {
        "start": "00:03:32,400",
        "end": "00:03:34,800",
        "en": "Datang, ya? Jangan lupa.",
        "pa": "Don't forget to come!",
        "vi": "Nhớ đến dự nhé!"
    },
    {
        "start": "00:03:34,880",
        "end": "00:03:36,720",
        "en": "Ini untuk kau.",
        "pa": "You have to come, too.",
        "vi": "Anh cũng phải đến đấy."
    },
    {
        "start": "00:03:37,320",
        "end": "00:03:38,480",
        "en": "Terima kasih, Itik.",
        "pa": "Thank you, Itik!",
        "vi": "Cảm ơn Itik!"
    },
    {
        "start": "00:03:38,560",
        "end": "00:03:41,600",
        "en": "- Kau pun kena datang tahu. - Mestilah.",
        "pa": "-You must come, okay? -Of course!",
        "vi": "- Anh phải đến đấy, được chứ? - Dĩ nhiên rồi!"
    },
    {
        "start": "00:03:42,280",
        "end": "00:03:44,840",
        "en": "- Terima kasih Itik. - Sama-sama.",
        "pa": "Thanks, Itik!",
        "vi": "Cảm ơn Itik!"
    },
    {
        "start": "00:03:44,920",
        "end": "00:03:47,360",
        "en": "Tak sabarnya!",
        "pa": "-You're welcome! -I can't wait!",
        "vi": "- Không có chi! - Mình nôn nao quá!"
    },
    {
        "start": "00:03:47,440",
        "end": "00:03:50,000",
        "en": "Siapkan makanan banyak-banyak, tahu.",
        "pa": "Be sure to prepare lots of yummy food.",
        "vi": "Nhớ chuẩn bị thật nhiều thức ăn ngon nhé."
    },
    {
        "start": "00:03:50,080",
        "end": "00:03:52,280",
        "en": "Tahu. Jumpa malam ini.",
        "pa": "We will. See you tonight!",
        "vi": "Chúng tôi nhớ mà. Hẹn tối nay nhé!"
    },
    {
        "start": "00:03:52,360",
        "end": "00:03:54,880",
        "en": "- Okey, jumpa nanti. - Jumpa malam ini.",
        "pa": "-Okay! -See you!",
        "vi": "- Được! - Hẹn gặp lại!"
    },
    {
        "start": "00:04:11,600",
        "end": "00:04:15,400",
        "en": "Senangnya hati. Nak ke mana, Arnab?",
        "pa": "You look happy. Where are you off to, Arnab?",
        "vi": "Trông cậu vui thế. Cậu đi đâu vậy, Arnab?"
    },
    {
        "start": "00:04:15,480",
        "end": "00:04:17,920",
        "en": "Rumah Itiklah. Ada kenduri, bukan?",
        "pa": "To Itik's house. There's a big feast, remember?",
        "vi": "Đến nhà Itik. Cậu ta mở tiệc to mà, nhớ không?"
    },
    {
        "start": "00:04:19,519",
        "end": "00:04:23,200",
        "en": "Samalah kita",
        "pa": "Me, too!",
        "vi": "Tôi cũng đi!"
    },
    {
        "start": "00:04:29,360",
        "end": "00:04:31,080",
        "en": "Tak apalah.",
        "pa": "All right then.",
        "vi": "Được rồi."
    },
    {
        "start": "00:04:32,840",
        "end": "00:04:34,520",
        "en": "Jumpa di sana!",
        "pa": "See you there!",
        "vi": "Hẹn gặp anh ở đó!"
    },
    {
        "start": "00:04:51,720",
        "end": "00:04:53,040",
        "en": "Kura-kura.",
        "pa": "Oh, Kura-kura!",
        "vi": "Kura-kura!"
    },
    {
        "start": "00:04:54,600",
        "end": "00:04:57,040",
        "en": "Lajunya kau berguling-guling.",
        "pa": "You were so fast.",
        "vi": "Cậu chạy nhanh quá."
    },
    {
        "start": "00:04:57,120",
        "end": "00:04:59,360",
        "en": "Terkejut aku",
        "pa": "You startled me",
        "vi": "Làm tôi giật cả mình"
    },
    {
        "start": "00:04:59,440",
        "end": "00:05:01,120",
        "en": "Pasal makan, aku nombor satu.",
        "pa": "When it comes to food, I'm number one!",
        "vi": "Nói về ăn uống, tôi là nhất!"
    },
    {
        "start": "00:05:01,200",
        "end": "00:05:04,640",
        "en": "Tak boleh lepas. Mesti cepat, mesti sampai dulu.",
        "pa": "Won't miss it. Must be fast, must get there first!",
        "vi": "Tôi sẽ không bỏ lỡ đâu. Phải chạy thật nhanh, đến đó trước tiên."
    },
    {
        "start": "00:05:11,440",
        "end": "00:05:13,280",
        "en": "Jom balik!",
        "pa": "See you next time!\n Let's go back!",
        "vi": "Hẹn các cậu lần sau nữa nhé!\n Trở về thôi!"
    },
    {
        "start": "00:05:13,360",
        "end": "00:05:15,920",
        "en": "Kawan-kawan semua dah nak balik?",
        "pa": "Are you guys going home already?",
        "vi": "Các cậu quay về rồi ư?"
    },
    {
        "start": "00:05:16,000",
        "end": "00:05:17,000",
        "en": "Ya!",
        "pa": "Yeah!",
        "vi": "Phải!"
    },
    {
        "start": "00:05:18,440",
        "end": "00:05:20,920",
        "en": "Makanan sedap.",
        "pa": "The food was great!",
        "vi": "Thức ăn ngon tuyệt vời!"
    },
    {
        "start": "00:05:26,480",
        "end": "00:05:28,840",
        "en": "Terima kasih kerana datang.",
        "pa": "Thank you for coming.",
        "vi": "Cảm ơn các cậu đã đến."
    },
    {
        "start": "00:05:28,920",
        "end": "00:05:31,520",
        "en": "Itik, nanti jangan lupa ajak lagi.",
        "pa": "Don't forget to invite us again, Itik.",
        "vi": "Đừng quên mời chúng tôi nữa nhé, Itik."
    },
    {
        "start": "00:05:31,600",
        "end": "00:05:33,960",
        "en": "Jangan risau. Nanti aku jemput lagi.",
        "pa": "I will.",
        "vi": "Chắc chắn rồi."
    },
    {
        "start": "00:05:34,880",
        "end": "00:05:37,920",
        "en": "Kura-kura, kau baru sampai?",
        "pa": "Kura-kura, you just got here?",
        "vi": "Kura-kura, giờ anh mới đến à?"
    },
    {
        "start": "00:05:38,000",
        "end": "00:05:41,560",
        "en": "Dah habis",
        "pa": "All finished",
        "vi": "Tiệc tàn rồi"
    },
    {
        "start": "00:05:42,800",
        "end": "00:05:44,360",
        "en": "Kau lambat sangat.",
        "pa": "You're too slow.",
        "vi": "Anh chậm quá đấy."
    },
    {
        "start": "00:05:44,440",
        "end": "00:05:47,600",
        "en": "Tiada rezeki. Lain kali, Kura-kura.",
        "pa": "Too bad you missed it.",
        "vi": "Tiếc là anh lỡ mất bữa tiệc rồi."
    },
    {
        "start": "00:05:47,680",
        "end": "00:05:50,600",
        "en": "Tak apalah. Aku balik dulu.",
        "pa": "-Maybe next time, Kura-kura. -Never mind.\n I'll go now.",
        "vi": "- Thôi để lần sau nhé, Kura-kura. - Đừng bận tâm.\n Tôi về đây."
    },
    {
        "start": "00:05:52,400",
        "end": "00:05:55,000",
        "en": "Janganlah begitu, Kura-kura.",
        "pa": "Come on, don't be that way.",
        "vi": "Nào, đừng như vậy."
    },
    {
        "start": "00:05:59,640",
        "end": "00:06:03,600",
        "en": "Lepas ini, aku takkan pergi kenduri lagi.",
        "pa": "I won't come to any feast ever again!",
        "vi": "Tôi sẽ không bao giờ đi dự tiệc nữa!"
    },
    {
        "start": "00:06:13,360",
        "end": "00:06:16,440",
        "en": "Kura-kura, aku bawakan…",
        "pa": "Kura-kura, I brought some…",
        "vi": "Kura-kura, tôi mang đến cho anh…"
    },
    {
        "start": "00:06:16,520",
        "end": "00:06:19,680",
        "en": "Kura-kura, kau marah?",
        "pa": "Kura-kura, are you mad?",
        "vi": "Kura-kura, anh giận à?"
    },
    {
        "start": "00:06:20,560",
        "end": "00:06:23,800",
        "en": "Tak marah. Tapi merajuk.",
        "pa": "No. Just sulking!",
        "vi": "Không. Tôi chỉ đang dỗi thôi!"
    },
    {
        "start": "00:06:24,400",
        "end": "00:06:29,760",
        "en": "Janganlah begitu. Aku ada bawakan makanan. Khas untuk kau.",
        "pa": "Come on, cheer up!\n I brought food, especially for you.",
        "vi": "Thôi nào, vui lên!\n Tôi có mang thức ăn, đặc biệt dành riêng cho anh."
    },
    {
        "start": "00:06:34,200",
        "end": "00:06:35,880",
        "en": "Sedap, bukan, Kura-kura.",
        "pa": "Is it yummy?",
        "vi": "Ngon không?"
    },
    {
        "start": "00:06:37,560",
        "end": "00:06:38,600",
        "en": "Kura-kura?",
        "pa": "Kura-kura?",
        "vi": "Kura-kura?"
    },
    {
        "start": "00:06:47,040",
        "end": "00:06:48,680",
        "en": "Kura-kura?",
        "pa": "Kura-kura?",
        "vi": "Kura-kura?"
    },
    {
        "start": "00:06:56,480",
        "end": "00:06:58,360",
        "en": "Kau buat apa, Kancil?",
        "pa": "What have you done to me, Kancil?",
        "vi": "Cậu đã làm gì tôi thế, Kancil?"
    },
    {
        "start": "00:06:58,440",
        "end": "00:07:01,360",
        "en": "Jangan pura-pura tidur. Merajuk konon.",
        "pa": "Don't pretend to sleep.",
        "vi": "Đừng có giả vờ ngủ."
    },
    {
        "start": "00:07:01,440",
        "end": "00:07:04,400",
        "en": "Sendiri punya salah datang lambat, nak merajuk.",
        "pa": "Sulking? Really?\n It's your own fault you're late, you know.",
        "vi": "Dỗi à? Thật ư?\n Anh đến muộn là lỗi tại anh chứ ai."
    },
    {
        "start": "00:07:05,080",
        "end": "00:07:08,080",
        "en": "- Kau tak faham. - Apa yang aku tak faham?",
        "pa": "-You don't understand… -What is it?",
        "vi": "- Cậu không hiểu… - Cái gì cơ?"
    },
    {
        "start": "00:07:08,920",
        "end": "00:07:11,320",
        "en": "Terbalikkan aku dulu.",
        "pa": "Roll me over first.",
        "vi": "Xoay tôi lại với nào."
    },
    {
        "start": "00:07:17,400",
        "end": "00:07:21,240",
        "en": "Kau tengok rumah aku. Besar dan berat.",
        "pa": "You see my house?\n Big and heavy.",
        "vi": "Cậu thấy nhà tôi không?\n Vừa to vừa nặng."
    },
    {
        "start": "00:07:21,320",
        "end": "00:07:25,280",
        "en": "Kaki aku pendek. Jalan pun terseret-seret.",
        "pa": "My feet are short.\n And I have to crawl around.",
        "vi": "Chân tôi thì ngắn.\n Và tôi phải bò khắp nơi thế này."
    },
    {
        "start": "00:07:25,360",
        "end": "00:07:28,120",
        "en": "Mana boleh bergerak laju macam kau",
        "pa": "I can't move as fast as you or everyone else.",
        "vi": "Tôi không thể chạy nhanh như cậu hay các con thú khác được."
    },
    {
        "start": "00:07:28,200",
        "end": "00:07:30,360",
        "en": "dan kawan-kawan yang lain.",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:08:12,960",
        "end": "00:08:16,600",
        "en": "Tinggi lagi!",
        "pa": "Higher!",
        "vi": "Cao hơn nữa!"
    },
    {
        "start": "00:08:22,360",
        "end": "00:08:24,080",
        "en": "Apa yang tinggi?",
        "pa": "-What are you talking about? -I'm flying!",
        "vi": "- Anh đang nói gì vậy? - Tôi đang bay!"
    },
    {
        "start": "00:08:24,160",
        "end": "00:08:25,920",
        "en": "Akulah.",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:08:32,559",
        "end": "00:08:33,799",
        "en": "Malu aku Kancil.",
        "pa": "So you want to fly, don't you?",
        "vi": "Anh muốn bay được à?"
    },
    {
        "start": "00:08:33,880",
        "end": "00:08:36,640",
        "en": "Kau teringin nak terbang, betul tak?",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:08:39,320",
        "end": "00:08:44,480",
        "en": "Ya, aku teringin nak terbang",
        "pa": "Yeah, I've always wanted to fly",
        "vi": "Phải, tôi lúc nào cũng muốn bay"
    },
    {
        "start": "00:08:44,560",
        "end": "00:08:46,480",
        "en": "Nak tengok dunia luar.",
        "pa": "I want to see the outside world.",
        "vi": "Tôi muốn nhìn ngắm thế giới ngoài kia."
    },
    {
        "start": "00:08:46,560",
        "end": "00:08:49,520",
        "en": "Begitu rupanya.",
        "pa": "Oh, I see…",
        "vi": "Hiểu rồi…"
    },
    {
        "start": "00:08:51,400",
        "end": "00:08:54,520",
        "en": "Petang ini, kita jumpa di tepi tasik.",
        "pa": "Meet me by the lake later.",
        "vi": "Lát nữa đến gặp tôi ở bờ hồ nhé."
    },
    {
        "start": "00:08:54,600",
        "end": "00:08:56,640",
        "en": "Kenapa? Ada apa di sana?",
        "pa": "Why? What for?",
        "vi": "Để làm gì vậy?"
    },
    {
        "start": "00:08:56,720",
        "end": "00:08:57,920",
        "en": "Datang sajalah.",
        "pa": "Just come.",
        "vi": "Cứ đến sẽ biết."
    },
    {
        "start": "00:09:00,240",
        "end": "00:09:02,600",
        "en": "Indahnya hutan.",
        "pa": "Beautiful jungle!",
        "vi": "Khu rừng xinh đẹp!"
    },
    {
        "start": "00:09:07,440",
        "end": "00:09:11,440",
        "en": "- Mana Kancil? Kata… - Kura-kura!",
        "pa": "-Where is Kancil? I thought… -Kura-kura!",
        "vi": "- Kancil đâu? Tôi tưởng là… - Kura-kura!"
    },
    {
        "start": "00:09:12,520",
        "end": "00:09:15,160",
        "en": "Awalnya kau. Selalunya…",
        "pa": "You're early. You're usually--",
        "vi": "Anh đến sớm thế. Anh thường hay…"
    },
    {
        "start": "00:09:15,240",
        "end": "00:09:16,680",
        "en": "Hai, Kura-kura!",
        "pa": "Hi, Kura-kura!",
        "vi": "Chào Kura-kura!"
    },
    {
        "start": "00:09:17,320",
        "end": "00:09:20,320",
        "en": "Itik dan Itok pun datang?",
        "pa": "Itik and Itok, you're here, too?",
        "vi": "Itik và Itok, hai cậu cũng ở đây sao?"
    },
    {
        "start": "00:09:20,400",
        "end": "00:09:22,840",
        "en": "Ya. Kancil yang suruh.",
        "pa": "Yeah, Kancil told us to come.",
        "vi": "Phải, Kancil gọi bọn tôi đến."
    },
    {
        "start": "00:09:22,920",
        "end": "00:09:25,280",
        "en": "Kenapa kau suruh kami ke sini?",
        "pa": "So, what is it, Kancil?",
        "vi": "Có chuyện gì thế, Kancil?"
    },
    {
        "start": "00:09:25,360",
        "end": "00:09:29,840",
        "en": "Begini, Kura-kura teringin nak terbang melihat dunia luar.",
        "pa": "It's like this.\n Kura-kura wants to fly and see the outside world.",
        "vi": "Chuyện là vầy.\n Kura-kura muốn bay được và nhìn ngắm thế giới ngoài kia."
    },
    {
        "start": "00:09:31,440",
        "end": "00:09:32,720",
        "en": "- Jadi? - Jadi?",
        "pa": "-So? -So?",
        "vi": "- Thì sao? - Thì sao?"
    },
    {
        "start": "00:09:32,800",
        "end": "00:09:38,280",
        "en": "Jadi, boleh tak Itik dan Itok tolong Kura-kura?",
        "pa": "So, can you help him?",
        "vi": "Vậy hai cậu giúp anh ta được không?"
    },
    {
        "start": "00:09:38,360",
        "end": "00:09:40,600",
        "en": "Kita?",
        "pa": "What? Us?",
        "vi": "Gì cơ? Bọn tôi à?"
    },
    {
        "start": "00:09:41,880",
        "end": "00:09:46,360",
        "en": "Bolehkah?",
        "pa": "Can you? Please?",
        "vi": "Được không? Làm ơn nhé?"
    },
    {
        "start": "00:09:47,440",
        "end": "00:09:49,680",
        "en": "Bolehlah!",
        "pa": "Please, please!",
        "vi": "Đi mà ông!"
    },
    {
        "start": "00:09:50,480",
        "end": "00:09:52,920",
        "en": "- Ya, boleh. - Hore!",
        "pa": "Okay, all right.",
        "vi": "Được rồi."
    },
    {
        "start": "00:09:54,520",
        "end": "00:09:58,200",
        "en": "- Tapi, ada syaratnya. - Apa dia, Aki?",
        "pa": "-But there are conditions. -What are they, Aki?",
        "vi": "- Nhưng phải có điều kiện. - Điều kiện gì vậy ông?"
    },
    {
        "start": "00:09:58,280",
        "end": "00:10:01,080",
        "en": "Nantilah Aki cerita. Sabar.",
        "pa": "Be patient. I was just getting to that.",
        "vi": "Kiên nhẫn nào. Ông sắp kể đến đoạn đó rồi."
    },
    {
        "start": "00:10:01,160",
        "end": "00:10:07,440",
        "en": "Aris tahu. Syaratnya, mesti Itik dan Itok suruh Kura-kura buang cangkerangnya.",
        "pa": "I know! I bet Itik and Itok tell Kura-kura to get rid of his shell.",
        "vi": "Cháu biết rồi! Cháu cá là Itik và Itok sẽ bảo Kura-kura vứt cái mai đi."
    },
    {
        "start": "00:10:07,520",
        "end": "00:10:08,520",
        "en": "Betul tak?",
        "pa": "Am I right?",
        "vi": "Đúng không ạ?"
    },
    {
        "start": "00:10:09,680",
        "end": "00:10:11,280",
        "en": "Kenapa?",
        "pa": "But, why?",
        "vi": "Nhưng tại sao chứ?"
    },
    {
        "start": "00:10:11,360",
        "end": "00:10:14,000",
        "en": "Sebab nak ganti dengan sayap.",
        "pa": "To exchange it for wings!",
        "vi": "Để đổi lấy đôi cánh!"
    },
    {
        "start": "00:10:16,200",
        "end": "00:10:19,840",
        "en": "Baru boleh terbang. Begitu, Aki?",
        "pa": "Oh, then he can fly!\n Is that right, Aki?",
        "vi": "Vậy thì nó có thể bay rồi!\n Đúng không ông?"
    },
    {
        "start": "00:10:21,200",
        "end": "00:10:25,640",
        "en": "Mestilah tak. Dengar sini.",
        "pa": "Of course not!",
        "vi": "Dĩ nhiên là không rồi!"
    },
    {
        "start": "00:10:25,720",
        "end": "00:10:28,160",
        "en": "Lepas itu…",
        "pa": "Now, listen. After that…",
        "vi": "Giờ thì nghe này. Sau đó…"
    },
    {
        "start": "00:10:30,920",
        "end": "00:10:35,600",
        "en": "Itik, Itok. Bolehkah? Bolehlah.",
        "pa": "Itik, Itok. Can you? Please?",
        "vi": "Itik, Itok. Được không? Làm ơn nhé?"
    },
    {
        "start": "00:10:36,240",
        "end": "00:10:38,320",
        "en": "Tak boleh. Dia berat.",
        "pa": "We can't. He's too heavy!",
        "vi": "Không được. Anh ta nặng quá!"
    },
    {
        "start": "00:10:38,400",
        "end": "00:10:42,080",
        "en": "Lagipun dia tak ada kepak. Macam mana nak terbang?",
        "pa": "And he doesn't have wings!\n How can he fly?",
        "vi": "Anh ta cũng không có cánh!\n Vậy thì bay thế nào được?"
    },
    {
        "start": "00:10:56,880",
        "end": "00:11:01,240",
        "en": "Aku ada caranya. Kau berdua kena tolong.",
        "pa": "I've got it!\n You two must help.",
        "vi": "Tôi biết rồi!\n Hai cậu phải giúp đấy."
    },
    {
        "start": "00:11:01,320",
        "end": "00:11:02,440",
        "en": "- Boleh? - Tak boleh!",
        "pa": "-Okay! -No!",
        "vi": "- Được rồi! - Không!"
    },
    {
        "start": "00:11:04,920",
        "end": "00:11:09,120",
        "en": "Boleh. Mestilah kami boleh tolong.",
        "pa": "I mean, sure! Of course we can help.",
        "vi": "Ý tôi là được chứ! Dĩ nhiên chúng tôi có thể giúp."
    },
    {
        "start": "00:11:15,320",
        "end": "00:11:17,920",
        "en": "Bila nak terbang? Aku dah tak sabar.",
        "pa": "When do we start? I can't wait.",
        "vi": "Khi nào thì mình bắt đầu? Tôi sốt ruột quá."
    },
    {
        "start": "00:11:18,000",
        "end": "00:11:23,960",
        "en": "Dah malam!",
        "pa": "Sunset!\n Sunset!",
        "vi": "Hoàng hôn!"
    },
    {
        "start": "00:11:25,560",
        "end": "00:11:28,800",
        "en": "Dengar tak? Dah malam. Esok sajalah.",
        "pa": "See? It's getting dark. We'll start tomorrow.",
        "vi": "Thấy chưa? Trời sắp tối rồi. Mai chúng ta sẽ bắt đầu."
    },
    {
        "start": "00:11:28,880",
        "end": "00:11:30,720",
        "en": "Jumpa esok.",
        "pa": "See you then!",
        "vi": "Hẹn mai gặp!"
    },
    {
        "start": "00:11:33,920",
        "end": "00:11:37,600",
        "en": "Yalah. Esok pun esoklah.",
        "pa": "Okay, tomorrow then.",
        "vi": "Được rồi, mai thì mai."
    },
    {
        "start": "00:11:37,680",
        "end": "00:11:42,680",
        "en": "Janji dapat terbang",
        "pa": "As long as I get to fly",
        "vi": "Miễn tôi có thể bay là được"
    },
    {
        "start": "00:11:46,040",
        "end": "00:11:50,240",
        "en": "Tak Sabarnya. Lambat pula nak siang.",
        "pa": "I can't wait! Tomorrow's taking so long.",
        "vi": "Nôn quá đi! Sao ngày mai lâu đến quá đi."
    },
    {
        "start": "00:11:51,000",
        "end": "00:11:56,200",
        "en": "Terbang!",
        "pa": "Fly!",
        "vi": "Bay!"
    },
    {
        "start": "00:11:59,880",
        "end": "00:12:00,880",
        "en": "Satu!",
        "pa": "One…",
        "vi": "Một…"
    },
    {
        "start": "00:12:02,720",
        "end": "00:12:04,680",
        "en": "Dua!",
        "pa": "Two…",
        "vi": "Hai…"
    },
    {
        "start": "00:12:04,760",
        "end": "00:12:06,800",
        "en": "Tiga!",
        "pa": "Three!",
        "vi": "Ba!"
    },
    {
        "start": "00:12:39,920",
        "end": "00:12:44,200",
        "en": "Hari ini aku akan melihat dunia.",
        "pa": "I will see the world today!",
        "vi": "Hôm nay mình sẽ được ngắm thế giới!"
    },
    {
        "start": "00:12:51,120",
        "end": "00:12:53,480",
        "en": "- Semua dah ada? - Hai!",
        "pa": "-Everyone ready? -Hi!",
        "vi": "- Mọi người sẵn sàng chưa? - Xin chào!"
    },
    {
        "start": "00:12:56,240",
        "end": "00:12:58,160",
        "en": "Aku dah tak sabar.",
        "pa": "I can't wait!",
        "vi": "Tôi rất mong chờ đấy!"
    },
    {
        "start": "00:12:59,200",
        "end": "00:13:03,120",
        "en": "- Jom! - Kura-kura, tunggulah sekejap.",
        "pa": "-Come on! -Be patient, Kura-kura.",
        "vi": "- Nào! - Kiên nhẫn đi, Kura-kura."
    },
    {
        "start": "00:13:03,200",
        "end": "00:13:06,640",
        "en": "- Mana kepak aku? - Apa? Kepak?",
        "pa": "-Where are my wings? -Wings?",
        "vi": "- Cánh của tôi đâu? - Cánh à?"
    },
    {
        "start": "00:13:06,720",
        "end": "00:13:09,560",
        "en": "Yalah. Kalau tak…",
        "pa": "Yeah, how do you think…",
        "vi": "Phải rồi, thế các cậu nghĩ…"
    },
    {
        "start": "00:13:09,640",
        "end": "00:13:14,080",
        "en": "Macam mana aku nak terbang",
        "pa": "I'm going to fly?",
        "vi": "Tôi sẽ bay thế nào đây?"
    },
    {
        "start": "00:13:14,680",
        "end": "00:13:17,680",
        "en": "Betul juga. Kancil, macam mana…",
        "pa": "Oh, yeah. How, Kancil?",
        "vi": "Phải rồi. Bằng cách nào đây, Kancil?"
    },
    {
        "start": "00:13:19,040",
        "end": "00:13:20,520",
        "en": "Tengok sini…",
        "pa": "Here…",
        "vi": "Đây…"
    },
    {
        "start": "00:13:20,600",
        "end": "00:13:23,120",
        "en": "- Ini dia. - Kayu?",
        "pa": "-This is it! -A stick?",
        "vi": "- Nó đây! - Một khúc cây à?"
    },
    {
        "start": "00:13:23,200",
        "end": "00:13:27,600",
        "en": "Ya. Kura-kura, kau gigit di tengah kayu ini.",
        "pa": "Yeah. Kura-kura, you bite the stick.",
        "vi": "Phải. Kuru-kura, anh hãy ngậm khúc cây này."
    },
    {
        "start": "00:13:27,680",
        "end": "00:13:31,720",
        "en": "Itik dan Itok akan membawa kau jauh ke atas langit.",
        "pa": "Itik and Itok will bring you up in the sky!",
        "vi": "Itik và Itok sẽ nâng anh lên trời!"
    },
    {
        "start": "00:13:31,800",
        "end": "00:13:34,640",
        "en": "Barulah kau boleh melihat dunia.",
        "pa": "Then you can see the world.",
        "vi": "Rồi anh sẽ được thấy thế giới."
    },
    {
        "start": "00:13:35,800",
        "end": "00:13:38,280",
        "en": "Kura-kura, kau buat apa?",
        "pa": "What are you doing, Kura-kura?",
        "vi": "Anh làm gì thế, Kura-kura?"
    },
    {
        "start": "00:13:39,480",
        "end": "00:13:42,480",
        "en": "Cepatlah Itik, Itok. Aku dah tak sabar.",
        "pa": "Come on, Itik and Itok. I can't wait!",
        "vi": "Đi nào, Itik, Itok. Tôi sốt ruột lắm rồi!"
    },
    {
        "start": "00:13:43,480",
        "end": "00:13:47,720",
        "en": "Nanti. Ada syaratnya. Kau kena dengar baik-baik.",
        "pa": "Just a minute. There are conditions.\n Listen carefully.",
        "vi": "Đợi một chút. Có điều kiện đấy.\n Nghe cho kĩ nhé."
    },
    {
        "start": "00:13:49,560",
        "end": "00:13:52,760",
        "en": "Yalah. Apa dia? Aku dengar.",
        "pa": "Fine. What is it? I'm all ears.",
        "vi": "Được. Điều kiện là gì? Tôi đang nghe đây."
    },
    {
        "start": "00:13:52,840",
        "end": "00:13:55,800",
        "en": "Kau jangan buka mulut. Nanti menyesal.",
        "pa": "Don't ever open your mouth. You'll regret it.",
        "vi": "Đừng bao giờ há miệng ra. Không thì anh sẽ hối hận đấy."
    },
    {
        "start": "00:13:55,880",
        "end": "00:13:57,600",
        "en": "Gigit kayu itu kuat-kuat.",
        "pa": "Bite down hard on that stick.",
        "vi": "Cắn khúc cây thật chặt vào."
    },
    {
        "start": "00:13:57,680",
        "end": "00:13:59,520",
        "en": "- Kalau tak… - Ya, aku faham.",
        "pa": "-If not… -Yeah. I get it. I'll remember.",
        "vi": "- Nếu không… - Phải. Tôi biết rồi. Tôi nhớ mà."
    },
    {
        "start": "00:13:59,600",
        "end": "00:14:02,680",
        "en": "Aku akan ingat pesanan kau. Terima kasih, Kancil.",
        "pa": "Thanks, Kancil.",
        "vi": "Cảm ơn Kancil."
    },
    {
        "start": "00:14:05,080",
        "end": "00:14:08,720",
        "en": "- Sedia? - Sedia!",
        "pa": "-Ready? -Ready!",
        "vi": "- Sẵn sàng chưa? - Sẵn sàng!"
    },
    {
        "start": "00:14:23,320",
        "end": "00:14:25,320",
        "en": "Hore!",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:14:29,160",
        "end": "00:14:33,160",
        "en": "Aku terbang!",
        "pa": "I'm flying!",
        "vi": "Mình đang bay!"
    },
    {
        "start": "00:14:35,360",
        "end": "00:14:37,480",
        "en": "Arnab, tengok aku!",
        "pa": "-Look I'm flying! -Look at him go.",
        "vi": "- Nhìn này, tôi đang bay! - Nhìn anh ta kìa."
    },
    {
        "start": "00:14:37,560",
        "end": "00:14:39,640",
        "en": "Dapat juga dia terbang.",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:15:04,440",
        "end": "00:15:05,640",
        "en": "Kura-kura!",
        "pa": "Kura-kura! Is that you?",
        "vi": "Kura-kura! Là anh đấy ư?"
    },
    {
        "start": "00:15:06,800",
        "end": "00:15:08,200",
        "en": "- Itu kau? - Ya.",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:15:09,520",
        "end": "00:15:10,960",
        "en": "Yalah.",
        "pa": "Yeah, I'm…",
        "vi": "Phải, tôi…"
    },
    {
        "start": "00:15:11,520",
        "end": "00:15:12,720",
        "en": "Kau terbang?",
        "pa": "You're flying? That's awesome!",
        "vi": "Anh đang bay? Thật tuyệt vời!"
    },
    {
        "start": "00:15:13,360",
        "end": "00:15:18,560",
        "en": "Wah! Hebat. Mesti kau raja, Kura-kura.",
        "pa": "You're the King of the Tortoises!",
        "vi": "Giờ anh là Vua Rùa Cạn rồi!"
    },
    {
        "start": "00:15:20,800",
        "end": "00:15:26,800",
        "en": "Itu betul. Akulah raja!",
        "pa": "Yeah! I'm the King…",
        "vi": "Phải! Tôi là Vua…"
    },
    {
        "start": "00:15:29,640",
        "end": "00:15:30,960",
        "en": "Alamak!",
        "pa": "Oh no…",
        "vi": "Ôi không…"
    },
    {
        "start": "00:15:36,280",
        "end": "00:15:39,920",
        "en": "- Kura-kura! - Sabar Kura-kura! Sabar!",
        "pa": "Be patient, Kura-kura!",
        "vi": "Kura-kura!"
    },
    {
        "start": "00:15:40,680",
        "end": "00:15:44,360",
        "en": "Tunggu, Kura-kura!",
        "pa": "Wait! Kura-kura!",
        "vi": "Đợi đã! Kura-kura!"
    },
    {
        "start": "00:15:45,480",
        "end": "00:15:48,560",
        "en": "Tunggu, Kura-kura. Aku datang!",
        "pa": "Hurry!\n Kura-kura! I'm coming!",
        "vi": "Kura-kura! Tôi đến đây!"
    },
    {
        "start": "00:15:49,320",
        "end": "00:15:51,480",
        "en": "Itok!",
        "pa": "Itok, hurry!",
        "vi": "Itok!"
    },
    {
        "start": "00:15:55,480",
        "end": "00:15:57,200",
        "en": "Aduh!",
        "pa": "Ouch!",
        "vi": "Ui da!"
    },
    {
        "start": "00:16:11,200",
        "end": "00:16:12,320",
        "en": "Itu apa?",
        "pa": "What's that?",
        "vi": "Cái gì thế kia?"
    },
    {
        "start": "00:16:14,840",
        "end": "00:16:16,360",
        "en": "Kancil, tolong!",
        "pa": "Kancil! Help!",
        "vi": "Kancil! Cứu tôi!"
    },
    {
        "start": "00:16:16,440",
        "end": "00:16:18,640",
        "en": "- Kancil! - Kura-kura!",
        "pa": "-Kancil! -Oh, Kura-kura…",
        "vi": "- Kancil! - Ôi, Kura-kura…"
    },
    {
        "start": "00:16:18,720",
        "end": "00:16:19,800",
        "en": "Kura-kura!",
        "pa": "",
        "vi": "Kura-kura?"
    },
    {
        "start": "00:16:19,880",
        "end": "00:16:22,840",
        "en": "Aku di sini. Aku akan tangkap kau.",
        "pa": "Over here! I got you! Here!",
        "vi": "Kura-kura! Bên này! Tôi đón được anh rồi! Đây!"
    },
    {
        "start": "00:16:22,920",
        "end": "00:16:25,400",
        "en": "Sini!",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:16:28,160",
        "end": "00:16:30,120",
        "en": "Kura-kura!",
        "pa": "",
        "vi": "Kura-kura!"
    },
    {
        "start": "00:16:31,040",
        "end": "00:16:33,200",
        "en": "Kura-kura, bangun!",
        "pa": "Wake up, Kura-kura!",
        "vi": "Tỉnh lại đi, Kura-kura!"
    },
    {
        "start": "00:16:35,320",
        "end": "00:16:38,480",
        "en": "Kura-kura, bangunlah.",
        "pa": "Please, wake up…",
        "vi": "Làm ơn tỉnh lại đi…"
    },
    {
        "start": "00:16:40,680",
        "end": "00:16:42,800",
        "en": "Kura-kura!",
        "pa": "",
        "vi": "Kura-kura!"
    },
    {
        "start": "00:16:42,880",
        "end": "00:16:45,000",
        "en": "Selamat tak? Macam mana dengan dia?",
        "pa": "-How is he? Is he okay? -What do we do, Kancil?",
        "vi": "- Anh ta sao rồi? Không sao chứ? - Chúng ta làm gì đây, Kancil?"
    },
    {
        "start": "00:16:45,080",
        "end": "00:16:46,200",
        "en": "Macam mana, Kancil?",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:16:46,280",
        "end": "00:16:47,960",
        "en": "Apa kita nak buat?",
        "pa": "What should we do?",
        "vi": "Chúng ta phải làm gì đây?"
    },
    {
        "start": "00:16:48,040",
        "end": "00:16:52,200",
        "en": "Bangunlah, Kura-kura.",
        "pa": "Kura-kura, wake up…",
        "vi": "Kura-kura, tỉnh lại đi…"
    },
    {
        "start": "00:16:56,600",
        "end": "00:16:59,280",
        "en": "- Kura-kura! - Kau selamat!",
        "pa": "You're alive!",
        "vi": "- Kura-kura! - Anh còn sống!"
    },
    {
        "start": "00:17:03,240",
        "end": "00:17:06,720",
        "en": "- Kura-kura! - Kenapa?",
        "pa": "",
        "vi": "- Kura-kura… - Cái gì thế này?"
    },
    {
        "start": "00:17:06,800",
        "end": "00:17:08,680",
        "en": "Cangkerang aku.",
        "pa": "What is it?\n My shell!",
        "vi": "Mai của tôi!"
    },
    {
        "start": "00:17:08,760",
        "end": "00:17:12,640",
        "en": "Dah retak!",
        "pa": "It's cracked!",
        "vi": "Bị nứt rồi!"
    },
    {
        "start": "00:17:14,280",
        "end": "00:17:20,119",
        "en": "Nasib baik ada cangkerang. Kalau tak, tak tahulah apa jadi.",
        "pa": "Good thing you had your shell on.\n If you hadn't, who knows what would have happened to you?",
        "vi": "May mà anh có cái mai. Nếu không thì…\n Ai biết anh sẽ xảy ra chuyện gì chứ."
    },
    {
        "start": "00:17:22,200",
        "end": "00:17:26,359",
        "en": "Itulah. Aku dah pesan, jangan buka mulut.",
        "pa": "See? I told you not to open your mouth.",
        "vi": "Thấy chưa, tôi đã bảo anh đừng mở miệng ra mà."
    },
    {
        "start": "00:17:26,440",
        "end": "00:17:31,440",
        "en": "Ya. Aku yang salah. Tak dengar cakap kau.",
        "pa": "I know, I was wrong.",
        "vi": "Tôi biết, tôi sai rồi."
    },
    {
        "start": "00:17:31,520",
        "end": "00:17:34,680",
        "en": "Aku menyesal.",
        "pa": "I didn't listen. I'm sorry…",
        "vi": "Tôi đã không nghe lời cậu. Xin lỗi…"
    },
    {
        "start": "00:17:36,320",
        "end": "00:17:40,080",
        "en": "Tak apa, yang penting kau selamat.",
        "pa": "It's okay. As long as you're safe.",
        "vi": "Không sao. Miễn là anh vẫn an toàn là được rồi."
    },
    {
        "start": "00:17:44,240",
        "end": "00:17:46,320",
        "en": "- Nak terbang lagi? - Tak nak dah.",
        "pa": "-Want to do it again? -No, thank you.",
        "vi": "- Bay nữa không? - Thôi, cảm ơn."
    },
    {
        "start": "00:17:48,600",
        "end": "00:17:50,600",
        "en": "Cantiknya cangkerang!",
        "pa": "What a pretty shell!",
        "vi": "Cái mai thật đẹp!"
    },
    {
        "start": "00:17:52,280",
        "end": "00:17:57,960",
        "en": "Sebab itulah cangkerang Kura-kura retak dan tak dapat dibetulkan lagi.",
        "pa": "That's why tortoises have cracked shells!",
        "vi": "Đó là lí do vì sao mai rùa có những vết nứt!"
    },
    {
        "start": "00:17:58,040",
        "end": "00:18:01,920",
        "en": "- Habis cerita. - Belum lagi, Aki.",
        "pa": "It can't be undone ever again.\n -The end… -Not yet, Aki!",
        "vi": "Không thể nào giống trước kia được nữa.\n - Hết chuyện… - Vẫn chưa, ông à!"
    },
    {
        "start": "00:18:02,680",
        "end": "00:18:03,760",
        "en": "Apa lagi?",
        "pa": "What now?",
        "vi": "Còn gì nữa?"
    },
    {
        "start": "00:18:03,840",
        "end": "00:18:07,560",
        "en": "Nasi jadi bubur itu apa?",
        "pa": "You didn't say anything about the spilled milk!",
        "vi": "Ông không có nhắc gì đến sữa bị đổ cả!"
    },
    {
        "start": "00:18:09,440",
        "end": "00:18:11,080",
        "en": "Lupa!",
        "pa": "Oh, I forgot!",
        "vi": "Ông quên mất!"
    },
    {
        "start": "00:18:11,840",
        "end": "00:18:15,200",
        "en": "Nasi sudah menjadi bubur itu maksudnya,",
        "pa": "It's no use crying over spilled milk.",
        "vi": "Sữa đổ hết rồi ngồi khóc cũng như không. Câu đó nghĩa là…"
    },
    {
        "start": "00:18:15,840",
        "end": "00:18:19,760",
        "en": "sesuatu yang dah rosak. Tak boleh dibaiki lagi.",
        "pa": "That means\n something that is broken beyond repair.",
        "vi": "Một thứ đã hỏng đến mức không thể sửa được nữa."
    },
    {
        "start": "00:18:21,280",
        "end": "00:18:25,000",
        "en": "- Macam cangkerang Kura-kura? - Ya.",
        "pa": "Oh, like Kura-kura's shell?",
        "vi": "Giống như mai của Kura-kura ạ?"
    },
    {
        "start": "00:18:25,080",
        "end": "00:18:28,840",
        "en": "Dan macam pasu bunga Aki ni. Tak boleh gam-gam.",
        "pa": "Yes. And like my vase here.",
        "vi": "Ừ. Giống cái bình của ông nữa. Không thể sửa được."
    },
    {
        "start": "00:18:28,920",
        "end": "00:18:30,320",
        "en": "Faham?",
        "pa": "You can't fix it.\n Understand?",
        "vi": "Cháu hiểu chưa?"
    },
    {
        "start": "00:18:31,160",
        "end": "00:18:33,520",
        "en": "- Ara, tengok sini. - Apa?",
        "pa": "-Ara, look at this! -What?",
        "vi": "- Ara, xem này! - Gì cơ?"
    },
    {
        "start": "00:18:33,600",
        "end": "00:18:37,920",
        "en": "- Ini kura-kura, ini penyu. - Sama saja.",
        "pa": "This is a tortoise, and this is a turtle.\n They look the same.",
        "vi": "Đây là rùa cạn, còn đây là rùa nước.\n Trông như nhau cả mà."
    },
    {
        "start": "00:18:40,080",
        "end": "00:18:43,600",
        "en": "Aki, kenapa kura-kura dengan penyu sama?",
        "pa": "Aki, why do they look the same?",
        "vi": "Ông à, sao chúng giống nhau thế ạ?"
    },
    {
        "start": "00:18:43,680",
        "end": "00:18:47,040",
        "en": "Mana ada sama. Cuba tengok kaki ia.",
        "pa": "No, they're not. Look at their feet.",
        "vi": "Không đâu. Cháu nhìn chân của chúng đi."
    },
    {
        "start": "00:18:47,960",
        "end": "00:18:51,680",
        "en": "- Yalah! Beza. - Mana ada sama.",
        "pa": "-Oh, yeah! They're different! -You're so smart, Aki!",
        "vi": "- Đúng rồi! Không giống nhau! - Ông thông minh quá, ông à!"
    },
    {
        "start": "00:18:51,760",
        "end": "00:18:53,880",
        "en": "- Pandai Aki. - Tengok sini.",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:18:53,960",
        "end": "00:18:57,320",
        "en": "Kaki kura-kura macam itu. Penyu begini.",
        "pa": "Tortoises look like this, and turtles look like that.",
        "vi": "Rùa cạn trông thế này, còn rùa nước trông giống thế kia."
    },
    {
        "start": "00:18:57,400",
        "end": "00:19:01,160",
        "en": "- Apalah Ara? - Ara pula. Abanglah yang salah.",
        "pa": "Don't you know that, Ara?\n -Me? You're the one who-- -That's enough! Go to bed.",
        "vi": "Ara…\n - Em à? Anh mới là người… - Đủ rồi! Đi ngủ nào."
    },
    {
        "start": "00:19:01,240",
        "end": "00:19:03,400",
        "en": "Dah, jangan bising- bising. Tidur.",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:19:03,480",
        "end": "00:19:06,200",
        "en": "Aki…",
        "pa": "-Aw, Aki… -Aw, Aki…",
        "vi": "Ông à…"
    },
    {
        "start": "00:19:22,080",
        "end": "00:19:24,280",
        "en": "",
        "pa": "",
        "vi": "Biên dịch: Hoàng Nguyên"
    }
]