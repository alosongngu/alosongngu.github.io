[
    {
        "start": "00:00:21,080",
        "end": "00:00:24,120",
        "en": "Pada zaman dahulu…",
        "pa": "Once upon a time…",
        "vi": "Ngày xửa ngày xưa…"
    },
    {
        "start": "00:00:40,440",
        "end": "00:00:41,720",
        "en": "Siap!",
        "pa": "Done!",
        "vi": "Xong!"
    },
    {
        "start": "00:00:43,440",
        "end": "00:00:45,160",
        "en": "Sejuknya.",
        "pa": "It's cool.",
        "vi": "Mát quá."
    },
    {
        "start": "00:00:47,160",
        "end": "00:00:49,240",
        "en": "Belum lagi.",
        "pa": "Not yet…",
        "vi": "Chưa xong đâu…"
    },
    {
        "start": "00:00:51,080",
        "end": "00:00:52,760",
        "en": "Aki nak pergi mana?",
        "pa": "Where is Aki going?",
        "vi": "Ông đi đâu thế nhỉ?"
    },
    {
        "start": "00:00:58,680",
        "end": "00:01:00,200",
        "en": "Itu apa?",
        "pa": "What's that?",
        "vi": "Đó là cái gì vậy ạ?"
    },
    {
        "start": "00:01:01,800",
        "end": "00:01:04,840",
        "en": "Mesin kisar padi!",
        "pa": "Paddy thresher machine!",
        "vi": "Máy tuốt lúa!\n BRỪM!"
    },
    {
        "start": "00:01:16,640",
        "end": "00:01:18,280",
        "en": "- Nak buat! - Ara pun!",
        "pa": "-Let me do it! -Me too!",
        "vi": "- Để cháu làm cho! - Cháu nữa!"
    },
    {
        "start": "00:01:18,360",
        "end": "00:01:20,600",
        "en": "Tak boleh, bahaya.",
        "pa": "You can't, it's dangerous!",
        "vi": "Không được, nguy hiểm lắm!"
    },
    {
        "start": "00:01:20,680",
        "end": "00:01:24,000",
        "en": "Biar Aki saja yang buat. Kau orang rehat dan tengok.",
        "pa": "Let me do it. Both of you, just rest and watch.\n So disappointing.",
        "vi": "Để ông làm cho. Hai cháu ngồi nghỉ và xem thôi.\n - Cụt hứng thật. - Cụt hứng thật."
    },
    {
        "start": "00:01:37,160",
        "end": "00:01:41,240",
        "en": "Aki! Dah penuh! Nanti tumpah.",
        "pa": "Aki, it's full!",
        "vi": "Ông à, đầy rồi!"
    },
    {
        "start": "00:01:41,320",
        "end": "00:01:44,400",
        "en": "Aris! Ambil guni itu dan letak di sini.",
        "pa": "It will spill.\n Aris, take that gunny sack. Put it here.",
        "vi": "Nó sẽ tràn đấy.\n Aris, lấy cái bao bố kia. Đặt nó ở đây."
    },
    {
        "start": "00:01:48,640",
        "end": "00:01:49,680",
        "en": "Beratnya.",
        "pa": "This is heavy.",
        "vi": "Nặng quá."
    },
    {
        "start": "00:01:50,320",
        "end": "00:01:51,720",
        "en": "Hati-hati Aki.",
        "pa": "Careful Aki.",
        "vi": "Cẩn thận đấy ông."
    },
    {
        "start": "00:02:04,960",
        "end": "00:02:08,199",
        "en": "Aris! Jangan main, bahaya.",
        "pa": "Aris! Stop playing! It's dangerous!",
        "vi": "Aris! Đừng nghịch nữa! Nguy hiểm đấy!"
    },
    {
        "start": "00:02:17,040",
        "end": "00:02:19,520",
        "en": "Abang, Ara pun nak buat.",
        "pa": "Aris, I want to do it too.",
        "vi": "Anh à, em cũng muốn làm nữa."
    },
    {
        "start": "00:02:29,240",
        "end": "00:02:32,240",
        "en": "Bukankah Aki dah pesan jangan main, rosak nanti.",
        "pa": "Didn't I tell you not to play with it? You could break it.",
        "vi": "Không phải ông đã bảo đừng nghịch à? Cháu có thể làm hỏng nó đấy."
    },
    {
        "start": "00:02:47,600",
        "end": "00:02:48,800",
        "en": "Aki!",
        "pa": "Aki!",
        "vi": "Ông!"
    },
    {
        "start": "00:02:49,600",
        "end": "00:02:51,560",
        "en": "Degil lagi! Dah rosak",
        "pa": "So stubborn! Now it's broken!",
        "vi": "Thật là cứng đầu! Giờ nó hỏng rồi!"
    },
    {
        "start": "00:02:51,640",
        "end": "00:02:53,480",
        "en": "- Ara yang buat. - Apa?",
        "pa": "Ara did it!",
        "vi": "Ara làm đấy ạ!"
    },
    {
        "start": "00:02:55,080",
        "end": "00:02:58,800",
        "en": "Apa pula! Ara tak buat apa-apa pun.",
        "pa": "I did not! I didn't do anything!",
        "vi": "Cháu không có! Cháu không làm gì cả!"
    },
    {
        "start": "00:02:58,880",
        "end": "00:03:02,480",
        "en": "Dah! Dua-dua degil. Kepala batu!",
        "pa": "Enough! Both of you are stubborn. Bullheaded.",
        "vi": "Đủ rồi! Cả hai đứa đều cứng đầu. Ngoan cố."
    },
    {
        "start": "00:03:02,560",
        "end": "00:03:04,000",
        "en": "Mesin itu bahaya.",
        "pa": "That machine is dangerous.",
        "vi": "Cái máy đó rất nguy hiểm."
    },
    {
        "start": "00:03:04,080",
        "end": "00:03:07,720",
        "en": "Kalau jadi apa-apa, Aki juga yang susah!",
        "pa": "What if something happens? I'll be in trouble!",
        "vi": "Nếu lỡ có chuyện gì xảy ra thì sao? Ông sẽ gặp rắc rối đấy!"
    },
    {
        "start": "00:03:07,800",
        "end": "00:03:09,920",
        "en": "- Minta maaf, Aki. - Minta maaf, Aki.",
        "pa": "I'm sorry, Aki.",
        "vi": "- Cháu xin lỗi ông. - Cháu xin lỗi ông."
    },
    {
        "start": "00:03:10,000",
        "end": "00:03:12,240",
        "en": "Kita orang janji tak buat lagi.",
        "pa": "We promise we won't do it again.",
        "vi": "Chúng cháu hứa sẽ không làm vậy nữa."
    },
    {
        "start": "00:03:14,320",
        "end": "00:03:18,920",
        "en": "Perangai kau berdua ini, teringat Aki cerita monyet.",
        "pa": "Both of your behavior reminds me of a Monyet story.",
        "vi": "Cách cư xử của hai cháu làm ông nhớ đến câu chuyện về Monyet."
    },
    {
        "start": "00:03:19,000",
        "end": "00:03:22,280",
        "en": "Cerita monyet? Nak dengar!",
        "pa": "A Monyet story? Tell us!",
        "vi": "Chuyện về Monyet? Kể cho chúng cháu nghe đi ông!"
    },
    {
        "start": "00:03:25,280",
        "end": "00:03:27,920",
        "en": "Tak boleh, nanti kerja tak siap.",
        "pa": "Not now, or we won't finish the work.",
        "vi": "Không phải bây giờ, không thì sẽ không xong việc đâu."
    },
    {
        "start": "00:03:28,720",
        "end": "00:03:31,360",
        "en": "Nanti kita buatlah sama-sama.",
        "pa": "We can do it together later.",
        "vi": "Mình cùng làm sau được mà ông."
    },
    {
        "start": "00:03:52,760",
        "end": "00:03:56,280",
        "en": "Pada zaman dahulu…",
        "pa": "Once upon a time…",
        "vi": "Ngày xửa ngày xưa…"
    },
    {
        "start": "00:04:22,520",
        "end": "00:04:26,080",
        "en": "Aduh!",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:04:31,840",
        "end": "00:04:34,960",
        "en": "Kancil, kau okey?",
        "pa": "Kancil, are you alright?",
        "vi": "Kancil, cậu không sao chứ?"
    },
    {
        "start": "00:04:35,600",
        "end": "00:04:36,840",
        "en": "Tak.",
        "pa": "I'm alright.",
        "vi": "Tôi không sao."
    },
    {
        "start": "00:04:38,000",
        "end": "00:04:39,920",
        "en": "Nasib baik.",
        "pa": "Thank goodness!",
        "vi": "Cảm ơn trời đất!"
    },
    {
        "start": "00:04:40,000",
        "end": "00:04:45,040",
        "en": "Kalau begitu, mari sini tolong aku. Ada kerja sikit.",
        "pa": "In that case, come here and help me.\n I have some things to do.",
        "vi": "Nếu vậy,\n đến đây và giúp tôi với. Tôi có việc cần làm."
    },
    {
        "start": "00:05:02,760",
        "end": "00:05:07,640",
        "en": "Senang hati aku Dah banyak makanan kita",
        "pa": "I feel relieved Now we have plenty of food",
        "vi": "Tôi thấy thật nhẹ nhõm. Giờ chúng ta có rất nhiều thức ăn."
    },
    {
        "start": "00:05:07,720",
        "end": "00:05:11,680",
        "en": "Yalah, tak payah susah payah cari makanan lagi.",
        "pa": "That's true. We don't have to look for food anymore.",
        "vi": "Đúng vậy. Chúng ta không cần đi tìm thức ăn nữa."
    },
    {
        "start": "00:05:11,760",
        "end": "00:05:13,240",
        "en": "Jom Kura-kura.",
        "pa": "Come, Kura-kura.",
        "vi": "Đến đây, Kura-kura."
    },
    {
        "start": "00:05:20,480",
        "end": "00:05:24,960",
        "en": "Aku dah lapar. Marilah kita makan sama-sama.",
        "pa": "I'm hungry. Let's eat together.",
        "vi": "Tôi đói rồi. Cùng nhau ăn đi."
    },
    {
        "start": "00:05:30,400",
        "end": "00:05:32,760",
        "en": "Ranumnya buah-buahan di sini!",
        "pa": "The fruit looks tasty here!",
        "vi": "Trái cây ở đây trông ngon quá!"
    },
    {
        "start": "00:05:39,640",
        "end": "00:05:41,520",
        "en": "Banyaknya Kura-kura.",
        "pa": "It's delicious, Kura-kura.",
        "vi": "Ngon thật, Kura-kura."
    },
    {
        "start": "00:05:46,600",
        "end": "00:05:50,080",
        "en": "Kancil dan Kura-kura rupanya.",
        "pa": "It's Kancil and Kura-kura.",
        "vi": "Là Kancil và Kura-kura."
    },
    {
        "start": "00:05:52,560",
        "end": "00:05:54,600",
        "en": "Inilah peluang aku.",
        "pa": "This is my chance.",
        "vi": "Cơ hội của mình đây rồi."
    },
    {
        "start": "00:05:56,880",
        "end": "00:06:00,680",
        "en": "Tolong!",
        "pa": "Help!",
        "vi": "Cứu tôi với!"
    },
    {
        "start": "00:06:02,120",
        "end": "00:06:06,080",
        "en": "Tolong! Aku lapar sangat.",
        "pa": "Help! I am so hungry.",
        "vi": "Cứu tôi với! Tôi đói quá."
    },
    {
        "start": "00:06:08,520",
        "end": "00:06:11,240",
        "en": "Sudah beberapa hari aku tak makan.",
        "pa": "It has been a few days since I've eaten anything.",
        "vi": "Đã mấy ngày rồi tôi chưa được ăn gì."
    },
    {
        "start": "00:06:11,960",
        "end": "00:06:15,720",
        "en": "Tolonglah berikan aku sedikit makanan.",
        "pa": "Please give me some food.",
        "vi": "Làm ơn cho tôi ít thức ăn đi."
    },
    {
        "start": "00:06:16,440",
        "end": "00:06:19,200",
        "en": "Kasihanilah aku.",
        "pa": "Pity me.",
        "vi": "Xin rủ lòng thương tôi."
    },
    {
        "start": "00:06:21,520",
        "end": "00:06:23,440",
        "en": "Kesian dia.",
        "pa": "Poor thing.",
        "vi": "Thật đáng thương."
    },
    {
        "start": "00:06:23,520",
        "end": "00:06:24,720",
        "en": "Baiklah monyet.",
        "pa": "Alright Monyet, just take whatever you want from our orchard.",
        "vi": "Được rồi Monyet, cứ lấy thứ gì cậu muốn trong vườn của chúng tôi."
    },
    {
        "start": "00:06:24,800",
        "end": "00:06:28,120",
        "en": "Ambillah apa saja yang kau mahu dari kebun kami.",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:06:28,200",
        "end": "00:06:30,000",
        "en": "Makanlah sepuas hati kau.",
        "pa": "Eat until you're full.",
        "vi": "Ăn đến khi nào no thì thôi."
    },
    {
        "start": "00:06:32,160",
        "end": "00:06:35,480",
        "en": "Terima kasih Kancil. Terima kasih Kura-kura.",
        "pa": "Thank you Kancil, thank you Kura-kura.",
        "vi": "Cảm ơn Kancil, cảm ơn Kura-kura."
    },
    {
        "start": "00:07:12,360",
        "end": "00:07:16,120",
        "en": "Jangan gelojoh sangat makan, nanti tercekik.",
        "pa": "Don't eat too fast, or you'll choke.",
        "vi": "Đừng ăn nhanh quá, nghẹn đấy."
    },
    {
        "start": "00:07:21,520",
        "end": "00:07:24,440",
        "en": "Itulah, degil lagi.",
        "pa": "I told you to slow down. You're so stubborn!",
        "vi": "Tôi đã bảo cậu ăn từ từ thôi mà. Cậu thật cứng đầu!"
    },
    {
        "start": "00:07:26,680",
        "end": "00:07:28,240",
        "en": "Nah minum.",
        "pa": "Here, drink it.",
        "vi": "Đây, uống đi."
    },
    {
        "start": "00:07:36,080",
        "end": "00:07:38,800",
        "en": "Aku nak itu!",
        "pa": "I want that!",
        "vi": "Tôi muốn cái đó!"
    },
    {
        "start": "00:07:39,920",
        "end": "00:07:43,600",
        "en": "Aku nak yang itu!",
        "pa": "I want that!",
        "vi": "Tôi muốn cái đó!"
    },
    {
        "start": "00:07:43,680",
        "end": "00:07:46,000",
        "en": "Aku nak yang itu!",
        "pa": "I want that!",
        "vi": "Tôi muốn cái đó!"
    },
    {
        "start": "00:07:47,920",
        "end": "00:07:51,240",
        "en": "Tak boleh Monyet. Kita tak boleh makan buah itu.",
        "pa": "You can't, Monyet! You cannot eat that fruit!",
        "vi": "Không được, Monyet! Cậu không được ăn quả đó!"
    },
    {
        "start": "00:07:51,320",
        "end": "00:07:53,520",
        "en": "Pilihlah buah yang lain.",
        "pa": "Just choose other fruits.",
        "vi": "Hãy chọn những quả khác đi."
    },
    {
        "start": "00:07:57,080",
        "end": "00:07:59,360",
        "en": "Tepi! Aku nak yang itu!",
        "pa": "Move! I want that!",
        "vi": "Tránh ra! Tôi muốn cái đó!"
    },
    {
        "start": "00:08:04,600",
        "end": "00:08:06,760",
        "en": "Aku tak peduli!",
        "pa": "I don't care!",
        "vi": "Tôi không quan tâm!"
    },
    {
        "start": "00:08:06,840",
        "end": "00:08:08,840",
        "en": "Aku nak buah itu juga!",
        "pa": "I want that fruit!",
        "vi": "Tôi muốn quả đó!"
    },
    {
        "start": "00:08:08,920",
        "end": "00:08:11,360",
        "en": "Jangan Monyet! Jangan!",
        "pa": "-I want it! -No, Monyet. No!",
        "vi": "- Không, Monyet. Đừng mà! - Tôi muốn ăn nó!"
    },
    {
        "start": "00:08:12,280",
        "end": "00:08:15,160",
        "en": "Nanti kau yang akan menanggung pedihnya.",
        "pa": "You'll regret it!",
        "vi": "Cậu sẽ hối hận cho xem!"
    },
    {
        "start": "00:08:25,800",
        "end": "00:08:29,720",
        "en": "Ini cantik! Tak, yang ini.",
        "pa": "This looks good. Oh no! No! This one.",
        "vi": "Trông ngon quá."
    },
    {
        "start": "00:08:45,120",
        "end": "00:08:50,640",
        "en": "Jangan!",
        "pa": "No!",
        "vi": "Không!"
    },
    {
        "start": "00:09:02,040",
        "end": "00:09:03,720",
        "en": "Pedas!",
        "pa": "Spicy!",
        "vi": "Cay quá!"
    },
    {
        "start": "00:09:03,800",
        "end": "00:09:06,960",
        "en": "Air! Air mana? Pedas!",
        "pa": "Water! Where is the water? Spic--",
        "vi": "Nước! Nước đâu rồi? Cay…"
    },
    {
        "start": "00:09:21,880",
        "end": "00:09:24,600",
        "en": "Monyet pun terlompat-lompat kepedasan.",
        "pa": "It was so spicy that Monyet started jumping.",
        "vi": "Vì quá cay nên Monyet cứ nhảy tưng tưng."
    },
    {
        "start": "00:09:24,680",
        "end": "00:09:27,560",
        "en": "Kesian Monyet.",
        "pa": "Poor Monyet…",
        "vi": "Monyet đáng thương…"
    },
    {
        "start": "00:09:27,640",
        "end": "00:09:31,960",
        "en": "Buat apa nak kesian, biar Monyet rasa. Dah degil sangat.",
        "pa": "Why do you pity him? Let Monyet be, he was so stubborn.",
        "vi": "Sao lại thương xót nó? Cứ mặc Monyet, nó thật cứng đầu."
    },
    {
        "start": "00:09:32,040",
        "end": "00:09:34,960",
        "en": "Macam kau berdua, degil.",
        "pa": "Just like both of you. Stubborn.",
        "vi": "Giống như hai cháu vậy. Cứng đầu ấy."
    },
    {
        "start": "00:09:35,040",
        "end": "00:09:38,600",
        "en": "Aki cakap jangan main mesin itu. Kau berdua main juga.",
        "pa": "I told you not to play with the machine, but you didn't listen.",
        "vi": "Ông đã bảo đừng nghịch cái máy, nhưng hai đứa không nghe."
    },
    {
        "start": "00:09:38,680",
        "end": "00:09:42,640",
        "en": "Dahlah kau masukkan banyak kedap padi itu.",
        "pa": "You even put lots of paddy into it.",
        "vi": "Cháu còn cho thật nhiều lúa vào nữa."
    },
    {
        "start": "00:09:42,720",
        "end": "00:09:48,480",
        "en": "Sekarang mesin dah rosak, bantinglah kedap padi ini sampai habis. Rasakan.",
        "pa": "Now that the machine is broken, you need to hit the paddy till it's done.",
        "vi": "Giờ cái máy bị hỏng rồi, cháu phải đập lúa khi nào xong thì thôi."
    },
    {
        "start": "00:09:49,680",
        "end": "00:09:51,080",
        "en": "Boleh Aki, boleh.",
        "pa": "Learn your lesson.\n Alright, Aki.",
        "vi": "Hãy nhớ lấy bài học này.\n Vâng ạ."
    },
    {
        "start": "00:09:51,800",
        "end": "00:09:55,320",
        "en": "Aki, apa jadi pada Monyet itu?",
        "pa": "Aki, what happened to Monyet?",
        "vi": "Ông à, thế chuyện gì xảy ra với Monyet?"
    },
    {
        "start": "00:09:55,400",
        "end": "00:10:00,080",
        "en": "Apa lagi, Monyet berguling-guling sambil menjerit-jerit.",
        "pa": "What else, Monyet rolled around screaming.",
        "vi": "Còn gì nữa, Monyet vừa lăn lộn vừa la hét."
    },
    {
        "start": "00:10:18,840",
        "end": "00:10:20,520",
        "en": "Rasakan.",
        "pa": "I told you so.",
        "vi": "Tôi đã bảo cậu rồi."
    },
    {
        "start": "00:10:20,600",
        "end": "00:10:25,640",
        "en": "Beginilah jadinya kalau degil",
        "pa": "This is what happens when you're stubborn",
        "vi": "Đây là hậu quả vì cậu cứng đầu đấy"
    },
    {
        "start": "00:10:25,720",
        "end": "00:10:27,520",
        "en": "Tidak mendengar nasihat.",
        "pa": "you didn't listen to us.",
        "vi": "Cậu không chịu nghe lời bọn tôi."
    },
    {
        "start": "00:10:28,920",
        "end": "00:10:34,040",
        "en": "Kesian, ingat Monyet! Jangan diulang lagi.",
        "pa": "Poor you, remember Monyet. Don't repeat your mistake.",
        "vi": "Tội nghiệp cậu, nhớ đấy Monyet. Đừng lặp lại sai lầm nhé."
    },
    {
        "start": "00:10:36,720",
        "end": "00:10:40,480",
        "en": "Yalah, aku akan ingat sampai bila-bila.",
        "pa": "Alright. I will always remember that.",
        "vi": "Được rồi. Tôi sẽ mãi ghi nhớ điều đó."
    },
    {
        "start": "00:10:40,560",
        "end": "00:10:43,520",
        "en": "Aku tak nak makan buah itu lagi.",
        "pa": "I don't want to eat that fruit anymore.",
        "vi": "Tôi không muốn ăn loại quả đó nữa đâu."
    },
    {
        "start": "00:10:51,520",
        "end": "00:10:53,080",
        "en": "Makan pisang lagi sedap.",
        "pa": "Eating bananas is way better!",
        "vi": "Ăn chuối vẫn ngon hơn!"
    },
    {
        "start": "00:10:53,160",
        "end": "00:10:58,280",
        "en": "Jangan monyet. Pisang itu belum cukup masak.",
        "pa": "Don't, Monyet. Those bananas aren't ripe.",
        "vi": "Đừng, Monyet. Mấy quả chuối đó chưa chín đâu."
    },
    {
        "start": "00:11:01,440",
        "end": "00:11:04,520",
        "en": "Tak nak! Nak makan juga!",
        "pa": "I don't care, I still want to eat them.",
        "vi": "Tôi không quan tâm, tôi vẫn cứ muốn ăn đấy."
    },
    {
        "start": "00:11:10,280",
        "end": "00:11:11,920",
        "en": "Tak sedap!",
        "pa": "Disgusting!",
        "vi": "Dở tệ!"
    },
    {
        "start": "00:11:16,920",
        "end": "00:11:18,280",
        "en": "Tak sedap!",
        "pa": "Disgusting!",
        "vi": "Dở tệ!"
    },
    {
        "start": "00:11:23,320",
        "end": "00:11:28,400",
        "en": "Monyet! Sudahlah! Habis rosak pokok-pokok pisang kami!",
        "pa": "Monyet! That's enough! You're destroying our banana tree.",
        "vi": "Monyet! Đủ rồi! Cậu đang phá hỏng cây chuối của chúng tôi đấy."
    },
    {
        "start": "00:11:34,760",
        "end": "00:11:36,080",
        "en": "Monyet!",
        "pa": "Monyet!",
        "vi": "Monyet!"
    },
    {
        "start": "00:11:41,360",
        "end": "00:11:46,280",
        "en": "Degilnya Monyet. Kepala batu.",
        "pa": "Monyet is stubborn. Bullheaded!",
        "vi": "Monyet thật là cứng đầu. Ngoan cố!"
    },
    {
        "start": "00:11:57,120",
        "end": "00:12:01,600",
        "en": "Manisnya! Yang ini baru sedap.",
        "pa": "It's not delicious!\n It's so sweet. Now, this is delicious.",
        "vi": "Không ngon gì cả!\n Ngọt quá. Cái này mới ngon này."
    },
    {
        "start": "00:12:07,080",
        "end": "00:12:09,240",
        "en": "Jangan habiskan pisang itu.",
        "pa": "Don't eat all of the bananas.",
        "vi": "Đừng ăn hết chuối. Chúng tôi còn chưa ăn một quả nào."
    },
    {
        "start": "00:12:09,320",
        "end": "00:12:12,200",
        "en": "Kami belum rasa lagi",
        "pa": "We haven't tasted them yet",
        "vi": ""
    },
    {
        "start": "00:12:13,680",
        "end": "00:12:15,400",
        "en": "Nasiblah.",
        "pa": "Too bad.",
        "vi": "Tiếc quá…"
    },
    {
        "start": "00:12:15,480",
        "end": "00:12:19,240",
        "en": "Campaklah satu pisang yang dah masak itu.",
        "pa": "Throw me one ripe banana.",
        "vi": "Ném cho tôi một quả chuối chín đi."
    },
    {
        "start": "00:12:19,320",
        "end": "00:12:20,920",
        "en": "Aku nak juga!",
        "pa": "I want one too!",
        "vi": "Tôi cũng muốn một quả!"
    },
    {
        "start": "00:12:25,040",
        "end": "00:12:30,160",
        "en": "Tak nak! Pisang ini semua aku punya!",
        "pa": "No way! All of these bananas are mine.",
        "vi": "Không đời nào! Tất cả chỗ chuối này là của tôi."
    },
    {
        "start": "00:12:33,600",
        "end": "00:12:36,280",
        "en": "Kami punyalah! Kami yang tanam!",
        "pa": "They're ours! We planted them.",
        "vi": "Của chúng tôi chứ! Chính chúng tôi đã trồng mà."
    },
    {
        "start": "00:12:36,880",
        "end": "00:12:38,120",
        "en": "Monyet!",
        "pa": "Monyet!",
        "vi": "Monyet!"
    },
    {
        "start": "00:12:39,720",
        "end": "00:12:43,160",
        "en": "Tak mengenang budi!",
        "pa": "You're ungrateful!",
        "vi": "Cậu thật vô ơn!"
    },
    {
        "start": "00:12:43,840",
        "end": "00:12:46,120",
        "en": "Baik kau pergi dari sini!",
        "pa": "You'd better leave now!",
        "vi": "Cậu nên đi ngay đi!"
    },
    {
        "start": "00:12:46,200",
        "end": "00:12:50,960",
        "en": "Tak nak. Aku nak makan semua pisang ini.",
        "pa": "No way! I want to eat all of the bananas,",
        "vi": "Không đời nào! Tôi muốn ăn hết tất cả chuối ở đây,"
    },
    {
        "start": "00:12:51,040",
        "end": "00:12:53,760",
        "en": "Dan buah-buahan yang lain juga.",
        "pa": "and the other fruits as well.",
        "vi": "và mấy loại quả khác nữa."
    },
    {
        "start": "00:13:28,640",
        "end": "00:13:32,000",
        "en": "Engkaulah pujaan hati aku.",
        "pa": "You are the love of my life.",
        "vi": "Bọn mày là tình yêu của đời tao."
    },
    {
        "start": "00:13:33,640",
        "end": "00:13:36,280",
        "en": "Aku sayang padamu.",
        "pa": "I love you.",
        "vi": "Tao yêu bọn mày."
    },
    {
        "start": "00:13:42,400",
        "end": "00:13:44,400",
        "en": "Apa kita nak buat Kancil?",
        "pa": "What should we do, Kancil?",
        "vi": "Ta nên làm gì đây, Kancil?"
    },
    {
        "start": "00:14:07,560",
        "end": "00:14:09,800",
        "en": "Begitu!",
        "pa": "I get it…",
        "vi": "Mình biết rồi…"
    },
    {
        "start": "00:14:10,560",
        "end": "00:14:12,200",
        "en": "Kura-kura, ikut aku!",
        "pa": "Kura-kura, follow me!",
        "vi": "Kura-kura, theo tôi nào!"
    },
    {
        "start": "00:14:51,560",
        "end": "00:14:55,000",
        "en": "Balasan sudah tiba.",
        "pa": "It's revenge time.",
        "vi": "Đến lúc trả đũa rồi."
    },
    {
        "start": "00:15:05,600",
        "end": "00:15:10,560",
        "en": "Dahlah Kancil, mari kita makan tembikai manis.",
        "pa": "Let's go Kancil.\n -So yummy! -Let's eat those sweet watermelons.",
        "vi": "Đi nào Kancil! Hãy ăn mấy quả dưa hấu ngon ngọt kia nào."
    },
    {
        "start": "00:15:10,640",
        "end": "00:15:14,200",
        "en": "- Lagi sedap dari pisang. - Betul.",
        "pa": "-They're better than bananas. -Yes.",
        "vi": "- Chúng ngon hơn chuối nhiều. - Đúng vậy."
    },
    {
        "start": "00:15:27,080",
        "end": "00:15:30,840",
        "en": "Tembikai? Mana tembikai?",
        "pa": "Watermelon? Where is it? Where?",
        "vi": "Dưa hấu? Ở đâu? Đâu nào?"
    },
    {
        "start": "00:16:26,160",
        "end": "00:16:27,880",
        "en": "Sakit!",
        "pa": "It hurts!",
        "vi": "Đau quá!"
    },
    {
        "start": "00:16:37,520",
        "end": "00:16:39,720",
        "en": "Aduh! Sakit!",
        "pa": "Ouch! It hurts!",
        "vi": "Đau quá!"
    },
    {
        "start": "00:16:54,480",
        "end": "00:16:56,920",
        "en": "Sakit!",
        "pa": "It hurts!",
        "vi": "Đau quá!"
    },
    {
        "start": "00:16:58,840",
        "end": "00:17:00,760",
        "en": "Kura-kura.",
        "pa": "Kura-kura.",
        "vi": "Kura-kura."
    },
    {
        "start": "00:17:01,880",
        "end": "00:17:04,480",
        "en": "Menangis! Kuatlah lagi!",
        "pa": "Go ahead and cry! Louder!",
        "vi": "Cứ ngồi đó mà khóc đi! Khóc to hơn nữa!"
    },
    {
        "start": "00:17:05,079",
        "end": "00:17:07,319",
        "en": "Itulah degil lagi!",
        "pa": "You're being stubborn again!",
        "vi": "Cậu lại giở thói cứng đầu!"
    },
    {
        "start": "00:17:07,400",
        "end": "00:17:10,560",
        "en": "Kepala batu. Tak pernah nak dengar nasihat.",
        "pa": "You're so bullheaded. You never listen to advice.",
        "vi": "Cậu đúng là ngoan cố. Không bao giờ nghe ai khuyên nhủ."
    },
    {
        "start": "00:17:10,640",
        "end": "00:17:12,480",
        "en": "Dah kena balasannya.",
        "pa": "This is what you get.",
        "vi": "Đây là thứ cậu phải nhận."
    },
    {
        "start": "00:17:15,680",
        "end": "00:17:18,280",
        "en": "Aku menyesal.",
        "pa": "I regret it.",
        "vi": "Tôi hối hận rồi."
    },
    {
        "start": "00:17:18,359",
        "end": "00:17:21,640",
        "en": "Aku janji akan dengar nasihat kau berdua.",
        "pa": "I promise I'll listen to your advice.",
        "vi": "Tôi hứa sẽ nghe lời khuyên của cậu."
    },
    {
        "start": "00:17:21,720",
        "end": "00:17:24,000",
        "en": "Dan tak akan degil lagi.",
        "pa": "And I won't be stubborn again.",
        "vi": "Và sẽ không cứng đầu nữa."
    },
    {
        "start": "00:17:26,800",
        "end": "00:17:28,480",
        "en": "Nah makanlah.",
        "pa": "Here, eat it.",
        "vi": "Đây, ăn đi."
    },
    {
        "start": "00:17:32,480",
        "end": "00:17:34,920",
        "en": "Untuk aku? Tapi aku..",
        "pa": "For me? But I--",
        "vi": "Cho tôi à? Nhưng tôi…"
    },
    {
        "start": "00:17:35,760",
        "end": "00:17:38,760",
        "en": "Kami buat semua ini untuk beri kau pengajaran saja.",
        "pa": "We did all this because we want to teach you a lesson.",
        "vi": "Chúng tôi làm vậy chỉ vì muốn dạy cho cậu một bài học thôi."
    },
    {
        "start": "00:17:39,520",
        "end": "00:17:43,640",
        "en": "Walau apa pun yang terjadi, kau tetap kawan kami.",
        "pa": "No matter what happens, you're still our friend.",
        "vi": "Dù có chuyện gì đi nữa, cậu vẫn là bạn của chúng tôi."
    },
    {
        "start": "00:17:49,200",
        "end": "00:17:52,240",
        "en": "Kau berdua memang baik.",
        "pa": "You're so kind!",
        "vi": "Các cậu thật tốt bụng!"
    },
    {
        "start": "00:18:02,320",
        "end": "00:18:05,440",
        "en": "Begitulah ceritanya.",
        "pa": "That's the story.",
        "vi": "Câu chuyện là thế đấy."
    },
    {
        "start": "00:18:05,520",
        "end": "00:18:09,120",
        "en": "Monyet yang degil itu pun akhirnya menyesal.",
        "pa": "The stubborn Monyet left feeling sad and regretful.",
        "vi": "Monyet cứng đầu vừa thấy buồn vừa thấy hối hận."
    },
    {
        "start": "00:18:12,680",
        "end": "00:18:15,040",
        "en": "Jadi kepala batu maksudnya",
        "pa": "So bullheaded means",
        "vi": "Vậy ngoan cố nghĩa là"
    },
    {
        "start": "00:18:15,120",
        "end": "00:18:18,200",
        "en": "orang yang degil dan tak mahu dengar nasihat, betul tak Aki?",
        "pa": "someone who is stubborn and refuses to listen to advice, right Aki?",
        "vi": "một người cứng đầu và không chịu nghe lời khuyên, phải không ạ?"
    },
    {
        "start": "00:18:18,280",
        "end": "00:18:21,640",
        "en": "Ya, macam kau berdua.",
        "pa": "Yes, like you two.",
        "vi": "Phải, giống như hai cháu vậy."
    },
    {
        "start": "00:18:24,520",
        "end": "00:18:26,880",
        "en": "Dah, mari cepat tolong Aki.",
        "pa": "Enough, come and help me.",
        "vi": "Đủ rồi, giờ đến giúp ông nào."
    },
    {
        "start": "00:18:27,920",
        "end": "00:18:30,160",
        "en": "Cepat abang.",
        "pa": "Quick Aris, quick.",
        "vi": "Nhanh lên anh."
    },
    {
        "start": "00:18:31,080",
        "end": "00:18:32,400",
        "en": "Ambil seorang sedikit.",
        "pa": "Each of you take some.",
        "vi": "Mỗi đứa lấy một ít đi."
    },
    {
        "start": "00:18:41,400",
        "end": "00:18:46,360",
        "en": "Aki, macam mana Kura-kura dan Kancil nak makan pisang?",
        "pa": "Aki, how would Kancil and Kura-kura eat the bananas?",
        "vi": "Ông à, làm thế nào Kancil và Kura-kura ăn chuối được ạ?"
    },
    {
        "start": "00:18:47,120",
        "end": "00:18:51,680",
        "en": "Pokok pisang tinggi, Kancil dan Kura-kura mana boleh panjat pokok.",
        "pa": "Banana trees are tall.\n Kancil and Kura-kura can't climb trees.",
        "vi": "Cây chuối thì cao.\n Kancil và Kura-kura không biết leo cây."
    },
    {
        "start": "00:18:51,760",
        "end": "00:18:53,440",
        "en": "Bukankah Monyet ada.",
        "pa": "Monyet is there, isn't he?",
        "vi": "Monyet có ở đó mà, đúng không?"
    },
    {
        "start": "00:18:53,520",
        "end": "00:18:55,480",
        "en": "Yalah.",
        "pa": "Yes!",
        "vi": "Đúng rồi!"
    },
    {
        "start": "00:18:55,560",
        "end": "00:18:57,640",
        "en": "Tapi Aki…",
        "pa": "But Aki…",
        "vi": "Nhưng ông à…"
    },
    {
        "start": "00:18:58,560",
        "end": "00:19:00,800",
        "en": "Tidur pula.",
        "pa": "Aki is asleep…",
        "vi": "Ông đã ngủ rồi…"
    },
    {
        "start": "00:19:00,880",
        "end": "00:19:02,760",
        "en": "Tak patut.",
        "pa": "That's not nice…",
        "vi": "Thật là…"
    },
    {
        "start": "00:19:02,840",
        "end": "00:19:05,240",
        "en": "- Selamat malam abang - Malam.",
        "pa": "-Good night Aris. -Good night.",
        "vi": "- Chúc anh ngủ ngon. - Chúc em ngủ ngon."
    },
    {
        "start": "00:19:20,200",
        "end": "00:19:22,200",
        "en": "",
        "pa": "",
        "vi": "Biên dịch: Hoàng Nguyên"
    }
]