[
    {
        "start": "00:00:34,120",
        "end": "00:00:35,200",
        "en": "Nah, minum.",
        "pa": "Here, drink.",
        "vi": "Đây, ông uống đi."
    },
    {
        "start": "00:00:42,600",
        "end": "00:00:45,440",
        "en": "Tersedak ini, mesti cucu awak yang sebut.",
        "pa": "Looks like your grandchildren are talking about you.",
        "vi": "Hình như cháu nội lại nhắc ông rồi."
    },
    {
        "start": "00:00:46,320",
        "end": "00:00:50,280",
        "en": "Cucu apanya? Kopi ini pahit. Tak ada gula.",
        "pa": "Grandchildren? There's no sugar in this coffee. It's so bitter!",
        "vi": "Cháu nội? Cà phê không có đường. Đắng quá!"
    },
    {
        "start": "00:00:50,880",
        "end": "00:00:54,520",
        "en": "Memang pun, gula sudah habis. Pergilah beli.",
        "pa": "That's because we don't have any sugar. Why don't you get us some?",
        "vi": "Bởi vì nhà hết đường rồi. Ông đi mua một ít được không?"
    },
    {
        "start": "00:00:56,400",
        "end": "00:00:57,680",
        "en": "Yalah.",
        "pa": "Fine…",
        "vi": "Được rồi…"
    },
    {
        "start": "00:01:08,040",
        "end": "00:01:09,080",
        "en": "Yakah?",
        "pa": "You're here?",
        "vi": "Bọn con đến rồi à?"
    },
    {
        "start": "00:01:10,560",
        "end": "00:01:12,160",
        "en": "Aki!",
        "pa": "",
        "vi": "Ông ơi!"
    },
    {
        "start": "00:01:25,240",
        "end": "00:01:26,440",
        "en": "Aki!",
        "pa": "",
        "vi": "Chào bác!"
    },
    {
        "start": "00:01:38,280",
        "end": "00:01:39,440",
        "en": "- Aki! - Aki!",
        "pa": "",
        "vi": "- Ông ơi! - Ông!"
    },
    {
        "start": "00:01:43,760",
        "end": "00:01:45,120",
        "en": "Aris, Ara.",
        "pa": "",
        "vi": "Aris, Ara."
    },
    {
        "start": "00:01:48,120",
        "end": "00:01:50,280",
        "en": "- Aki! - Aki!",
        "pa": "",
        "vi": "- Ông ơi! - Ông!"
    },
    {
        "start": "00:01:50,800",
        "end": "00:01:54,120",
        "en": "- Cucu-cucu Aki. - Assalamualaikum, Abah.",
        "pa": "-My dear grandchildren… -Peace be upon you, Dad.",
        "vi": "- Cháu cưng của ông… - Bố mạnh khỏe ạ."
    },
    {
        "start": "00:01:55,320",
        "end": "00:01:56,880",
        "en": "Waalaikumussalam.",
        "pa": "Peace be upon you, too.",
        "vi": "Hai đứa mạnh giỏi."
    },
    {
        "start": "00:01:56,960",
        "end": "00:01:59,680",
        "en": "Kau ini pun. Balik bukan nak beritahu.",
        "pa": "Don't you know how to call before showing up?",
        "vi": "Sao hai con không gọi trước khi đến?"
    },
    {
        "start": "00:01:59,760",
        "end": "00:02:02,200",
        "en": "Sudah. Wan kata Aki sudah keluar.",
        "pa": "We did! Wan said you were out.",
        "vi": "Có ạ! Bà bảo ông ra ngoài rồi."
    },
    {
        "start": "00:02:03,800",
        "end": "00:02:05,480",
        "en": "Sudah sampai baru nak telefon.",
        "pa": "Then you should have called earlier.",
        "vi": "Đáng lẽ con nên gọi sớm hơn."
    },
    {
        "start": "00:02:07,160",
        "end": "00:02:09,960",
        "en": "- Lupa. - Apalah Papa ini.",
        "pa": "-I forgot. -You're always like that.",
        "vi": "- Con quên ạ. - Bố lúc nào cũng thế."
    },
    {
        "start": "00:02:10,039",
        "end": "00:02:12,600",
        "en": "Kamu balik dulu. Aki nak pergi kedai kejap.",
        "pa": "I'll see you at home. I'm going to the grocery store.",
        "vi": "Hẹn gặp các cháu ở nhà nhé. Ông ra tiệm tạp hóa đã."
    },
    {
        "start": "00:02:13,360",
        "end": "00:02:14,960",
        "en": "Kedai? Nak ikut.",
        "pa": "The store? I want to come!",
        "vi": "Tiệm tạp hóa? Con muốn đi!"
    },
    {
        "start": "00:02:15,040",
        "end": "00:02:16,800",
        "en": "- Boleh? - Boleh tak, Papa?",
        "pa": "-Can we? -Can we, Dad?",
        "vi": "- Được không ạ? - Được không bố?"
    },
    {
        "start": "00:02:16,880",
        "end": "00:02:19,400",
        "en": "- Pergilah. - Jalan baik-baik.",
        "pa": "-Go on. -Be careful!",
        "vi": "- Đi đi. - Cẩn thận đấy!"
    },
    {
        "start": "00:02:19,480",
        "end": "00:02:21,000",
        "en": "Selamat tinggal.",
        "pa": "",
        "vi": "- Tạm biệt… - Tạm biệt…"
    },
    {
        "start": "00:02:21,520",
        "end": "00:02:23,120",
        "en": "Aki, lajulah sikit.",
        "pa": "Can't you go any faster?",
        "vi": "Chạy nhanh hơn được không ông?"
    },
    {
        "start": "00:02:23,200",
        "end": "00:02:25,280",
        "en": "Tengok itu. Mesra.",
        "pa": "Look at those kids. So happy!",
        "vi": "Nhìn bọn trẻ kìa. Chúng rất vui!"
    },
    {
        "start": "00:02:25,360",
        "end": "00:02:28,040",
        "en": "Siapa cakap mereka tak boleh duduk kampung?",
        "pa": "Who says they can't live in the village?",
        "vi": "Ai bảo chúng không sống ở quê được nhỉ?"
    },
    {
        "start": "00:02:28,120",
        "end": "00:02:29,120",
        "en": "Awaklah.",
        "pa": "You did.",
        "vi": "Thì anh chứ ai."
    },
    {
        "start": "00:02:39,280",
        "end": "00:02:42,560",
        "en": "Aki, Ara rindu Aki.",
        "pa": "Aki! I missed you so much.",
        "vi": "Ông ơi! Cháu nhớ ông lắm."
    },
    {
        "start": "00:02:43,880",
        "end": "00:02:46,960",
        "en": "Ya? Aris, tak rindu kat Akikah?",
        "pa": "Really? Aris, did you miss me too?",
        "vi": "Vậy à? Aris có nhớ ông không?"
    },
    {
        "start": "00:02:47,800",
        "end": "00:02:48,840",
        "en": "Tak.",
        "pa": "No.",
        "vi": "Không ạ."
    },
    {
        "start": "00:02:50,160",
        "end": "00:02:51,880",
        "en": "Yalah itu.",
        "pa": "Whatever you say.",
        "vi": "Sao cũng được."
    },
    {
        "start": "00:02:51,960",
        "end": "00:02:53,480",
        "en": "Aki, apa itu?",
        "pa": "Aki, what's that?",
        "vi": "Ông ơi, cái gì thế kia?"
    },
    {
        "start": "00:03:00,440",
        "end": "00:03:02,720",
        "en": "Nasib baik tak jatuh.",
        "pa": "That was close!",
        "vi": "Suýt nữa là cán rồi!"
    },
    {
        "start": "00:03:04,560",
        "end": "00:03:07,200",
        "en": "Tak apa. Itu ular sawa.",
        "pa": "Don't worry. It's just a python.",
        "vi": "Đừng lo. Là một con trăn thôi."
    },
    {
        "start": "00:03:07,280",
        "end": "00:03:08,840",
        "en": "- Dia sesat itu. - Besarnya.",
        "pa": "-It must be lost. -It's huge!",
        "vi": "- Chắc nó đi lạc. - Nó to quá."
    },
    {
        "start": "00:03:10,280",
        "end": "00:03:11,320",
        "en": "Tunggu sini.",
        "pa": "Wait here.",
        "vi": "Đợi ở đây nhé."
    },
    {
        "start": "00:03:12,880",
        "end": "00:03:14,960",
        "en": "Apa kita nak buat ini Aki?",
        "pa": "What are we going to do?",
        "vi": "Chúng ta làm gì đây ông?"
    },
    {
        "start": "00:03:15,040",
        "end": "00:03:18,120",
        "en": "Abang, Ara takut.",
        "pa": "Aris, I'm scared.",
        "vi": "Anh ơi, em sợ quá."
    },
    {
        "start": "00:03:18,200",
        "end": "00:03:20,160",
        "en": "Kalau Ara takut, Abang lagi takut.",
        "pa": "If you're scared, then I'm even more scared!",
        "vi": "Nếu em sợ, anh còn sợ hơn!"
    },
    {
        "start": "00:03:23,400",
        "end": "00:03:25,320",
        "en": "Aki, hati-hati.",
        "pa": "Be careful, Aki.",
        "vi": "Cẩn thận đấy ông."
    },
    {
        "start": "00:03:27,520",
        "end": "00:03:30,760",
        "en": "Pukul dia Aki! Pukul dia. Biar mati.",
        "pa": "Hit his head, Aki! Hit it!",
        "vi": "Đập đầu nó đi ông!"
    },
    {
        "start": "00:03:31,280",
        "end": "00:03:34,960",
        "en": "Lagi Aki, lagi! Pukul kuat-kuat.",
        "pa": "Give it all you got!",
        "vi": "Đập thật mạnh vào!"
    },
    {
        "start": "00:03:36,800",
        "end": "00:03:38,960",
        "en": "Ia pergi mana itu?",
        "pa": "Where is it going?",
        "vi": "Nó đi đâu rồi?"
    },
    {
        "start": "00:03:42,000",
        "end": "00:03:47,680",
        "en": "Aki, tak bahayakah lepaskan ular itu? Nanti kalau dia patuk orang macam mana?",
        "pa": "Aki, why did you let it go? What if it bites somebody?",
        "vi": "Ông ơi, sao ông để nó đi? Nếu lỡ nó cắn người thì sao?"
    },
    {
        "start": "00:03:47,760",
        "end": "00:03:50,600",
        "en": "Ular sawa tak patuk orang. Ia tak ada bisa.",
        "pa": "Pythons don't bite. They don't have any venom.",
        "vi": "Trăn không cắn người. Nó không có nọc độc."
    },
    {
        "start": "00:03:50,680",
        "end": "00:03:53,600",
        "en": "Ia belit saja mangsanya, lepas itu telan.",
        "pa": "It just wraps its victim up and swallows them!",
        "vi": "Nó chỉ siết chặt con mồi rồi nuốt chửng thôi."
    },
    {
        "start": "00:03:54,920",
        "end": "00:03:57,840",
        "en": "- Belit? - Ya.",
        "pa": "-Wrap? -Yes.",
        "vi": "- Siết ạ? - Ừ."
    },
    {
        "start": "00:03:57,920",
        "end": "00:04:01,400",
        "en": "Ular sawa kalau sudah belit, macam tali pinggang.",
        "pa": "The python can wrap around its prey like a powerful belt!",
        "vi": "Trăn có thể quấn chặt con mồi như chiếc thắt lưng vậy!"
    },
    {
        "start": "00:04:01,480",
        "end": "00:04:05,720",
        "en": "Ia tak akan lepaskan, sampai mangsa itu mati.",
        "pa": "And it will only let them go once they have died.",
        "vi": "Nó chỉ thả ra khi nào con mồi đã chết thôi."
    },
    {
        "start": "00:04:07,240",
        "end": "00:04:08,600",
        "en": "Takutnya.",
        "pa": "That's scary.",
        "vi": "Đáng sợ quá."
    },
    {
        "start": "00:04:08,680",
        "end": "00:04:12,320",
        "en": "Aki, tak akanlah satu pun tak pernah terlepas?",
        "pa": "Come on, Aki, there must be one animal that got away.",
        "vi": "Kìa ông, chắc phải có con mồi nào đó thoát ra được chứ."
    },
    {
        "start": "00:04:15,880",
        "end": "00:04:19,079",
        "en": "Ada! Ada yang pernah terlepas.",
        "pa": "There is! There is one animal that got away.",
        "vi": "Có đấy! Có một con thoát được."
    },
    {
        "start": "00:04:19,160",
        "end": "00:04:20,120",
        "en": "Nak dengar cerita?",
        "pa": "Do you want to hear a story?",
        "vi": "Ông kể nhé?"
    },
    {
        "start": "00:04:21,120",
        "end": "00:04:22,560",
        "en": "Ceritalah.",
        "pa": "Tell us, tell us!",
        "vi": "Kể đi ông!"
    },
    {
        "start": "00:04:24,520",
        "end": "00:04:27,520",
        "en": "Pada zaman dahulu…",
        "pa": "Once upon a time…",
        "vi": "Ngày xửa ngày xưa…"
    },
    {
        "start": "00:04:54,160",
        "end": "00:04:56,440",
        "en": "Kenapa?",
        "pa": "Why?",
        "vi": "Tại sao chứ?"
    },
    {
        "start": "00:04:57,480",
        "end": "00:04:58,520",
        "en": "Siapa itu?",
        "pa": "Who's there?",
        "vi": "Ai đấy?"
    },
    {
        "start": "00:05:06,520",
        "end": "00:05:10,800",
        "en": "Tolong aku, bantu aku!",
        "pa": "Help me, save me!",
        "vi": "Giúp tôi với, cứu tôi với!"
    },
    {
        "start": "00:05:12,040",
        "end": "00:05:15,120",
        "en": "Sang Ular, kenapa merintih ini?",
        "pa": "Dear Ular, why are you crying?",
        "vi": "Ular à, vì sao chị khóc?"
    },
    {
        "start": "00:05:16,160",
        "end": "00:05:21,120",
        "en": "Telur aku. Telur-telur aku, sudah kena curi.",
        "pa": "My eggs! My beautiful eggs have been stolen!",
        "vi": "Trứng của tôi! Mấy quả trứng xinh xắn bị trộm rồi."
    },
    {
        "start": "00:05:22,600",
        "end": "00:05:23,760",
        "en": "Kena curi?",
        "pa": "Stolen?",
        "vi": "Trộm ư?"
    },
    {
        "start": "00:05:24,600",
        "end": "00:05:27,240",
        "en": "Tadi, aku pergi cari makan.",
        "pa": "I went out looking for food,",
        "vi": "Tôi ra ngoài tìm thức ăn,"
    },
    {
        "start": "00:05:27,320",
        "end": "00:05:30,680",
        "en": "Bila balik saja, sudah hilang!",
        "pa": "and when I came back, they were gone!",
        "vi": "khi trở lại thì chúng đã mất rồi!"
    },
    {
        "start": "00:05:32,120",
        "end": "00:05:35,240",
        "en": "Oh, kejamnya dunia…",
        "pa": "What a cruel world…",
        "vi": "Thế giới này thật nghiệt ngã…"
    },
    {
        "start": "00:05:35,320",
        "end": "00:05:37,200",
        "en": "Mana kau letak telur-telur itu?",
        "pa": "Do you remember where you left them?",
        "vi": "Chị có nhớ mình bỏ trứng ở đâu không?"
    },
    {
        "start": "00:05:38,520",
        "end": "00:05:40,480",
        "en": "Sarang aku!",
        "pa": "In my nest!",
        "vi": "Trong tổ ấy!"
    },
    {
        "start": "00:05:42,600",
        "end": "00:05:47,480",
        "en": "Aku kasihan dekat kau Ular, tapi aku tak tahu macam mana nak tolong.",
        "pa": "I really pity you, Ular, but I don't know what to do.",
        "vi": "Khổ thân chị, nhưng tôi không biết phải làm gì cả."
    },
    {
        "start": "00:05:47,560",
        "end": "00:05:48,880",
        "en": "Aku pergi dulu ya.",
        "pa": "I'll see you later.",
        "vi": "Hẹn gặp lại nhé."
    },
    {
        "start": "00:05:50,480",
        "end": "00:05:52,080",
        "en": "Kancil! Aku tahu.",
        "pa": "Kancil! I know!",
        "vi": "Kancil! Tôi biết rồi!"
    },
    {
        "start": "00:05:52,160",
        "end": "00:05:53,320",
        "en": "Tahu apa?",
        "pa": "Know what?",
        "vi": "Biết gì?"
    },
    {
        "start": "00:05:54,040",
        "end": "00:05:55,480",
        "en": "Sang Biawak yang curi!",
        "pa": "Biawak stole them!",
        "vi": "Chính Iguana đã trộm chúng!"
    },
    {
        "start": "00:05:55,560",
        "end": "00:05:57,560",
        "en": "- Mana ada bi… - Ada Kancil.",
        "pa": "-But there aren't any-- -There is!",
        "vi": "- Nhưng ở đây không có… - Có!"
    },
    {
        "start": "00:05:57,640",
        "end": "00:06:00,400",
        "en": "Hari itu dia datang sini.",
        "pa": "He was here yesterday,",
        "vi": "Hôm qua hắn đã đến đây"
    },
    {
        "start": "00:06:00,480",
        "end": "00:06:03,240",
        "en": "Patutlah dia baik semacam.",
        "pa": "and he was acting all weird!",
        "vi": "và cư xử kì lạ lắm!"
    },
    {
        "start": "00:06:03,320",
        "end": "00:06:08,440",
        "en": "Rupanya, nak curi anak-anak aku!",
        "pa": "Now I know why. He wanted to kidnap my children!",
        "vi": "Giờ tôi biết rồi. Hắn muốn bắt con của tôi!"
    },
    {
        "start": "00:06:10,000",
        "end": "00:06:14,480",
        "en": "Tolong aku Kancil, tolonglah cari anak-anak aku!",
        "pa": "Help me find my eggs, Kancil. I'm begging you!",
        "vi": "Giúp tôi tìm trứng đi, Kancil. Tôi xin cậu đấy!"
    },
    {
        "start": "00:06:14,560",
        "end": "00:06:19,440",
        "en": "- Tolong aku, bantu… - Baiklah. Aku tolong kau.",
        "pa": "-Help me, save me! -All right. I'll do it.",
        "vi": "- Giúp tôi, cứu tôi với! - Được rồi. Tôi sẽ giúp mà."
    },
    {
        "start": "00:06:20,560",
        "end": "00:06:24,200",
        "en": "Terima kasih banyak-banyak Kancil. Kau memang baik.",
        "pa": "Thank you so much, Kancil! You're the best.",
        "vi": "Cảm ơn cậu rất nhiều! Cậu là số một."
    },
    {
        "start": "00:06:24,280",
        "end": "00:06:26,800",
        "en": "- Mana sarang biawak itu? - Mari aku tunjuk.",
        "pa": "-Where can we find Biawak? -Follow me!",
        "vi": "- Giờ tìm Iguana ở đâu đây? - Theo tôi!"
    },
    {
        "start": "00:06:34,120",
        "end": "00:06:35,760",
        "en": "Sini.",
        "pa": "Come on!",
        "vi": "Đi nào!"
    },
    {
        "start": "00:06:37,120",
        "end": "00:06:40,080",
        "en": "- Jauhkah? - Taklah, depan itu saja.",
        "pa": "-Is it far from here? -It's just around the corner.",
        "vi": "- Có xa lắm không? - Chỉ quanh đây thôi."
    },
    {
        "start": "00:06:40,160",
        "end": "00:06:44,000",
        "en": "Ada udang di sebalik batu.",
        "pa": "Something fishy is going on.",
        "vi": "Có mùi đáng nghi lắm đây."
    },
    {
        "start": "00:06:57,320",
        "end": "00:07:01,400",
        "en": "Cepatlah Kancil. Nanti dimakannya anak-anak aku.",
        "pa": "Hurry up, Kancil, my children are about to be eaten!",
        "vi": "Nhanh lên, Kancil, con của tôi sắp bị xơi tái mất rồi!"
    },
    {
        "start": "00:07:02,120",
        "end": "00:07:03,480",
        "en": "Biawak kejam!",
        "pa": "Biawak, you mean little iguana.",
        "vi": "Tên Iguana nhỏ bé xấu xa."
    },
    {
        "start": "00:07:03,560",
        "end": "00:07:06,920",
        "en": "Nanti aku makan anak-anak dia pula. Padan muka.",
        "pa": "I'm gonna get your children and eat them too.\n Serves you right.",
        "vi": "Ta sẽ bắt con của ngươi và nuốt luôn.\n Đáng đời ngươi."
    },
    {
        "start": "00:07:11,040",
        "end": "00:07:13,400",
        "en": "Tak ada. Jom.",
        "pa": "Sorry, I got carried away there. Come on.",
        "vi": "Xin lỗi, tôi hơi nói quá một chút. Đi nào."
    },
    {
        "start": "00:07:20,120",
        "end": "00:07:21,920",
        "en": "Sudah nak sampai ini.",
        "pa": "We're almost there.",
        "vi": "Gần đến nơi rồi."
    },
    {
        "start": "00:07:25,920",
        "end": "00:07:28,360",
        "en": "Kancil, sini.",
        "pa": "Kancil! Here.",
        "vi": "Kancil! Ở đây rồi."
    },
    {
        "start": "00:07:29,840",
        "end": "00:07:31,200",
        "en": "Cepatlah.",
        "pa": "Quick!",
        "vi": "Nhanh lên!"
    },
    {
        "start": "00:07:33,880",
        "end": "00:07:36,120",
        "en": "Kau pasti sarang Biawak itu di sini?",
        "pa": "Are you sure Biawak lives here?",
        "vi": "Chị có chắc Iguana sống ở đây không?"
    },
    {
        "start": "00:07:36,200",
        "end": "00:07:38,280",
        "en": "Pasti.",
        "pa": "Sure.",
        "vi": "Chắc chắn."
    },
    {
        "start": "00:07:39,000",
        "end": "00:07:41,800",
        "en": "Kau cari di sana, aku cari di sini.",
        "pa": "You look over there, I'll look over here.",
        "vi": "Cậu tìm bên kia, tôi tìm bên này."
    },
    {
        "start": "00:07:42,960",
        "end": "00:07:44,200",
        "en": "Yalah.",
        "pa": "All right.",
        "vi": "Được rồi."
    },
    {
        "start": "00:08:15,320",
        "end": "00:08:16,720",
        "en": "Aku punya.",
        "pa": "Mine!",
        "vi": "Ngươi là của ta!"
    },
    {
        "start": "00:08:24,480",
        "end": "00:08:25,800",
        "en": "Tak ada ini.",
        "pa": "Nothing here.",
        "vi": "Không có gì cả."
    },
    {
        "start": "00:08:31,080",
        "end": "00:08:32,240",
        "en": "Tak ada juga.",
        "pa": "Nothing here either.",
        "vi": "Ở đây cũng không."
    },
    {
        "start": "00:08:46,400",
        "end": "00:08:49,360",
        "en": "Macam mana rupa telur Ular? Lupa aku nak tanya.",
        "pa": "What do her eggs look like? I'd better ask.",
        "vi": "Trứng của chị ta trông thế nào nhỉ? Mình nên hỏi đã."
    },
    {
        "start": "00:08:49,440",
        "end": "00:08:51,640",
        "en": "Macam ini!",
        "pa": "Like this!",
        "vi": "Như thế này!"
    },
    {
        "start": "00:08:54,280",
        "end": "00:08:56,040",
        "en": "- Sini kau! - Kenapa ini?",
        "pa": "-Come here! -What's going on?",
        "vi": "- Đến đây! - Có chuyện gì thế?"
    },
    {
        "start": "00:09:13,800",
        "end": "00:09:15,800",
        "en": "Aku sudah dapat kau!",
        "pa": "You're mine!",
        "vi": "Ngươi là của ta!"
    },
    {
        "start": "00:09:19,600",
        "end": "00:09:21,680",
        "en": "Kenyanglah aku!",
        "pa": "I'm going to have a feast.",
        "vi": "Ta sẽ chén một bữa no nê."
    },
    {
        "start": "00:09:22,560",
        "end": "00:09:27,040",
        "en": "Kancil. Tak sangka kau terpedaya.",
        "pa": "Kancil, Kancil… I never knew you were so gullible.",
        "vi": "Kancil à… Không ngờ ngươi dễ dụ như vậy."
    },
    {
        "start": "00:09:28,880",
        "end": "00:09:32,040",
        "en": "Lepaskan aku!",
        "pa": "Let me go!",
        "vi": "Thả tôi ra!"
    },
    {
        "start": "00:09:32,120",
        "end": "00:09:35,280",
        "en": "Tak guna kau Ular. Kau tipu aku.",
        "pa": "You slippery serpent! You tricked me!",
        "vi": "Đồ trăn tráo trở! Chị lừa tôi!"
    },
    {
        "start": "00:09:37,080",
        "end": "00:09:40,760",
        "en": "- Jaga kau Ular. - Kancil yang cerdik, sudah terpedaya.",
        "pa": "-I'm going to get you. -The wise Kancil has been fooled.",
        "vi": "- Ta sẽ xơi ngươi. - Kancil mưu trí đã bị lừa."
    },
    {
        "start": "00:09:40,840",
        "end": "00:09:43,040",
        "en": "Tiada siapa boleh tolong kau!",
        "pa": "No one can help you!",
        "vi": "Không ai có thể giúp ngươi đâu!"
    },
    {
        "start": "00:09:45,720",
        "end": "00:09:47,560",
        "en": "Sakitnya!",
        "pa": "That hurts!",
        "vi": "Đau quá!"
    },
    {
        "start": "00:09:47,640",
        "end": "00:09:51,760",
        "en": "Kancil, lepaskan aku!",
        "pa": "Kancil! Let me go…",
        "vi": "Kancil! Thả ta ra…"
    },
    {
        "start": "00:09:51,840",
        "end": "00:09:53,400",
        "en": "Tipu saja.",
        "pa": "Just kidding.",
        "vi": "Đùa thôi."
    },
    {
        "start": "00:09:55,400",
        "end": "00:09:57,480",
        "en": "Kancil.",
        "pa": "Kancil, Kancil. You have no clue about pythons, do you?",
        "vi": "Kancil à."
    },
    {
        "start": "00:09:57,560",
        "end": "00:10:01,960",
        "en": "Tak akan kau tak tahu kelemahan Ular Sawa?",
        "pa": "",
        "vi": "Ngươi không biết gì về trăn, phải không?"
    },
    {
        "start": "00:10:05,280",
        "end": "00:10:09,000",
        "en": "Cubalah sekuat hati kau Kancil!",
        "pa": "Try as hard as you can, Kancil!",
        "vi": "Cứ cố vùng vẫy mạnh lên đi!"
    },
    {
        "start": "00:10:12,040",
        "end": "00:10:15,480",
        "en": "Aku sudah lama nak makan daging Kancil ini.",
        "pa": "I've been dreaming of this day.",
        "vi": "Ta mơ đến ngày này lâu lắm rồi."
    },
    {
        "start": "00:10:16,000",
        "end": "00:10:18,280",
        "en": "Mesti sedap, bukan.",
        "pa": "You must be delicious.",
        "vi": "Thịt ngươi chắc là ngon lắm."
    },
    {
        "start": "00:10:18,360",
        "end": "00:10:22,560",
        "en": "Manis, lemak, lembut.",
        "pa": "Sweet, juicy, and tender.",
        "vi": "Vừa ngọt, nhiều nước, lại mềm."
    },
    {
        "start": "00:10:25,200",
        "end": "00:10:26,720",
        "en": "Mesti sedap!",
        "pa": "You must be delicious.",
        "vi": ""
    },
    {
        "start": "00:10:28,120",
        "end": "00:10:30,160",
        "en": "Biar betul!",
        "pa": "Are you kidding me?",
        "vi": "Đùa tôi đấy à?"
    },
    {
        "start": "00:10:31,640",
        "end": "00:10:34,040",
        "en": "Habislah kau.",
        "pa": "You're finished.",
        "vi": "Ngươi xong rồi."
    },
    {
        "start": "00:10:36,200",
        "end": "00:10:38,560",
        "en": "Padan muka kau Kancil.",
        "pa": "Serves you right, Kancil.",
        "vi": "Đáng đời lắm, Kancil."
    },
    {
        "start": "00:10:41,000",
        "end": "00:10:43,800",
        "en": "Tak sangka pula ajalku hari ini.",
        "pa": "At last, I've met my doom.",
        "vi": "Cuối cùng cũng đến ngày tàn của mình."
    },
    {
        "start": "00:10:43,880",
        "end": "00:10:46,920",
        "en": "Syabas Ular, kau memang bijak.",
        "pa": "Congratulations, Ular. You are truly clever!",
        "vi": "Chúc mừng chị, Ular à. Chị thật là khôn khéo!"
    },
    {
        "start": "00:10:47,000",
        "end": "00:10:49,920",
        "en": "Biasalah.",
        "pa": "Of course.",
        "vi": "Dĩ nhiên rồi."
    },
    {
        "start": "00:10:50,840",
        "end": "00:10:54,680",
        "en": "Engkaulah satu-satunya binatang yang berjaya menipu aku.",
        "pa": "You are the only animal that has ever tricked me.",
        "vi": "Chị là loài thú duy nhất từng lừa được tôi đấy."
    },
    {
        "start": "00:10:55,800",
        "end": "00:10:57,640",
        "en": "Oh, mesti.",
        "pa": "Thank you!",
        "vi": "Cảm ơn!"
    },
    {
        "start": "00:10:57,720",
        "end": "00:11:01,480",
        "en": "Aku ini bukan calang-calang ular tahu.",
        "pa": "I'm not your average python, you know?",
        "vi": "Ta không phải dạng vừa đâu, biết chưa?"
    },
    {
        "start": "00:11:01,560",
        "end": "00:11:05,560",
        "en": "Akulah Ular Sawa Batik!",
        "pa": "I am a reticulated python!",
        "vi": "Ta là trăn gấm mà!"
    },
    {
        "start": "00:11:07,080",
        "end": "00:11:11,640",
        "en": "Tapi sebelum kau makan aku, boleh tak aku minta sesuatu?",
        "pa": "Before you eat me, can I have one last request?",
        "vi": "Trước khi chị xơi tôi, cho tôi một thỉnh cầu được không?"
    },
    {
        "start": "00:11:11,720",
        "end": "00:11:13,080",
        "en": "Apa dia?",
        "pa": "What is it?",
        "vi": "Là gì thế?"
    },
    {
        "start": "00:11:13,160",
        "end": "00:11:16,120",
        "en": "Aku ingin salam dengan kau.",
        "pa": "I wish to shake your tail.",
        "vi": "Tôi muốn được bắt đuôi của chị."
    },
    {
        "start": "00:11:16,200",
        "end": "00:11:19,360",
        "en": "Itu saja? Oh, boleh.",
        "pa": "That's it? Sure.",
        "vi": "Thế thôi à? Được."
    },
    {
        "start": "00:11:20,360",
        "end": "00:11:21,600",
        "en": "Nah.",
        "pa": "Here.",
        "vi": "Đây."
    },
    {
        "start": "00:11:22,680",
        "end": "00:11:24,400",
        "en": "Tak sampailah.",
        "pa": "I can't reach.",
        "vi": "Tôi không với tới được."
    },
    {
        "start": "00:11:24,480",
        "end": "00:11:28,560",
        "en": "Macam mana nak salam? Badan kau kena belit, bukan.",
        "pa": "How are you going to shake my tail? You're all tied up.",
        "vi": "Làm sao ngươi bắt đuôi ta được? Ngươi bị siết chặt rồi mà."
    },
    {
        "start": "00:11:30,080",
        "end": "00:11:32,480",
        "en": "Tak apalah. Kau tepuk saja kepala aku.",
        "pa": "Never mind. A pat on the head will do.",
        "vi": "Đừng lo. Vỗ nhẹ vào đầu tôi một cái là được."
    },
    {
        "start": "00:11:41,080",
        "end": "00:11:44,520",
        "en": "Aduh! Sakitnya!",
        "pa": "That hurts!",
        "vi": "Đau quá!"
    },
    {
        "start": "00:11:44,600",
        "end": "00:11:47,480",
        "en": "Aku tak tipu! Sakit!",
        "pa": "No kidding, it hurts!",
        "vi": "Không đùa đâu, đau quá!"
    },
    {
        "start": "00:11:47,560",
        "end": "00:11:50,080",
        "en": "Lepaskan aku Kancil, lepaskan!",
        "pa": "Release me, release me!",
        "vi": "Thả ta ra!"
    },
    {
        "start": "00:11:50,160",
        "end": "00:11:52,760",
        "en": "Padan muka.",
        "pa": "-Serves you right! -Let go, Kancil!",
        "vi": "- Đáng đời! - Nhả ra đi, Kancil!"
    },
    {
        "start": "00:11:53,920",
        "end": "00:11:57,440",
        "en": "Lepaskan aku, Kancil! Lepaskan aku!",
        "pa": "Let me go!",
        "vi": "Nhả ra đi!"
    },
    {
        "start": "00:12:30,720",
        "end": "00:12:32,920",
        "en": "- Aduh. - Alamak.",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:12:59,120",
        "end": "00:13:01,680",
        "en": "Raja rimba sudah sampai pun.",
        "pa": "The mighty King of the Jungle has arrived.",
        "vi": "Chúa Tể Rừng Xanh đã đến rồi."
    },
    {
        "start": "00:13:01,760",
        "end": "00:13:04,160",
        "en": "Tengok! Ini hadiah untuk kau.",
        "pa": "Your gift awaits!",
        "vi": "Món quà đang đợi anh đấy."
    },
    {
        "start": "00:13:09,240",
        "end": "00:13:11,760",
        "en": "- Hadiah? - Marilah tengok.",
        "pa": "-Gift? -Have a look.",
        "vi": "- Quà à? - Nhìn đi."
    },
    {
        "start": "00:13:11,840",
        "end": "00:13:12,960",
        "en": "Yakah ini?",
        "pa": "Really?",
        "vi": "Thật ư?"
    },
    {
        "start": "00:13:13,040",
        "end": "00:13:16,440",
        "en": "Ya, hadiah. Cantik, bukan?",
        "pa": "Yes, a gift! Isn't it pretty?",
        "vi": "Phải, một món quà! Đẹp quá phải không?"
    },
    {
        "start": "00:13:21,960",
        "end": "00:13:23,320",
        "en": "Mahal ini.",
        "pa": "It's really expensive.",
        "vi": "Lại đắt tiền nữa."
    },
    {
        "start": "00:13:24,680",
        "end": "00:13:26,640",
        "en": "Ada aku tanya?",
        "pa": "Did I ask?",
        "vi": "Ta có đòi quà à?"
    },
    {
        "start": "00:13:26,720",
        "end": "00:13:29,960",
        "en": "Kau jangan main-main dengan aku.",
        "pa": "Don't play a fool with me.",
        "vi": "Đừng lừa gạt ta."
    },
    {
        "start": "00:13:37,800",
        "end": "00:13:40,760",
        "en": "Raja Sulaiman anugerahkan tali pinggang hikmat ini",
        "pa": "King Sulaiman awarded this magic belt to you, King of the Jungle.",
        "vi": "Vua Sulaiman đã trao tặng chiếc thắt lưng thần kì này cho anh."
    },
    {
        "start": "00:13:40,840",
        "end": "00:13:43,040",
        "en": "kepada engkau Raja rimba.",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:13:43,120",
        "end": "00:13:44,000",
        "en": "Bangga tak?",
        "pa": "Aren't you proud?",
        "vi": "Anh tự hào chứ?"
    },
    {
        "start": "00:13:50,360",
        "end": "00:13:52,440",
        "en": "Tapi kenapa aku tak tahu?",
        "pa": "Why haven't I heard about this?",
        "vi": "Sao ta không hay biết gì cả?"
    },
    {
        "start": "00:13:57,560",
        "end": "00:14:00,440",
        "en": "Gagak! Gagak tak beritahu kau?",
        "pa": "Didn't the crow tell you?",
        "vi": "Gagak chưa báo tin cho anh à?"
    },
    {
        "start": "00:14:02,200",
        "end": "00:14:05,960",
        "en": "Gagak. Dia lupalah itu.",
        "pa": "Poor crow, he must have forgotten.",
        "vi": "Gagak tội nghiệp, chắc cậu ta quên mất rồi."
    },
    {
        "start": "00:14:06,040",
        "end": "00:14:08,480",
        "en": "Habis itu, kenapa engkau yang pakai?",
        "pa": "I see. Why are you wearing it then?",
        "vi": "Hiểu rồi. Vậy sao ngươi lại mang nó?"
    },
    {
        "start": "00:14:08,560",
        "end": "00:14:13,320",
        "en": "Aku tak pakai. Aku tolong bawa saja. Tapi…",
        "pa": "I'm not. I just wanted to give it to you. But…",
        "vi": "Không phải. Tôi muốn đưa nó cho anh. Nhưng…"
    },
    {
        "start": "00:14:15,000",
        "end": "00:14:16,960",
        "en": "Aku terbelit.",
        "pa": "I got stuck.",
        "vi": "Tôi bị kẹt rồi."
    },
    {
        "start": "00:14:20,480",
        "end": "00:14:23,480",
        "en": "Kalau sudah terbelit…",
        "pa": "Well, if you're stuck…",
        "vi": "Nếu ngươi bị kẹt thì…"
    },
    {
        "start": "00:14:30,720",
        "end": "00:14:31,840",
        "en": "Apa hal?",
        "pa": "What's the problem?",
        "vi": "Có chuyện gì thế?"
    },
    {
        "start": "00:14:33,400",
        "end": "00:14:34,960",
        "en": "Sudah. Keluar.",
        "pa": "Get out!",
        "vi": "Ra ngoài!"
    },
    {
        "start": "00:14:36,760",
        "end": "00:14:39,400",
        "en": "Tak tahu pakai, tak payah pakai.",
        "pa": "If you don't know how to use it, don't try it!",
        "vi": "Nếu không biết cách sử dụng thì đừng có thử!"
    },
    {
        "start": "00:14:39,960",
        "end": "00:14:43,560",
        "en": "Tengok ini. Biar Raja rimba yang pakai.",
        "pa": "Here! Let the King of the Jungle wear it.",
        "vi": "Đây! Để Chúa Tể Rừng Xanh mang cho ngươi xem."
    },
    {
        "start": "00:14:44,200",
        "end": "00:14:45,280",
        "en": "Macam ini.",
        "pa": "Like this.",
        "vi": "Như thế này đây."
    },
    {
        "start": "00:14:49,320",
        "end": "00:14:52,840",
        "en": "Nak ke mana itu? Jangan nak lari.",
        "pa": "Where do you think you're going? You stay put.",
        "vi": "Ngươi đi đâu đấy? Đứng yên đó."
    },
    {
        "start": "00:14:57,200",
        "end": "00:14:59,400",
        "en": "Macam mana? Bergaya tak?",
        "pa": "How do I look? Good?",
        "vi": "Trông ta thế nào? Bảnh không?"
    },
    {
        "start": "00:14:59,960",
        "end": "00:15:02,200",
        "en": "Wah, bergayalah.",
        "pa": "Wow, so good!",
        "vi": "Rất bảnh!"
    },
    {
        "start": "00:15:03,200",
        "end": "00:15:06,560",
        "en": "Mestilah. Aku Raja Rimba, bukan!",
        "pa": "Of course, I'm the King of the Jungle!",
        "vi": "Dĩ nhiên rồi, ta là Chúa Tể Rừng Xanh mà!"
    },
    {
        "start": "00:15:09,560",
        "end": "00:15:10,960",
        "en": "Apa itu?",
        "pa": "What is that?",
        "vi": "Cái gì thế?"
    },
    {
        "start": "00:15:16,640",
        "end": "00:15:19,040",
        "en": "Tak apa. Biar aku tengokkan.",
        "pa": "Leave it to me! I'll have a look.",
        "vi": "Để đó cho tôi! Tôi sẽ xem cho."
    },
    {
        "start": "00:15:21,240",
        "end": "00:15:23,240",
        "en": "Boleh juga. Cepat sikit!",
        "pa": "Good idea. Hurry up!",
        "vi": "Ý hay đấy. Nhanh đi!"
    },
    {
        "start": "00:15:27,120",
        "end": "00:15:28,640",
        "en": "Ada apa itu Kancil?",
        "pa": "What is it?",
        "vi": "Cái gì thế?"
    },
    {
        "start": "00:15:29,760",
        "end": "00:15:33,640",
        "en": "Ini. Patutlah nampak longgar. Kejap.",
        "pa": "I see, no wonder it's loose. One moment, please.",
        "vi": "Hiểu rồi, thảo nào nó bị lỏng. Đợi tôi một chút."
    },
    {
        "start": "00:15:37,240",
        "end": "00:15:39,760",
        "en": "Sekarang baru ketat.",
        "pa": "Now it fits!",
        "vi": "Giờ thì vừa rồi!"
    },
    {
        "start": "00:15:40,880",
        "end": "00:15:42,400",
        "en": "Terima kasih Kancil.",
        "pa": "Thank you, Kancil.",
        "vi": "Cảm ơn, Kancil."
    },
    {
        "start": "00:15:42,480",
        "end": "00:15:46,800",
        "en": "Menyusahkan kau saja. Kirim salamlah dekat Raja Sulaiman.",
        "pa": "Please send my regards to King Sulaiman.",
        "vi": "Gửi lời cảm ơn của ta đến Vua Sulaiman nhé."
    },
    {
        "start": "00:15:46,880",
        "end": "00:15:48,520",
        "en": "Aku suka tali pinggang ini.",
        "pa": "I really like this belt!",
        "vi": "Ta rất thích chiếc thắt lưng này!"
    },
    {
        "start": "00:15:51,080",
        "end": "00:15:53,640",
        "en": "Macam ketat sangat saja.",
        "pa": "It is a bit tight though.",
        "vi": "Dù hơi chật một chút."
    },
    {
        "start": "00:15:53,720",
        "end": "00:15:57,320",
        "en": "- Kancil, oh Kancil? - Siapa kau?",
        "pa": "Who are you?",
        "vi": "- Kancil… - Ngươi là ai?"
    },
    {
        "start": "00:15:58,600",
        "end": "00:15:59,520",
        "en": "Kau siapa?",
        "pa": "Who are you?",
        "vi": "Ngươi là ai?"
    },
    {
        "start": "00:16:01,400",
        "end": "00:16:04,640",
        "en": "Tak kisahlah. Kau lagi besar!",
        "pa": "I don't care. You're bigger!",
        "vi": "Ta không quan tâm. Ngươi to hơn!"
    },
    {
        "start": "00:16:09,880",
        "end": "00:16:11,560",
        "en": "Siap kau.",
        "pa": "I'll teach you…",
        "vi": "Ta sẽ dạy cho ngươi…"
    },
    {
        "start": "00:16:24,640",
        "end": "00:16:25,760",
        "en": "Rasakan!",
        "pa": "I'll teach you…\n Take that!",
        "vi": "Nhận lấy này!"
    },
    {
        "start": "00:16:42,120",
        "end": "00:16:43,560",
        "en": "Ambil kau!",
        "pa": "How do you like that?!",
        "vi": "Thấy thế nào hả?"
    },
    {
        "start": "00:16:43,640",
        "end": "00:16:46,320",
        "en": "Lepaskan aku!",
        "pa": "Let go of me!",
        "vi": "Thả ta ra!"
    },
    {
        "start": "00:16:46,400",
        "end": "00:16:49,600",
        "en": "Kau lepaskan dulu!",
        "pa": "You let go of me first!",
        "vi": "Ngươi thả ta ra trước!"
    },
    {
        "start": "00:16:51,520",
        "end": "00:16:53,280",
        "en": "Lepas!",
        "pa": "Let me go!",
        "vi": "Thả ta ra!"
    },
    {
        "start": "00:16:53,360",
        "end": "00:16:57,160",
        "en": "Kancil. Nasib baik kau bijak.",
        "pa": "Kancil. You are wise indeed!",
        "vi": "Kancil, cậu đúng là mưu trí!"
    },
    {
        "start": "00:16:57,240",
        "end": "00:17:00,840",
        "en": "- Habislah kau! - Matilah!",
        "pa": "-Serves you right. -Kancil!",
        "vi": "- Đáng đời lắm. - Kancil!"
    },
    {
        "start": "00:17:02,000",
        "end": "00:17:06,319",
        "en": "- Sang Kancil! - Sang Kancil!",
        "pa": "",
        "vi": "- Kancil… - Kancil…"
    },
    {
        "start": "00:17:20,160",
        "end": "00:17:22,599",
        "en": "Habislah cerita.",
        "pa": "The end.",
        "vi": "Hết chuyện."
    },
    {
        "start": "00:17:22,680",
        "end": "00:17:28,240",
        "en": "Aki, kenapa Tupai cakap ada udang di sebalik batu?",
        "pa": "Aki! Why did Tupai say something was fishy?",
        "vi": "Ông ơi! Tại sao Tupai lại nói có mùi đáng nghi lắm đây ạ?"
    },
    {
        "start": "00:17:29,280",
        "end": "00:17:32,680",
        "en": "Itulah maksudnya orang yang ada muslihat.",
        "pa": "That means someone is up to something.",
        "vi": "Câu đó nghĩa là một ai đó đang có âm mưu gì đấy."
    },
    {
        "start": "00:17:32,760",
        "end": "00:17:36,880",
        "en": "Macam Ular Sawa. Dia pura-pura saja minta tolong.",
        "pa": "Just like Ular! She was only pretending to ask for help.",
        "vi": "Giống Ular đấy! Nó chỉ giả vờ nhờ giúp đỡ thôi."
    },
    {
        "start": "00:17:36,960",
        "end": "00:17:41,400",
        "en": "Telur dia tak kena curi pun. Sebenarnya dia nak makan Sang Kancil.",
        "pa": "Her eggs weren't stolen. What she really wanted was to eat Kancil.",
        "vi": "Ular không bị trộm trứng mà chỉ muốn ăn thịt Kancil thôi."
    },
    {
        "start": "00:17:41,480",
        "end": "00:17:43,600",
        "en": "Jahatnya Ular itu!",
        "pa": "Ular is so mean!",
        "vi": "Loài trăn xấu xa thật đấy!"
    },
    {
        "start": "00:17:43,680",
        "end": "00:17:47,400",
        "en": "Sebab itu Aris tak nak Aki lepaskan dia. Pukul biar mati.",
        "pa": "You should have killed that python, Aki!",
        "vi": "Lẽ ra ông nên đập chết nó đi!"
    },
    {
        "start": "00:17:49,000",
        "end": "00:17:52,920",
        "en": "Ular pun nak hidup juga. Kalau Ular tak ada,",
        "pa": "Pythons have to live too!",
        "vi": "Trăn cũng phải sinh tồn!"
    },
    {
        "start": "00:17:53,000",
        "end": "00:17:55,360",
        "en": "habislah padi dimakan tikus.",
        "pa": "If there weren't any pythons, all these crops would be eaten by rats.",
        "vi": "Nếu không có trăn, mùa màng sẽ bị chuột phá hết đấy cháu."
    },
    {
        "start": "00:17:55,440",
        "end": "00:17:57,880",
        "en": "Itu kitaran alam, bukan.",
        "pa": "Rats eat the crops, and pythons eat the rats.",
        "vi": "Chuột ăn cây trồng, còn trăn thì ăn chuột."
    },
    {
        "start": "00:17:57,960",
        "end": "00:18:01,360",
        "en": "Padi dimakan tikus, tikus dimakan ular.",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:18:01,440",
        "end": "00:18:05,000",
        "en": "Semua kehidupan ini, ada keperluannya.",
        "pa": "Everything in this world has its purpose.",
        "vi": "Mọi thứ tồn tại trên đời đều có mục đích cả."
    },
    {
        "start": "00:18:06,080",
        "end": "00:18:09,400",
        "en": "Habis itu, binatang apa yang makan ular?",
        "pa": "Then which animal eats the python?",
        "vi": "Vậy con gì ăn con trăn ạ?"
    },
    {
        "start": "00:18:12,200",
        "end": "00:18:13,680",
        "en": "Sudah sampai pun.",
        "pa": "We're home!",
        "vi": "Về đến nhà rồi!"
    },
    {
        "start": "00:18:13,760",
        "end": "00:18:16,000",
        "en": "Cepat Aki, cepat!",
        "pa": "Hurry up, Aki!",
        "vi": "Nhanh lên, ông ơi!"
    },
    {
        "start": "00:18:19,080",
        "end": "00:18:20,760",
        "en": "Mama! Papa!",
        "pa": "",
        "vi": "Bố ơi, mẹ ơi…"
    },
    {
        "start": "00:18:21,720",
        "end": "00:18:25,200",
        "en": "Tadi Mama, Ara nampak ular!",
        "pa": "Mom, I saw a python!",
        "vi": "Mẹ ơi, con đã nhìn thấy trăn đấy!"
    },
    {
        "start": "00:18:25,720",
        "end": "00:18:28,960",
        "en": "- Ular? - Ya, besar.",
        "pa": "-A python? -Yeah, it was huge!",
        "vi": "- Trăn à? - Phải, nó to lắm!"
    },
    {
        "start": "00:18:29,040",
        "end": "00:18:30,920",
        "en": "Bahayanya!",
        "pa": "That's dangerous!",
        "vi": "Nguy hiểm quá!"
    },
    {
        "start": "00:18:31,000",
        "end": "00:18:33,400",
        "en": "Tak apa. Sudah buang.",
        "pa": "Don't worry, I got rid of it.",
        "vi": "Đừng lo, bố đuổi nó đi rồi."
    },
    {
        "start": "00:18:33,480",
        "end": "00:18:37,240",
        "en": "Jangan main-main. Aki engkau itu hero!",
        "pa": "Don't you know Aki is a superhero?",
        "vi": "Các con không biết ông là anh hùng à?"
    },
    {
        "start": "00:18:38,120",
        "end": "00:18:40,520",
        "en": "- Betul itu Papa. - Ada saja kau ini.",
        "pa": "-That's right, Dad. -Nonsense.",
        "vi": "- Đúng đấy ạ. - Vớ vẩn."
    },
    {
        "start": "00:18:40,600",
        "end": "00:18:43,760",
        "en": "Sudah. Mari Ara, ikut Wan buat teh.",
        "pa": "Come, Ara, let's make some tea.",
        "vi": "Nào Ara, đi pha trà nào."
    },
    {
        "start": "00:18:45,400",
        "end": "00:18:48,760",
        "en": "Akinya, mana gula?",
        "pa": "Aki? Where's the sugar?",
        "vi": "Ông ơi? Đường đâu?"
    },
    {
        "start": "00:18:48,840",
        "end": "00:18:52,280",
        "en": "Alamak, lupa pula.",
        "pa": "I'm sorry! I forgot!",
        "vi": "Xin lỗi! Tôi quên mua rồi!"
    },
    {
        "start": "00:18:53,600",
        "end": "00:18:55,680",
        "en": "Tidur sajalah.",
        "pa": "Off to bed then.",
        "vi": "Vậy đi ngủ thôi."
    },
    {
        "start": "00:18:55,760",
        "end": "00:18:58,040",
        "en": "Aduhai.",
        "pa": "-No! -No!",
        "vi": "- Không! - Không!"
    },
    {
        "start": "00:19:26,920",
        "end": "00:19:28,920",
        "en": "",
        "pa": "",
        "vi": "Biên dịch: Hoàng Nguyên"
    }
]