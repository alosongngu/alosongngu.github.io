[
    {
        "start": "00:00:16,600",
        "end": "00:00:18,080",
        "en": "Aki ada satu cerita.",
        "pa": "Listen to this story.",
        "vi": "Nghe câu chuyện này nhé."
    },
    {
        "start": "00:00:20,720",
        "end": "00:00:23,480",
        "en": "Pada zaman dahulu",
        "pa": "Once upon a time…",
        "vi": "Ngày xửa ngày xưa…"
    },
    {
        "start": "00:00:34,080",
        "end": "00:00:35,320",
        "en": "Dapat gula-gula!",
        "pa": "Yay, candy!",
        "vi": "Kẹo!"
    },
    {
        "start": "00:00:35,400",
        "end": "00:00:39,240",
        "en": "Nampak. Kalau tak, habuk pun tak dapat.",
        "pa": "Otherwise, you wouldn't even get one!",
        "vi": "Nếu không thì một cái cũng không có!"
    },
    {
        "start": "00:00:53,920",
        "end": "00:00:58,200",
        "en": "Tak sabarnya cucu Aki. Nak tinggalkan Aki, bukan?",
        "pa": "My little grandchildren. Can't wait to leave me, eh?",
        "vi": "Cháu của ông, hai cháu nôn được về nhà quá nhỉ?"
    },
    {
        "start": "00:01:00,560",
        "end": "00:01:04,040",
        "en": "Taklah. Aki tetap nombor satu.",
        "pa": "Of course not! You're still number one, Aki.",
        "vi": "Dĩ nhiên là không rồi ạ! Ông vẫn là số một mà."
    },
    {
        "start": "00:01:04,760",
        "end": "00:01:08,960",
        "en": "Yakah? Dahlah, mari makan.",
        "pa": "Really? Come, let's eat.",
        "vi": "Thật à? Đến đây cùng ăn nào."
    },
    {
        "start": "00:01:11,160",
        "end": "00:01:15,200",
        "en": "Aki. Ini ubi yang Aris tanam, betulkah?",
        "pa": "Aki, are these the cassava I planted?",
        "vi": "Ông à, đây là khoai mì cháu trồng à?"
    },
    {
        "start": "00:01:15,880",
        "end": "00:01:19,920",
        "en": "Tak. Ini yang Aki tanam. Kau punya dah mati.",
        "pa": "No, I planted these. Yours are dead.",
        "vi": "Không, đây là khoai ông trồng. Khoai cháu trồng chết cả rồi."
    },
    {
        "start": "00:01:22,800",
        "end": "00:01:23,800",
        "en": "Gelak pula.",
        "pa": "-How dare you laugh at me?! -That's a lot!",
        "vi": ""
    },
    {
        "start": "00:01:23,880",
        "end": "00:01:25,360",
        "en": "Banyaknya.",
        "pa": "",
        "vi": "Nhiều quá!"
    },
    {
        "start": "00:01:26,000",
        "end": "00:01:29,320",
        "en": "Inilah hasilnya kalau kita berbudi dengan tanah.",
        "pa": "This is what we get when we're kind to the earth.",
        "vi": "Đây là những gì chúng ta có được nếu đối xử tử tế với thiên nhiên."
    },
    {
        "start": "00:01:30,560",
        "end": "00:01:32,920",
        "en": "Ara, cepatlah bagi!",
        "pa": "Ara, give it to him.",
        "vi": "Ara, đưa cho ông đi."
    },
    {
        "start": "00:01:36,400",
        "end": "00:01:38,280",
        "en": "Ini untuk Aki!",
        "pa": "For Aki!",
        "vi": "Cái này cho ông ạ!"
    },
    {
        "start": "00:01:38,920",
        "end": "00:01:42,040",
        "en": "Ini apa? Susah-susah saja.",
        "pa": "What's this? You shouldn't have…",
        "vi": "Cái gì thế? Các cháu không cần phải…"
    },
    {
        "start": "00:01:42,800",
        "end": "00:01:44,560",
        "en": "Tak apa Aki.",
        "pa": "No problem, Aki.",
        "vi": "Không có chi ạ."
    },
    {
        "start": "00:01:44,640",
        "end": "00:01:48,480",
        "en": "Cikgu Aris kata, hancur badan",
        "pa": "Like my teacher said, \"A good deed…\"",
        "vi": "Như cô cháu đã dạy: \"Làm việc tốt…\""
    },
    {
        "start": "00:01:52,200",
        "end": "00:01:55,480",
        "en": "- Budi Aki kita kenang. - Budi Aki kita kenang!",
        "pa": "-Aki's kindness is never lost! -Aki's kindness is never lost!",
        "vi": "- Lòng tốt của ông sẽ được ghi nhớ! - Lòng tốt của ông sẽ được ghi nhớ!"
    },
    {
        "start": "00:01:55,560",
        "end": "00:01:58,360",
        "en": "Peribahasa itu.",
        "pa": "Oh, that idiom.",
        "vi": "Ra là câu đó."
    },
    {
        "start": "00:01:58,440",
        "end": "00:02:03,280",
        "en": "Hancur badan di kandung tanah, budi yang baik dikenang juga.",
        "pa": "A good deed is never lost.",
        "vi": "Làm ơn không bao giờ thiệt."
    },
    {
        "start": "00:02:03,360",
        "end": "00:02:08,120",
        "en": "Itulah. Aki jaga kami dengan baik.",
        "pa": "That's the one! You took such good care of us.",
        "vi": "Đúng là câu đó! Ông đã chăm sóc chúng cháu rất tốt."
    },
    {
        "start": "00:02:08,199",
        "end": "00:02:11,640",
        "en": "Apa yang kita nak, semua Aki bagi.",
        "pa": "You gave us whatever we wanted.",
        "vi": "Chúng cháu muốn gì ông đều cho."
    },
    {
        "start": "00:02:11,720",
        "end": "00:02:14,000",
        "en": "Aki sayang kami, betul tak?",
        "pa": "You love us, right, Aki?",
        "vi": "Ông thương bọn cháu mà, đúng không ông?"
    },
    {
        "start": "00:02:14,080",
        "end": "00:02:18,600",
        "en": "Mestilah sayang. Kau orang cucu Aki.",
        "pa": "Of course I do. You're my grandchildren.",
        "vi": "Dĩ nhiên rồi. Các cháu là cháu của ông mà."
    },
    {
        "start": "00:02:19,320",
        "end": "00:02:23,320",
        "en": "Aki ada cerita? Satu saja.",
        "pa": "Can you tell us a story? Just one.",
        "vi": "Ông kể chuyện cho chúng cháu được không? Một câu chuyện thôi ạ."
    },
    {
        "start": "00:02:23,400",
        "end": "00:02:25,880",
        "en": "Ya, sebelum mama dan papa sampai.",
        "pa": "Before Mom and Dad get here.",
        "vi": "Trước khi bố mẹ đến ạ."
    },
    {
        "start": "00:02:26,960",
        "end": "00:02:28,720",
        "en": "Dah nak balik pun nak dengar cerita?",
        "pa": "You still want to hear a story?",
        "vi": "Hai cháu vẫn muốn nghe kể chuyện à?"
    },
    {
        "start": "00:02:30,440",
        "end": "00:02:33,200",
        "en": "- Boleh tak Aki? - Yalah. Nak dengar cerita apa?",
        "pa": "All right. What kind of story?",
        "vi": "Được rồi. Chuyện gì đây?"
    },
    {
        "start": "00:02:34,440",
        "end": "00:02:36,640",
        "en": "Kancil yang bijak.",
        "pa": "The Clever Kancil!",
        "vi": "Về Kancil mưu trí ạ!"
    },
    {
        "start": "00:02:37,560",
        "end": "00:02:39,560",
        "en": "Ada tak cerita itu, Aki?",
        "pa": "Come on, Aki.",
        "vi": "Đi mà, ông."
    },
    {
        "start": "00:02:40,440",
        "end": "00:02:42,960",
        "en": "Pada zaman dahulu",
        "pa": "Once upon a time…",
        "vi": "Ngày xửa ngày xưa…"
    },
    {
        "start": "00:03:22,280",
        "end": "00:03:24,840",
        "en": "Mengantuknya.",
        "pa": "So sleepy…",
        "vi": "Buồn ngủ quá…"
    },
    {
        "start": "00:03:28,200",
        "end": "00:03:29,440",
        "en": "Dah tidur?",
        "pa": "Sleeping already?",
        "vi": "Mới giờ mà đã ngủ rồi à?"
    },
    {
        "start": "00:03:44,640",
        "end": "00:03:46,320",
        "en": "Tikus!",
        "pa": "Tikus!",
        "vi": "Tikus!"
    },
    {
        "start": "00:03:47,480",
        "end": "00:03:49,240",
        "en": "Kancil? Ada apa?",
        "pa": "Kancil? What's up?",
        "vi": "Kancil? Có chuyện gì thế?"
    },
    {
        "start": "00:03:50,200",
        "end": "00:03:53,560",
        "en": "Jangan bising. Itu Singa.",
        "pa": "Pipe down! See that? It's Singa.",
        "vi": "Khẽ thôi! Thấy chưa? Singa kia kìa."
    },
    {
        "start": "00:03:55,760",
        "end": "00:03:58,120",
        "en": "Singa? Itu apa?",
        "pa": "Singa? Who's that?",
        "vi": "Singa? Là ai thế?"
    },
    {
        "start": "00:03:59,520",
        "end": "00:04:00,880",
        "en": "Kau tak tahu?",
        "pa": "You don't know?",
        "vi": "Cậu không biết à?"
    },
    {
        "start": "00:04:00,960",
        "end": "00:04:05,040",
        "en": "Singa macam Harimau. Tapi dia lagi ganas.",
        "pa": "Singa is just like Harimau. But he's more violent!",
        "vi": "Singa cũng giống Harimau vậy. Nhưng hắn còn hung tợn hơn!"
    },
    {
        "start": "00:04:05,560",
        "end": "00:04:10,440",
        "en": "Kau jangan dekat dengan dia. Bahaya! Nanti dia makan kau.",
        "pa": "Don't go near him. It's dangerous. He'll eat you up!",
        "vi": "Đừng đến gần hắn. Nguy hiểm lắm. Hắn sẽ ăn thịt cậu đấy!"
    },
    {
        "start": "00:04:13,400",
        "end": "00:04:16,040",
        "en": "Baiklah. Terima kasih, Kancil.",
        "pa": "All right. Thanks, Kancil.",
        "vi": "Được rồi. Cảm ơn Kancil."
    },
    {
        "start": "00:04:17,399",
        "end": "00:04:18,880",
        "en": "Jumpa lagi.",
        "pa": "See you!",
        "vi": "Hẹn gặp lại!"
    },
    {
        "start": "00:04:25,800",
        "end": "00:04:27,840",
        "en": "Dapat pun!",
        "pa": "Got it!",
        "vi": "Được rồi!"
    },
    {
        "start": "00:04:45,560",
        "end": "00:04:46,520",
        "en": "Selamat",
        "pa": "That was close.",
        "vi": "Suýt nữa thì."
    },
    {
        "start": "00:04:58,000",
        "end": "00:05:03,040",
        "en": "Tikus! Kenapa kau ganggu aku tidur?",
        "pa": "Tikus, why did you wake me up?",
        "vi": "Tikus, sao ngươi lại làm ta thức giấc?"
    },
    {
        "start": "00:05:03,120",
        "end": "00:05:05,560",
        "en": "Aku tak sengaja.",
        "pa": "I… It was an accident.",
        "vi": "Tôi chỉ… lỡ tay thôi."
    },
    {
        "start": "00:05:05,640",
        "end": "00:05:08,200",
        "en": "Tak sengaja?",
        "pa": "An accident?",
        "vi": "Lỡ tay à?"
    },
    {
        "start": "00:05:11,320",
        "end": "00:05:13,760",
        "en": "Jangan!",
        "pa": "No!",
        "vi": "Không!"
    },
    {
        "start": "00:05:16,560",
        "end": "00:05:19,200",
        "en": "Tolong! Tolong aku!",
        "pa": "Help! Help me!",
        "vi": "Cứu! Cứu tôi với!"
    },
    {
        "start": "00:05:20,440",
        "end": "00:05:22,880",
        "en": "Kenapa kau kacau aku?",
        "pa": "Why did you bother me?",
        "vi": "Sao ngươi lại làm phiền ta?"
    },
    {
        "start": "00:05:22,960",
        "end": "00:05:28,720",
        "en": "Sekarang aku dah terjaga dan aku lapar.",
        "pa": "Now, I'm awake and I'm hungry.",
        "vi": "Giờ ta đã thức dậy và ta đang đói đây."
    },
    {
        "start": "00:05:29,320",
        "end": "00:05:32,360",
        "en": "Tolong!",
        "pa": "Help! Help me!",
        "vi": "Cứu! Cứu tôi với!"
    },
    {
        "start": "00:05:32,440",
        "end": "00:05:33,600",
        "en": "Berhenti!",
        "pa": "Stop!",
        "vi": "Dừng lại!"
    },
    {
        "start": "00:05:37,040",
        "end": "00:05:40,200",
        "en": "Kancil, tolong!",
        "pa": "Kancil, help me…",
        "vi": "Kancil, cứu tôi với…"
    },
    {
        "start": "00:05:42,320",
        "end": "00:05:45,680",
        "en": "Singa, kenapa kau nak makan Tikus?",
        "pa": "Singa, why do you want to eat Tikus?",
        "vi": "Singa, sao anh lại muốn ăn thịt Tikus?"
    },
    {
        "start": "00:05:45,760",
        "end": "00:05:50,720",
        "en": "Kenapa? Sebab dia kacau aku tidur.",
        "pa": "Why? Because he bothered me.",
        "vi": "Vì sao ư? Vì nó làm phiền ta."
    },
    {
        "start": "00:05:50,800",
        "end": "00:05:55,600",
        "en": "Siapa kacau aku tidur, jadi makanan aku.",
        "pa": "And whoever bothers me will be my lunch.",
        "vi": "Ai làm phiền ta thì sẽ bị ta biến thành bữa trưa."
    },
    {
        "start": "00:05:55,680",
        "end": "00:05:57,960",
        "en": "Betul tak, Tikus?",
        "pa": "Isn't that right, Tikus?",
        "vi": "Đúng không, Tikus?"
    },
    {
        "start": "00:06:01,720",
        "end": "00:06:04,600",
        "en": "Singa, cuba kau tengok Tikus.",
        "pa": "Singa, take a good look at Tikus.",
        "vi": "Singa, nhìn Tikus cho kĩ vào."
    },
    {
        "start": "00:06:04,680",
        "end": "00:06:10,400",
        "en": "Mana boleh Singa yang gagah macam kau, makan Tikus yang kecil macam itu.",
        "pa": "How can a strong lion like you eat just a small mouse?",
        "vi": "Tại sao một con sư tử mạnh mẽ như anh lại đi ăn thịt một con chuột bé tí?"
    },
    {
        "start": "00:06:11,360",
        "end": "00:06:12,520",
        "en": "Betul!",
        "pa": "Yes, yes, yes!",
        "vi": "Đúng rồi đó!"
    },
    {
        "start": "00:06:13,440",
        "end": "00:06:17,920",
        "en": "Tikus kecil pun, makanan juga.",
        "pa": "He's still food!",
        "vi": "Nó vẫn là thức ăn mà!"
    },
    {
        "start": "00:06:27,120",
        "end": "00:06:29,160",
        "en": "Maafkan aku, Singa.",
        "pa": "I'm sorry, Singa!",
        "vi": "Xin lỗi, Singa!"
    },
    {
        "start": "00:06:29,680",
        "end": "00:06:32,160",
        "en": "Tolonglah lepaskan aku.",
        "pa": "Please let me go.",
        "vi": "Làm ơn tha cho tôi đi."
    },
    {
        "start": "00:06:32,240",
        "end": "00:06:36,720",
        "en": "Suatu hari nanti, aku akan membalas kebaikan kau.",
        "pa": "Someday, I will repay your kindness!",
        "vi": "Ngày nào đó, tôi sẽ trả ơn cho anh!"
    },
    {
        "start": "00:06:53,160",
        "end": "00:06:55,440",
        "en": "Kau nak tolong aku?",
        "pa": "You will help me?",
        "vi": "Ngươi sẽ giúp ta à?"
    },
    {
        "start": "00:06:55,520",
        "end": "00:06:57,160",
        "en": "Jangan nak buat lawak.",
        "pa": "You're joking, right?",
        "vi": "Ngươi đang đùa phải không?"
    },
    {
        "start": "00:06:57,240",
        "end": "00:07:02,920",
        "en": "Kau, Tikus yang kecil. Nak tolong aku, Singa yang gagah?",
        "pa": "A tiny mouse like you wants to help me, the great lion?",
        "vi": "Một con chuột bé tí như ngươi muốn giúp sư tử vĩ đại như ta ư?"
    },
    {
        "start": "00:07:21,080",
        "end": "00:07:23,840",
        "en": "Pergilah. Aku lepaskan kau.",
        "pa": "Fine, I will release you.",
        "vi": "Được, ta sẽ thả ngươi."
    },
    {
        "start": "00:07:23,920",
        "end": "00:07:29,160",
        "en": "Manalah tahu, suatu hari nanti aku perlukan pertolongan kau.",
        "pa": "Who knows, I might need your help in the future.",
        "vi": "Ai mà biết được, có thể ta sẽ cần ngươi giúp một ngày nào đó."
    },
    {
        "start": "00:07:31,200",
        "end": "00:07:32,280",
        "en": "Betulkah, Singa?",
        "pa": "You mean it, Singa?",
        "vi": "Thật à, Singa?"
    },
    {
        "start": "00:07:33,120",
        "end": "00:07:37,120",
        "en": "Pergi cepat. Sebelum aku tukar fikiran.",
        "pa": "Go! Before I change my mind.",
        "vi": "Đi đi! Trước khi ta đổi ý."
    },
    {
        "start": "00:07:45,000",
        "end": "00:07:47,240",
        "en": "Baik sungguh hati kau, Singa.",
        "pa": "That was kind of you--",
        "vi": "Anh thật là tốt bụng…"
    },
    {
        "start": "00:07:50,400",
        "end": "00:07:53,560",
        "en": "Singa, kenapa kau pandang aku begitu?",
        "pa": "Singa, why are you looking at me like that?",
        "vi": "Singa, sao anh tại nhìn tôi như vậy?"
    },
    {
        "start": "00:07:53,640",
        "end": "00:08:00,360",
        "en": "Buat apa nak makan Tikus, bila ada Kancil yang enak di depan mata.",
        "pa": "Why eat Tikus when Kancil is right in front of you?",
        "vi": "Sao ta phải ăn Tikus khi mà Kancil ở ngay trước mặt chứ?"
    },
    {
        "start": "00:08:02,880",
        "end": "00:08:05,560",
        "en": "Tolong!",
        "pa": "Help!",
        "vi": "Cứu tôi với!"
    },
    {
        "start": "00:08:07,040",
        "end": "00:08:09,280",
        "en": "Tidak!",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:09:08,800",
        "end": "00:09:10,240",
        "en": "Selamat!",
        "pa": "Safe at last!",
        "vi": "Cuối cùng cũng an toàn!"
    },
    {
        "start": "00:09:21,240",
        "end": "00:09:22,800",
        "en": "Baru kacak.",
        "pa": "Hello, handsome.",
        "vi": "Xin chào anh chàng đẹp trai."
    },
    {
        "start": "00:09:25,840",
        "end": "00:09:27,640",
        "en": "Laparlah pula.",
        "pa": "So hungry…",
        "vi": "Đói quá…"
    },
    {
        "start": "00:09:28,680",
        "end": "00:09:29,720",
        "en": "Pemburu?",
        "pa": "The hunter?",
        "vi": "Thợ săn?"
    },
    {
        "start": "00:09:37,000",
        "end": "00:09:39,120",
        "en": "Makanan!",
        "pa": "Lunch!",
        "vi": "Bữa trưa!"
    },
    {
        "start": "00:09:55,360",
        "end": "00:09:56,760",
        "en": "- Aduh, panas. - Kenapa?",
        "pa": "-Ouch, it's hot. -What's the matter?",
        "vi": "- Nóng quá. - Gì thế?"
    },
    {
        "start": "00:09:58,640",
        "end": "00:09:59,920",
        "en": "Panas.",
        "pa": "It's too hot.",
        "vi": "Nóng quá ạ."
    },
    {
        "start": "00:10:04,360",
        "end": "00:10:07,520",
        "en": "Memanglah panas. Tiup dulu.",
        "pa": "It is hot. Just blow first.",
        "vi": "Nó vẫn còn nóng. Cháu phải thổi trước."
    },
    {
        "start": "00:10:10,360",
        "end": "00:10:11,960",
        "en": "Jangan gelojoh.",
        "pa": "Eat slowly.",
        "vi": "Ăn chậm thôi."
    },
    {
        "start": "00:10:12,040",
        "end": "00:10:14,920",
        "en": "Aki, cepatlah sambung cerita itu.",
        "pa": "Aki, tell us more!",
        "vi": "Ông à, kể tiếp đi!"
    },
    {
        "start": "00:10:15,000",
        "end": "00:10:17,160",
        "en": "Apa jadi dekat Kancil?",
        "pa": "What happened to Kancil?",
        "vi": "Chuyện gì xảy ra với Kancil ạ?"
    },
    {
        "start": "00:10:18,160",
        "end": "00:10:21,000",
        "en": "- Kejap - Aki!",
        "pa": "-Hold on… -Aki!",
        "vi": "- Đợi đã… - Ông!"
    },
    {
        "start": "00:10:22,040",
        "end": "00:10:23,240",
        "en": "Lepas itu",
        "pa": "And then…",
        "vi": "Sau đó…"
    },
    {
        "start": "00:10:45,960",
        "end": "00:10:47,440",
        "en": "Apa yang kau buat, Kancil?",
        "pa": "What have you done, Kancil!",
        "vi": "Ngươi đã làm gì thế, Kancil!"
    },
    {
        "start": "00:10:49,200",
        "end": "00:10:50,880",
        "en": "Padan muka kau, Singa.",
        "pa": "Serves you right, Singa!",
        "vi": "Đáng đời anh, Singa!"
    },
    {
        "start": "00:10:50,960",
        "end": "00:10:54,120",
        "en": "Itulah, nak makan aku lagi.",
        "pa": "That's what you get for chasing me.",
        "vi": "Đó là cái giá phải trả vì đuổi bắt tôi đấy."
    },
    {
        "start": "00:10:56,200",
        "end": "00:10:58,640",
        "en": "Kancil. Jangan tinggalkan aku.",
        "pa": "Kancil! Don't leave me!",
        "vi": "Kancil! Đừng bỏ mặc ta!"
    },
    {
        "start": "00:10:58,720",
        "end": "00:11:00,600",
        "en": "Lepaskan aku.",
        "pa": "Let me go!",
        "vi": "Thả ta ra đi!"
    },
    {
        "start": "00:11:00,680",
        "end": "00:11:01,880",
        "en": "Lepaskan kau?",
        "pa": "Let you go?",
        "vi": "Thả anh ra?"
    },
    {
        "start": "00:11:02,960",
        "end": "00:11:05,080",
        "en": "Jangan mimpi.",
        "pa": "In your dreams.",
        "vi": "Cứ mơ đi…"
    },
    {
        "start": "00:11:06,160",
        "end": "00:11:09,400",
        "en": "Tinggallah kau di sini, sampai pemburu datang.",
        "pa": "You stay put until the hunter gets here.",
        "vi": "Anh cứ ở yên đó đến khi gã thợ săn đến."
    },
    {
        "start": "00:11:10,000",
        "end": "00:11:11,280",
        "en": "Pemburu?",
        "pa": "The hunter?",
        "vi": "Thợ săn?"
    },
    {
        "start": "00:11:12,040",
        "end": "00:11:15,520",
        "en": "Tolong aku Kancil. Mati aku kalau pemburu datang.",
        "pa": "Help me, Kancil. I'll die if the hunter comes.",
        "vi": "Cứu ta, Kancil. Ta sẽ chết mất nếu thợ săn đến."
    },
    {
        "start": "00:11:16,800",
        "end": "00:11:20,760",
        "en": "Kau hebat. Lepaskanlah diri sendiri.",
        "pa": "You're big and strong. Find your own way.",
        "vi": "Anh vừa to vừa khỏe. Tự tìm cách đi."
    },
    {
        "start": "00:11:24,760",
        "end": "00:11:27,400",
        "en": "Kasihanilah aku Kancil.",
        "pa": "Have mercy, Kancil!",
        "vi": "Hãy rủ lòng thương ta, Kancil!"
    },
    {
        "start": "00:11:28,000",
        "end": "00:11:32,560",
        "en": "Ingat tak hari itu, aku pernah lepaskan kau dan Tikus.",
        "pa": "Remember the day I let you and Tikus go?",
        "vi": "Nhớ cái ngày ta tha cho ngươi và Tikus không?"
    },
    {
        "start": "00:11:36,680",
        "end": "00:11:38,560",
        "en": "Ingat tak Kancil?",
        "pa": "Do you remember, Kancil?",
        "vi": "Nhớ không, Kancil?"
    },
    {
        "start": "00:11:40,800",
        "end": "00:11:41,920",
        "en": "Yalah.",
        "pa": "Fine.",
        "vi": "Được rồi."
    },
    {
        "start": "00:12:02,680",
        "end": "00:12:04,280",
        "en": "Aku dah tahu!",
        "pa": "I got it!",
        "vi": "Mình biết rồi!"
    },
    {
        "start": "00:12:08,200",
        "end": "00:12:13,440",
        "en": "Tunggu. Jangan pergi. Lepaskan aku dulu!",
        "pa": "Wait! Don't go! Let me go!",
        "vi": "Đợi đã! Đừng đi! Thả ta ra!"
    },
    {
        "start": "00:12:16,480",
        "end": "00:12:17,960",
        "en": "Singa?",
        "pa": "Singa?",
        "vi": "Singa?"
    },
    {
        "start": "00:12:24,040",
        "end": "00:12:29,000",
        "en": "Tikus. Nasib baik jumpa. Aku perlukan pertolongan kau. Singa",
        "pa": "Tikus, thank goodness you're here. I need your help. Singa…",
        "vi": "Tikus, ơn trời cậu ở đây. Tôi cần cậu giúp. Singa…"
    },
    {
        "start": "00:12:40,560",
        "end": "00:12:42,720",
        "en": "Kasihan.",
        "pa": "Poor Singa…",
        "vi": "Singa đáng thương…"
    },
    {
        "start": "00:12:42,800",
        "end": "00:12:47,240",
        "en": "Tapi apa aku boleh buat? Macam mana aku nak tolong?",
        "pa": "But, what can I do to help?",
        "vi": "Nhưng, tôi giúp như thế nào đây?"
    },
    {
        "start": "00:12:47,320",
        "end": "00:12:48,680",
        "en": "Begini.",
        "pa": "Like this.",
        "vi": "Như thế này."
    },
    {
        "start": "00:12:50,640",
        "end": "00:12:55,200",
        "en": "Boleh. Tapi, kalau dia makan aku?",
        "pa": "Sure! But, what if he eats me?",
        "vi": "Được! Nhưng nếu hắn ăn thịt tôi thì sao?"
    },
    {
        "start": "00:13:02,720",
        "end": "00:13:05,360",
        "en": "Habislah kau, Tikus.",
        "pa": "Tikus is doomed.",
        "vi": "Tikus tiêu đời rồi."
    },
    {
        "start": "00:13:16,360",
        "end": "00:13:18,960",
        "en": "Habislah aku.",
        "pa": "I'm dead…",
        "vi": "Mình tiêu rồi…"
    },
    {
        "start": "00:13:35,480",
        "end": "00:13:38,560",
        "en": "Habislah aku. Jangan makan aku.",
        "pa": "I'm doomed! Don't eat me!",
        "vi": "Mình tiêu rồi! Đừng ăn thịt tôi!"
    },
    {
        "start": "00:13:38,640",
        "end": "00:13:40,040",
        "en": "Tolong!",
        "pa": "Help!",
        "vi": "Cứu tôi với!"
    },
    {
        "start": "00:13:40,120",
        "end": "00:13:41,400",
        "en": "Singa!",
        "pa": "Singa, Singa!",
        "vi": "Singa!"
    },
    {
        "start": "00:13:42,800",
        "end": "00:13:43,920",
        "en": "Hai!",
        "pa": "Hi!",
        "vi": "Xin chào!"
    },
    {
        "start": "00:13:46,640",
        "end": "00:13:49,360",
        "en": "Tikus! Nasib baik bukan pemburu.",
        "pa": "Tikus! I thought it was the hunter.",
        "vi": "Tikus! Ta tưởng ngươi là tay thợ săn."
    },
    {
        "start": "00:13:49,440",
        "end": "00:13:50,600",
        "en": "Tolong lepaskan aku.",
        "pa": "Can you help me?",
        "vi": "Ngươi giúp ta nhé?"
    },
    {
        "start": "00:13:50,680",
        "end": "00:13:52,640",
        "en": "Aku memang datang nak tolong kau.",
        "pa": "That's what I'm here for.",
        "vi": "Đó là lí do tôi đến đây."
    },
    {
        "start": "00:13:53,280",
        "end": "00:13:56,920",
        "en": "Macam mana? Kau kecil. Bolehkah?",
        "pa": "How? You're so small. Can you do it?",
        "vi": "Làm cách nào? Ngươi bé tí. Ngươi giúp ta được không?"
    },
    {
        "start": "00:13:57,600",
        "end": "00:14:00,520",
        "en": "Boleh! Kancil yang suruh aku datang",
        "pa": "Sure! Kancil told me to come here.",
        "vi": "Được chứ! Kancil đã bảo tôi đến đây."
    },
    {
        "start": "00:14:03,280",
        "end": "00:14:04,760",
        "en": "dan tolong lepaskan kau.",
        "pa": "And help free you.",
        "vi": "Và giúp giải cứu anh."
    },
    {
        "start": "00:14:05,400",
        "end": "00:14:11,960",
        "en": "Apa? Dalam banyak-banyak binatang di hutan ini, Kancil suruh kau?",
        "pa": "What? Of all the animals in the jungle, Kancil sent you?",
        "vi": "Cái gì? Trong tất cả muôn thú trong rừng, Kancil lại chọn ngươi à?"
    },
    {
        "start": "00:14:15,160",
        "end": "00:14:21,040",
        "en": "Habislah aku? Macam manalah Tikus ini nak lepaskan aku?",
        "pa": "I'm doomed! How can a mouse help me?",
        "vi": "Mình tiêu rồi! Một con chuột thì làm sao giúp mình chứ?"
    },
    {
        "start": "00:14:21,120",
        "end": "00:14:23,760",
        "en": "Panggillah haiwan yang besar sikit.",
        "pa": "He could've at least sent me someone bigger.",
        "vi": "Kancil nên cử một con vật nào to hơn."
    },
    {
        "start": "00:14:24,360",
        "end": "00:14:26,160",
        "en": "Mungkin Gajah?",
        "pa": "Gajah, maybe?",
        "vi": "Gajah chẳng hạn?"
    },
    {
        "start": "00:14:26,680",
        "end": "00:14:30,960",
        "en": "Sekurang-kurangnya Gajah boleh tarik aku keluar daripada sangkar ini.",
        "pa": "At least Gajah could have pulled me out.",
        "vi": "Ít ra Gajah có thể kéo mình ra."
    },
    {
        "start": "00:14:31,040",
        "end": "00:14:33,040",
        "en": "Beruang?",
        "pa": "Or Beruang…",
        "vi": "Hoặc là Beruang…"
    },
    {
        "start": "00:14:33,120",
        "end": "00:14:36,280",
        "en": "Ini Tikus. Apa dia boleh buat?",
        "pa": "Why sent Tikus to me?",
        "vi": "Tại sao lại cử Tikus?"
    },
    {
        "start": "00:14:36,360",
        "end": "00:14:39,120",
        "en": "Macam mana aku nak lepas?",
        "pa": "How can he get me out?",
        "vi": "Nó sẽ giúp mình thoát bằng cách nào đây?"
    },
    {
        "start": "00:14:40,480",
        "end": "00:14:42,680",
        "en": "Apa aku nak buat?",
        "pa": "What am I going to do?",
        "vi": "Mình sẽ làm gì đây?"
    },
    {
        "start": "00:14:43,760",
        "end": "00:14:45,160",
        "en": "Buaya?",
        "pa": "SCANNING",
        "vi": "ĐANG DÒ"
    },
    {
        "start": "00:14:47,200",
        "end": "00:14:49,800",
        "en": "Habislah aku ditembak dengan pemburu.",
        "pa": "Help me…",
        "vi": "Cứu tôi với…"
    },
    {
        "start": "00:14:51,040",
        "end": "00:14:53,080",
        "en": "Aku tak nak mati lagi.",
        "pa": "WEIGHT, WIDTH, THICKNESS\n I'm too young to die…",
        "vi": "CÂN NẶNG, NGANG, DÀY\n Mình còn quá trẻ để chết…"
    },
    {
        "start": "00:14:54,720",
        "end": "00:14:56,280",
        "en": "Kancil!",
        "pa": "Kancil… help me…",
        "vi": "Kancil…"
    },
    {
        "start": "00:14:57,320",
        "end": "00:14:58,960",
        "en": "Kau kata nak tolong aku.",
        "pa": "",
        "vi": "Cứu ta với…"
    },
    {
        "start": "00:14:59,040",
        "end": "00:15:02,360",
        "en": "Tebalnya. Bolehkah?",
        "pa": "-Why are you doing this to me? -So thick. Can I really do this?",
        "vi": "- Sao ngươi lại làm vậy với ta? - Dày quá. Mình làm được không?"
    },
    {
        "start": "00:15:02,880",
        "end": "00:15:03,960",
        "en": "Baiklah.",
        "pa": "-You trick me. -Here goes…",
        "vi": "- Ngươi lừa ta. - Làm thôi…"
    },
    {
        "start": "00:15:13,520",
        "end": "00:15:16,360",
        "en": "Sini! Sini pula! Cepat!",
        "pa": "Here! Come on!",
        "vi": "Bên đây! Nào!"
    },
    {
        "start": "00:15:27,120",
        "end": "00:15:29,760",
        "en": "",
        "pa": "Can't you do it?",
        "vi": "Ta vẫn không tin ngươi có thể làm được."
    },
    {
        "start": "00:15:41,880",
        "end": "00:15:43,400",
        "en": "Jangan!",
        "pa": "Stop!",
        "vi": "Dừng lại!"
    },
    {
        "start": "00:15:44,440",
        "end": "00:15:45,920",
        "en": "Itu ekor aku.",
        "pa": "That's my tail!",
        "vi": "Đó là đuôi của ta!"
    },
    {
        "start": "00:15:46,000",
        "end": "00:15:47,520",
        "en": "Sana!",
        "pa": "Over there!",
        "vi": "Bên kia!"
    },
    {
        "start": "00:16:01,680",
        "end": "00:16:04,840",
        "en": "Bebas! Aku bebas!",
        "pa": "I'm free!",
        "vi": "Ta tự do rồi!"
    },
    {
        "start": "00:16:10,560",
        "end": "00:16:14,080",
        "en": "Tikus, kenapa kau lari? Sinilah.",
        "pa": "Tikus? Why are you hiding? Come here.",
        "vi": "Tikus? Sao ngươi lại trốn? Đến đây."
    },
    {
        "start": "00:16:16,000",
        "end": "00:16:19,520",
        "en": "Nanti kau makan aku. Tak nak.",
        "pa": "No way! You'll eat me!",
        "vi": "Không đời nào! Anh sẽ ăn thịt tôi!"
    },
    {
        "start": "00:16:19,600",
        "end": "00:16:22,960",
        "en": "Tikus, aku tak akan buat begitu.",
        "pa": "Tikus, I would never do that.",
        "vi": "Tikus, ta sẽ không bao giờ làm vậy."
    },
    {
        "start": "00:16:26,360",
        "end": "00:16:29,200",
        "en": "Aku tak percaya!",
        "pa": "I don't believe you!",
        "vi": "Tôi không tin đâu!"
    },
    {
        "start": "00:16:34,800",
        "end": "00:16:36,360",
        "en": "Jangan! Jangan makan aku!",
        "pa": "No! Don't eat me!",
        "vi": "Đừng! Đừng ăn thịt tôi!"
    },
    {
        "start": "00:16:37,880",
        "end": "00:16:40,680",
        "en": "Terima kasih, Tikus. Terima kasih!",
        "pa": "Thank you, Tikus! Thank you!",
        "vi": "Cảm ơn, Tikus! Cảm ơn nhé!"
    },
    {
        "start": "00:16:40,760",
        "end": "00:16:44,520",
        "en": "Kalau tak, tamatlah riwayat aku.",
        "pa": "If it weren't for you, I'd have been dead meat.",
        "vi": "Nếu không có cậu, tôi đã mất mạng rồi."
    },
    {
        "start": "00:16:45,920",
        "end": "00:16:47,280",
        "en": "Sama-sama.",
        "pa": "No problem.",
        "vi": "Không có gì."
    },
    {
        "start": "00:16:47,360",
        "end": "00:16:51,760",
        "en": "Aku dah kata, aku akan balas jasa kau. Kau tak percaya.",
        "pa": "I told you I would repay you. You didn't believe me.",
        "vi": "Tôi đã nói sẽ trả ơn anh mà. Anh không chịu tin tôi."
    },
    {
        "start": "00:16:51,840",
        "end": "00:16:56,080",
        "en": "Tak sangka. Kau yang kecil ini, dapat menolong aku.",
        "pa": "I had no idea someone so small could help me.",
        "vi": "Tôi không biết rằng một con vật bé nhỏ lại có thể giúp được mình."
    },
    {
        "start": "00:16:56,160",
        "end": "00:16:59,320",
        "en": "Tak sia-sia aku lepaskan kau hari itu.",
        "pa": "It's lucky I let you go that day.",
        "vi": "May mà hôm đó tôi đã thả cậu đi."
    },
    {
        "start": "00:16:59,400",
        "end": "00:17:01,760",
        "en": "Kau memang mengenang jasa.",
        "pa": "You really do know gratitude.",
        "vi": "Cậu đúng là kẻ có lòng biết ơn."
    },
    {
        "start": "00:17:02,480",
        "end": "00:17:05,040",
        "en": "Mulai sekarang, kita bersahabat.",
        "pa": "From now on, we're friends.",
        "vi": "Từ giờ trở đi, chúng ta sẽ là bạn."
    },
    {
        "start": "00:17:05,119",
        "end": "00:17:08,599",
        "en": "Kalau kau perlukan pertolongan, cari saja aku.",
        "pa": "If you need help, just come find me.",
        "vi": "Nếu cậu cần giúp đỡ, cứ tìm tôi."
    },
    {
        "start": "00:17:09,119",
        "end": "00:17:10,440",
        "en": "Kau pun sama.",
        "pa": "Same to you.",
        "vi": "Anh cũng vậy."
    },
    {
        "start": "00:17:12,359",
        "end": "00:17:14,880",
        "en": "- Pemburu! - Mari, Tikus.",
        "pa": "-The hunter! -Come on, Tikus!",
        "vi": "- Thợ săn! - Đi nào, Tikus!"
    },
    {
        "start": "00:17:21,160",
        "end": "00:17:25,400",
        "en": "Sejak hari itu, Singa dan Tikus pun jadi kawan baik.",
        "pa": "From that day on, the lion and the mouse became best friends.",
        "vi": "Kể từ đó, sư tử và chuột trở thành bạn tốt của nhau."
    },
    {
        "start": "00:17:25,480",
        "end": "00:17:28,200",
        "en": "Ke mana-mana pergi, mesti bersama.",
        "pa": "Wherever they go, they go together.",
        "vi": "Bất cứ nơi nào, cả hai cũng đi cùng nhau."
    },
    {
        "start": "00:17:28,280",
        "end": "00:17:31,760",
        "en": "Itulah dia. Hancur badan di kandung tanah.",
        "pa": "There goes the saying,",
        "vi": "Có một câu nói thế này:"
    },
    {
        "start": "00:17:31,840",
        "end": "00:17:34,760",
        "en": "Budi yang baik dikenang juga.",
        "pa": "\"A good deed is never lost.\"",
        "vi": "\"Làm ơn không bao giờ thiệt\"."
    },
    {
        "start": "00:17:35,720",
        "end": "00:17:39,440",
        "en": "- Ara tak faham. - Memanglah tak faham.",
        "pa": "I don't get it.\n Of course you don't!",
        "vi": "- Cháu không hiểu. - Dĩ nhiên là không rồi!"
    },
    {
        "start": "00:17:39,520",
        "end": "00:17:42,480",
        "en": "Ara kecil lagi. Aris, faham.",
        "pa": "You're just a kid. But I do.",
        "vi": "Em còn bé mà. Nhưng anh hiểu."
    },
    {
        "start": "00:17:43,200",
        "end": "00:17:45,600",
        "en": "Apa yang Aris faham? Cuba cerita.",
        "pa": "What do you understand? Tell me.",
        "vi": "Cháu hiểu gì nào? Nói ông nghe xem."
    },
    {
        "start": "00:17:46,920",
        "end": "00:17:50,080",
        "en": "Pada zaman dahulu",
        "pa": "Once upon a time…",
        "vi": "Ngày xửa ngày xưa…"
    },
    {
        "start": "00:17:50,680",
        "end": "00:17:52,320",
        "en": "Aris tanam ubi.",
        "pa": "I planted cassava.",
        "vi": "Cháu trồng khoai mì."
    },
    {
        "start": "00:17:52,880",
        "end": "00:17:57,360",
        "en": "Tapi tak tahu berbudi. Ubi pun mati.",
        "pa": "But I wasn't kind, so it died.",
        "vi": "Nhưng vì cháu không tốt, nên khoai mì chết hết."
    },
    {
        "start": "00:17:57,880",
        "end": "00:18:01,640",
        "en": "Aki tanam ubi. Aki berbudi.",
        "pa": "Aki planted cassava, and Aki was kind.",
        "vi": "Ông cũng trồng khoai mì và ông tốt bụng."
    },
    {
        "start": "00:18:02,480",
        "end": "00:18:04,440",
        "en": "Sebab itu, ubi jadi.",
        "pa": "That's why it grew!",
        "vi": "Thế nên khoai mới mọc!"
    },
    {
        "start": "00:18:08,360",
        "end": "00:18:09,680",
        "en": "Bukan begitu.",
        "pa": "Not that way.",
        "vi": "Không phải vậy."
    },
    {
        "start": "00:18:09,760",
        "end": "00:18:11,840",
        "en": "Maksud peribahasa itu,",
        "pa": "It means that every act of kindness will always be remembered",
        "vi": "Câu đó nghĩa là khi cháu làm việc tốt thì sẽ luôn được ghi nhớ,"
    },
    {
        "start": "00:18:11,920",
        "end": "00:18:16,040",
        "en": "sesuatu perbuatan yang baik, akan sentiasa dikenang.",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:18:16,120",
        "end": "00:18:19,600",
        "en": "Dan tidak akan dilupakan, sampai bila-bila.",
        "pa": "and never forgotten.",
        "vi": "và không bao giờ bị quên lãng."
    },
    {
        "start": "00:18:19,680",
        "end": "00:18:23,840",
        "en": "Macam Abang bagi Ara gula-gula hari itu,",
        "pa": "Oh, like when Aris gave me candy that day.",
        "vi": "Giống như hôm nọ anh cho cháu viên kẹo."
    },
    {
        "start": "00:18:23,920",
        "end": "00:18:25,600",
        "en": "sampai sekarang Ara ingat.",
        "pa": "I still remember.",
        "vi": "Cháu vẫn nhớ."
    },
    {
        "start": "00:18:25,680",
        "end": "00:18:29,480",
        "en": "Yakah? Jadi Abang dah berbudi pada Ara.",
        "pa": "You do? So I was being kind to you.",
        "vi": "Vậy à? Vậy là anh đã đối xử tốt với em."
    },
    {
        "start": "00:18:29,560",
        "end": "00:18:30,880",
        "en": "Bila nak balas?",
        "pa": "When are you going to repay me?",
        "vi": "Khi nào em trả ơn đây?"
    },
    {
        "start": "00:18:31,960",
        "end": "00:18:34,800",
        "en": "Orang berbudi, bukan nak harapkan balasan.",
        "pa": "Being kind means you don't expect anything in return.",
        "vi": "Làm việc tốt thì không chờ đợi được trả ơn."
    },
    {
        "start": "00:18:34,880",
        "end": "00:18:36,960",
        "en": "- Kau Aris - Assalamualaikum!",
        "pa": "-You silly boy… -Peace be upon you!",
        "vi": "- Thằng bé này… - Có ai ở nhà không?"
    },
    {
        "start": "00:18:38,600",
        "end": "00:18:40,480",
        "en": "- Hore! Dah sampai! - Dah sampai!",
        "pa": "-They're here! -You are here!",
        "vi": "- Họ đến rồi! - Bố mẹ đến rồi!"
    },
    {
        "start": "00:18:40,560",
        "end": "00:18:42,320",
        "en": "Papa!",
        "pa": "Dad!",
        "vi": "- Bố! - Chào con!"
    },
    {
        "start": "00:18:42,920",
        "end": "00:18:45,600",
        "en": "Rindu Papa. Mama.",
        "pa": "-I missed you! -Mom!",
        "vi": "- Con nhớ bố lắm! - Mẹ!"
    },
    {
        "start": "00:18:46,320",
        "end": "00:18:48,480",
        "en": "Abah, saya masuk dalam.",
        "pa": "Dad, I'm going inside.",
        "vi": "Chào bố, con vào trong đây ạ."
    },
    {
        "start": "00:18:49,760",
        "end": "00:18:51,040",
        "en": "Assalamualaikum Abah.",
        "pa": "Peace be upon you, Dad.",
        "vi": "Bố vẫn khỏe ạ?"
    },
    {
        "start": "00:18:51,120",
        "end": "00:18:52,400",
        "en": "Duduk sini, Papa.",
        "pa": "Come here, Dad!",
        "vi": "Đến đây đi bố!"
    },
    {
        "start": "00:18:52,920",
        "end": "00:18:54,080",
        "en": "Makan.",
        "pa": "Come, let's eat!",
        "vi": "Nào, cùng ăn đi!"
    },
    {
        "start": "00:18:54,160",
        "end": "00:18:56,600",
        "en": "- Cubalah rasa. Cubalah. - Panas lagi.",
        "pa": "-Come on, eat some! -They're hot.",
        "vi": "- Bố ăn đi! - Nóng đấy ạ."
    },
    {
        "start": "00:18:56,680",
        "end": "00:18:58,000",
        "en": "Memanglah. Baru masak.",
        "pa": "Freshly cooked.",
        "vi": "Vừa mới luộc luôn."
    },
    {
        "start": "00:19:00,360",
        "end": "00:19:03,600",
        "en": "Sedapnya. Lagi sedap dari beli di kedai.",
        "pa": "Delicious! There's nothing like homemade cooking.",
        "vi": "Ngon quá! Không gì ngon hơn đồ ăn nhà làm cả."
    },
    {
        "start": "00:19:04,200",
        "end": "00:19:06,840",
        "en": "Mestilah. Ini tanam sendiri.",
        "pa": "Of course. I grew these myself.",
        "vi": "Dĩ nhiên rồi. Tự tay bố trồng đấy."
    },
    {
        "start": "00:19:07,440",
        "end": "00:19:09,640",
        "en": "Papa kena berbudi dengan tanah.",
        "pa": "You have to be kind to the earth.",
        "vi": "Bố phải đối xử tử tế với thiên nhiên."
    },
    {
        "start": "00:19:09,720",
        "end": "00:19:11,320",
        "en": "Baru sedap.",
        "pa": "Then it'll taste good!",
        "vi": "Vậy thì thức ăn sẽ ngon!"
    },
    {
        "start": "00:19:13,000",
        "end": "00:19:14,160",
        "en": "Papa!",
        "pa": "Dad…",
        "vi": "Bố…"
    },
    {
        "start": "00:19:35,320",
        "end": "00:19:36,760",
        "en": "Macam itu, Aki?",
        "pa": "Isn't that right, Aki?",
        "vi": "Đúng không ông?"
    },
    {
        "start": "00:19:39,480",
        "end": "00:19:43,960",
        "en": "Tadi Aki cerita pasal Singa dengan Tikus jadi kawan baik.",
        "pa": "Just now Aki told us a story about\n a lion and a mouse who became best friends!",
        "vi": "Ông vừa kể bọn con nghe câu chuyện về\n sư tử và chuột trở thành bạn tốt đấy ạ!"
    },
    {
        "start": "00:19:45,640",
        "end": "00:19:49,080",
        "en": "Macam Aris, singa. Ara, tikus.",
        "pa": "Oh, like Aris is the lion, and Ara is the mouse.",
        "vi": "Giống như Aris là sư tử còn Ara là chuột."
    },
    {
        "start": "00:19:49,160",
        "end": "00:19:51,600",
        "en": "Dah jadi kawan baik? Atau",
        "pa": "Are you best friends yet? Or…?",
        "vi": "Hai con đã là bạn tốt chưa? Hay là…"
    },
    {
        "start": "00:19:52,600",
        "end": "00:19:54,000",
        "en": "Masih bergaduh lagi.",
        "pa": "They still fight sometimes.",
        "vi": "Thỉnh thoảng vẫn cãi nhau."
    },
    {
        "start": "00:19:59,080",
        "end": "00:20:00,880",
        "en": "Sunyi dah.",
        "pa": "It's so quiet now…",
        "vi": "Giờ yên ắng quá…"
    },
    {
        "start": "00:20:05,400",
        "end": "00:20:06,640",
        "en": "Helo?",
        "pa": "Hello?",
        "vi": "A-lô?"
    },
    {
        "start": "00:20:06,720",
        "end": "00:20:10,480",
        "en": "Apa? Tayar pancit? Yalah.",
        "pa": "What? A flat tire? All right then.",
        "vi": "Cái gì? Thủng lốp à? Được rồi."
    },
    {
        "start": "00:20:10,560",
        "end": "00:20:13,720",
        "en": "Wan, sediakan tempat tidur. Malam ini ramai!",
        "pa": "Wan, get the beds ready. We have a full house tonight!",
        "vi": "Bà à, chuẩn bị giường đi. Đêm nay nhà mình sẽ đông người đấy!\n Biên dịch: Hoàng Nguyên"
    }
]