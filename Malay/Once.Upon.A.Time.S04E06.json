[
    {
        "start": "00:00:21,000",
        "end": "00:00:23,920",
        "en": "Pada zaman dahulu…",
        "pa": "Once upon a time…",
        "vi": "Ngày xửa ngày xưa…"
    },
    {
        "start": "00:00:47,000",
        "end": "00:00:48,880",
        "en": "Mana semua orang?",
        "pa": "Where is everyone?",
        "vi": "Cả nhà đâu hết rồi?"
    },
    {
        "start": "00:00:53,480",
        "end": "00:00:55,200",
        "en": "Aris! Ara!",
        "pa": "Aris! Ara!",
        "vi": "Aris! Ara!"
    },
    {
        "start": "00:00:57,480",
        "end": "00:00:58,640",
        "en": "- Aki! - Aki!",
        "pa": "-Aki! -Aki!",
        "vi": "- Ông! - Ông!"
    },
    {
        "start": "00:00:59,440",
        "end": "00:01:01,680",
        "en": "Aki, aiskrim ada tak?",
        "pa": "Aki, did you buy any ice cream?",
        "vi": "Ông à, ông có mua kem không?"
    },
    {
        "start": "00:01:02,440",
        "end": "00:01:05,519",
        "en": "Aiskrim tak ada. Buku ada.",
        "pa": "There's no ice cream. There's a book.",
        "vi": "Không có kem. Chỉ có một quyển sách."
    },
    {
        "start": "00:01:06,640",
        "end": "00:01:08,840",
        "en": "Nah, baca sama-sama.",
        "pa": "You can read it together.",
        "vi": "Hai cháu cùng đọc nhé."
    },
    {
        "start": "00:01:08,920",
        "end": "00:01:09,760",
        "en": "Hore!",
        "pa": "",
        "vi": "Vâng!"
    },
    {
        "start": "00:01:10,560",
        "end": "00:01:12,160",
        "en": "Abang, mari baca?",
        "pa": "Aris, let's read it.",
        "vi": "Anh à, cùng đọc sách đi."
    },
    {
        "start": "00:01:13,960",
        "end": "00:01:15,680",
        "en": "Ara bacalah dulu.",
        "pa": "You read it first.",
        "vi": "Em đọc trước đi."
    },
    {
        "start": "00:01:16,560",
        "end": "00:01:17,400",
        "en": "Terima kasih, Aki.",
        "pa": "Thank you, Aki.",
        "vi": "Cảm ơn ông."
    },
    {
        "start": "00:01:26,000",
        "end": "00:01:27,400",
        "en": "A…",
        "pa": "A-Aa.",
        "vi": "A."
    },
    {
        "start": "00:01:28,080",
        "end": "00:01:30,600",
        "en": "R…",
        "pa": "R-Rrr.",
        "vi": "R."
    },
    {
        "start": "00:01:31,240",
        "end": "00:01:34,280",
        "en": "Na…",
        "pa": "Na-Na.",
        "vi": "Na-Na."
    },
    {
        "start": "00:01:34,880",
        "end": "00:01:36,840",
        "en": "- B… - Arnab!",
        "pa": "-B… -Arnab!",
        "vi": "- B… - Arnab!"
    },
    {
        "start": "00:01:37,520",
        "end": "00:01:38,760",
        "en": "Arnab!",
        "pa": "Arnab!",
        "vi": "Arnab!"
    },
    {
        "start": "00:01:40,200",
        "end": "00:01:42,680",
        "en": "Da…",
        "pa": "D, A-Da…",
        "vi": "Vờ a va…"
    },
    {
        "start": "00:01:43,280",
        "end": "00:01:45,920",
        "en": "- N… - Dan Kura-kura!",
        "pa": "-N… -And Kura-kura!",
        "vi": "- Huyền… - Và Kura-kura!"
    },
    {
        "start": "00:01:46,000",
        "end": "00:01:50,480",
        "en": "\"Arnab dan Kura-kura\"! Comelnya!",
        "pa": "Arnab and Kura-Kura! So cute…",
        "vi": "Arnab và Kura-Kura! Đáng yêu quá…"
    },
    {
        "start": "00:01:53,640",
        "end": "00:01:55,800",
        "en": "Comelnya gambar ini.",
        "pa": "THE RABBIT AND THE TORTOISE\n What a cute picture.",
        "vi": "ARNAB VÀ KURA-KURA\n Bức tranh đáng yêu ghê."
    },
    {
        "start": "00:01:56,720",
        "end": "00:01:58,440",
        "en": "Abang tak nak baca?",
        "pa": "Don't you want to read it?",
        "vi": "Anh không muốn đọc à?"
    },
    {
        "start": "00:01:58,520",
        "end": "00:02:00,800",
        "en": "Tak naklah, baik main permainan video.",
        "pa": "No, it's better to play video games.",
        "vi": "Không, anh thà chơi trò chơi điện tử."
    },
    {
        "start": "00:02:01,320",
        "end": "00:02:02,480",
        "en": "Yalah.",
        "pa": "Okay.",
        "vi": "Được rồi."
    },
    {
        "start": "00:02:03,400",
        "end": "00:02:06,240",
        "en": "Pa…",
        "pa": "P, a-Pa…",
        "vi": "P, a-Pa…"
    },
    {
        "start": "00:02:07,080",
        "end": "00:02:09,680",
        "en": "- Abang, ini apa? - D!",
        "pa": "-What's this? -D!",
        "vi": "- Cái gì thế này? - D!"
    },
    {
        "start": "00:02:11,039",
        "end": "00:02:14,080",
        "en": "Da…",
        "pa": "D, a-Da…",
        "vi": "D, a-Da…"
    },
    {
        "start": "00:02:16,520",
        "end": "00:02:18,880",
        "en": "Sudahlah, mari Aki cerita.",
        "pa": "Enough. Let me tell you a story.",
        "vi": "Được rồi. Để ông kể chuyện cho cháu nghe."
    },
    {
        "start": "00:02:20,360",
        "end": "00:02:23,000",
        "en": "Cerita dalam buku ini, Aki?",
        "pa": "The story in this book, Aki?",
        "vi": "Câu chuyện trong sách này hả ông?"
    },
    {
        "start": "00:02:24,000",
        "end": "00:02:27,360",
        "en": "Bukan. Dengar sini.",
        "pa": "No. Listen to this.",
        "vi": "Không. Cháu nghe này."
    },
    {
        "start": "00:02:27,440",
        "end": "00:02:30,520",
        "en": "Pada zaman dahulu…",
        "pa": "Once upon a time…",
        "vi": "Ngày xửa ngày xưa…"
    },
    {
        "start": "00:02:49,360",
        "end": "00:02:51,040",
        "en": "Lapar.",
        "pa": "I'm so hungry.",
        "vi": "Đói quá."
    },
    {
        "start": "00:02:54,760",
        "end": "00:02:57,120",
        "en": "- Makan banyak bagi aku sikit. - Bagi aku sikit.",
        "pa": "-You already ate a lot. Give me some. -Get out of the way.",
        "vi": "- Cậu ăn đủ rồi, chừa một ít cho tôi với. - Tránh ra."
    },
    {
        "start": "00:02:57,200",
        "end": "00:02:58,520",
        "en": "Tepi!",
        "pa": "There's nothing left. Let's go.",
        "vi": "Không còn gì cả, đi thôi."
    },
    {
        "start": "00:03:04,080",
        "end": "00:03:05,880",
        "en": "Tulang!",
        "pa": "A bone!",
        "vi": "Một khúc xương!"
    },
    {
        "start": "00:03:13,680",
        "end": "00:03:15,640",
        "en": "Sedapnya…",
        "pa": "So delicious…",
        "vi": "Ngon quá…"
    },
    {
        "start": "00:03:21,520",
        "end": "00:03:23,360",
        "en": "Tak kenyang.",
        "pa": "I am still hungry.",
        "vi": "Mình vẫn còn đói."
    },
    {
        "start": "00:03:25,680",
        "end": "00:03:28,120",
        "en": "Teruk betul kemarau kali ini, betul tak Gajah?",
        "pa": "The drought is very bad this time.",
        "vi": "Cơn hạn hán lần này khắc nghiệt quá."
    },
    {
        "start": "00:03:28,200",
        "end": "00:03:32,080",
        "en": "Tahu tak apa. Nasib baik kita dah kumpul makanan.",
        "pa": "I know, right? Lucky for us we saved some food.",
        "vi": "Đúng nhỉ? May mà chúng ta đã dự trữ thức ăn."
    },
    {
        "start": "00:03:32,160",
        "end": "00:03:34,000",
        "en": "Kalau tak, habislah kita.",
        "pa": "Otherwise we'd be finished.",
        "vi": "Không thì chúng ta đi đời rồi."
    },
    {
        "start": "00:03:35,160",
        "end": "00:03:36,000",
        "en": "Makanan!",
        "pa": "-Hopefully we'll have enough. -Food!",
        "vi": "- Hi vọng chúng ta có đủ thức ăn. - Thức ăn!"
    },
    {
        "start": "00:03:36,080",
        "end": "00:03:37,640",
        "en": "Cukuplah makanan untuk kita nanti.",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:03:38,400",
        "end": "00:03:42,200",
        "en": "Gajah yang gagah. Arnab yang comel.",
        "pa": "Brave Gajah, cute Arnab.",
        "vi": "Gajah gan dạ, Arnab đáng yêu."
    },
    {
        "start": "00:03:42,280",
        "end": "00:03:46,000",
        "en": "Kamu nak ke mana? Boleh saya ikut?",
        "pa": "Where are you going? Can I come?",
        "vi": "Hai cậu đi đâu thế? Tôi đi cùng được không?"
    },
    {
        "start": "00:03:48,280",
        "end": "00:03:52,360",
        "en": "Gagak yang hitam manis, kami nak balik.",
        "pa": "Sweet Gagak, we are going home.",
        "vi": "Gagak ngọt ngào, chúng tôi đang về nhà."
    },
    {
        "start": "00:03:52,440",
        "end": "00:03:55,040",
        "en": "Tak boleh ikut.",
        "pa": "You can't come…",
        "vi": "Cậu không thể đi theo…"
    },
    {
        "start": "00:03:55,680",
        "end": "00:03:57,720",
        "en": "Manis? Aku?",
        "pa": "Sweet? Me?",
        "vi": "Ngọt ngào? Tôi ư?"
    },
    {
        "start": "00:03:58,840",
        "end": "00:04:01,400",
        "en": "- Aku manis? - Mestilah.",
        "pa": "-I'm sweet? -Of course.",
        "vi": "- Tôi ngọt ngào à? - Dĩ nhiên rồi."
    },
    {
        "start": "00:04:07,200",
        "end": "00:04:08,920",
        "en": "Tak sempat minta makan!",
        "pa": "I didn't even get to ask for food!",
        "vi": "Mình còn chưa kịp hỏi xin thức ăn!"
    },
    {
        "start": "00:04:28,520",
        "end": "00:04:29,920",
        "en": "Makanan!",
        "pa": "Food!",
        "vi": "Thức ăn!"
    },
    {
        "start": "00:04:54,280",
        "end": "00:04:56,720",
        "en": "Baik makan di atas pokok sahaja.",
        "pa": "I'd better just eat on this tree.",
        "vi": "Tốt hơn là mình nên ăn ở trên cây."
    },
    {
        "start": "00:04:56,800",
        "end": "00:04:59,360",
        "en": "Nanti tak diganggu oleh binatang lain.",
        "pa": "Then I won't be disturbed by other animals.",
        "vi": "Như thế mình sẽ không bị các con thú khác quấy rầy."
    },
    {
        "start": "00:05:28,280",
        "end": "00:05:30,520",
        "en": "Panasnya!",
        "pa": "It's so hot!",
        "vi": "Nóng quá!"
    },
    {
        "start": "00:05:43,040",
        "end": "00:05:45,360",
        "en": "Aku punya!",
        "pa": "It's mine!\n Mine!",
        "vi": "Của tôi!"
    },
    {
        "start": "00:06:05,840",
        "end": "00:06:08,160",
        "en": "Hore, aku dapat!",
        "pa": "Hurray, I got it!",
        "vi": "Hoan hô, mình ăn được rồi!"
    },
    {
        "start": "00:06:10,760",
        "end": "00:06:12,160",
        "en": "Aku dapat!",
        "pa": "I got it!",
        "vi": "Mình ăn được rồi!"
    },
    {
        "start": "00:06:15,040",
        "end": "00:06:16,280",
        "en": "Terlepas…",
        "pa": "Missed it…",
        "vi": "Trượt rồi…"
    },
    {
        "start": "00:06:48,440",
        "end": "00:06:49,280",
        "en": "Sedapnya!",
        "pa": "Delicious!",
        "vi": "Ngon quá!"
    },
    {
        "start": "00:06:57,880",
        "end": "00:06:59,880",
        "en": "Rezeki jangan ditolak.",
        "pa": "I can't miss out on this.",
        "vi": "Mình không thể để trượt mất cái này."
    },
    {
        "start": "00:07:09,920",
        "end": "00:07:11,000",
        "en": "Hai Gagak!",
        "pa": "Hi Gagak!",
        "vi": "Chào Gagak!"
    },
    {
        "start": "00:07:12,120",
        "end": "00:07:13,280",
        "en": "Itu siapa?",
        "pa": "Who's that?",
        "vi": "Ai đấy?"
    },
    {
        "start": "00:07:14,320",
        "end": "00:07:16,480",
        "en": "Apa yang kau sedang makan?",
        "pa": "What are you eating?",
        "vi": "Cậu đang ăn gì thế?"
    },
    {
        "start": "00:07:22,920",
        "end": "00:07:24,040",
        "en": "Tengok sana.",
        "pa": "Look over there.",
        "vi": "Nhìn bên kia kìa."
    },
    {
        "start": "00:07:26,840",
        "end": "00:07:28,840",
        "en": "Cantik sungguh Merpati itu.",
        "pa": "That Merpati is so beautiful.",
        "vi": "Chị Merpati kia thật xinh đẹp."
    },
    {
        "start": "00:07:29,760",
        "end": "00:07:33,160",
        "en": "Matanya lentik, berparuh biru",
        "pa": "Lovely eyes, cerulean beak, gorgeous crest.",
        "vi": "Cặp mắt dễ thương, mỏ màu thiên thanh,"
    },
    {
        "start": "00:07:33,240",
        "end": "00:07:34,920",
        "en": "dan berjambul di depan.",
        "pa": "",
        "vi": "chiếc mào rực rỡ."
    },
    {
        "start": "00:07:36,560",
        "end": "00:07:38,720",
        "en": "Selamat tinggal, si cantik.",
        "pa": "Goodbye, beautiful!",
        "vi": "Tạm biệt, người đẹp!"
    },
    {
        "start": "00:07:41,480",
        "end": "00:07:44,560",
        "en": "Tapi Gagak, kau tak cantik.",
        "pa": "But Gagak, you are ugly.",
        "vi": "Nhưng Gagak này, cậu xấu xí quá."
    },
    {
        "start": "00:07:45,440",
        "end": "00:07:47,520",
        "en": "Jadi apa keistimewaan kau?",
        "pa": "What's so special about you?",
        "vi": "Cậu thì có gì đặc biệt chứ?"
    },
    {
        "start": "00:07:50,960",
        "end": "00:07:54,280",
        "en": "Baru aku teringat.",
        "pa": "Now I remember!",
        "vi": "Giờ tôi nhớ rồi!"
    },
    {
        "start": "00:07:54,360",
        "end": "00:07:56,720",
        "en": "Suara kau sangat merdu.",
        "pa": "Your soothing voice.",
        "vi": "Giọng hót ngọt ngào."
    },
    {
        "start": "00:07:56,800",
        "end": "00:07:59,840",
        "en": "Semua binatang memuji suara kau.",
        "pa": "Every animal praises your voice.",
        "vi": "Con thú nào cũng tâng bốc giọng hót của cậu."
    },
    {
        "start": "00:08:03,000",
        "end": "00:08:05,720",
        "en": "Betul Gagak, aku tak tipu.",
        "pa": "It's true Gagak, I'm not lying.",
        "vi": "Thật đấy Gagak, tôi không nói dối đâu."
    },
    {
        "start": "00:08:05,800",
        "end": "00:08:07,240",
        "en": "Sepanjang perjalanan tadi…",
        "pa": "Throughout my whole journey--",
        "vi": "Qua suốt hành trình của tôi…"
    },
    {
        "start": "00:08:07,320",
        "end": "00:08:12,480",
        "en": "Sedapnya suara Gagak.",
        "pa": "Gagak's voice is nice.",
        "vi": "Giọng của Gagak rất hay."
    },
    {
        "start": "00:08:14,360",
        "end": "00:08:15,760",
        "en": "Nyanyilah untuk aku.",
        "pa": "Gagak's voice is nice.\n Sing for me, please.",
        "vi": "Giọng của Gagak rất hay.\n Hãy hót cho tôi nghe đi, làm ơn."
    },
    {
        "start": "00:08:16,280",
        "end": "00:08:19,600",
        "en": "Aku nak mendengar suara kau yang merdu itu.",
        "pa": "I want to hear that beautiful voice of yours.",
        "vi": "Tôi muốn nghe giọng hót thánh thót của cậu."
    },
    {
        "start": "00:08:19,680",
        "end": "00:08:22,200",
        "en": "Cepatlah Gagak, menyanyilah.",
        "pa": "Quick Gagak, sing for me.",
        "vi": "Nhanh đi Gagak, hót cho tôi nghe đi."
    },
    {
        "start": "00:08:25,120",
        "end": "00:08:28,000",
        "en": "Lagu tiga kupang",
        "pa": "Sing a song of six pence",
        "vi": ""
    },
    {
        "start": "00:08:30,120",
        "end": "00:08:33,159",
        "en": "Saku penuh padi",
        "pa": "And a pocket full of rye",
        "vi": ""
    },
    {
        "start": "00:08:37,120",
        "end": "00:08:39,960",
        "en": "Ayam aku!",
        "pa": "My chicken!",
        "vi": "Thịt gà của tôi!"
    },
    {
        "start": "00:08:42,880",
        "end": "00:08:46,800",
        "en": "Terima kasih kerana memberi makanan ini kepada aku, Gagak.",
        "pa": "Thank you Gagak, for giving me your food.",
        "vi": "Cảm ơn Gagak, vì đã cho tôi thức ăn."
    },
    {
        "start": "00:08:48,000",
        "end": "00:08:50,560",
        "en": "Aku tak bagi kau. Bagi balik ayam aku!",
        "pa": "I didn't! Give me back my chicken!",
        "vi": "Không hề! Trả miếng gà lại cho tôi!"
    },
    {
        "start": "00:08:51,760",
        "end": "00:08:52,960",
        "en": "Bagi balik!",
        "pa": "Give it back!",
        "vi": "Trả lại đây!"
    },
    {
        "start": "00:08:58,000",
        "end": "00:09:00,680",
        "en": "Senang betul nak tipu kau.",
        "pa": "It's so easy to trick you.",
        "vi": "Lừa cậu thật là dễ dàng."
    },
    {
        "start": "00:09:01,200",
        "end": "00:09:05,120",
        "en": "Sebenarnya suara kau tak sedap.",
        "pa": "One more thing. Your voice is actually bad.",
        "vi": "Còn một chuyện nữa. Giọng của cậu thực sự dở ẹc."
    },
    {
        "start": "00:09:05,200",
        "end": "00:09:06,280",
        "en": "Berlatih lagi tahu.",
        "pa": "Keep practicing.",
        "vi": "Cứ tập luyện thêm nhé."
    },
    {
        "start": "00:09:08,880",
        "end": "00:09:09,920",
        "en": "Hei Gagak.",
        "pa": "Hey Gagak.",
        "vi": "Này Gagak."
    },
    {
        "start": "00:09:16,320",
        "end": "00:09:17,160",
        "en": "Nak?",
        "pa": "Want it?",
        "vi": "Muốn ăn không?"
    },
    {
        "start": "00:09:18,520",
        "end": "00:09:20,040",
        "en": "Carilah sendiri.",
        "pa": "Get one yourself.",
        "vi": "Tự đi tìm đi."
    },
    {
        "start": "00:09:20,800",
        "end": "00:09:22,000",
        "en": "Musang!",
        "pa": "Musang!",
        "vi": "Musang!\n BỐP!"
    },
    {
        "start": "00:09:28,200",
        "end": "00:09:30,480",
        "en": "Selamat tinggal!",
        "pa": "Good bye!",
        "vi": "Tạm biệt!"
    },
    {
        "start": "00:09:34,760",
        "end": "00:09:36,120",
        "en": "Ayam aku.",
        "pa": "My chicken…",
        "vi": "Miếng gà của tôi…"
    },
    {
        "start": "00:09:50,640",
        "end": "00:09:51,560",
        "en": "Gagak!",
        "pa": "Gagak!",
        "vi": "Gagak!"
    },
    {
        "start": "00:09:52,360",
        "end": "00:09:53,720",
        "en": "Kenapa dengan kau?",
        "pa": "What's wrong?",
        "vi": "Có chuyện gì thế?"
    },
    {
        "start": "00:09:55,440",
        "end": "00:09:56,840",
        "en": "Kancil!",
        "pa": "Kancil!",
        "vi": "Kancil!"
    },
    {
        "start": "00:09:57,720",
        "end": "00:09:58,560",
        "en": "Apa?",
        "pa": "",
        "vi": "Gì thế?"
    },
    {
        "start": "00:10:00,120",
        "end": "00:10:05,320",
        "en": "Musang ambil makanan aku.",
        "pa": "Musang.\n Musang took my food.",
        "vi": "Musang.\n Musang lấy thức ăn của tôi rồi."
    },
    {
        "start": "00:10:10,120",
        "end": "00:10:13,840",
        "en": "Aku lapar!",
        "pa": "I'm hungry!",
        "vi": "Tôi đói quá!"
    },
    {
        "start": "00:10:18,000",
        "end": "00:10:22,000",
        "en": "Cari sajalah makanan lain. Buat apa menangis?",
        "pa": "Just go and find other food. Why must you cry?",
        "vi": "Thì cứ đi tìm thức ăn khác. Sao cậu lại phải khóc?"
    },
    {
        "start": "00:10:22,840",
        "end": "00:10:25,280",
        "en": "Tapi, susah payah aku cari.",
        "pa": "I worked hard for it,",
        "vi": "Tôi đã tìm nó rất vất vả,"
    },
    {
        "start": "00:10:25,360",
        "end": "00:10:27,760",
        "en": "Senang saja musang ambil.",
        "pa": "and Musang just took it!",
        "vi": "Musang cứ thế lấy mất!"
    },
    {
        "start": "00:10:28,600",
        "end": "00:10:30,880",
        "en": "Sudahlah Gagak. Kau usahalah lagi.",
        "pa": "It's alright, Gagak. Try harder.",
        "vi": "Được rồi, Gagak. Hãy cố gắng hơn nhé."
    },
    {
        "start": "00:10:30,960",
        "end": "00:10:32,480",
        "en": "Jangan putus asa.",
        "pa": "Never give up.",
        "vi": "Đừng bao giờ bỏ cuộc."
    },
    {
        "start": "00:10:33,360",
        "end": "00:10:36,840",
        "en": "Tengoklah aku. Tak jumpa lagi makanan.",
        "pa": "Look at me, I still haven't found any food.",
        "vi": "Nhìn tôi đi, tôi vẫn chưa tìm được tí thức ăn nào."
    },
    {
        "start": "00:10:36,920",
        "end": "00:10:38,880",
        "en": "Tapi aku tak putus asa.",
        "pa": "However, I'm not giving up.",
        "vi": "Nhưng mà, tôi sẽ không bỏ cuộc."
    },
    {
        "start": "00:10:38,960",
        "end": "00:10:41,400",
        "en": "Aku akan terus mencari.",
        "pa": "I will continue to search for it.",
        "vi": "Tôi sẽ tiếp tục đi tìm."
    },
    {
        "start": "00:10:41,480",
        "end": "00:10:42,560",
        "en": "Pergi dulu!",
        "pa": "I'll be on my way.",
        "vi": "Tôi đi đây."
    },
    {
        "start": "00:10:45,680",
        "end": "00:10:47,240",
        "en": "Betul cakap Kancil.",
        "pa": "Kancil is right.",
        "vi": "Kancil nói đúng."
    },
    {
        "start": "00:10:47,320",
        "end": "00:10:49,520",
        "en": "Aku kena terus berusaha.",
        "pa": "I should never give up.",
        "vi": "Mình không nên bỏ cuộc."
    },
    {
        "start": "00:10:55,480",
        "end": "00:10:59,360",
        "en": "Panasnya! Terasa haus pula.",
        "pa": "It's so hot. I'm thirsty now.",
        "vi": "Nóng quá. Giờ mình lại khát nước."
    },
    {
        "start": "00:11:18,480",
        "end": "00:11:19,320",
        "en": "Air!",
        "pa": "Water!",
        "vi": "Nước!"
    },
    {
        "start": "00:11:43,280",
        "end": "00:11:47,360",
        "en": "- Kesian Gagak, mesti sakit. - Tak sakitlah.",
        "pa": "-Poor Gagak, it must have hurt. -It didn't hurt.",
        "vi": "- Tội nghiệp Gagak, chắc là đau lắm. - Không đâu."
    },
    {
        "start": "00:11:47,440",
        "end": "00:11:49,200",
        "en": "Itu tak penting.",
        "pa": "That's not important!",
        "vi": "Điều đó không quan trọng!"
    },
    {
        "start": "00:11:50,600",
        "end": "00:11:52,440",
        "en": "Jadi apa yang penting?",
        "pa": "So what is important?",
        "vi": "Vậy cái gì mới quan trọng?"
    },
    {
        "start": "00:11:52,520",
        "end": "00:11:55,000",
        "en": "Gagak dapat minum air atau tidak?",
        "pa": "Did Gagak manage to drink the water?",
        "vi": "Gagak có uống được nước không?"
    },
    {
        "start": "00:11:55,080",
        "end": "00:11:56,920",
        "en": "Kancil macam mana?",
        "pa": "How about Kancil?",
        "vi": "Còn Kancil thì sao?"
    },
    {
        "start": "00:11:57,000",
        "end": "00:11:59,560",
        "en": "- Jumpa makanan tak? - Ara.",
        "pa": "-Did he find food? -Ara.",
        "vi": "- Nó có tìm được thức ăn không ạ? - Ara."
    },
    {
        "start": "00:11:59,640",
        "end": "00:12:03,280",
        "en": "Aki cerita pasal Gagak. Dengarlah dulu.",
        "pa": "Aki is talking about Gagak. Let's hear that first.",
        "vi": "Ông đang nói về Gagak mà. Nghe về nó trước đã."
    },
    {
        "start": "00:12:05,400",
        "end": "00:12:07,360",
        "en": "Lepas itu…",
        "pa": "After that…",
        "vi": "Sau đó…"
    },
    {
        "start": "00:12:16,080",
        "end": "00:12:18,200",
        "en": "Sakitnya.",
        "pa": "It hurts. I didn't get water,",
        "vi": "Đau quá. Mình không uống được nước,"
    },
    {
        "start": "00:12:18,280",
        "end": "00:12:21,240",
        "en": "Air tak dapat, paruh cedera.",
        "pa": "and my beak hurts.",
        "vi": "còn cái mỏ thì bị đau."
    },
    {
        "start": "00:12:23,200",
        "end": "00:12:25,480",
        "en": "Macam mana nak minum air ini?",
        "pa": "How do I drink that water?",
        "vi": "Làm sao uống được nước đây?"
    },
    {
        "start": "00:13:44,920",
        "end": "00:13:46,080",
        "en": "Kau rupanya.",
        "pa": "It's just you.",
        "vi": "Thì ra là cậu."
    },
    {
        "start": "00:13:47,200",
        "end": "00:13:49,080",
        "en": "Kau jangan minum air aku.",
        "pa": "Don't you drink my water!",
        "vi": "Đừng uống nước của tôi!"
    },
    {
        "start": "00:13:50,480",
        "end": "00:13:53,440",
        "en": "Tak nak! Aku tak haus pun.",
        "pa": "I won't. I'm not even thirsty.",
        "vi": "Không đâu. Tôi cũng đâu có khát nước."
    },
    {
        "start": "00:13:55,880",
        "end": "00:13:56,760",
        "en": "Tak boleh.",
        "pa": "You can't…",
        "vi": "Cậu không thể…"
    },
    {
        "start": "00:13:59,000",
        "end": "00:14:00,800",
        "en": "Kau dah minum atau belum?",
        "pa": "Have you drunk it yet?",
        "vi": "Cậu đã uống chưa?"
    },
    {
        "start": "00:14:05,560",
        "end": "00:14:07,440",
        "en": "Belum!",
        "pa": "I haven't.",
        "vi": "Vẫn chưa."
    },
    {
        "start": "00:14:12,000",
        "end": "00:14:15,040",
        "en": "Paruh aku tak sampai.",
        "pa": "My beak can't reach it…",
        "vi": "Mỏ của tôi không với tới…"
    },
    {
        "start": "00:14:21,880",
        "end": "00:14:23,360",
        "en": "Agak dah.",
        "pa": "I knew it…",
        "vi": "Tôi biết mà…"
    },
    {
        "start": "00:14:25,480",
        "end": "00:14:26,720",
        "en": "Wahai Si Gagak.",
        "pa": "It's okay, Gagak.",
        "vi": "Không sao đâu, Gagak."
    },
    {
        "start": "00:14:28,920",
        "end": "00:14:32,040",
        "en": "Di mana ada kemahuan,",
        "pa": "Where there's a will,",
        "vi": "Có chí"
    },
    {
        "start": "00:14:32,560",
        "end": "00:14:35,840",
        "en": "di situ ada jalan.",
        "pa": "there's a way…",
        "vi": "thì nên…"
    },
    {
        "start": "00:14:41,400",
        "end": "00:14:42,640",
        "en": "Aku nak minum.",
        "pa": "I want to drink it!",
        "vi": "Tôi muốn uống nước!"
    },
    {
        "start": "00:14:43,840",
        "end": "00:14:45,080",
        "en": "Sabar gagak!",
        "pa": "Calm down Gagak, calm down.",
        "vi": "Bình tĩnh nào, Gagak."
    },
    {
        "start": "00:14:50,120",
        "end": "00:14:51,320",
        "en": "Kuatnya!",
        "pa": "So strong!",
        "vi": "Khỏe thế!"
    },
    {
        "start": "00:15:16,600",
        "end": "00:15:18,400",
        "en": "Begitu!",
        "pa": "Now I understand!",
        "vi": "Giờ mình hiểu rồi!"
    },
    {
        "start": "00:15:19,920",
        "end": "00:15:21,640",
        "en": "Aku boleh tolong kau, Gagak.",
        "pa": "I can help you, Gagak!",
        "vi": "Tôi có thể giúp cậu đấy, Gagak!"
    },
    {
        "start": "00:15:23,960",
        "end": "00:15:25,160",
        "en": "Macam mana Kancil?",
        "pa": "How, Kancil?",
        "vi": "Bằng cách nào, Kancil?"
    },
    {
        "start": "00:15:27,440",
        "end": "00:15:28,800",
        "en": "Mula-mula…",
        "pa": "Firstly…",
        "vi": "Đầu tiên…"
    },
    {
        "start": "00:15:46,640",
        "end": "00:15:50,480",
        "en": "Betul cakap Kancil. Kancil memang bijak.",
        "pa": "Kancil was right! Kancil is smart.",
        "vi": "Kancil nói đúng! Kancil thật thông minh."
    },
    {
        "start": "00:15:50,560",
        "end": "00:15:52,960",
        "en": "Kenapa aku tak terfikir dari tadi?",
        "pa": "Why didn't I think of that before?",
        "vi": "Sao mình lại không nghĩ ra cách đó nhỉ?"
    },
    {
        "start": "00:15:58,160",
        "end": "00:16:01,880",
        "en": "Kalau guna batu besar, mungkin lebih cepat air naik.",
        "pa": "If I use a bigger rock, maybe the water will rise faster.",
        "vi": "Nếu mình dùng viên đá to hơn có lẽ nước sẽ dâng lên nhanh hơn."
    },
    {
        "start": "00:16:10,440",
        "end": "00:16:12,400",
        "en": "Macam mana nak buat?",
        "pa": "What should I do?",
        "vi": "Mình nên làm gì đây?"
    },
    {
        "start": "00:16:16,720",
        "end": "00:16:18,520",
        "en": "Tolong!",
        "pa": "Help!",
        "vi": "Giúp tôi với!"
    },
    {
        "start": "00:16:25,520",
        "end": "00:16:28,560",
        "en": "Nasib baik.",
        "pa": "That was close.",
        "vi": "Suýt nữa thì hỏng."
    },
    {
        "start": "00:16:52,200",
        "end": "00:16:54,120",
        "en": "Leganya.",
        "pa": "Refreshing…",
        "vi": "Sảng khoái quá…"
    },
    {
        "start": "00:17:04,040",
        "end": "00:17:05,720",
        "en": "Gagak…",
        "pa": "Gagak--",
        "vi": "Gagak…"
    },
    {
        "start": "00:17:06,520",
        "end": "00:17:07,480",
        "en": "aku…",
        "pa": "I--",
        "vi": "Tôi…"
    },
    {
        "start": "00:17:08,000",
        "end": "00:17:09,000",
        "en": "air…",
        "pa": "that--",
        "vi": "cái đó…"
    },
    {
        "start": "00:17:09,079",
        "end": "00:17:10,200",
        "en": "itu…",
        "pa": "water--",
        "vi": "nước…"
    },
    {
        "start": "00:17:10,280",
        "end": "00:17:12,079",
        "en": "Kesian kau Kancil. Tunggu sebentar.",
        "pa": "Poor you, Kancil. Wait here!",
        "vi": "Kancil đáng thương. Đợi ở đây!"
    },
    {
        "start": "00:17:28,400",
        "end": "00:17:29,600",
        "en": "Kancil.",
        "pa": "Kancil.",
        "vi": "Kancil."
    },
    {
        "start": "00:17:40,880",
        "end": "00:17:43,320",
        "en": "Akhirnya, gagak dan kancil",
        "pa": "Finally, Gagak and Kancil",
        "vi": "Cuối cùng, Gagak và Kancil"
    },
    {
        "start": "00:17:43,400",
        "end": "00:17:46,400",
        "en": "dapat minum air sampai puas.",
        "pa": "got to drink until they were satisfied.",
        "vi": "đã được uống nước cho đã đời."
    },
    {
        "start": "00:17:46,480",
        "end": "00:17:48,560",
        "en": "Habis.",
        "pa": "The end.",
        "vi": "Hết chuyện…"
    },
    {
        "start": "00:17:48,640",
        "end": "00:17:50,480",
        "en": "Air habis?",
        "pa": "Did they finish the water?",
        "vi": "Chúng có uống hết nước không ạ?"
    },
    {
        "start": "00:17:50,560",
        "end": "00:17:52,320",
        "en": "Ya.",
        "pa": "Yes.",
        "vi": "Có."
    },
    {
        "start": "00:17:52,840",
        "end": "00:17:55,720",
        "en": "Nanti bila haus lagi, macam mana?",
        "pa": "What if they get thirsty again?",
        "vi": "Lỡ đâu chúng lại khát nước thì sao?"
    },
    {
        "start": "00:17:56,240",
        "end": "00:17:57,800",
        "en": "Itulah.",
        "pa": "That's why…",
        "vi": "Thế nên mới có câu…"
    },
    {
        "start": "00:17:57,880",
        "end": "00:18:01,320",
        "en": "\"Di mana ada kemahuan, di situ ada jalan\".",
        "pa": "Where there's a will, there's a way.",
        "vi": "Có chí thì nên."
    },
    {
        "start": "00:18:02,080",
        "end": "00:18:05,720",
        "en": "Bila kita mahukan sesuatu,",
        "pa": "When we want something,",
        "vi": "Khi chúng ta muốn một thứ gì đó,"
    },
    {
        "start": "00:18:05,800",
        "end": "00:18:10,280",
        "en": "kita pasti akan menemui cara untuk mendapatkannya.",
        "pa": "we'll surely find a way to get it.",
        "vi": "chúng ta sẽ tìm ra cách để đạt được."
    },
    {
        "start": "00:18:11,040",
        "end": "00:18:12,680",
        "en": "Macam Ara!",
        "pa": "Like me!",
        "vi": "Giống như cháu vậy!"
    },
    {
        "start": "00:18:12,760",
        "end": "00:18:16,120",
        "en": "Ara nak tahu cerita dalam buku ini.",
        "pa": "I want to know the story in this book.",
        "vi": "Cháu muốn biết câu chuyện trong quyển sách này."
    },
    {
        "start": "00:18:16,200",
        "end": "00:18:19,680",
        "en": "Jadi Ara usaha juga untuk baca.",
        "pa": "So I will work hard to read it.",
        "vi": "Vì thế cháu sẽ thật cố gắng để có thể đọc được."
    },
    {
        "start": "00:18:19,760",
        "end": "00:18:22,680",
        "en": "Walaupun merangkak-rangkak.",
        "pa": "Even if you're slow at first.",
        "vi": "Dù ban đầu cháu còn hơi chậm."
    },
    {
        "start": "00:18:22,760",
        "end": "00:18:26,680",
        "en": "Itu satu usaha yang baik. Ini barulah cucu Aki.",
        "pa": "That's a good effort. Now that's my grandchild.",
        "vi": "Nỗ lực như thế là tốt. Thế mới là cháu của ông."
    },
    {
        "start": "00:18:29,560",
        "end": "00:18:31,600",
        "en": "Tengoklah siapa yang ajar.",
        "pa": "Look who taught you.",
        "vi": "Xem ai dạy cho em đã."
    },
    {
        "start": "00:18:31,680",
        "end": "00:18:32,880",
        "en": "Abang!",
        "pa": "Aris!",
        "vi": "Anh!"
    },
    {
        "start": "00:18:33,680",
        "end": "00:18:35,600",
        "en": "Ara, sambung baca.",
        "pa": "Ara, continue.",
        "vi": "Ara, tiếp tục đi."
    },
    {
        "start": "00:18:39,280",
        "end": "00:18:41,520",
        "en": "G… A…",
        "pa": "THE RABBIT AND THE TORTOISE\n G, A.",
        "vi": "ARNAB VÀ KURA-KURA\n G, A."
    },
    {
        "start": "00:18:43,000",
        "end": "00:18:44,320",
        "en": "Ini apa?",
        "pa": "What's this?",
        "vi": "Cái gì đây anh?"
    },
    {
        "start": "00:18:47,320",
        "end": "00:18:48,680",
        "en": "Gagak.",
        "pa": "Gagak.",
        "vi": "Gagak."
    },
    {
        "start": "00:18:49,800",
        "end": "00:18:52,640",
        "en": "Aris, ajar betul-betul.",
        "pa": "Aris, teach her properly.",
        "vi": "Aris, dạy em cháu cho tử tế vào."
    },
    {
        "start": "00:18:54,640",
        "end": "00:18:56,240",
        "en": "Gajah.",
        "pa": "Gajah.",
        "vi": "Gajah."
    },
    {
        "start": "00:18:57,360",
        "end": "00:19:00,720",
        "en": "Dengar teka-teki ini. Ada seekor kambing,",
        "pa": "Listen to this riddle.\n There was once a goat.",
        "vi": "Nghe ông đố nhé."
    },
    {
        "start": "00:19:00,800",
        "end": "00:19:05,960",
        "en": "kambing itu boleh ke depan, ke kiri dan ke kanan.",
        "pa": "This goat could move forward, left, and right.",
        "vi": "Có một con dê.\n Nó có thể đi lên phía trước, đi sang trái và sang phải."
    },
    {
        "start": "00:19:06,040",
        "end": "00:19:07,720",
        "en": "Tapi tak boleh ke belakang.",
        "pa": "But it couldn't move backward.",
        "vi": "Nhưng nó không thể đi lùi lại."
    },
    {
