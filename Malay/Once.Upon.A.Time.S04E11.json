[
    {
        "start": "00:00:21,000",
        "end": "00:00:24,080",
        "en": "Pada zaman dahulu…",
        "pa": "Once upon a time…",
        "vi": "Ngày xửa ngày xưa…"
    },
    {
        "start": "00:00:39,400",
        "end": "00:00:40,560",
        "en": "Kamu nak ikut Aki?",
        "pa": "Do you want to follow me?",
        "vi": "Đi theo ông không?"
    },
    {
        "start": "00:00:41,160",
        "end": "00:00:42,160",
        "en": "Pergi mana?",
        "pa": "Where to?",
        "vi": "Đi đâu ạ?"
    },
    {
        "start": "00:00:43,000",
        "end": "00:00:45,360",
        "en": "Pikat burung!",
        "pa": "To get some birds!",
        "vi": "Đi bắt chim!"
    },
    {
        "start": "00:00:57,840",
        "end": "00:00:58,840",
        "en": "Aduh!",
        "pa": "",
        "vi": "Gì thế?"
    },
    {
        "start": "00:01:00,520",
        "end": "00:01:02,200",
        "en": "Tak ada burung pun.",
        "pa": "Still no bird…",
        "vi": "Vẫn chưa có con chim nào…"
    },
    {
        "start": "00:01:02,280",
        "end": "00:01:04,360",
        "en": "Sampai bila nak tunggu?",
        "pa": "How long do we have to wait?",
        "vi": "Mình phải đợi bao lâu nữa vậy ông?"
    },
    {
        "start": "00:01:05,080",
        "end": "00:01:08,840",
        "en": "Bila burung nak datang? Ara dah penat.",
        "pa": "When is the bird coming? I'm so tired.",
        "vi": "Khi nào chim mới đến? Cháu mệt quá."
    },
    {
        "start": "00:01:09,840",
        "end": "00:01:12,760",
        "en": "Jangan bising. Nanti burung lari.",
        "pa": "Don't be noisy, or the birds will fly away.",
        "vi": "Đừng làm ồn, lũ chim bay mất bây giờ."
    },
    {
        "start": "00:01:12,840",
        "end": "00:01:15,680",
        "en": "Nak pikat burung, kena banyak sabar.",
        "pa": "If you want catch them, you must be patient.",
        "vi": "Nếu cháu muốn bắt chim thì phải kiên nhẫn."
    },
    {
        "start": "00:01:31,200",
        "end": "00:01:32,480",
        "en": "Aki!",
        "pa": "Aki!",
        "vi": "Ông ơi!"
    },
    {
        "start": "00:01:33,040",
        "end": "00:01:35,280",
        "en": "Guna telefon bimbit, lagi senang.",
        "pa": "Use a cell phone, it's easier.",
        "vi": "Dùng điện thoại di động đi, sẽ dễ hơn."
    },
    {
        "start": "00:01:35,960",
        "end": "00:01:39,360",
        "en": "Main saja bunyi itu ulang-ulang, tak payah tiup-tiup.",
        "pa": "You can keep repeating the sound. No need to play the flute.",
        "vi": "Ông chỉ cần phát lại tiếng chim thôi. Không cần phải thổi sáo."
    },
    {
        "start": "00:01:43,040",
        "end": "00:01:47,680",
        "en": "Tapi Aki tak ada telefon bimbit. Guna ini pun dah cukup.",
        "pa": "I don't have a cell phone. This is good enough.",
        "vi": "Ông không có điện thoại di động. Cái này cũng tốt rồi."
    },
    {
        "start": "00:01:54,800",
        "end": "00:01:57,840",
        "en": "Aki!",
        "pa": "Aki!",
        "vi": "Ông!"
    },
    {
        "start": "00:01:58,360",
        "end": "00:02:00,040",
        "en": "Aki, lapar!",
        "pa": "Aki! I'm hungry…",
        "vi": "Ông à! Cháu đói quá…"
    },
    {
        "start": "00:02:06,160",
        "end": "00:02:07,560",
        "en": "Yalah.",
        "pa": "Alright.",
        "vi": "Được rồi."
    },
    {
        "start": "00:02:14,880",
        "end": "00:02:18,120",
        "en": "Ini untuk Ara dan yang ini untuk Aris.",
        "pa": "This is for Ara, and this is for Aris",
        "vi": "Phần này của Ara, còn phần này của Aris."
    },
    {
        "start": "00:02:20,280",
        "end": "00:02:22,160",
        "en": "Sedapnya!",
        "pa": "Looks delicious…",
        "vi": "Trông ngon quá…"
    },
    {
        "start": "00:02:34,480",
        "end": "00:02:35,960",
        "en": "Ara, abang nak sikit?",
        "pa": "Ara, can I have some?",
        "vi": "Ara, cho anh một ít nhé?"
    },
    {
        "start": "00:02:37,080",
        "end": "00:02:38,760",
        "en": "Tak boleh.",
        "pa": "You can't.",
        "vi": "Không cho."
    },
    {
        "start": "00:02:40,480",
        "end": "00:02:41,560",
        "en": "Nak?",
        "pa": "Do you want some?",
        "vi": "Em muốn ăn không?"
    },
    {
        "start": "00:02:46,400",
        "end": "00:02:48,440",
        "en": "- Ara tengok sana! - Apa?",
        "pa": "-Ara! Look over there! -What?",
        "vi": "- Ara! Nhìn bên kia kìa! - Gì cơ?"
    },
    {
        "start": "00:02:50,160",
        "end": "00:02:51,640",
        "en": "Tak ada apa-apa.",
        "pa": "It's nothing.",
        "vi": "Không có gì."
    },
    {
        "start": "00:02:58,360",
        "end": "00:02:59,800",
        "en": "- Pelangi! - Mana?",
        "pa": "-Rainbow! -Where?",
        "vi": "- Cầu vồng! - Ở đâu nào?"
    },
    {
        "start": "00:03:00,880",
        "end": "00:03:04,000",
        "en": "Tak ada pun. Abang tipu.",
        "pa": "There's nothing. You're lying.",
        "vi": "Không có gì cả. Anh nói dối."
    },
    {
        "start": "00:03:06,320",
        "end": "00:03:08,760",
        "en": "Tinggal sikit?",
        "pa": "There are only a few left?",
        "vi": "Chỉ còn lại một ít thôi à?"
    },
    {
        "start": "00:03:09,960",
        "end": "00:03:12,200",
        "en": "Dah asyik makan, memanglah tinggal sikit.",
        "pa": "You kept eating, of course there are only a few left.",
        "vi": "Em cứ ăn nên dĩ nhiên chỉ còn một ít rồi."
    },
    {
        "start": "00:03:13,040",
        "end": "00:03:16,560",
        "en": "Tadi banyak. Abang ambil, betul tak?",
        "pa": "There were a lot just now. Did you take them?",
        "vi": "Mới nãy còn nhiều lắm mà. Anh có lấy không đấy?"
    },
    {
        "start": "00:03:16,640",
        "end": "00:03:20,200",
        "en": "Mana ada, jangan tuduh-tuduh. Dosa!",
        "pa": "I did not! Don't start accusing! It's wrong!",
        "vi": "Không có! Đừng buộc tội anh! Làm thế là sai đấy!"
    },
    {
        "start": "00:03:22,600",
        "end": "00:03:26,120",
        "en": "Bukan tuduh, memang betul Aris ambil.",
        "pa": "That's not accusing. It's true Aris took them.",
        "vi": "Cái đó không phải buộc tội. Sự thật là Aris lấy đấy."
    },
    {
        "start": "00:03:26,200",
        "end": "00:03:27,960",
        "en": "Empat mata ini yang nampak.",
        "pa": "These four eyes saw it.",
        "vi": "Có bốn con mắt nhìn thấy này."
    },
    {
        "start": "00:03:32,800",
        "end": "00:03:34,800",
        "en": "Aris gurau-gurau saja.",
        "pa": "I was just playing…",
        "vi": "Cháu chỉ đùa…"
    },
    {
        "start": "00:03:36,560",
        "end": "00:03:38,080",
        "en": "Ganti balik!",
        "pa": "Give them back!",
        "vi": "Trả lại cho em!"
    },
    {
        "start": "00:03:39,560",
        "end": "00:03:40,920",
        "en": "Aris.",
        "pa": "Aris.",
        "vi": "Aris à."
    },
    {
        "start": "00:03:41,520",
        "end": "00:03:45,760",
        "en": "Tengok perangai kau, teringat Aki satu cerita.",
        "pa": "Your behavior reminds me of a story.",
        "vi": "Cách cư xử của cháu làm ông nhớ đến một câu chuyện."
    },
    {
        "start": "00:03:46,920",
        "end": "00:03:49,120",
        "en": "Aki! Ceritalah!",
        "pa": "Aki! Tell us the story.",
        "vi": "Ông à! Kể cho chúng cháu nghe đi."
    },
    {
        "start": "00:03:51,040",
        "end": "00:03:53,840",
        "en": "Pada zaman dahulu…",
        "pa": "Once upon a time…",
        "vi": "Ngày xửa ngày xưa…"
    },
    {
        "start": "00:04:28,280",
        "end": "00:04:29,360",
        "en": "Sedapnya!",
        "pa": "It's so delicious…",
        "vi": "Ngon quá…"
    },
    {
        "start": "00:04:39,080",
        "end": "00:04:43,280",
        "en": "Sedapnya! Tapi sikit sangat.",
        "pa": "So delicious! But it's not enough.",
        "vi": "Ngon quá! Nhưng vẫn chưa đủ."
    },
    {
        "start": "00:05:02,480",
        "end": "00:05:04,640",
        "en": "Puaslah aku makan!",
        "pa": "Now I'll be satisfied!",
        "vi": "Giờ thì mình sẽ no nê đây!"
    },
    {
        "start": "00:05:24,240",
        "end": "00:05:26,800",
        "en": "Tidak!",
        "pa": "No!",
        "vi": "Không!"
    },
    {
        "start": "00:05:29,800",
        "end": "00:05:32,280",
        "en": "Telur-telur aku!",
        "pa": "My eggs!",
        "vi": "Trứng của tôi!"
    },
    {
        "start": "00:05:34,840",
        "end": "00:05:37,280",
        "en": "Telur aku hilang!",
        "pa": "My eggs are missing!",
        "vi": "Trứng của tôi bị mất rồi!"
    },
    {
        "start": "00:05:41,480",
        "end": "00:05:42,480",
        "en": "Serigala!",
        "pa": "Serigala!",
        "vi": "Serigala!"
    },
    {
        "start": "00:05:45,400",
        "end": "00:05:47,960",
        "en": "Tekukur! Apa hal?",
        "pa": "Terkukur. What's the matter?",
        "vi": "Terkukur. Có chuyện gì thế?"
    },
    {
        "start": "00:05:48,560",
        "end": "00:05:52,000",
        "en": "Serigala! Kau ada nampak sarang dan telur-telur aku?",
        "pa": "Serigala! Did you see my nest and my eggs?",
        "vi": "Serigala! Anh có thấy tổ và trứng của tôi không?"
    },
    {
        "start": "00:05:56,000",
        "end": "00:05:57,040",
        "en": "Tak ada.",
        "pa": "I did not.",
        "vi": "Không thấy."
    },
    {
        "start": "00:05:57,920",
        "end": "00:06:01,040",
        "en": "Kau jalan ikut sini tadi. Takkan tak nampak apa-apa?",
        "pa": "You were passing here just now! You must have seen something!",
        "vi": "Anh vừa đi ngang qua đây! Chắc hẳn anh phải nhìn thấy gì đó."
    },
    {
        "start": "00:06:01,680",
        "end": "00:06:05,520",
        "en": "Sarang dan telur-telur aku dah hilang! Dah kena curi!",
        "pa": "My nest and my eggs are missing! They've been stolen…",
        "vi": "Tổ và trứng của tôi biến mất rồi! Chúng đã bị trộm…"
    },
    {
        "start": "00:06:06,440",
        "end": "00:06:07,800",
        "en": "Apa? Kesian kau.",
        "pa": "Poor thing.",
        "vi": "Khổ thân chị."
    },
    {
        "start": "00:06:07,880",
        "end": "00:06:11,200",
        "en": "Itulah, lain kali jaga sarang kau baik-baik.",
        "pa": "Next time, take better care of your nest.",
        "vi": "Lần sau, nhớ trông nom tổ cẩn thận hơn nhé."
    },
    {
        "start": "00:06:13,160",
        "end": "00:06:16,800",
        "en": "Serigala, apa yang warna kuning dekat bibir kau?",
        "pa": "Serigala! What is that yellow stuff on your lips?",
        "vi": "Serigala! Môi anh dính cái gì vàng vàng thế?"
    },
    {
        "start": "00:06:16,880",
        "end": "00:06:18,560",
        "en": "Macam…",
        "pa": "Looks like…",
        "vi": "Trông cứ như…"
    },
    {
        "start": "00:06:20,200",
        "end": "00:06:21,720",
        "en": "Mana ada apa-apa.",
        "pa": "It's nothing!",
        "vi": "Không có gì cả!"
    },
    {
        "start": "00:06:22,360",
        "end": "00:06:24,920",
        "en": "Entah-entah…",
        "pa": "Maybe…",
        "vi": "Phải chăng…"
    },
    {
        "start": "00:06:25,000",
        "end": "00:06:26,240",
        "en": "Kau yang makan telur aku?",
        "pa": "You're the one who ate my eggs?",
        "vi": "Anh đã ăn trứng của tôi?"
    },
    {
        "start": "00:06:28,840",
        "end": "00:06:30,000",
        "en": "Jangan memandai.",
        "pa": "Don't accuse me!",
        "vi": "Đừng buộc tội tôi!"
    },
    {
        "start": "00:06:30,080",
        "end": "00:06:31,960",
        "en": "Nak tuduh, biar ada bukti!",
        "pa": "If you want to accuse me, you must have proof!",
        "vi": "Muốn buộc tội tôi thì phải có chứng cứ!"
    },
    {
        "start": "00:06:42,080",
        "end": "00:06:44,840",
        "en": "Merbah! Kau nak ke mana?",
        "pa": "Merbah, where are you going?",
        "vi": "Merbah, chị đi đâu đấy?"
    },
    {
        "start": "00:06:52,080",
        "end": "00:06:53,840",
        "en": "Aku nak cari makanan.",
        "pa": "I'm going out to find food.",
        "vi": "Tôi đi ra ngoài tìm thức ăn!"
    },
    {
        "start": "00:06:53,920",
        "end": "00:06:56,440",
        "en": "Kawasan sarang aku dah kekurangan makanan.",
        "pa": "There's not much food near my nest.",
        "vi": "Gần tổ của tôi không có nhiều thức ăn lắm."
    },
    {
        "start": "00:06:56,520",
        "end": "00:06:59,720",
        "en": "Yakah? Di mana sarang kau?",
        "pa": "Really? Where is your nest?",
        "vi": "Thật à? Tổ của chị ở đâu?"
    },
    {
        "start": "00:06:59,800",
        "end": "00:07:00,960",
        "en": "Di situ.",
        "pa": "Over there!",
        "vi": "Bên kia kìa!"
    },
    {
        "start": "00:07:02,280",
        "end": "00:07:05,320",
        "en": "Kalau begitu, eloklah kau ke sana.",
        "pa": "If that's the case, it's better if you go that side.",
        "vi": "Nếu vậy thì tốt hơn là chị nên đi hướng bên kia."
    },
    {
        "start": "00:07:06,360",
        "end": "00:07:08,040",
        "en": "Di sana ada pokok mangga.",
        "pa": "There's a mango tree over there.",
        "vi": "Ở bên đó có một cây xoài."
    },
    {
        "start": "00:07:08,120",
        "end": "00:07:10,800",
        "en": "Buahnya sedang masak ranum.",
        "pa": "The fruit is ripe.",
        "vi": "Trái đã chín mọng."
    },
    {
        "start": "00:07:10,880",
        "end": "00:07:14,520",
        "en": "Mangga! Sedapnya.",
        "pa": "Mango! Delicious.",
        "vi": "Xoài! Ngon quá."
    },
    {
        "start": "00:07:14,600",
        "end": "00:07:15,920",
        "en": "Dah lama tak makan.",
        "pa": "I haven't eaten that for a while.",
        "vi": "Lâu rồi tôi chưa ăn xoài."
    },
    {
        "start": "00:07:16,600",
        "end": "00:07:18,440",
        "en": "Terima kasih, Serigala.",
        "pa": "Thank you, Serigala.",
        "vi": "Cảm ơn Serigala."
    },
    {
        "start": "00:07:22,080",
        "end": "00:07:24,440",
        "en": "Mesti ada telur Merbah di sana.",
        "pa": "There must be some of Merbah's eggs there.",
        "vi": "Chắc phải có vài quả trứng của Merbah ở đó."
    },
    {
        "start": "00:07:25,960",
        "end": "00:07:28,080",
        "en": "Kenyanglah aku.",
        "pa": "I'll be so full…",
        "vi": "Mình sẽ no căng…"
    },
    {
        "start": "00:07:55,000",
        "end": "00:07:56,360",
        "en": "Aduh!",
        "pa": "Ouch!",
        "vi": ""
    },
    {
        "start": "00:07:59,080",
        "end": "00:08:01,120",
        "en": "Takkan tiada seekor burung pun yang bertelur.",
        "pa": "I can't believe not even one bird laid an egg!",
        "vi": "Không tin nổi không con chim nào đẻ trứng!"
    },
    {
        "start": "00:08:05,360",
        "end": "00:08:08,160",
        "en": "Kau cari telur burung, Serigala?",
        "pa": "You're looking for bird's eggs, right, Serigala?",
        "vi": "Cậu tìm trứng chim à, Serigala?"
    },
    {
        "start": "00:08:08,240",
        "end": "00:08:11,720",
        "en": "Tupai! Mana ada.",
        "pa": "Tupai! No, I wasn't.",
        "vi": "Tupai! Không, đâu có."
    },
    {
        "start": "00:08:11,800",
        "end": "00:08:14,320",
        "en": "Aku nak letak balik sarang yang jatuh ini.",
        "pa": "I was putting this nest back because it fell.",
        "vi": "Tôi chỉ đặt cái tổ này lại vì nó bị rơi."
    },
    {
        "start": "00:08:21,120",
        "end": "00:08:23,360",
        "en": "Kau ingat aku tak tahu?",
        "pa": "You think you can fool me?",
        "vi": "Cậu nghĩ cậu lừa được tôi à?"
    },
    {
        "start": "00:08:24,400",
        "end": "00:08:25,600",
        "en": "Pergi kau dari sini!",
        "pa": "Get away from here now!",
        "vi": "Cút khỏi đây mau!"
    },
    {
        "start": "00:08:33,679",
        "end": "00:08:35,919",
        "en": "Aku akan datang lagi.",
        "pa": "I'll be back.",
        "vi": "Mình sẽ trở lại."
    },
    {
        "start": "00:08:37,320",
        "end": "00:08:38,400",
        "en": "Tupai?",
        "pa": "Tupai?",
        "vi": "Tupai?"
    },
    {
        "start": "00:08:38,480",
        "end": "00:08:40,320",
        "en": "Kau buat apa dekat sarang aku?",
        "pa": "What are you doing here at my nest?",
        "vi": "Ông làm gì ở tổ của tôi vậy?"
    },
    {
        "start": "00:08:41,600",
        "end": "00:08:45,920",
        "en": "Tadi Serigala datang, nak makan telur-telur kau.",
        "pa": "Serigala was here, trying to eat your eggs.",
        "vi": "Serigala vừa mới đến đây định ăn trứng của chị đấy."
    },
    {
        "start": "00:08:46,920",
        "end": "00:08:48,760",
        "en": "Tupai, mana telur aku?",
        "pa": "Tupai, where are my eggs?",
        "vi": "Tupai, trứng của tôi đâu?"
    },
    {
        "start": "00:08:48,840",
        "end": "00:08:52,480",
        "en": "- Mana telur-telur aku Tupai? - Situ, aku pindahkan ke dahan tinggi.",
        "pa": "-Where are my eggs? -There, I moved them to a higher place.",
        "vi": "- Trứng của tôi đâu? - Trên kia, tôi đã dời chúng lên cao."
    },
    {
        "start": "00:08:53,400",
        "end": "00:08:56,160",
        "en": "Nasib baik. Terima kasih, Tupai.",
        "pa": "Thank goodness. Thank you, Tupai.",
        "vi": "Tạ ơn trời đất. Cảm ơn Tupai."
    },
    {
        "start": "00:08:56,920",
        "end": "00:09:01,120",
        "en": "Tapi Serigala pasti akan kembali.",
        "pa": "However, Serigala will surely come again.",
        "vi": "Nhưng mà, Serigala chắc chắn sẽ quay trở lại."
    },
    {
        "start": "00:09:01,200",
        "end": "00:09:03,160",
        "en": "Tolong bagi tahu burung-burung lain.",
        "pa": "Please tell the other birds to be careful.",
        "vi": "Hãy cảnh báo những con chim khác."
    },
    {
        "start": "00:09:03,240",
        "end": "00:09:09,360",
        "en": "Supaya berhati-hati! Jangan tinggalkan sarang tanpa dijaga.",
        "pa": "Don't leave your nest unattended!",
        "vi": "Đừng bỏ tổ không ai trông!"
    },
    {
        "start": "00:09:18,040",
        "end": "00:09:21,440",
        "en": "Aki, mesti burung-burung semua risau?",
        "pa": "Aki, the birds must be worried, right?",
        "vi": "Ông à, mấy con chim chắc là lo lắm, phải không?"
    },
    {
        "start": "00:09:22,840",
        "end": "00:09:27,280",
        "en": "Mestilah. Tapi burung-burung itu tak tahu nak buat apa.",
        "pa": "Of course, but the birds don't know what to do.",
        "vi": "Dĩ nhiên rồi, nhưng lũ chim không biết phải làm gì."
    },
    {
        "start": "00:09:27,880",
        "end": "00:09:31,320",
        "en": "Serigala pula makin sedap makan telur burung.",
        "pa": "As for Serigala, he is really enjoying eating the eggs.",
        "vi": "Về phần Serigala, nó rất thích ăn trứng."
    },
    {
        "start": "00:09:32,960",
        "end": "00:09:34,360",
        "en": "Buatlah sarang di tempat tinggi.",
        "pa": "Make your nests higher up.",
        "vi": "Vậy thì làm tổ cao hơn."
    },
    {
        "start": "00:09:35,960",
        "end": "00:09:38,840",
        "en": "Sarang tinggi ada bahaya lain pula.",
        "pa": "A higher nest has its own danger.",
        "vi": "Tổ cao hơn cũng có nguy hiểm đấy."
    },
    {
        "start": "00:09:38,920",
        "end": "00:09:40,920",
        "en": "Kesian burung semua.",
        "pa": "Poor birds.",
        "vi": "Lũ chim đáng thương quá."
    },
    {
        "start": "00:09:41,000",
        "end": "00:09:42,920",
        "en": "Habis itu macam mana Aki? Macam mana?",
        "pa": "Then what happened, Aki? What happened?",
        "vi": "Sau đó thì sao ông? Chuyện gì xảy ra?"
    },
    {
        "start": "00:09:47,240",
        "end": "00:09:50,320",
        "en": "Habis itu, burung-burung pun…",
        "pa": "So then, the birds…",
        "vi": "Thế là lũ chim…"
    },
    {
        "start": "00:09:53,000",
        "end": "00:09:54,840",
        "en": "Sampai bila kita nak begini?",
        "pa": "When is this going to stop?",
        "vi": "Đến bao giờ chuyện này mới kết thúc?"
    },
    {
        "start": "00:09:54,920",
        "end": "00:09:57,880",
        "en": "Nak terbang bebas cari makan pun susah.",
        "pa": "We can't even fly freely to find food.",
        "vi": "Chúng ta không thể tự do bay đi tìm thức ăn."
    },
    {
        "start": "00:09:57,960",
        "end": "00:10:02,320",
        "en": "Serigala dah melampau. Kita mesti halau dia dari hutan ini.",
        "pa": "Serigala is getting out of hand! We must get him out of this jungle!",
        "vi": "Serigala đã vượt khỏi tầm kiểm soát rồi! Chúng ta phải đuổi hắn khỏi khu rừng này!"
    },
    {
        "start": "00:10:02,400",
        "end": "00:10:04,360",
        "en": "Tapi atas sebab apa?",
        "pa": "But what's our reason?",
        "vi": "Nhưng lí do là gì đây?"
    },
    {
        "start": "00:10:04,440",
        "end": "00:10:07,760",
        "en": "Kita tak ada bukti yang dia makan telur-telur itu.",
        "pa": "We don't have proof he ate our eggs.",
        "vi": "Chúng ta không có bằng chứng hắn đã ăn trứng của chúng ta."
    },
    {
        "start": "00:10:09,560",
        "end": "00:10:12,400",
        "en": "Itu betul. Apa kata kita jumpa Tupai.",
        "pa": "That's right! Why don't we go and see Tupai?",
        "vi": "Đúng rồi! Sao chúng ta không đến gặp Tupai?"
    },
    {
        "start": "00:10:21,840",
        "end": "00:10:23,040",
        "en": "Tupai!",
        "pa": "-Tupai! -Tupai!",
        "vi": "- Tupai! - Tupai!"
    },
    {
        "start": "00:10:46,760",
        "end": "00:10:48,440",
        "en": "Busuk!",
        "pa": "Smelly…",
        "vi": "BINH!\n Thối quá…"
    },
    {
        "start": "00:10:50,640",
        "end": "00:10:54,400",
        "en": "Maaf Kancil, aku tak sengaja.",
        "pa": "Sorry, Kancil. It was an accident.",
        "vi": "Xin lỗi, Kancil. Chỉ là tai nạn thôi."
    },
    {
        "start": "00:10:54,480",
        "end": "00:10:57,040",
        "en": "Apa hal bising-bising?",
        "pa": "What's all this noise?",
        "vi": "Vụ ồn ào này là sao đây?"
    },
    {
        "start": "00:10:57,120",
        "end": "00:10:59,560",
        "en": "- Kami ada masalah. - Tangkap Serigala!",
        "pa": "We have a problem!",
        "vi": "Chúng tôi gặp chuyện rồi!"
    },
    {
        "start": "00:10:59,640",
        "end": "00:11:02,520",
        "en": "- Serigala tak guna! - Habis telur-telur aku.",
        "pa": "Catch Serigala.\n Serigala is mean!\n My eggs are gone!",
        "vi": "Bắt Serigala.\n Serigala thật xấu xa!\n Trứng của tôi mất cả rồi!"
    },
    {
        "start": "00:11:02,600",
        "end": "00:11:06,280",
        "en": "Diam! Biar aku yang cakap.",
        "pa": "Silence! Let me do the talking.",
        "vi": "Im lặng nào! Để tôi nói cho."
    },
    {
        "start": "00:11:07,400",
        "end": "00:11:10,960",
        "en": "Tadi telur Merbah hampir dimakan Serigala.",
        "pa": "Just now, Serigala almost ate Merbah's eggs.",
        "vi": "Mới vừa rồi, Serigala suýt nữa đã ăn trứng của Merbah."
    },
    {
        "start": "00:11:11,560",
        "end": "00:11:13,760",
        "en": "Tapi aku sempat halang.",
        "pa": "However, I was there to stop it.",
        "vi": "Nhưng mà có ta ở đó chặn hắn lại."
    },
    {
        "start": "00:11:14,280",
        "end": "00:11:16,360",
        "en": "Aku sangka hanya Merbah,",
        "pa": "I thought it was only Merbah,",
        "vi": "Ta tưởng chỉ có mỗi Merbah,"
    },
    {
        "start": "00:11:17,040",
        "end": "00:11:20,960",
        "en": "rupanya semua burung telah menjadi mangsa Serigala.",
        "pa": "but it turns out every bird was a victim of Serigala.",
        "vi": "nhưng thì ra tất cả các loài chim đều là nạn nhân của Serigala."
    },
    {
        "start": "00:11:22,840",
        "end": "00:11:24,040",
        "en": "Jadi?",
        "pa": "Then?",
        "vi": "Rồi sao?"
    },
    {
        "start": "00:11:25,080",
        "end": "00:11:27,120",
        "en": "Kita perlu buat sesuatu.",
        "pa": "We need to do something.",
        "vi": "Chúng ta phải làm gì đó."
    },
    {
        "start": "00:11:27,760",
        "end": "00:11:30,840",
        "en": "Jika tidak, telur-telur burung semua,",
        "pa": "If not,\n all the birds' eggs will be eaten!",
        "vi": "Nếu không,\n tất cả trứng chim sẽ bị ăn hết!"
    },
    {
        "start": "00:11:30,920",
        "end": "00:11:32,800",
        "en": "akan habis dimakannya.",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:11:33,720",
        "end": "00:11:36,280",
        "en": "Betulkah Serigala yang makan?",
        "pa": "Is it true that Serigala ate them?",
        "vi": "Có đúng là Serigala đã ăn chúng không?"
    },
    {
        "start": "00:11:36,360",
        "end": "00:11:37,360",
        "en": "Kau ada nampak?",
        "pa": "Did you see it?",
        "vi": "Các chị có thấy không?"
    },
    {
        "start": "00:11:39,080",
        "end": "00:11:40,080",
        "en": "Tak ada.",
        "pa": "I didn't.",
        "vi": "Không thấy."
    },
    {
        "start": "00:11:41,320",
        "end": "00:11:42,440",
        "en": "Tak nampak.",
        "pa": "I didn't.",
        "vi": "Không thấy."
    },
    {
        "start": "00:11:42,520",
        "end": "00:11:45,200",
        "en": "Tapi kami pasti, dia yang makan!",
        "pa": "But we are sure he ate them!",
        "vi": "Nhưng chúng tôi chắc chắn là hắn đã ăn chúng!"
    },
    {
        "start": "00:11:46,400",
        "end": "00:11:48,560",
        "en": "Malangnya Serigala tak nak mengaku.",
        "pa": "Unfortunately, Serigala doesn't want to admit it!",
        "vi": "Không may là Serigala không chịu thừa nhận!"
    },
    {
        "start": "00:11:48,640",
        "end": "00:11:50,840",
        "en": "Sebab tak ada bukti!",
        "pa": "Because there's no proof!",
        "vi": "Bởi vì không có bằng chứng!"
    },
    {
        "start": "00:11:55,040",
        "end": "00:11:58,280",
        "en": "Kancil, tolonglah kami!",
        "pa": "Kancil, please help us!",
        "vi": "Kancil, làm ơn giúp chúng tôi đi!"
    },
    {
        "start": "00:12:02,240",
        "end": "00:12:06,200",
        "en": "Kalau begitu, apa kata kita perangkap Serigala.",
        "pa": "In that case, why don't we set a trap for Serigala?",
        "vi": "Nếu vậy thì, sao chúng ta không gài bẫy Serigala nhỉ?"
    },
    {
        "start": "00:12:07,160",
        "end": "00:12:12,480",
        "en": "Kalau benar dia yang makan, biar dia sendiri yang bongkarkan kejahatannya.",
        "pa": "If it's true that he ate them, let him reveal his evil deed.",
        "vi": "Nếu đúng là hắn đã ăn chúng, cứ để hắn khai ra tội xấu của mình."
    },
    {
        "start": "00:12:12,560",
        "end": "00:12:13,800",
        "en": "Tapi macam mana?",
        "pa": "How do we do that?",
        "vi": "Nhưng làm cách nào đây?"
    },
    {
        "start": "00:12:20,880",
        "end": "00:12:23,080",
        "en": "Begitu!",
        "pa": "I've got it.",
        "vi": "Mình biết rồi."
    },
    {
        "start": "00:12:28,400",
        "end": "00:12:32,040",
        "en": "Aku tahu apa yang kau fikirkan.",
        "pa": "I know what you're thinking of…",
        "vi": "Ta biết cậu đang nghĩ gì…"
    },
    {
        "start": "00:12:50,840",
        "end": "00:12:52,560",
        "en": "Usah bimbang burung.",
        "pa": "Don't worry, birds.",
        "vi": "Đừng lo, các chị chim."
    },
    {
        "start": "00:12:52,640",
        "end": "00:12:55,560",
        "en": "Aku ada satu rancangan untuk kenakan Serigala.",
        "pa": "I have a plan on how we can get back at Serigala.",
        "vi": "Tôi có kế hoạch để trả đũa tên Serigala rồi."
    },
    {
        "start": "00:12:59,600",
        "end": "00:13:00,960",
        "en": "Aku setuju!",
        "pa": "I agree!",
        "vi": "Đồng ý!"
    },
    {
        "start": "00:13:30,240",
        "end": "00:13:32,320",
        "en": "Gembiranya!",
        "pa": "I'm so happy!",
        "vi": "Hạnh phúc quá đi!"
    },
    {
        "start": "00:13:33,480",
        "end": "00:13:38,720",
        "en": "Telur aku banyak!",
        "pa": "I have lots of eggs, lots of eggs",
        "vi": "Mình có thật là nhiều, thật là nhiều trứng"
    },
    {
        "start": "00:13:38,800",
        "end": "00:13:41,440",
        "en": "Telur aku banyak!",
        "pa": "Lots of eggs",
        "vi": "Thật là nhiều trứng"
    },
    {
        "start": "00:13:41,520",
        "end": "00:13:46,000",
        "en": "Sangat banyak! Telur aku banyak!",
        "pa": "I have lots of eggs, lots of eggs",
        "vi": "Mình có thật là nhiều, thật là nhiều trứng"
    },
    {
        "start": "00:13:59,120",
        "end": "00:14:03,240",
        "en": "Bagus! Pergi terbang jauh-jauh.",
        "pa": "Good! Go fly, far, far away.",
        "vi": "Tốt lắm! Cứ bay xa thật là xa đi."
    },
    {
        "start": "00:14:04,320",
        "end": "00:14:07,560",
        "en": "Biar aku jaga telur kau.",
        "pa": "Let me take care of your eggs.",
        "vi": "Để ta chăm sóc trứng của ngươi cho."
    },
    {
        "start": "00:14:11,320",
        "end": "00:14:12,920",
        "en": "Sedapnya!",
        "pa": "Delicious…",
        "vi": "Ngon tuyệt…"
    },
    {
        "start": "00:14:13,960",
        "end": "00:14:16,000",
        "en": "Tentu aku kenyang hari ini.",
        "pa": "I will surely be full today.",
        "vi": "Chắc chắn hôm nay mình sẽ no bụng."
    },
    {
        "start": "00:14:42,720",
        "end": "00:14:45,800",
        "en": "Tak sedap pula rasa telur Merbah.",
        "pa": "Merbah's eggs don't taste so good.",
        "vi": "Trứng của Merbah ăn không ngon lắm."
    },
    {
        "start": "00:14:49,880",
        "end": "00:14:52,880",
        "en": "Tapi, makan sajalah.",
        "pa": "However… I'll just eat them.",
        "vi": "Mà thôi… cứ ăn đại vậy."
    },
    {
        "start": "00:15:00,720",
        "end": "00:15:03,160",
        "en": "Serigala!",
        "pa": "Serigala!",
        "vi": "Serigala!"
    },
    {
        "start": "00:15:05,000",
        "end": "00:15:07,280",
        "en": "Kenapa dengan kau?",
        "pa": "What's wrong with you?",
        "vi": "Anh bị sao thế?"
    },
    {
        "start": "00:15:07,360",
        "end": "00:15:10,720",
        "en": "Entahlah. Tiba-tiba rasa tak sedap badan.",
        "pa": "I don't know. Suddenly I don't feel so good.",
        "vi": "Tôi không biết. Đột nhiên tôi thấy không khỏe lắm."
    },
    {
        "start": "00:15:15,120",
        "end": "00:15:18,120",
        "en": "Aduh!",
        "pa": "Ouch!",
        "vi": ""
    },
    {
        "start": "00:15:18,200",
        "end": "00:15:22,040",
        "en": "Tolong! Sakitnya perut.",
        "pa": "My tummy hurts.",
        "vi": "Bụng tôi đau quá."
    },
    {
        "start": "00:15:23,360",
        "end": "00:15:24,800",
        "en": "Apa yang kau makan?",
        "pa": "What did you eat?",
        "vi": "Anh đã ăn gì thế?"
    },
    {
        "start": "00:15:24,880",
        "end": "00:15:27,200",
        "en": "Aku makan…",
        "pa": "I ate eg--",
        "vi": "Tôi đã ăn trứng…"
    },
    {
        "start": "00:15:31,840",
        "end": "00:15:35,080",
        "en": "Dah sah, memang kau yang makan telur-telur kami!",
        "pa": "It's true, you were the one who ate our eggs all along!",
        "vi": "Thì ra chính anh là kẻ đã ăn trứng của chúng tôi!"
    },
    {
        "start": "00:15:35,800",
        "end": "00:15:37,840",
        "en": "Tidak! Mana ada!",
        "pa": "No! That's not true!",
        "vi": "Không! Không đúng!"
    },
    {
        "start": "00:15:37,920",
        "end": "00:15:42,480",
        "en": "Sudah! Jangan berlindung di sebalik telunjuk!",
        "pa": "Enough! Stop trying to keep an open secret.",
        "vi": "Đủ rồi! Đừng cố che giấu bí mật đã bị lộ."
    },
    {
        "start": "00:16:00,120",
        "end": "00:16:01,720",
        "en": "Sudahlah!",
        "pa": "Stop it!",
        "vi": "Dừng lại đi!"
    },
    {
        "start": "00:16:04,200",
        "end": "00:16:05,200",
        "en": "Aduh!",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:16:07,240",
        "end": "00:16:09,360",
        "en": "Sakitnya perut.",
        "pa": "My tummy hurts.",
        "vi": "Bụng tôi đau quá."
    },
    {
        "start": "00:16:13,000",
        "end": "00:16:14,840",
        "en": "Kenapa sakit begini?",
        "pa": "What's happening?",
        "vi": "Chuyện gì đang xảy ra thế?"
    },
    {
        "start": "00:16:14,920",
        "end": "00:16:17,800",
        "en": "Kau dah makan telur busuk, Serigala.",
        "pa": "You just ate rotten eggs, Serigala.",
        "vi": "Anh đã ăn phải trứng thối, Serigala à."
    },
    {
        "start": "00:16:17,880",
        "end": "00:16:19,440",
        "en": "Sebab itulah kau sakit.",
        "pa": "That's why you're in pain!",
        "vi": "Thế nên anh mới đau bụng đấy!"
    },
    {
        "start": "00:16:21,320",
        "end": "00:16:27,360",
        "en": "Sampai hati kamu semua mempermainkan aku.",
        "pa": "How could you trick me like this?",
        "vi": "Sao các người lại lừa tôi như vậy?"
    },
    {
        "start": "00:16:28,640",
        "end": "00:16:30,240",
        "en": "Sakit tahu!",
        "pa": "It hurts!",
        "vi": "Đau quá!"
    },
    {
        "start": "00:16:30,320",
        "end": "00:16:31,480",
        "en": "Kami lagi sakit!",
        "pa": "We're hurt too.",
        "vi": "Chúng tôi cũng đau vậy."
    },
    {
        "start": "00:16:32,480",
        "end": "00:16:33,640",
        "en": "Sakit hati!",
        "pa": "We're angry!",
        "vi": "Chúng tôi rất tức giận!"
    },
    {
        "start": "00:16:33,720",
        "end": "00:16:35,600",
        "en": "Habis semua telur kami kau makan.",
        "pa": "Our eggs are gone, because you ate them.",
        "vi": "Trứng của chúng tôi đã bị anh ăn mất."
    },
    {
        "start": "00:16:36,280",
        "end": "00:16:38,320",
        "en": "Maafkan aku.",
        "pa": "I'm sorry.",
        "vi": "Tôi xin lỗi."
    },
    {
        "start": "00:16:39,280",
        "end": "00:16:42,120",
        "en": "Aku menyesal.",
        "pa": "I regret it.",
        "vi": "Tôi hối hận rồi."
    },
    {
        "start": "00:16:43,800",
        "end": "00:16:45,000",
        "en": "Aduh!",
        "pa": "Ouch!",
        "vi": ""
    },
    {
        "start": "00:16:45,800",
        "end": "00:16:48,280",
        "en": "Sakitnya.",
        "pa": "It hurts so bad!",
        "vi": "Đau quá đi mất!"
    },
    {
        "start": "00:16:49,440",
        "end": "00:16:54,160",
        "en": "Aku serik nak makan telur burung lagi.",
        "pa": "I will never eat bird eggs again.",
        "vi": "Tôi sẽ không bao giờ ăn trứng chim nữa."
    },
    {
        "start": "00:16:55,360",
        "end": "00:16:58,000",
        "en": "Kalau begitu, pergi kau dari sini!",
        "pa": "So leave this place now!",
        "vi": "Vậy thì rời khỏi nơi này mau!"
    },
    {
        "start": "00:17:09,480",
        "end": "00:17:11,400",
        "en": "- Serigala tiada lagi! - Hore!",
        "pa": "Hurrah!",
        "vi": "Hoan hô!"
    },
    {
        "start": "00:17:12,240",
        "end": "00:17:15,000",
        "en": "Terima kasih, Kancil! Kau memang bijak.",
        "pa": "Thank you, Kancil. You are really smart.",
        "vi": "Cảm ơn Kancil. Cậu thật thông minh."
    },
    {
        "start": "00:17:24,880",
        "end": "00:17:26,240",
        "en": "Terima kasih!",
        "pa": "Thank you!",
        "vi": "- Cảm ơn. - Cảm ơn!"
    },
    {
        "start": "00:17:28,920",
        "end": "00:17:31,560",
        "en": "Begitulah ceritanya.",
        "pa": "And that's the story…",
        "vi": "Câu chuyện là thế đấy…"
    },
    {
        "start": "00:17:32,920",
        "end": "00:17:38,760",
        "en": "Sejak itu, burung-burung pun tak risau nak tinggalkan sarang mereka.",
        "pa": "Since then, the birds did not have to worry about leaving their nests.",
        "vi": "Từ đó trở đi, lũ chim không phải lo lắng khi rời khỏi tổ nữa."
    },
    {
        "start": "00:17:50,280",
        "end": "00:17:51,800",
        "en": "Mana boleh, Aki.",
        "pa": "It's impossible.",
        "vi": "Không thể nào."
    },
    {
        "start": "00:17:52,680",
        "end": "00:17:56,320",
        "en": "Tadi tupai kata, berlindung di sebalik telunjuk.",
        "pa": "When Tupai said \"stop trying to keep an open secret.\"",
        "vi": "Khi Tupai nói: \"Đừng cố giữ bí mật đã bị lộ\"…"
    },
    {
        "start": "00:17:58,040",
        "end": "00:17:59,760",
        "en": "Itu peribahasa.",
        "pa": "That was a proverb.",
        "vi": "Đó là một câu thành ngữ."
    },
    {
        "start": "00:18:00,440",
        "end": "00:18:04,000",
        "en": "Maksudnya hendak menyembunyikan sesuatu rahsia,",
        "pa": "It means you want to keep something a secret,",
        "vi": "Câu đó nghĩa là cháu muốn giữ bí mật một chuyện gì đó,"
    },
    {
        "start": "00:18:04,760",
        "end": "00:18:07,840",
        "en": "sedangkan rahsia itu telah diketahui orang.",
        "pa": "but everybody already knows the secret.",
        "vi": "nhưng mọi người đều đã biết cả rồi."
    },
    {
        "start": "00:18:09,600",
        "end": "00:18:14,200",
        "en": "Macam abang! Diam-diam makan cekodok Ara tapi tak mengaku.",
        "pa": "Like you! You took my food quietly, but you didn't want to admit it.",
        "vi": "Giống anh! Anh lén lút ăn thức ăn của em nhưng lại không thừa nhận."
    },
    {
        "start": "00:18:14,280",
        "end": "00:18:16,560",
        "en": "Nasib baik Aki nampak.",
        "pa": "Luckily, Aki saw it.",
        "vi": "May mà có ông nhìn thấy."
    },
    {
        "start": "00:18:16,640",
        "end": "00:18:19,440",
        "en": "Yalah, lain kali tak buat lagi.",
        "pa": "Alright, I won't do it next time.",
        "vi": "Được rồi, lần sau anh sẽ không làm thế nữa."
    },
    {
        "start": "00:18:19,520",
        "end": "00:18:21,520",
        "en": "Kalau abang nak, abang minta.",
        "pa": "If I want something, I'll ask for it.",
        "vi": "Nếu anh muốn thứ gì đó, anh sẽ hỏi xin."
    },
    {
        "start": "00:18:21,600",
        "end": "00:18:24,480",
        "en": "Tapi kalau Ara tak bagi, abang nak juga.",
        "pa": "But if you don't give it, I'll still take it.",
        "vi": "Nhưng nếu em không cho thì anh vẫn sẽ lấy đấy."
    },
    {
        "start": "00:18:25,560",
        "end": "00:18:26,680",
        "en": "Abang!",
        "pa": "-Aris! -You can't catch me. Come on, try!",
        "vi": "- Anh! - Đừng hòng bắt được anh. Thử đi nào!"
    },
    {
        "start": "00:18:26,760",
        "end": "00:18:29,480",
        "en": "Ara tak dapat! Kejarlah Ara, cepatlah kejar.",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:18:30,840",
        "end": "00:18:33,480",
        "en": "Siapa pandai? Jawab teka teki ini.",
        "pa": "Answer this riddle, if you're smart.",
        "vi": "Nếu các cháu thông minh thì trả lời câu đố này đi."
    },
    {
        "start": "00:18:33,560",
        "end": "00:18:36,440",
        "en": "Dalam hutan ada lapan ekor burung.",
        "pa": "There are eight birds in the jungle,",
        "vi": "Có tám con chim trong rừng,"
    },
    {
        "start": "00:18:36,520",
        "end": "00:18:39,400",
        "en": "Empat ekor ditembak oleh pemburu.",
        "pa": "and four birds got shot by the hunter.",
        "vi": "bốn con bị thợ săn bắn."
    },
    {
        "start": "00:18:40,000",
        "end": "00:18:41,160",
        "en": "Berapa yang lari?",
        "pa": "How many ran away?",
        "vi": "Vậy có mấy con chạy mất?"
    },
    {
        "start": "00:18:43,120",
        "end": "00:18:44,960",
        "en": "Empat ekorlah.",
        "pa": "Four birds of course…",
        "vi": "Dĩ nhiên là bốn con ạ…"
    },
    {
        "start": "00:18:48,040",
        "end": "00:18:50,320",
        "en": "Tiada yang lari.",
        "pa": "No bird ran away.",
        "vi": "Không con nào chạy cả."
    },
    {
        "start": "00:18:50,400",
        "end": "00:18:51,360",
        "en": "Kenapa?",
        "pa": "Why?",
        "vi": "Tại sao?"
    },
    {
        "start": "00:18:52,880",
        "end": "00:18:56,280",
        "en": "Sebab burung terbang, bukan lari.",
        "pa": "Birds fly, they don't run.",
        "vi": "Chim chỉ bay chứ không có chạy."
    },
    {
        "start": "00:18:56,360",
        "end": "00:18:58,920",
        "en": "Betul! Pandai Ara.",
        "pa": "Correct! Ara, you're smart.",
        "vi": "Đúng rồi! Ara, cháu thông minh lắm."
    },
    {
        "start": "00:19:00,520",
        "end": "00:19:02,480",
        "en": "Ada pula begitu.",
        "pa": "Really?",
        "vi": "Thật ạ?"
    },
    {
        "start": "00:19:03,360",
        "end": "00:19:04,920",
        "en": "Marah nampak.",
        "pa": "You look angry.",
        "vi": "Trông cháu tức giận thế."
    },
    {
        "start": "00:19:06,240",
        "end": "00:19:08,120",
        "en": "Dah, pergi tidur.",
        "pa": "Enough. Go to bed.",
        "vi": "Đủ rồi. Đi ngủ nào."
    },
    {
        "start": "00:19:08,200",
        "end": "00:19:09,520",
        "en": "Selamat malam, Aki!",
        "pa": "Good night Aki…",
        "vi": "Chúc ông ngủ ngon…"
    },
    {
        "start": "00:19:10,480",
        "end": "00:19:11,920",
        "en": "Selamat malam.",
        "pa": "Good night.",
        "vi": "Chúc cháu ngủ ngon."
    },
    {
        "start": "00:19:12,680",
        "end": "00:19:13,560",
        "en": "Aris?",
        "pa": "Aris?",
        "vi": "Aris?"
    },
    {
        "start": "00:19:14,600",
        "end": "00:19:15,800",
        "en": "Selamat malam.",
        "pa": "Good night.",
        "vi": "Chúc ông ngủ ngon."
    },
    {
        "start": "00:19:32,480",
        "end": "00:19:34,480",
        "en": "",
        "pa": "",
        "vi": "Biên dịch: Hoàng Nguyên"
    }
]