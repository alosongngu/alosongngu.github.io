[
    {
        "start": "00:00:28,200",
        "end": "00:00:29,360",
        "en": "Kiki nak minum tak?",
        "pa": "You want to drink, Kiki?",
        "vi": "Em uống không, Kiki?"
    },
    {
        "start": "00:00:37,280",
        "end": "00:00:38,840",
        "en": "Sedapkan Kiki?",
        "pa": "Isn't it delicious?",
        "vi": "Ngon thật nhỉ?"
    },
    {
        "start": "00:00:46,280",
        "end": "00:00:47,840",
        "en": "Tak sedaplah.",
        "pa": "Disgusting!",
        "vi": "Dở ẹc!"
    },
    {
        "start": "00:00:49,000",
        "end": "00:00:51,760",
        "en": "- Abang, itu Ara punya. - Kejarlah!",
        "pa": "-Brother, that's mine! -Come get it!",
        "vi": "- Anh à, của em mà! - Đến đây lấy đi!"
    },
    {
        "start": "00:00:52,880",
        "end": "00:00:55,000",
        "en": "- Bagilah balik, itu Ara punya. - Tak dapat!",
        "pa": "-Give it back! -Nope! Too high!",
        "vi": "- Trả cho em! - Không! Cao quá!"
    },
    {
        "start": "00:00:55,080",
        "end": "00:00:59,760",
        "en": "Aris! Jangan kacau adik. Mari tolong Aki.",
        "pa": "Aris, stop that. Come over here!",
        "vi": "Aris, dừng lại đi. Đến đây nào!"
    },
    {
        "start": "00:01:02,200",
        "end": "00:01:04,040",
        "en": "Bagilah balik abang.",
        "pa": "Brother, give it back!",
        "vi": "Anh, trả lại cho em!"
    },
    {
        "start": "00:01:23,120",
        "end": "00:01:24,000",
        "en": "Ara.",
        "pa": "Ara…",
        "vi": "Ara…"
    },
    {
        "start": "00:01:24,520",
        "end": "00:01:25,400",
        "en": "Pulu!",
        "pa": "Ghost!",
        "vi": "Ma đây!"
    },
    {
        "start": "00:01:27,720",
        "end": "00:01:29,040",
        "en": "Penakut!",
        "pa": "Scaredy-cat!",
        "vi": "Đồ nhát gan!"
    },
    {
        "start": "00:01:29,120",
        "end": "00:01:31,560",
        "en": "Pulu!",
        "pa": "Ghost!",
        "vi": "Ma này!"
    },
    {
        "start": "00:01:33,280",
        "end": "00:01:35,120",
        "en": "Abang!",
        "pa": "Brother!",
        "vi": "Anh!"
    },
    {
        "start": "00:01:38,240",
        "end": "00:01:39,080",
        "en": "Jangan Ara!",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:39,880",
        "end": "00:01:42,520",
        "en": "Aki!",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:01:42,600",
        "end": "00:01:45,000",
        "en": "Garangnya.",
        "pa": "What was that?",
        "vi": "Có chuyện gì thế?"
    },
    {
        "start": "00:01:45,720",
        "end": "00:01:47,440",
        "en": "Senyum sikit.",
        "pa": "Give me a smile…",
        "vi": "Cười với ông nào…"
    },
    {
        "start": "00:01:48,880",
        "end": "00:01:52,080",
        "en": "Janganlah marah, abang gurau saja.",
        "pa": "Don't be mad. I was just kidding.",
        "vi": "Đừng giận mà. Anh chỉ đùa thôi."
    },
    {
        "start": "00:01:52,840",
        "end": "00:01:55,200",
        "en": "Marahnya cucu Aki.",
        "pa": "You really are angry, aren't you?",
        "vi": "Cháu đang rất giận, phải không?"
    },
    {
        "start": "00:01:56,600",
        "end": "00:02:02,160",
        "en": "Aris, tahu tak peribahasa, \"jangan bangunkan kucing tidur\".",
        "pa": "Aris, do you know that saying, \"Let sleeping dogs lie\"?",
        "vi": "Aris, cháu có biết câu \"Đừng chọc chú chó đang ngủ\" không?"
    },
    {
        "start": "00:02:02,680",
        "end": "00:02:06,520",
        "en": "- Macam itulah. - Aki! Ara dengarlah!",
        "pa": "-That's what it means. -Aki! I can hear you!",
        "vi": "- Nó có nghĩa là vậy đấy. - Ông à! Cháu nghe rồi đấy!"
    },
    {
        "start": "00:02:07,600",
        "end": "00:02:11,000",
        "en": "Dengar? Dengar cerita ini.",
        "pa": "You can? Then listen to this story.",
        "vi": "Cháu nghe rồi à? Vậy nghe câu chuyện này nhé."
    },
    {
        "start": "00:02:11,080",
        "end": "00:02:13,560",
        "en": "Pada zaman dahulu…",
        "pa": "Once upon a time…",
        "vi": "Ngày xửa ngày xưa…"
    },
    {
        "start": "00:02:25,560",
        "end": "00:02:27,800",
        "en": "Mengantuknya…",
        "pa": "So sleepy…",
        "vi": "Buồn ngủ quá…"
    },
    {
        "start": "00:02:29,600",
        "end": "00:02:33,280",
        "en": "Tiap-tiap malam ada saja masalah.",
        "pa": "Every single night…",
        "vi": "Đêm nào cũng…"
    },
    {
        "start": "00:02:33,360",
        "end": "00:02:36,520",
        "en": "Aku juga yang kena jadi hakim.",
        "pa": "Can't they solve their own problems?",
        "vi": "Họ không tự giải quyết được chuyện của mình hay sao?"
    },
    {
        "start": "00:02:45,600",
        "end": "00:02:48,320",
        "en": "Nyamannya…",
        "pa": "This feels nice…",
        "vi": "Êm ái quá…"
    },
    {
        "start": "00:02:57,360",
        "end": "00:02:59,720",
        "en": "Kancil…",
        "pa": "Kancil…",
        "vi": "Kancil…"
    },
    {
        "start": "00:03:07,360",
        "end": "00:03:13,400",
        "en": "Harimau, hari ini saja. Kau jangan ganggu aku boleh tak?",
        "pa": "Harimau, can you not bother me for just one day?",
        "vi": "Harimau, anh đừng làm phiền tôi dù chỉ một ngày được không?"
    },
    {
        "start": "00:03:13,480",
        "end": "00:03:17,280",
        "en": "Tak boleh! Hari ini kau takkan terlepas!",
        "pa": "No! This time, you won't escape me.",
        "vi": "Không! Lần này, ngươi sẽ không thoát được đâu."
    },
    {
        "start": "00:03:19,480",
        "end": "00:03:21,440",
        "en": "Nak tidur pun tak senang.",
        "pa": "Not a moment's peace…",
        "vi": "Muốn yên một phút cũng không được…"
    },
    {
        "start": "00:03:22,440",
        "end": "00:03:25,320",
        "en": "Harimau, cuba kau baring dekat sini?",
        "pa": "Harimau, why don't you try sitting here?",
        "vi": "Harimau, anh thử đến đây nằm xem?"
    },
    {
        "start": "00:03:25,400",
        "end": "00:03:26,680",
        "en": "Selesa!",
        "pa": "It's so comfortable!",
        "vi": "Rất thoải mái đấy!"
    },
    {
        "start": "00:03:26,760",
        "end": "00:03:30,640",
        "en": "Aku sebenarnya tengah cari tempat rehat untuk Raja Sulaiman.",
        "pa": "I'm looking for a place for King Suleiman to rest.",
        "vi": "Tôi đang tìm một nơi cho Vua Sulaiman nghỉ ngơi."
    },
    {
        "start": "00:03:30,720",
        "end": "00:03:33,720",
        "en": "Hei, kau jangan nak tipu aku lagi!",
        "pa": "I won't fall for your tricks again!",
        "vi": "Ta không để bị ngươi lừa nữa đâu!"
    },
    {
        "start": "00:03:36,440",
        "end": "00:03:37,680",
        "en": "Ini apa?",
        "pa": "What's this?",
        "vi": "Gì thế này?"
    },
    {
        "start": "00:03:42,960",
        "end": "00:03:46,720",
        "en": "Betullah cakap kau. Selesanya.",
        "pa": "You're right! So comfy.",
        "vi": "Ngươi nói đúng! Thoải mái thật."
    },
    {
        "start": "00:03:49,080",
        "end": "00:03:51,760",
        "en": "Betulkan? Aku tak tipu.",
        "pa": "See? I didn't lie.",
        "vi": "Thấy chưa? Tôi đâu có nói dối."
    },
    {
        "start": "00:03:53,800",
        "end": "00:03:57,160",
        "en": "Kau carilah tempat rehat lain untuk Raja Sulaiman.",
        "pa": "You have to find King Suleiman someplace else.",
        "vi": "Ngươi phải đi tìm nơi khác cho Vua Sulaiman thôi."
    },
    {
        "start": "00:03:57,680",
        "end": "00:04:00,040",
        "en": "Tempat ini, aku punya.",
        "pa": "I'm claiming this spot.",
        "vi": "Ta tuyên bố chiếm lấy chỗ này."
    },
    {
        "start": "00:04:08,960",
        "end": "00:04:10,080",
        "en": "Kancil!",
        "pa": "Kancil!",
        "vi": "Kancil!"
    },
    {
        "start": "00:04:15,360",
        "end": "00:04:16,800",
        "en": "Tunggu kau!",
        "pa": "Just you wait!",
        "vi": "Đợi đã!"
    },
    {
        "start": "00:04:18,519",
        "end": "00:04:20,000",
        "en": "Kancil!",
        "pa": "Kancil!",
        "vi": "Kancil!"
    },
    {
        "start": "00:04:23,880",
        "end": "00:04:25,360",
        "en": "Aduh…",
        "pa": "Ouch…",
        "vi": ""
    },
    {
        "start": "00:04:31,760",
        "end": "00:04:33,000",
        "en": "Harimau!",
        "pa": "Harimau!",
        "vi": "Harimau!"
    },
    {
        "start": "00:04:33,640",
        "end": "00:04:35,720",
        "en": "Apa kau buat dekat sini?",
        "pa": "What are you doing here?",
        "vi": "Ngươi làm gì ở đây?"
    },
    {
        "start": "00:04:38,520",
        "end": "00:04:39,840",
        "en": "Aduh!",
        "pa": "",
        "vi": "Đau quá!"
    },
    {
        "start": "00:04:40,760",
        "end": "00:04:41,880",
        "en": "Kenapa badan kau kecil?",
        "pa": "When do you get so small?",
        "vi": "Sao ngươi bé thế?"
    },
    {
        "start": "00:04:43,080",
        "end": "00:04:44,800",
        "en": "- Lama tak jumpa kau! - Tak guna punya kancil!",
        "pa": "-Long time no see! -I'll get you, Kancil.",
        "vi": "- Đã lâu không gặp! - Kancil!"
    },
    {
        "start": "00:04:45,600",
        "end": "00:04:46,640",
        "en": "Kancil?",
        "pa": "Kancil?",
        "vi": "Kancil?"
    },
    {
        "start": "00:04:47,320",
        "end": "00:04:49,240",
        "en": "Mana dia?",
        "pa": "Where is he? Where?",
        "vi": "Nó ở đâu? Ở đâu vậy?"
    },
    {
        "start": "00:04:49,320",
        "end": "00:04:51,280",
        "en": "Kau jangan ganggu dia.",
        "pa": "Have you done something to him?",
        "vi": "Ngươi đã làm gì nó à?"
    },
    {
        "start": "00:04:54,720",
        "end": "00:04:56,120",
        "en": "Kenapa pula?",
        "pa": "What's the matter with you?",
        "vi": "Ngươi bị sao thế?"
    },
    {
        "start": "00:04:56,200",
        "end": "00:04:57,920",
        "en": "Kancil itu makanan aku.",
        "pa": "I'm having Kancil for dinner.",
        "vi": "Ta định xơi Kancil cho bữa tối."
    },
    {
        "start": "00:04:59,080",
        "end": "00:05:01,120",
        "en": "Kau boleh makan binatang lain.",
        "pa": "You can always eat something else.",
        "vi": "Ngươi ăn thứ khác cũng được mà."
    },
    {
        "start": "00:05:01,200",
        "end": "00:05:06,400",
        "en": "Macam tupai, rusa, arnab…",
        "pa": "Like…\n Squirrels, deer, rabbits…",
        "vi": "Như là…\n Sóc, nai, thỏ này…"
    },
    {
        "start": "00:05:07,440",
        "end": "00:05:12,680",
        "en": "Sedap! Tapi, Kancil itu bijak. Kau takkan dapat tangkap.",
        "pa": "Yum! But Kancil's too smart for you. You can't catch him.",
        "vi": "Ngon lắm! Kancil quá thông minh. Ngươi không bắt nó được đâu."
    },
    {
        "start": "00:05:12,760",
        "end": "00:05:14,240",
        "en": "Baik jangan ganggu dia.",
        "pa": "Don't you mess with him.",
        "vi": "Đừng chọc vào nó."
    },
    {
        "start": "00:05:16,800",
        "end": "00:05:18,840",
        "en": "Aku pun bijak juga!",
        "pa": "I'm smart, too.",
        "vi": "Ta cũng thông minh vậy."
    },
    {
        "start": "00:05:20,160",
        "end": "00:05:23,600",
        "en": "Kali ini, dia takkan terlepas!",
        "pa": "This time, I will get him.",
        "vi": "Lần này, ta sẽ bắt nó."
    },
    {
        "start": "00:05:26,360",
        "end": "00:05:30,160",
        "en": "Bijak? Dah berapa kali Kancil terlepas.",
        "pa": "Really? How many times has he escaped you?",
        "vi": "Thật ư? Ngươi để nó chạy thoát bao nhiêu lần rồi?"
    },
    {
        "start": "00:05:33,360",
        "end": "00:05:35,960",
        "en": "Beruang, kau nak tolong aku tangkap Kancil?",
        "pa": "Beruang, you want to help me catch him?",
        "vi": "Beruang, ngươi muốn giúp ta bắt nó không?"
    },
    {
        "start": "00:05:36,040",
        "end": "00:05:41,400",
        "en": "Kau yang tegap dan aku yang pantas, mesti dia tak terlepas!",
        "pa": "With your brawn and my speed, he won't stand a chance.",
        "vi": "Với sức mạnh và tốc độ của ngươi, nó sẽ không thoát nổi đâu."
    },
    {
        "start": "00:05:41,480",
        "end": "00:05:42,880",
        "en": "Tangkap Kancil?",
        "pa": "Catch Kancil?",
        "vi": "Bắt Kancil à?"
    },
    {
        "start": "00:05:45,640",
        "end": "00:05:48,280",
        "en": "- Aku tak berani. - Biar betul?",
        "pa": "-I don't want to. -Seriously?",
        "vi": "- Ta không muốn. - Thật ư?"
    },
    {
        "start": "00:05:48,360",
        "end": "00:05:51,880",
        "en": "Badan saja besar, dengan Kancil yang kecil itu pun takut.",
        "pa": "You're a lot bigger and you're afraid of him?",
        "vi": "Ngươi to hơn nó gấp mấy lần mà lại sợ nó à?"
    },
    {
        "start": "00:05:53,960",
        "end": "00:05:55,440",
        "en": "Memang aku takut.",
        "pa": "Yes, I am.",
        "vi": "Đúng vậy."
    },
    {
        "start": "00:05:55,960",
        "end": "00:05:57,120",
        "en": "Kau ingat tak…",
        "pa": "Don't you remember?",
        "vi": "Ngươi không nhớ à?"
    },
    {
        "start": "00:06:10,360",
        "end": "00:06:15,360",
        "en": "Jangan ganggu Kancil lagi…",
        "pa": "Don't mess with Kancil…",
        "vi": "Đừng chọc vào Kancil…"
    },
    {
        "start": "00:06:15,960",
        "end": "00:06:20,200",
        "en": "Kancil, jangan ganggu Kancil.",
        "pa": "Don't mess with Kancil…",
        "vi": "Đừng chọc vào Kancil…"
    },
    {
        "start": "00:06:20,280",
        "end": "00:06:23,440",
        "en": "Jangan ganggu Kancil.",
        "pa": "Don't mess with Kancil…",
        "vi": "Đừng chọc vào Kancil…"
    },
    {
        "start": "00:06:23,520",
        "end": "00:06:27,480",
        "en": "Jangan ganggu Kancil.",
        "pa": "Don't mess with Kancil…",
        "vi": "Đừng chọc vào Kancil…"
    },
    {
        "start": "00:06:27,560",
        "end": "00:06:29,600",
        "en": "Jangan ganggu.",
        "pa": "Don't mess…",
        "vi": "Đừng chọc…"
    },
    {
        "start": "00:06:42,800",
        "end": "00:06:44,160",
        "en": "Itupun Kancil.",
        "pa": "There's Kancil!",
        "vi": "Kancil kia rồi!"
    },
    {
        "start": "00:06:44,240",
        "end": "00:06:47,120",
        "en": "Diam-diam. Nanti Kancil bangun.",
        "pa": "Be quiet! He'll wake up.",
        "vi": "Im lặng! Cậu đánh thức cậu ta bây giờ."
    },
    {
        "start": "00:06:47,640",
        "end": "00:06:50,320",
        "en": "Satu, dua…",
        "pa": "One, two…",
        "vi": "Một, hai…"
    },
    {
        "start": "00:06:56,360",
        "end": "00:06:58,480",
        "en": "Bisingnya!",
        "pa": "Stop that!",
        "vi": "Dừng lại đi!"
    },
    {
        "start": "00:06:58,560",
        "end": "00:07:01,040",
        "en": "Macam mana aku nak tidur?",
        "pa": "How am I supposed to get any sleep?",
        "vi": "Làm sao tôi mới có thể ngủ được đây?"
    },
    {
        "start": "00:07:01,560",
        "end": "00:07:03,160",
        "en": "Kau tak tidur malam tadi?",
        "pa": "Didn't you sleep last night?",
        "vi": "Tối qua cậu không ngủ à?"
    },
    {
        "start": "00:07:03,240",
        "end": "00:07:04,600",
        "en": "Taklah!",
        "pa": "No!",
        "vi": "Không!"
    },
    {
        "start": "00:07:07,560",
        "end": "00:07:11,240",
        "en": "Pagi-pagi kenalah bangun, bukan tidur.",
        "pa": "You're supposed to be awake in the daytime, not sleep.",
        "vi": "Ban ngày cậu phải thức dậy chứ đừng nằm ườn ra ngủ như thế."
    },
    {
        "start": "00:07:11,320",
        "end": "00:07:14,360",
        "en": "Kalau asyik tidur, macam mana kau nak jadi…",
        "pa": "If you sleep the day away…",
        "vi": "Nếu cậu cứ ngủ hết ngày…"
    },
    {
        "start": "00:07:15,520",
        "end": "00:07:16,680",
        "en": "Nak tidur pun susah.",
        "pa": "What a racket…",
        "vi": "Ồn ào quá đi…"
    },
    {
        "start": "00:07:19,680",
        "end": "00:07:23,400",
        "en": "Mari Gajah. Kita kacau Arnab pula.",
        "pa": "Come on, Gajah. Let's scare Arnab next!",
        "vi": "Gajah. Giờ mình đi dọa Arnab tiếp đi!"
    },
    {
        "start": "00:07:24,400",
        "end": "00:07:26,000",
        "en": "Ganas!",
        "pa": "Come on!",
        "vi": "Đi nào!"
    },
    {
        "start": "00:08:23,040",
        "end": "00:08:26,440",
        "en": "Bukan aku Kancil!",
        "pa": "I'm sorry, Kancil! I didn't do anything!",
        "vi": "Xin lỗi, Kancil! Ta không làm gì cả."
    },
    {
        "start": "00:08:26,520",
        "end": "00:08:28,320",
        "en": "Siap kau Kancil.",
        "pa": "You're doomed, Kancil.",
        "vi": "Ngươi tiêu rồi, Kancil."
    },
    {
        "start": "00:08:41,039",
        "end": "00:08:43,080",
        "en": "Kancil.",
        "pa": "Kancil…",
        "vi": "Kancil…"
    },
    {
        "start": "00:08:43,159",
        "end": "00:08:44,720",
        "en": "Kau buat apa?",
        "pa": "What are you doing?",
        "vi": "Ngươi đang làm gì thế?"
    },
    {
        "start": "00:08:46,280",
        "end": "00:08:48,760",
        "en": "Apa lagi?",
        "pa": "What now?",
        "vi": "Gì nữa đây?"
    },
    {
        "start": "00:09:13,000",
        "end": "00:09:15,720",
        "en": "Sang Kancil!",
        "pa": "Sang Kancil!",
        "vi": "Kancil!"
    },
    {
        "start": "00:09:36,920",
        "end": "00:09:39,520",
        "en": "Penatnya!",
        "pa": "I'm so exhausted…",
        "vi": "Mình đuối lắm rồi…"
    },
    {
        "start": "00:09:39,600",
        "end": "00:09:43,240",
        "en": "Bila aku boleh tidur dengan aman?",
        "pa": "When can I get some peace?",
        "vi": "Khi nào mình mới được chút bình yên đây?"
    },
    {
        "start": "00:09:51,040",
        "end": "00:09:52,800",
        "en": "Kuatnya.",
        "pa": "That was loud.",
        "vi": "Âm vọng to thật đấy."
    },
    {
        "start": "00:09:55,760",
        "end": "00:09:57,040",
        "en": "Ini bau apa?",
        "pa": "What's this smell?",
        "vi": "Mùi gì thế nhỉ?"
    },
    {
        "start": "00:10:02,720",
        "end": "00:10:04,040",
        "en": "Masam.",
        "pa": "It's sour!",
        "vi": "Chua quá!"
    },
    {
        "start": "00:10:16,320",
        "end": "00:10:20,280",
        "en": "- Lepas itu Aki? Lepas itu? - Apa yang jadi Aki?",
        "pa": "-Then what, Aki? -What happens next?",
        "vi": "- Rồi sao nữa ông? - Chuyện gì xảy ra tiếp theo ạ?"
    },
    {
        "start": "00:10:24,320",
        "end": "00:10:26,520",
        "en": "Leganya.",
        "pa": "That's a relief.",
        "vi": "Đã ngứa quá."
    },
    {
        "start": "00:10:29,680",
        "end": "00:10:32,560",
        "en": "Aki, cepatlah cerita.",
        "pa": "Aki, come on! What's next?",
        "vi": "Thôi nào ông! Tiếp theo thế nào ạ?"
    },
    {
        "start": "00:10:34,280",
        "end": "00:10:37,320",
        "en": "Ya! Dengar Aki sambung.",
        "pa": "All right, listen.",
        "vi": "Được rồi, nghe tiếp nhé."
    },
    {
        "start": "00:10:37,960",
        "end": "00:10:40,320",
        "en": "Lepas itu…",
        "pa": "After that…",
        "vi": "Sau đó…"
    },
    {
        "start": "00:10:59,480",
        "end": "00:11:03,160",
        "en": "Mana pula Kancil? Asyik terlepas saja!",
        "pa": "Where did he go? He couldn't have escaped.",
        "vi": "Nó đi đâu rồi nhỉ? Nó không thể nào trốn thoát được."
    },
    {
        "start": "00:11:06,040",
        "end": "00:11:07,520",
        "en": "Itu bunyi apa?",
        "pa": "What's that?",
        "vi": "Cái gì thế kia?"
    },
    {
        "start": "00:11:18,400",
        "end": "00:11:21,920",
        "en": "Sedapnya pekasam mata Harimau…",
        "pa": "This preserved tiger's eye is delicious!",
        "vi": "Món mắt hổ muối chua này ngon quá!"
    },
    {
        "start": "00:11:23,480",
        "end": "00:11:24,720",
        "en": "Mata Harimau?",
        "pa": "Tiger's eye?",
        "vi": "Mắt hổ?"
    },
    {
        "start": "00:11:30,240",
        "end": "00:11:31,680",
        "en": "Kenapa aku menggigil?",
        "pa": "What's wrong with me?",
        "vi": "Mình bị sao thế nhỉ?"
    },
    {
        "start": "00:11:31,760",
        "end": "00:11:36,000",
        "en": "Nanti petang, aku mahu mencari Harimau.",
        "pa": "I'm going to look for Harimau later.",
        "vi": "Mình sẽ đi tìm Harimau sau."
    },
    {
        "start": "00:11:36,080",
        "end": "00:11:38,480",
        "en": "Mata dia aku buat pekasam.",
        "pa": "I want to make preserves from his eyes.",
        "vi": "Mình muốn lấy mắt của hắn để muối chua."
    },
    {
        "start": "00:11:38,560",
        "end": "00:11:41,360",
        "en": "Sedap!",
        "pa": "Yummy!",
        "vi": "Ngon quá!"
    },
    {
        "start": "00:11:41,440",
        "end": "00:11:43,920",
        "en": "Pekasam Mata Harimau!",
        "pa": "Preserved tiger's eyes!",
        "vi": "Mắt hổ muối chua!"
    },
    {
        "start": "00:11:46,640",
        "end": "00:11:50,040",
        "en": "Sangat lazat!",
        "pa": "So delicious!",
        "vi": "Ngon quá đi mất!"
    },
    {
        "start": "00:11:51,040",
        "end": "00:11:53,680",
        "en": "Siapa itu? Harimau?",
        "pa": "Who's that? Harimau?",
        "vi": "Ai thế? Harimau à?"
    },
    {
        "start": "00:12:02,480",
        "end": "00:12:05,720",
        "en": "Tahu pun takut. Ganggu aku lagi.",
        "pa": "You see? Never mess with me.",
        "vi": "Thấy chưa? Đừng chọc vào tôi."
    },
    {
        "start": "00:12:12,520",
        "end": "00:12:17,600",
        "en": "Baik aku tidur dekat sini, lagi selamat.",
        "pa": "I think this cave will make a wonderful sleeping spot.",
        "vi": "Mình nghĩ cái hang này sẽ là chỗ ngủ rất lí tưởng."
    },
    {
        "start": "00:12:42,240",
        "end": "00:12:43,240",
        "en": "Tolong!",
        "pa": "Beruang!",
        "vi": "Beruang!"
    },
    {
        "start": "00:12:44,600",
        "end": "00:12:47,360",
        "en": "Beruang!",
        "pa": "Beruang!",
        "vi": "Beruang!"
    },
    {
        "start": "00:12:58,040",
        "end": "00:12:59,720",
        "en": "Harimau!",
        "pa": "Harimau!",
        "vi": "Harimau!"
    },
    {
        "start": "00:13:02,680",
        "end": "00:13:06,200",
        "en": "Kenapa dengan kau? Aku yang besar pun kau tak nampak?",
        "pa": "What's the matter with you? Can't you even see me?",
        "vi": "Ngươi bị sao thế? Ngươi không nhìn thấy ta à?"
    },
    {
        "start": "00:13:06,280",
        "end": "00:13:09,920",
        "en": "Asyik nak langgar saja. Kau kena kejar hantu?",
        "pa": "Why do you always have to run into me?",
        "vi": "Sao lúc nào ngươi cũng đâm sầm vào ta thế?"
    },
    {
        "start": "00:13:10,000",
        "end": "00:13:12,120",
        "en": "Tadi dekat gua…",
        "pa": "Just now… at the cave…",
        "vi": "Mới nãy… ở cái hang…"
    },
    {
        "start": "00:13:12,200",
        "end": "00:13:15,160",
        "en": "ada makhluk makan pekasam mata Harimau.",
        "pa": "Someone is eating preserved tiger's eyes!",
        "vi": "Có kẻ đang ăn mắt hổ muối chua!"
    },
    {
        "start": "00:13:16,880",
        "end": "00:13:20,560",
        "en": "Mata Harimau. Tak apalah.",
        "pa": "Oh, tiger's eyes. That's fine.",
        "vi": "Mắt hổ à. Ngon đấy."
    },
    {
        "start": "00:13:21,840",
        "end": "00:13:25,920",
        "en": "Sekarang mata aku. Esok lusa mata kau!",
        "pa": "Who knows, it might be craving bear's eyes next!",
        "vi": "Biết đâu tiếp theo nó sẽ thèm ăn mắt gấu đấy!"
    },
    {
        "start": "00:13:27,360",
        "end": "00:13:29,960",
        "en": "Kalau macam itu, baik aku pergi sembunyi!",
        "pa": "Then I need to hide!",
        "vi": "Vậy ta phải đi trốn thôi!"
    },
    {
        "start": "00:13:30,040",
        "end": "00:13:31,200",
        "en": "Aku nak sembunyi dekat mana?",
        "pa": "Where can I hide?",
        "vi": "Trốn ở đâu bây giờ?"
    },
    {
        "start": "00:13:38,600",
        "end": "00:13:40,120",
        "en": "Nanti!",
        "pa": "Wait a minute!",
        "vi": "Đợi một chút!"
    },
    {
        "start": "00:13:40,200",
        "end": "00:13:43,160",
        "en": "Apa kata, kita berdua pergi siasat!",
        "pa": "Why don't we go investigate?",
        "vi": "Sao chúng ta không đi thăm dò?"
    },
    {
        "start": "00:13:43,240",
        "end": "00:13:45,960",
        "en": "Kita tengok, apa gerangan makhluk dalam gua itu?",
        "pa": "See what kind of creature is in that cave.",
        "vi": "Xem thử bên trong hang là sinh vật gì."
    },
    {
        "start": "00:13:46,040",
        "end": "00:13:49,200",
        "en": "- Nak tak? - Tak naklah.",
        "pa": "-Come on. -No!",
        "vi": "- Đi nào. - Không!"
    },
    {
        "start": "00:13:50,080",
        "end": "00:13:54,240",
        "en": "Jangan takut! Aku yang hebat dan kuat ini kan ada.",
        "pa": "Don't worry. I'm strong and powerful.",
        "vi": "Đừng lo. Ta vừa mạnh vừa khỏe."
    },
    {
        "start": "00:13:54,320",
        "end": "00:13:56,120",
        "en": "Aku boleh jaga kau!",
        "pa": "I can protect you.",
        "vi": "Ta sẽ bảo vệ ngươi."
    },
    {
        "start": "00:13:56,200",
        "end": "00:13:57,800",
        "en": "Tak boleh!",
        "pa": "No, you can't!",
        "vi": "Không, ngươi không hề!"
    },
    {
        "start": "00:13:57,880",
        "end": "00:14:00,800",
        "en": "Kalau tertangkap, kau mesti tinggalkan aku.",
        "pa": "If we get caught, you'll run away and leave me.",
        "vi": "Nếu hai ta bị bắt, ngươi sẽ chạy trước và bỏ lại ta."
    },
    {
        "start": "00:14:00,880",
        "end": "00:14:02,160",
        "en": "Kau lari laju.",
        "pa": "You're fast, remember?",
        "vi": "Ngươi rất nhanh, nhớ chứ?"
    },
    {
        "start": "00:14:03,240",
        "end": "00:14:05,360",
        "en": "Kalau begitu, mari kita berjanji.",
        "pa": "If that's so, let's make a pact.",
        "vi": "Nếu vậy thì hãy giao ước với nhau."
    },
    {
        "start": "00:14:05,440",
        "end": "00:14:06,680",
        "en": "Berjanji?",
        "pa": "A pact?",
        "vi": "Giao ước?"
    },
    {
        "start": "00:14:06,760",
        "end": "00:14:11,160",
        "en": "Ya! Kita ikat ekor kita bersama-sama. Macam ini!",
        "pa": "Yes. We'll tie our tails together like this.",
        "vi": "Phải. Hai ta sẽ cột đuôi lại như thế này."
    },
    {
        "start": "00:14:13,480",
        "end": "00:14:17,400",
        "en": "Kalau jadi apa-apa, aku tak boleh lari tinggalkan kau.",
        "pa": "If anything happens, I can't run and leave you behind.",
        "vi": "Nếu có chuyện xảy ra, ta cũng không thể chạy và bỏ ngươi lại."
    },
    {
        "start": "00:14:19,400",
        "end": "00:14:23,320",
        "en": "- Baiklah, aku setuju. - Bagus, mari!",
        "pa": "-All right. I agree. -Good. Let's go!",
        "vi": "- Được rồi. Đồng ý. - Tốt. Đi thôi!"
    },
    {
        "start": "00:14:39,440",
        "end": "00:14:40,600",
        "en": "Sinilah!",
        "pa": "Hurry up!",
        "vi": "Nhanh lên!"
    },
    {
        "start": "00:14:42,240",
        "end": "00:14:43,680",
        "en": "Aku nak ambil saja.",
        "pa": "I'm hungry.",
        "vi": "Ta đói quá."
    },
    {
        "start": "00:14:43,760",
        "end": "00:14:46,680",
        "en": "Kau jangan buang masa! Kita kena pergi sekarang juga!",
        "pa": "Stop wasting time! We have to go now!",
        "vi": "Đừng lãng phí thời gian! Ta phải đi ngay mới được!"
    },
    {
        "start": "00:14:46,760",
        "end": "00:14:50,000",
        "en": "Sekejap saja, bolehlah Harimau. Bolehlah…",
        "pa": "-Just a minute. Come on, Harimau -No!",
        "vi": "- Chỉ một tí thôi. Đi mà, Harimau. - Không!"
    },
    {
        "start": "00:14:50,080",
        "end": "00:14:54,280",
        "en": "Tak boleh! Kita datang sini bukan nak makan. Kau lupa?",
        "pa": "We didn't come here for honey!",
        "vi": "Chúng ta không đến đây để tìm mật ong!"
    },
    {
        "start": "00:14:54,360",
        "end": "00:14:55,200",
        "en": "Kita datang nak tengok",
        "pa": "Have you forgotten?\n -We came to see the creature in the cave. -What's going on now?",
        "vi": "Ngươi quên rồi à?\n - Ta đến vì sinh vật trong hang. - Có chuyện gì thế?"
    },
    {
        "start": "00:14:55,280",
        "end": "00:14:56,880",
        "en": "- makhluk dalam gua itu. - Masih nak ganggu aku?",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:14:56,960",
        "end": "00:14:59,240",
        "en": "- Tapi aku lapar… - Tak serik!",
        "pa": "-But I'm hungry… -They never learn!",
        "vi": "- Nhưng ta đói… - Bọn họ vẫn chưa chừa!"
    },
    {
        "start": "00:14:59,320",
        "end": "00:15:00,680",
        "en": "Yalah! Cepat!",
        "pa": "Come on!",
        "vi": "Đi nào!"
    },
    {
        "start": "00:15:02,520",
        "end": "00:15:03,680",
        "en": "Sedapnya.",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:15:04,440",
        "end": "00:15:07,960",
        "en": "Jangan risau! Aku ada cara nak takutkan makhluk itu.",
        "pa": "Don't worry. I'll scare it away!",
        "vi": "Đừng lo. Ta sẽ dọa cho nó chạy mất!"
    },
    {
        "start": "00:15:10,040",
        "end": "00:15:12,360",
        "en": "- Macam mana? - Tengok sini!",
        "pa": "-How? -Watch this!",
        "vi": "- Bằng cách nào? - Xem này!"
    },
    {
        "start": "00:15:24,680",
        "end": "00:15:27,400",
        "en": "Itupun kau Harimau!",
        "pa": "There you are, Harimau!",
        "vi": "Thì ra ngươi đã đến, Harimau!"
    },
    {
        "start": "00:15:29,920",
        "end": "00:15:33,520",
        "en": "Dia kenal kau? Macam mana dia tahu kau dekat sini?",
        "pa": "It knows you? How does it know you're here?",
        "vi": "Nó biết ngươi à? Sao nó biết ngươi ở đây?"
    },
    {
        "start": "00:15:34,560",
        "end": "00:15:38,160",
        "en": "Kan aku mengaum tadi, mestilah dia tahu kita dekat sini.",
        "pa": "I just roared at it. Of course it knows!",
        "vi": "Ta vừa gầm nó. Dĩ nhiên nó biết rồi!"
    },
    {
        "start": "00:15:39,360",
        "end": "00:15:40,760",
        "en": "Jadi kenapa…",
        "pa": "Then why--",
        "vi": "Vậy thì sao…"
    },
    {
        "start": "00:15:42,040",
        "end": "00:15:45,960",
        "en": "Jangan bising. Biar aku uruskan.",
        "pa": "Be quiet. Let me handle this.",
        "vi": "Im lặng nào. Để ta giải quyết cho."
    },
    {
        "start": "00:15:46,480",
        "end": "00:15:48,760",
        "en": "Siapa kau?",
        "pa": "Who are you?",
        "vi": "Ngươi là ai?"
    },
    {
        "start": "00:15:50,960",
        "end": "00:15:54,120",
        "en": "Jangan tanya siapa aku!",
        "pa": "How dare you ask me that question!",
        "vi": "Sao ngươi dám hỏi ta câu đó!"
    },
    {
        "start": "00:15:54,200",
        "end": "00:15:56,360",
        "en": "Kau dah lupa wasiat itu?",
        "pa": "Have you forgotten our agreement?",
        "vi": "Ngươi đã quên thỏa thuận của chúng ta à?"
    },
    {
        "start": "00:15:57,960",
        "end": "00:16:00,080",
        "en": "Wasiat apa?",
        "pa": "What agreement?",
        "vi": "Thỏa thuận gì?"
    },
    {
        "start": "00:16:01,400",
        "end": "00:16:03,920",
        "en": "Apa dia? Aku dah tak ingat!",
        "pa": "What are you talking about? What is it?",
        "vi": "Ngươi đang nói gì thế? Đó là cái gì?"
    },
    {
        "start": "00:16:05,400",
        "end": "00:16:07,560",
        "en": "Dasar Harimau tua!",
        "pa": "Stupid old tiger!",
        "vi": "Đồ Harimau già ngu ngốc!"
    },
    {
        "start": "00:16:09,200",
        "end": "00:16:10,240",
        "en": "Tua?",
        "pa": "\"Old\"?",
        "vi": "\"Harimau già\"?"
    },
    {
        "start": "00:16:14,280",
        "end": "00:16:16,320",
        "en": "Dengar sini baik-baik!",
        "pa": "Listen well!",
        "vi": "Nghe cho rõ này!"
    },
    {
        "start": "00:16:16,840",
        "end": "00:16:20,600",
        "en": "Dalam wasiat itu, kau berhutang beruang putih.",
        "pa": "According to that agreement, you owe me a white bear!",
        "vi": "Theo thỏa thuận của chúng ta, ngươi nợ ta một con gấu trắng!"
    },
    {
        "start": "00:16:20,680",
        "end": "00:16:24,480",
        "en": "Sekarang, kau nak bayar dengan beruang hitam?",
        "pa": "But you've brought me a black bear instead!",
        "vi": "Nhưng ngươi lại mang đến một con gấu đen!"
    },
    {
        "start": "00:16:25,120",
        "end": "00:16:28,200",
        "en": "Tak apalah. Hitam pun hitamlah.",
        "pa": "Whatever, I'm flexible.",
        "vi": "Nhưng sao cũng được, ta dễ lắm."
    },
    {
        "start": "00:16:28,280",
        "end": "00:16:30,160",
        "en": "Cepat, bawa dia ke mari!",
        "pa": "Bring him forward!",
        "vi": "Mang nó đến đây!"
    },
    {
        "start": "00:16:31,440",
        "end": "00:16:35,200",
        "en": "Apa? Kau nak jadikan aku makanan dia?",
        "pa": "What? You're feeding me to it?",
        "vi": "Gì cơ? Ngươi dâng ta cho nó à?"
    },
    {
        "start": "00:16:35,920",
        "end": "00:16:37,400",
        "en": "Tak! Beruang!",
        "pa": "No! Beruang, listen to me.",
        "vi": "Không! Beruang à, nghe ta nói này."
    },
    {
        "start": "00:16:37,480",
        "end": "00:16:39,080",
        "en": "Nanti dulu Beruang. Dengar dulu Beruang.",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:16:39,160",
        "end": "00:16:42,320",
        "en": "Aku tak sangka kau Harimau, aku tak nak!",
        "pa": "How could you, Harimau?\n I'm out!",
        "vi": "Sao ngươi có thể làm thế?\n Ta đi đây!"
    },
    {
        "start": "00:16:44,000",
        "end": "00:16:45,480",
        "en": "Lepaskan aku Harimau.",
        "pa": "Let me go!",
        "vi": "Để ta đi!"
    },
    {
        "start": "00:16:45,560",
        "end": "00:16:47,040",
        "en": "Beruang!",
        "pa": "Beruang!",
        "vi": "Beruang!"
    },
    {
        "start": "00:16:49,480",
        "end": "00:16:52,720",
        "en": "Aduh!",
        "pa": "Ouch!",
        "vi": ""
    },
    {
        "start": "00:16:52,800",
        "end": "00:16:55,840",
        "en": "Tolong, sakit!",
        "pa": "Help me! It hurts!",
        "vi": "Cứu tôi với! Đau quá!"
    },
    {
        "start": "00:16:55,920",
        "end": "00:16:58,440",
        "en": "Beruang, ekor kau!",
        "pa": "Beruang, your tail!",
        "vi": "Beruang, đuôi của ngươi này!"
    },
    {
        "start": "00:17:01,520",
        "end": "00:17:03,520",
        "en": "Tunggu Beruang! Tunggu aku!",
        "pa": "Wait for me!",
        "vi": "Đợi ta với!"
    },
    {
        "start": "00:17:06,119",
        "end": "00:17:08,680",
        "en": "Padan muka, ganggu aku lagi.",
        "pa": "Serves you right for messing with me!",
        "vi": "Đáng đời ngươi vì dám chọc vào ta!"
    },
    {
        "start": "00:17:13,560",
        "end": "00:17:17,880",
        "en": "Kancil memang bijak, seperti selalu.",
        "pa": "Kancil wins, as always.",
        "vi": "Kancil lại thắng như mọi khi."
    },
    {
        "start": "00:17:19,640",
        "end": "00:17:22,920",
        "en": "Begitulah ceritanya.",
        "pa": "That's how it goes.",
        "vi": "Chuyện là thế đấy."
    },
    {
        "start": "00:17:23,000",
        "end": "00:17:25,280",
        "en": "Kancil kenakan Harimau dan Beruang.",
        "pa": "Kancil tricked both Harimau and Beruang.",
        "vi": "Kancil đã lừa cả Harimau và Beruang."
    },
    {
        "start": "00:17:26,560",
        "end": "00:17:27,720",
        "en": "Faham tak Ara?",
        "pa": "Do you get it?",
        "vi": "Cháu hiểu không?"
    },
    {
        "start": "00:17:29,440",
        "end": "00:17:34,560",
        "en": "Tapi, apa maksud \"jangan bangunkan kucing yang tidur\"?",
        "pa": "But what does \"Let sleeping dogs lie\" mean?",
        "vi": "Nhưng câu \"Đừng chọc chú chó đang ngủ\" nghĩa là sao ạ?"
    },
    {
        "start": "00:17:34,640",
        "end": "00:17:39,840",
        "en": "Maksudnya, jangan diganggu musuh yang tidak berbuat apa-apa.",
        "pa": "It means don't mess with anyone who hasn't done anything to you.",
        "vi": "Câu đó nghĩa là đừng chọc những ai không làm gì hại đến mình."
    },
    {
        "start": "00:17:39,920",
        "end": "00:17:42,080",
        "en": "Kalau kita asyik kacau orang itu,",
        "pa": "If you keep bothering them,",
        "vi": "Nếu các cháu cứ chọc phá họ,"
    },
    {
        "start": "00:17:42,160",
        "end": "00:17:44,760",
        "en": "mesti akhirnya dia akan balas.",
        "pa": "you'll get it sooner or later.",
        "vi": "sớm hay muộn cháu sẽ bị trừng phạt."
    },
    {
        "start": "00:17:44,840",
        "end": "00:17:48,240",
        "en": "- Macam Ara pukul Aris. - Begitu rupanya.",
        "pa": "-Like when Ara hit you just now. -Oh, I see.",
        "vi": "- Như lúc nãy Ara đánh cháu đấy. - Cháu hiểu rồi."
    },
    {
        "start": "00:17:48,320",
        "end": "00:17:52,800",
        "en": "Ara Kancil, abang Harimau yang jahat!",
        "pa": "I'm Kancil. You're the mean Harimau!",
        "vi": "Em là Kancil. Anh là Harimau xấu tính!"
    },
    {
        "start": "00:17:52,880",
        "end": "00:17:55,960",
        "en": "Ara, tak baik cakap macam itu.",
        "pa": "Ara, don't say that.",
        "vi": "Ara, đừng nói vậy."
    },
    {
        "start": "00:17:56,040",
        "end": "00:18:00,720",
        "en": "Aki, ekor Beruang panjang? Macam Harimau.",
        "pa": "Aki, is Beruang's tail long? Like Harimau's?",
        "vi": "Ông à, đuôi của Beruang có dài không? Dài như đuôi Harimau ấy?"
    },
    {
        "start": "00:18:00,800",
        "end": "00:18:04,000",
        "en": "Abang! Pendeklah, tengok sini.",
        "pa": "No, it's not. It's short!",
        "vi": "Không. Nó khá là ngắn!"
    },
    {
        "start": "00:18:04,080",
        "end": "00:18:06,160",
        "en": "- Panjang. - Pendek.",
        "pa": "-See? -Long.",
        "vi": "- Thấy chưa? - Dài."
    },
    {
        "start": "00:18:06,240",
        "end": "00:18:08,240",
        "en": "- Panjang. - Pendek.",
        "pa": "-Short! -Long!",
        "vi": "- Ngắn! - Dài!"
    },
    {
        "start": "00:18:08,320",
        "end": "00:18:09,320",
        "en": "- Panjang. - Pendek.",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:18:09,400",
        "end": "00:18:12,880",
        "en": "- Itu patung saja. Panjang! - Pendek!",
        "pa": "That's just a soft toy.\n -Short! -Long!",
        "vi": "Đây là thú bông mà.\n - Ngắn! - Dài!"
    },
    {
        "start": "00:18:13,480",
        "end": "00:18:16,200",
        "en": "Aki, pekasam itu apa?",
        "pa": "Aki, what's a preserve?",
        "vi": "Ông à, muối chua là gì ạ?"
    },
    {
        "start": "00:18:16,280",
        "end": "00:18:20,040",
        "en": "Pekasam itu ikan yang diperap untuk dimakan.",
        "pa": "It's like when you marinate a fish to eat.",
        "vi": "Giống như khi cháu muối cá để ăn đấy."
    },
    {
        "start": "00:18:20,120",
        "end": "00:18:25,600",
        "en": "Tapi dalam cerita Aki, kenapa pekasam itu mata harimau?",
        "pa": "But it wasn't a fish in that story. It was a tiger's eye.",
        "vi": "Nhưng trong truyện không có cá mà lại là mắt hổ."
    },
    {
        "start": "00:18:26,800",
        "end": "00:18:29,200",
        "en": "Esok kita makan ikan pekasam, nak?",
        "pa": "Let's eat preserved fish tomorrow.",
        "vi": "Ngày mai mình cùng muối cá nhé."
    },
    {
        "start": "00:18:29,280",
        "end": "00:18:33,240",
        "en": "- Sedapkah Aki? - Sedap!",
        "pa": "-Is it good? -Of course!",
        "vi": "- Có ngon không ông? - Ngon chứ!"
    },
    {
        "start": "00:18:33,320",
        "end": "00:18:37,520",
        "en": "- Nak! - Tapi janji, jangan gaduh lagi.",
        "pa": "-We want some! -You have to promise not to fight.",
        "vi": "- Chúng cháu muốn ăn! - Hai đứa phải hứa sẽ không cãi nhau."
    },
    {
        "start": "00:18:37,600",
        "end": "00:18:38,680",
        "en": "Janji!",
        "pa": "We promise!",
        "vi": "Chúng cháu xin hứa!"
    },
    {
        "start": "00:18:38,760",
        "end": "00:18:42,440",
        "en": "Sayang abang. Abang sayang Ara?",
        "pa": "I love you, Brother! Do you love me?",
        "vi": "Em thương anh, anh à! Anh có thương em không?"
    },
    {
        "start": "00:18:45,480",
        "end": "00:18:47,200",
        "en": "Bertuah punya budak.",
        "pa": "This kid…",
        "vi": "Đứa nhỏ này…"
    },
    {
        "start": "00:19:11,240",
        "end": "00:19:13,960",
        "en": "",
        "pa": "",
        "vi": "Biên dịch: Hoàng Nguyên"
    }
]