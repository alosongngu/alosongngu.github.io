[
    {
        "start": "00:00:32,800",
        "end": "00:00:34,600",
        "en": "Kiki comel.",
        "pa": "Kiki, you're so cute.",
        "vi": "Kiki."
    },
    {
        "start": "00:00:35,640",
        "end": "00:00:37,600",
        "en": "Aki, sapu ke mana ini?",
        "pa": "Aki, where do I sweep this?",
        "vi": "Ông ơi, cháu quét vào đâu đây ạ?"
    },
    {
        "start": "00:00:37,680",
        "end": "00:00:40,760",
        "en": "Sapu saja dekat pokok rambutan itu.",
        "pa": "Just behind the rambutan tree.",
        "vi": "Quét ra sau cây chôm chôm ấy."
    },
    {
        "start": "00:00:48,120",
        "end": "00:00:49,680",
        "en": "Abang,",
        "pa": "Aris.",
        "vi": "Anh ơi."
    },
    {
        "start": "00:00:50,280",
        "end": "00:00:51,680",
        "en": "situ.",
        "pa": "Over there.",
        "vi": "Bên này."
    },
    {
        "start": "00:00:55,800",
        "end": "00:00:59,240",
        "en": "Dekat sini pula. Sini pun ada. Dekat sini.",
        "pa": "There's some here and there too. Here!",
        "vi": "Bên này cũng còn, cả bên này nữa. Đây!"
    },
    {
        "start": "00:00:59,320",
        "end": "00:01:02,080",
        "en": "Abang tahu. Janganlah kacau.",
        "pa": "I know. Leave me alone.",
        "vi": "Biết rồi, để anh yên nào."
    },
    {
        "start": "00:01:03,120",
        "end": "00:01:05,040",
        "en": "Ara nak tolong saja.",
        "pa": "I was just trying to help.",
        "vi": "Em chỉ giúp anh thôi mà."
    },
    {
        "start": "00:01:06,400",
        "end": "00:01:08,760",
        "en": "Orang nak tolong pun marah.",
        "pa": "You don't have to scold me.",
        "vi": "Sao anh lại mắng em chứ."
    },
    {
        "start": "00:01:10,640",
        "end": "00:01:13,200",
        "en": "Tak apa Kiki, kita main sendiri ya?",
        "pa": "That's okay, Kiki. We'll just play together.",
        "vi": "Không sao đâu Kiki, chúng ta cùng chơi nhé."
    },
    {
        "start": "00:01:16,000",
        "end": "00:01:21,240",
        "en": "Ya. Sayang Kiki, Kiki baik.",
        "pa": "Really? I love you, Kiki! You're the best.",
        "vi": "Thật ư? Chị yêu em lắm, Kiki! Em là số một."
    },
    {
        "start": "00:01:24,720",
        "end": "00:01:29,320",
        "en": "Abang, tengok ini. Cepatlah Abang, tengok ini.",
        "pa": "Aris! Look!",
        "vi": "Anh ơi! Nhìn này! Đến đây xem cái này nhanh lên!"
    },
    {
        "start": "00:01:29,920",
        "end": "00:01:31,520",
        "en": "- Ini. - Apa lagi Ara?",
        "pa": "Come on, Aris! Look at this, quick!\n -Look! -What now?",
        "vi": "- Nhìn này! - Gì nữa đây?"
    },
    {
        "start": "00:01:34,160",
        "end": "00:01:37,360",
        "en": "Banyaknya. Ara nak?",
        "pa": "Wow! There are so many! Do you want one?",
        "vi": "Nhiều quá! Em muốn ăn không?"
    },
    {
        "start": "00:01:38,680",
        "end": "00:01:40,440",
        "en": "Ambil banyak-banyak Abang.",
        "pa": "Take all of them!",
        "vi": "Hái hết nào!"
    },
    {
        "start": "00:01:41,240",
        "end": "00:01:43,720",
        "en": "Aris buat apa itu?",
        "pa": "Aris? What are you doing?",
        "vi": "Aris? Cháu làm gì thế?"
    },
    {
        "start": "00:01:44,400",
        "end": "00:01:46,840",
        "en": "Jangan panjat jatuh nanti.",
        "pa": "Don't climb that tree. You'll hurt yourself.",
        "vi": "Đừng trèo cây, cháu sẽ bị thương đấy."
    },
    {
        "start": "00:01:46,920",
        "end": "00:01:50,480",
        "en": "- Aki, tak jatuhnya. - Jangan.",
        "pa": "-Aki! He won't fall. -No.",
        "vi": "- Ông à! Anh sẽ không ngã đâu. - Không được."
    },
    {
        "start": "00:01:50,560",
        "end": "00:01:54,480",
        "en": "Kamu itu tak biasa memanjat. Tunggu Aki ambil pengait.",
        "pa": "You're not used to climbing. Wait here, I'll get a stick.",
        "vi": "Cháu không quen trèo cây. Đợi ở đây, để ông đi lấy cây sào."
    },
    {
        "start": "00:01:57,200",
        "end": "00:01:58,960",
        "en": "Sedapnya.",
        "pa": "They look so good.",
        "vi": "Trông hấp dẫn quá."
    },
    {
        "start": "00:02:01,920",
        "end": "00:02:06,240",
        "en": "Panjatlah Abang, panjatlah, cepatlah Abang.",
        "pa": "Climb the tree, Aris, climb it. Please climb it, Aris.",
        "vi": "Trèo lên đi anh. Trèo lên cây đi mà."
    },
    {
        "start": "00:02:07,120",
        "end": "00:02:08,160",
        "en": "Kejap.",
        "pa": "Okay, wait.",
        "vi": "Được rồi, đợi anh."
    },
    {
        "start": "00:02:12,800",
        "end": "00:02:14,680",
        "en": "Hati-hati Abang.",
        "pa": "Be careful, Aris!",
        "vi": "Cẩn thận nhé!"
    },
    {
        "start": "00:02:16,240",
        "end": "00:02:18,480",
        "en": "Itu, dekat situ.",
        "pa": "There! Over there.",
        "vi": "Kia kìa! Bên kia ấy."
    },
    {
        "start": "00:02:25,240",
        "end": "00:02:26,880",
        "en": "Sedapnya.",
        "pa": "Delicious!",
        "vi": "Ngon quá!"
    },
    {
        "start": "00:02:28,640",
        "end": "00:02:31,160",
        "en": "Abang, bagilah Ara.",
        "pa": "Give me one, Aris, give me one!",
        "vi": "Cho em một quả với!"
    },
    {
        "start": "00:02:32,520",
        "end": "00:02:34,080",
        "en": "Abang ini.",
        "pa": "Aris.",
        "vi": "Anh à."
    },
    {
        "start": "00:02:34,160",
        "end": "00:02:37,160",
        "en": "- Sedap. - Nak juga.",
        "pa": "-So good. -I want to eat them too.",
        "vi": "- Ngon quá. - Em cũng muốn ăn nữa."
    },
    {
        "start": "00:02:38,040",
        "end": "00:02:40,360",
        "en": "Cepatlah Abang",
        "pa": "Come on, Aris.",
        "vi": "Thôi nào, anh à."
    },
    {
        "start": "00:02:40,440",
        "end": "00:02:42,480",
        "en": "Kejap, Abang ambilkan.",
        "pa": "Okay, I'll get you one.",
        "vi": "Được rồi, để anh hái cho em một quả."
    },
    {
        "start": "00:02:42,560",
        "end": "00:02:44,200",
        "en": "Itu, yang itu.",
        "pa": "That one.",
        "vi": "Quả kia kìa."
    },
    {
        "start": "00:02:48,160",
        "end": "00:02:50,160",
        "en": "Sikit lagi Bang, sikit lagi.",
        "pa": "A little closer, Aris.",
        "vi": "Thêm chút nữa."
    },
    {
        "start": "00:02:55,640",
        "end": "00:02:57,520",
        "en": "Abang!",
        "pa": "Aris!",
        "vi": "Anh ơi!"
    },
    {
        "start": "00:03:02,040",
        "end": "00:03:05,680",
        "en": "Tolong Abang, Ara!",
        "pa": "Help me, help!",
        "vi": "Cứu cháu với!"
    },
    {
        "start": "00:03:07,280",
        "end": "00:03:11,600",
        "en": "- Itulah Abang, tak bagi Ara lagi. - Cepatlah Ara, tolong abang.",
        "pa": "-You should have given me one. -Come on, Ara, help me.",
        "vi": "- Em bảo anh cho em một quả mà! - Mau lên Ara, cứu anh với!"
    },
    {
        "start": "00:03:12,360",
        "end": "00:03:15,760",
        "en": "Padan muka Abang. Sudah kena.",
        "pa": "-Serves you right. -Aki, help!",
        "vi": "Đáng đời anh lắm.\n Đáng đời."
    },
    {
        "start": "00:03:16,400",
        "end": "00:03:20,000",
        "en": "Bertuah punya budak! Bukan Aki sudah cakap jangan panjat!",
        "pa": "You stubborn little boy, didn't I tell you not to climb that tree?",
        "vi": "Thằng nhóc cứng đầu này, ông đã bảo đừng trèo cây mà?"
    },
    {
        "start": "00:03:20,080",
        "end": "00:03:21,160",
        "en": "Degil.",
        "pa": "You're so stubborn.",
        "vi": "Cháu nghịch quá đấy."
    },
    {
        "start": "00:03:21,240",
        "end": "00:03:24,960",
        "en": "Tolong! Tolong Aki, Aki, tolong!",
        "pa": "Help, Aki! Help me, please!",
        "vi": "Cứu cháu với, ông ơi! Làm ơn đi mà!"
    },
    {
        "start": "00:03:26,400",
        "end": "00:03:30,400",
        "en": "Kau punya degil ini teringat Aki satu cerita.",
        "pa": "You and your stubbornness remind me of a story.",
        "vi": "Cái tính cứng đầu của cháu làm ông nhớ đến một câu chuyện."
    },
    {
        "start": "00:03:30,480",
        "end": "00:03:32,280",
        "en": "Cerita? Nak dengar!",
        "pa": "Story? I want to hear it!",
        "vi": "Câu chuyện ạ? Cháu muốn nghe!"
    },
    {
        "start": "00:03:32,360",
        "end": "00:03:34,120",
        "en": "Nanti.",
        "pa": "Hold your horses.",
        "vi": "Từ từ nào."
    },
    {
        "start": "00:03:34,200",
        "end": "00:03:37,280",
        "en": "- Cepatlah! - Aki!",
        "pa": "I want to hear it!",
        "vi": "- Cháu muốn nghe! - Ông ơi!"
    },
    {
        "start": "00:03:39,360",
        "end": "00:03:40,440",
        "en": "Boleh.",
        "pa": "Okay!",
        "vi": "Được rồi!"
    },
    {
        "start": "00:03:41,320",
        "end": "00:03:44,200",
        "en": "Pada zaman dahulu…",
        "pa": "Once upon a time…",
        "vi": "Ngày xửa ngày xưa…"
    },
    {
        "start": "00:03:44,280",
        "end": "00:03:45,840",
        "en": "Aki.",
        "pa": "",
        "vi": "Ông ơi…"
    },
    {
        "start": "00:04:05,280",
        "end": "00:04:06,480",
        "en": "Mana kau nak lari!",
        "pa": "Where are you going to run?",
        "vi": "Giờ ngươi chạy đi đâu?"
    },
    {
        "start": "00:04:55,160",
        "end": "00:04:58,600",
        "en": "Kau tak boleh lari dari aku Kancil.",
        "pa": "You'll never get away from me, Kancil.",
        "vi": "Ngươi sẽ không thoát được đâu, Kancil."
    },
    {
        "start": "00:05:04,320",
        "end": "00:05:05,600",
        "en": "Apa ini?",
        "pa": "What's this?",
        "vi": "Cái gì đây?"
    },
    {
        "start": "00:05:07,360",
        "end": "00:05:09,800",
        "en": "Harimau. Jangan bergerak!",
        "pa": "Harimau, you'd better stop moving.",
        "vi": "Harimau, đừng cử động nhé!"
    },
    {
        "start": "00:05:09,880",
        "end": "00:05:13,000",
        "en": "Nanti lagi cepat kau tenggelam.",
        "pa": "Otherwise, you'll sink faster.",
        "vi": "Nếu không anh sẽ chìm nhanh hơn đấy."
    },
    {
        "start": "00:05:18,000",
        "end": "00:05:20,600",
        "en": "Bukan aku sudah cakap? Degil!",
        "pa": "I told you so! Stubborn cat.",
        "vi": "Tôi đã bảo rồi! Anh cứng đầu thật."
    },
    {
        "start": "00:05:20,680",
        "end": "00:05:22,280",
        "en": "Selamat tinggal!",
        "pa": "Farewell!",
        "vi": "Tạm biệt!"
    },
    {
        "start": "00:05:26,400",
        "end": "00:05:29,080",
        "en": "Tunggu kau Kancil!",
        "pa": "Just you wait, Kancil!",
        "vi": "Cứ đợi đấy, Kancil!"
    },
    {
        "start": "00:05:32,840",
        "end": "00:05:35,360",
        "en": "Harimau.",
        "pa": "Oh, Harimau. Don't you ever learn?",
        "vi": "Harimau à."
    },
    {
        "start": "00:05:35,440",
        "end": "00:05:38,800",
        "en": "Tak serik-serik lagikah?",
        "pa": "",
        "vi": "Ngươi vẫn chưa rút ra được bài học à?"
    },
    {
        "start": "00:05:38,880",
        "end": "00:05:41,960",
        "en": "Asyik kena tipu saja.",
        "pa": "You keep getting tricked.",
        "vi": "Ngươi cứ luôn bị lừa."
    },
    {
        "start": "00:05:42,040",
        "end": "00:05:44,960",
        "en": "Sedangkan aku yang hebat,",
        "pa": "Even though I'm great,",
        "vi": "Dù ta có tài giỏi,"
    },
    {
        "start": "00:05:45,040",
        "end": "00:05:49,280",
        "en": "licik dan lawa ini pun tak dapat tangkap Kancil,",
        "pa": "scheming, and beautiful, I still can't catch Kancil.",
        "vi": "gian xảo và xinh đẹp thế này, ta vẫn không bắt được Kancil."
    },
    {
        "start": "00:05:49,360",
        "end": "00:05:51,600",
        "en": "inikan pula engkau?",
        "pa": "And you?",
        "vi": "Còn ngươi?"
    },
    {
        "start": "00:05:53,600",
        "end": "00:05:55,760",
        "en": "Aku Raja Rimba!",
        "pa": "Silence! I'm the King of the Jungle.",
        "vi": "Im mồm! Ta là Chúa Tể Rừng Xanh."
    },
    {
        "start": "00:05:55,840",
        "end": "00:05:59,480",
        "en": "Apa yang aku nak, pasti akan dapat.",
        "pa": "Whatever I want, I get.",
        "vi": "Ta muốn gì thì sẽ có được cái đó."
    },
    {
        "start": "00:05:59,560",
        "end": "00:06:02,120",
        "en": "Sakit!",
        "pa": "It hurts!",
        "vi": "Đau quá!"
    },
    {
        "start": "00:06:02,200",
        "end": "00:06:04,960",
        "en": "Lepas!",
        "pa": "Let me go!",
        "vi": "Thả ta ra!"
    },
    {
        "start": "00:06:05,040",
        "end": "00:06:07,400",
        "en": "Cekik!",
        "pa": "I'm choking!",
        "vi": "Ta ngạt thở rồi!"
    },
    {
        "start": "00:06:07,480",
        "end": "00:06:10,240",
        "en": "Aku tak akan tertipu lagi!",
        "pa": "I won't be made a fool of again!",
        "vi": "Ta sẽ không bị lừa như một thằng ngố nữa đâu!"
    },
    {
        "start": "00:06:22,120",
        "end": "00:06:24,440",
        "en": "Semoga berjaya.",
        "pa": "I wish you the best of luck.",
        "vi": "Chúc may mắn nhé."
    },
    {
        "start": "00:08:09,720",
        "end": "00:08:11,720",
        "en": "Merdunya!",
        "pa": "It's so beautiful!",
        "vi": "Đẹp quá!"
    },
    {
        "start": "00:08:32,720",
        "end": "00:08:34,440",
        "en": "Bukan aku sudah cakap?",
        "pa": "Didn't I tell you?",
        "vi": "Không phải ta đã nói à?"
    },
    {
        "start": "00:08:34,520",
        "end": "00:08:36,600",
        "en": "Kau tak boleh lari dari aku.",
        "pa": "You can never get away from me.",
        "vi": "Ngươi sẽ không thể nào thoát khỏi ta."
    },
    {
        "start": "00:08:39,880",
        "end": "00:08:40,840",
        "en": "Bunyi apa itu?",
        "pa": "What's that sound?",
        "vi": "Tiếng gì thế?"
    },
    {
        "start": "00:08:48,760",
        "end": "00:08:51,480",
        "en": "Itu bunyi seruling Raja Sulaiman.",
        "pa": "That's the sound of King Sulaiman's flute!",
        "vi": "Đó là tiếng sáo của Vua Sulaiman!"
    },
    {
        "start": "00:08:53,040",
        "end": "00:08:56,760",
        "en": "Seruling Raja Sulaiman? Yakah?",
        "pa": "King Sulaiman's flute? Really?",
        "vi": "Sáo của Vua Sulaiman? Thật sao?"
    },
    {
        "start": "00:08:56,840",
        "end": "00:08:57,880",
        "en": "Memang!",
        "pa": "Of course!",
        "vi": "Dĩ nhiên là thật!"
    },
    {
        "start": "00:08:57,960",
        "end": "00:09:01,600",
        "en": "Hanya mereka yang dikasihi Raja Sulaiman saja yang tahu!",
        "pa": "Only those who are loved by King Sulaiman know about it.",
        "vi": "Chỉ có ai được Vua Sulaiman ưu ái mới biết về tiếng sáo này."
    },
    {
        "start": "00:09:02,840",
        "end": "00:09:06,720",
        "en": "Begitu. Mana seruling itu? Mana?",
        "pa": "I see. Where is that flute? Where?",
        "vi": "Hiểu rồi. Thế cây sáo ở đâu? Ở đâu nào?"
    },
    {
        "start": "00:09:06,800",
        "end": "00:09:08,440",
        "en": "Semua ini?",
        "pa": "Right here.",
        "vi": "Ngay đây."
    },
    {
        "start": "00:09:21,440",
        "end": "00:09:23,920",
        "en": "Macam mana seruling ini boleh berbunyi?",
        "pa": "How does it make that…sound?",
        "vi": "Sao cái này có thể làm ra… âm thanh đó được?"
    },
    {
        "start": "00:09:24,000",
        "end": "00:09:27,680",
        "en": "Sang Angin yang sedang meniupnya. Merdu, bukan?",
        "pa": "With the help of the wind, of course. Don't you love it?",
        "vi": "Đương nhiên là nhờ có gió rồi. Anh thích nó không?"
    },
    {
        "start": "00:09:29,440",
        "end": "00:09:30,600",
        "en": "Merdu.",
        "pa": "I do!",
        "vi": "Thích!"
    },
    {
        "start": "00:09:31,920",
        "end": "00:09:36,560",
        "en": "Kau belum dengar lagi Kerbau tiup. Lagi merdu.",
        "pa": "Kerbau can play that flute too! He's really good.",
        "vi": "Kerbau cũng có thể thổi sáo! Rất giỏi là đằng khác."
    },
    {
        "start": "00:09:36,640",
        "end": "00:09:38,040",
        "en": "Kerbau?",
        "pa": "",
        "vi": "Kerbau à?"
    },
    {
        "start": "00:09:49,080",
        "end": "00:09:51,360",
        "en": "Tak boleh jadi. Aku pun nak tiup!",
        "pa": "That isn't possible. I want to blow too!",
        "vi": "Không thể nào? Ta cũng muốn thổi sáo!"
    },
    {
        "start": "00:09:51,440",
        "end": "00:09:55,760",
        "en": "Tak boleh! Kau kena dapatkan kebenaran Raja Sulaiman dulu.",
        "pa": "You can't! You have to get King Sulaiman's permission first.",
        "vi": "Không được! Anh phải xin phép Vua Sulaiman trước."
    },
    {
        "start": "00:09:56,680",
        "end": "00:09:59,880",
        "en": "Ini Raja Rimba. Tak payah nak minta kebenaran.",
        "pa": "I am the King of the Jungle! I don't need permission.",
        "vi": "Ta là Chúa Tể Rừng Xanh! Ta không cần xin phép ai cả."
    },
    {
        "start": "00:09:59,960",
        "end": "00:10:01,600",
        "en": "Tak boleh!",
        "pa": "No, you can't!",
        "vi": "Không được!"
    },
    {
        "start": "00:10:01,680",
        "end": "00:10:06,000",
        "en": "Aku penjaga seruling ini, nanti Raja Sulaiman murka.",
        "pa": "I'm the guardian of this flute. You'll make the king angry.",
        "vi": "Tôi là người canh giữ sáo. Anh sẽ làm nhà vua nổi giận đấy."
    },
    {
        "start": "00:10:08,040",
        "end": "00:10:09,640",
        "en": "Aku nak tiup juga.",
        "pa": "I'm still going to blow it.",
        "vi": "Ta cứ thổi đấy thì sao nào."
    },
    {
        "start": "00:10:15,080",
        "end": "00:10:18,480",
        "en": "Harimau. Memang degil!",
        "pa": "Oh, Harimau. You are truly stubborn.",
        "vi": "Harimau, ngươi đúng là cứng đầu thật."
    },
    {
        "start": "00:10:20,520",
        "end": "00:10:23,320",
        "en": "Baiklah, tapi aku tak boleh duduk sini.",
        "pa": "Fine, but I cannot be seen with you or I'll be in trouble.",
        "vi": "Được, nhưng tôi không thể bị bắt gặp đang đứng với anh được,"
    },
    {
        "start": "00:10:23,400",
        "end": "00:10:25,480",
        "en": "Nanti aku kena marah!",
        "pa": "",
        "vi": "nếu không tôi sẽ gặp rắc rối."
    },
    {
        "start": "00:10:25,560",
        "end": "00:10:28,040",
        "en": "Biar aku berkawal di hujung hutan.",
        "pa": "Let me keep an eye out, and wait for my signal.",
        "vi": "Để tôi đi canh chừng, anh hãy đợi tín hiệu của tôi nhé."
    },
    {
        "start": "00:10:28,120",
        "end": "00:10:29,720",
        "en": "Kau tunggu isyarat aku.",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:10:32,160",
        "end": "00:10:33,600",
        "en": "Macam mana aku nak tiup ini?",
        "pa": "How do I blow this thing?",
        "vi": "Nhưng thổi sáo thế nào?"
    },
    {
        "start": "00:10:34,920",
        "end": "00:10:36,800",
        "en": "Mudah saja.",
        "pa": "Easy.",
        "vi": "Dễ lắm."
    },
    {
        "start": "00:10:36,880",
        "end": "00:10:40,440",
        "en": "Jelirkan lidah kau dekat situ, lepas itu tiuplah.",
        "pa": "Just stick your tongue out like this, and blow.",
        "vi": "Anh chỉ cần thè lưỡi ra thế này rồi thổi là được."
    },
    {
        "start": "00:10:50,920",
        "end": "00:10:51,760",
        "en": "Sudah boleh tiup?",
        "pa": "Can I blow it now?",
        "vi": "Giờ ta thử nhé?"
    },
    {
        "start": "00:10:53,160",
        "end": "00:10:54,280",
        "en": "Belum lagi!",
        "pa": "Not yet!",
        "vi": "Chưa được!"
    },
    {
        "start": "00:11:03,880",
        "end": "00:11:05,040",
        "en": "Sekarang boleh?",
        "pa": "How about now?",
        "vi": "Giờ được chưa?"
    },
    {
        "start": "00:11:06,640",
        "end": "00:11:07,560",
        "en": "Kancil?",
        "pa": "",
        "vi": "Kancil?"
    },
    {
        "start": "00:11:12,280",
        "end": "00:11:13,320",
        "en": "Aku tiup sajalah.",
        "pa": "I'm not waiting anymore.",
        "vi": "Ta không đợi nữa đâu."
    },
    {
        "start": "00:11:28,280",
        "end": "00:11:30,560",
        "en": "Kancil!",
        "pa": "",
        "vi": "Kancil!"
    },
    {
        "start": "00:11:31,160",
        "end": "00:11:33,480",
        "en": "Itulah, degil lagi.",
        "pa": "Serves you right!",
        "vi": "Đáng đời lắm!"
    },
    {
        "start": "00:11:51,320",
        "end": "00:11:54,480",
        "en": "Kenapa ini? Semua sudah busuk.",
        "pa": "What happened here? Everything is spoiled.",
        "vi": "Có chuyện gì thế này? Mọi thứ bị hỏng cả rồi."
    },
    {
        "start": "00:12:28,240",
        "end": "00:12:30,440",
        "en": "Besarnya sarang tebuan!",
        "pa": "That's a big nest.",
        "vi": "Cái tổ này to quá."
    },
    {
        "start": "00:12:32,120",
        "end": "00:12:33,400",
        "en": "Bahaya ini.",
        "pa": "I'd better go.",
        "vi": "Mình nên đi thôi."
    },
    {
        "start": "00:12:37,640",
        "end": "00:12:42,000",
        "en": "Dapat juga akhirnya, Kancil!",
        "pa": "You're mine at last, Kancil!",
        "vi": "Cuối cùng ngươi cũng là của ta, Kancil!"
    },
    {
        "start": "00:12:52,320",
        "end": "00:12:54,440",
        "en": "Dapat juga…",
        "pa": "You're mine at…",
        "vi": "Ngươi là của ta…"
    },
    {
        "start": "00:12:55,920",
        "end": "00:12:58,680",
        "en": "Dapat juga akhirnya.",
        "pa": "You're mine at last!",
        "vi": "Rốt cuộc ngươi cũng là của ta!"
    },
    {
        "start": "00:12:59,480",
        "end": "00:13:02,320",
        "en": "Sekarang Raja Sulaiman…",
        "pa": "Nobody, not even King Sulaiman--",
        "vi": "Không ai, kể cả Vua Sulaiman…"
    },
    {
        "start": "00:13:19,400",
        "end": "00:13:21,240",
        "en": "Tahu takut.",
        "pa": "Irritating bug.",
        "vi": "Con bọ đáng ghét này."
    },
    {
        "start": "00:13:23,600",
        "end": "00:13:25,400",
        "en": "Apa aku nak cakap tadi.",
        "pa": "Where was I?",
        "vi": "Ta nói tới đâu rồi?"
    },
    {
        "start": "00:13:26,640",
        "end": "00:13:31,680",
        "en": "Sekarang Raja Sulaiman pun tak boleh selamatkan kau.",
        "pa": "Nobody, not even King Sulaiman, can save you now!",
        "vi": "Không ai, kể cả Vua Sulaiman có thể cứu ngươi!"
    },
    {
        "start": "00:13:33,400",
        "end": "00:13:35,400",
        "en": "Wahai penghuni rimba,",
        "pa": "Dear forest friends,",
        "vi": "Hỡi các anh em rừng xanh,"
    },
    {
        "start": "00:13:35,480",
        "end": "00:13:39,280",
        "en": "pertandingan gong Raja Sulaiman telah dibatalkan.",
        "pa": "the Gong of King Sulaiman competition has been canceled!",
        "vi": "cuộc thi Chiếc Cồng Vua Sulaiman đã bị hủy!"
    },
    {
        "start": "00:13:47,120",
        "end": "00:13:48,920",
        "en": "Sudah. Makanlah aku.",
        "pa": "All right, you can eat me.",
        "vi": "Được rồi, giờ mời anh dùng bữa."
    },
    {
        "start": "00:13:49,960",
        "end": "00:13:53,840",
        "en": "Pertandingan Gong Raja Sulaiman? Apa itu?",
        "pa": "The Gong of King Sulaiman competition?",
        "vi": "Cuộc thi Chiếc Cồng Vua Sulaiman? Là cái gì thế?"
    },
    {
        "start": "00:13:54,560",
        "end": "00:13:56,000",
        "en": "Kau tak tahu?",
        "pa": "What's that?\n You don't know?",
        "vi": "Anh không biết à?"
    },
    {
        "start": "00:13:56,080",
        "end": "00:13:59,760",
        "en": "Sesiapa yang dapat pecahkan Gong Raja Sulaiman",
        "pa": "Whoever breaks the Gong of King Sulaiman",
        "vi": "Bất cứ ai đánh vỡ chiếc cồng của Vua Sulaiman"
    },
    {
        "start": "00:13:59,840",
        "end": "00:14:03,920",
        "en": "akan dilantik menjadi Panglima Istana!",
        "pa": "will be awarded the title of His Royal Knight!",
        "vi": "sẽ được trao danh hiệu Hiệp Sĩ Hoàng Gia!"
    },
    {
        "start": "00:14:04,840",
        "end": "00:14:09,640",
        "en": "Beruntungnya. Ada pembantu, makanan banyak!",
        "pa": "What an honor.\n To be treated like royalty and served the most delicious meals!",
        "vi": "Đúng là một vinh hạnh.\n Anh sẽ được đối xử như hoàng tộc và ăn toàn món ngon!"
    },
    {
        "start": "00:14:10,680",
        "end": "00:14:12,200",
        "en": "Makanan?",
        "pa": "Meals?",
        "vi": "Món ngon?"
    },
    {
        "start": "00:14:12,280",
        "end": "00:14:15,280",
        "en": "Ya, semua jenis daging ada!",
        "pa": "Yes, all kinds of meat!",
        "vi": "Phải, toàn là thịt!"
    },
    {
        "start": "00:14:15,360",
        "end": "00:14:19,360",
        "en": "Rusa, Kerbau, Arnab, Gajah pun ada!",
        "pa": "Venison, buffalo, rabbit, even elephant meat!",
        "vi": "Nai này, trâu này, thỏ này và cả thịt voi nữa!"
    },
    {
        "start": "00:14:20,040",
        "end": "00:14:21,240",
        "en": "Sedap, bukan?",
        "pa": "Isn't that great?",
        "vi": "Thích quá phải không?"
    },
    {
        "start": "00:14:23,240",
        "end": "00:14:27,200",
        "en": "Aku tak akan terpedaya lagi dengan kata-kata kau Kancil!",
        "pa": "I won't buy any more of your rubbish, Kancil!",
        "vi": "Ta không thèm tin mấy lời vớ vẩn của nhà ngươi đâu, Kancil!"
    },
    {
        "start": "00:14:27,280",
        "end": "00:14:29,400",
        "en": "Tak! Ini betul!",
        "pa": "Wait! This is the truth!",
        "vi": "Đợi đã! Đây là sự thật mà!"
    },
    {
        "start": "00:14:29,480",
        "end": "00:14:32,120",
        "en": "Aku sudah tak berani tipu kau lagi.",
        "pa": "I wouldn't dare trick you again.",
        "vi": "Tôi không dám lừa anh lần nữa đâu."
    },
    {
        "start": "00:14:35,560",
        "end": "00:14:36,560",
        "en": "Dekat mana Gong itu?",
        "pa": "Where is that gong?",
        "vi": "Chiếc cồng đó ở đâu?"
    },
    {
        "start": "00:14:37,560",
        "end": "00:14:38,920",
        "en": "Itu!",
        "pa": "There!",
        "vi": "Bên kia!"
    },
    {
        "start": "00:14:52,600",
        "end": "00:14:53,960",
        "en": "Mudah saja ini!",
        "pa": "Piece of cake!",
        "vi": "Dễ ợt!"
    },
    {
        "start": "00:14:56,360",
        "end": "00:14:57,640",
        "en": "Jangan!",
        "pa": "Stop!",
        "vi": "Dừng lại!"
    },
    {
        "start": "00:14:57,720",
        "end": "00:15:01,160",
        "en": "Bukan aku sudah cakap, pertandingan dibatalkan.",
        "pa": "I just announced that the competition has been canceled.",
        "vi": "Tôi vừa thông báo cuộc thi bị hủy rồi."
    },
    {
        "start": "00:15:01,760",
        "end": "00:15:06,080",
        "en": "Buat apa nak batal? Pemenangnya dekat sini! Ini.",
        "pa": "Why cancel it now? The winner is standing right here!",
        "vi": "Sao lại hủy? Người chiến thắng đang đứng ngay đây này!"
    },
    {
        "start": "00:15:06,800",
        "end": "00:15:09,280",
        "en": "Jangan Harimau! Nanti menyesal.",
        "pa": "Don't do it, Harimau! You'll regret it.",
        "vi": "Đừng làm thế, Harimau! Anh sẽ hối hận đấy."
    },
    {
        "start": "00:15:12,240",
        "end": "00:15:14,040",
        "en": "Jangan…",
        "pa": "Don't do it, please…",
        "vi": "Đừng mà, làm ơn…"
    },
    {
        "start": "00:15:22,720",
        "end": "00:15:24,720",
        "en": "Raja Sulaiman!",
        "pa": "King Sulaiman!",
        "vi": "Vua Sulaiman!"
    },
    {
        "start": "00:15:24,800",
        "end": "00:15:28,120",
        "en": "Panglima Tuanku akan tiba sekejap lagi.",
        "pa": "Your Royal Knight will be at your side in no time.",
        "vi": "Hiệp Sĩ Hoàng Gia của ngài sẽ có mặt ngay lập tức."
    },
    {
        "start": "00:15:44,720",
        "end": "00:15:49,040",
        "en": "Aku berjaya! Akulah Panglima Diraja!",
        "pa": "I did it! I am the one and only Royal Knight!",
        "vi": "Ta làm được rồi. Giờ ta là Hiệp Sĩ Hoàng Gia duy nhất!"
    },
    {
        "start": "00:15:50,240",
        "end": "00:15:52,000",
        "en": "Aku…",
        "pa": "I--",
        "vi": "Ta…"
    },
    {
        "start": "00:15:56,280",
        "end": "00:15:57,800",
        "en": "Apa ini?",
        "pa": "What is this?",
        "vi": "Cái gì đây?"
    },
    {
        "start": "00:15:59,920",
        "end": "00:16:00,920",
        "en": "Aduh!",
        "pa": "Ouch!",
        "vi": ""
    },
    {
        "start": "00:16:10,840",
        "end": "00:16:12,320",
        "en": "",
        "pa": "Over there!",
        "vi": ""
    },
    {
        "start": "00:16:14,520",
        "end": "00:16:15,880",
        "en": "",
        "pa": "Ouch!",
        "vi": ""
    },
    {
        "start": "00:16:16,760",
        "end": "00:16:17,960",
        "en": "",
        "pa": "Ouch!",
        "vi": ""
    },
    {
        "start": "00:16:35,400",
        "end": "00:16:37,440",
        "en": "Harimau.",
        "pa": "No, please. No!\n Ouch, it hurts!\n -Oh, Harimau. -It hurts!",
        "vi": "Harimau à."
    },
    {
        "start": "00:16:44,840",
        "end": "00:16:47,440",
        "en": "Sang Kancil!",
        "pa": "",
        "vi": "Kancil!"
    },
    {
        "start": "00:16:51,640",
        "end": "00:16:54,240",
        "en": "Itulah akibatnya kalau degil.",
        "pa": "That's what you get for being stubborn.",
        "vi": "Đó là bài học cho kẻ cứng đầu."
    },
    {
        "start": "00:16:54,320",
        "end": "00:16:56,880",
        "en": "Sia-sia saja Kancil bagi nasihat.",
        "pa": "Kancil tried to warn him, but he never listened.",
        "vi": "Kancil đã cố cảnh báo nhưng Harimau không nghe."
    },
    {
        "start": "00:16:56,960",
        "end": "00:17:00,480",
        "en": "Bagai mencurah air ke daun keladi.",
        "pa": "It was like water off a duck's back.",
        "vi": "Cứ như nước đổ đầu vịt."
    },
    {
        "start": "00:17:00,560",
        "end": "00:17:02,640",
        "en": "Yalah, Aris faham.",
        "pa": "Yes, I understand.",
        "vi": "Vâng, cháu hiểu rồi."
    },
    {
        "start": "00:17:02,720",
        "end": "00:17:05,520",
        "en": "Aris! Terlupa pula Aki.",
        "pa": "Aris! I forgot all about you.",
        "vi": "Aris! Ông quên cháu mất tiêu."
    },
    {
        "start": "00:17:05,599",
        "end": "00:17:08,160",
        "en": "Abang! Apa yang Abang faham?",
        "pa": "Aris! What did you understand?",
        "vi": "Anh à! Anh hiểu được gì nào?"
    },
    {
        "start": "00:17:09,359",
        "end": "00:17:10,720",
        "en": "Apa yang kamu faham?",
        "pa": "What did you understand?",
        "vi": "Cháu đã hiểu được gì nào?"
    },
    {
        "start": "00:17:12,119",
        "end": "00:17:16,520",
        "en": "Bila orang bagi nasihat dengar. Jangan degil, macam Aris.",
        "pa": "Always listen to your elders. Don't be stubborn like Aris.",
        "vi": "Phải nghe lời người lớn. Đừng cứng đầu như Aris."
    },
    {
        "start": "00:17:18,720",
        "end": "00:17:19,880",
        "en": "Pandai cucu Aki.",
        "pa": "Such a clever boy.",
        "vi": "Đúng là cháu ngoan."
    },
    {
        "start": "00:17:23,800",
        "end": "00:17:25,400",
        "en": "Kau nak ke mana itu?",
        "pa": "Where are you going?",
        "vi": "Cháu đi đâu đấy?"
    },
    {
        "start": "00:17:25,480",
        "end": "00:17:26,839",
        "en": "Marilah makan rambutan.",
        "pa": "Let's have some rambutan!",
        "vi": "Cùng ăn chôm chôm nào!"
    },
    {
        "start": "00:17:28,079",
        "end": "00:17:31,880",
        "en": "Tak naklah Aki. Aris lapar, nak makan nasi.",
        "pa": "No thanks, Aki! I'm starving, I want to eat rice.",
        "vi": "Thôi ạ, cảm ơn ông! Cháu đói lắm rồi, cháu muốn ăn cơm."
    },
    {
        "start": "00:17:31,960",
        "end": "00:17:33,400",
        "en": "Ara pun!",
        "pa": "Me too!",
        "vi": "Cháu nữa!"
    },
    {
        "start": "00:17:34,720",
        "end": "00:17:37,440",
        "en": "- Abang, tunggu Ara! - Tak ajak Aki?",
        "pa": "-Aris, wait for me! -What about me?",
        "vi": "- Anh à, đợi em đã! - Còn ông thì sao?"
    },
    {
        "start": "00:17:37,520",
        "end": "00:17:39,520",
        "en": "Aki pun lapar juga.",
        "pa": "I'm starving too.",
        "vi": "Ông cũng đói rồi này."
    },
    {
        "start": "00:17:42,240",
        "end": "00:17:45,760",
        "en": "Aki, kasihan tebuan sudah tak ada sarang.",
        "pa": "Aki! Those poor hornets don't have a home anymore.",
        "vi": "Ông à! Tội nghiệp bầy ong không còn nhà nữa rồi."
    },
    {
        "start": "00:17:46,800",
        "end": "00:17:51,760",
        "en": "Tebuan itu bahaya. Dulu, Aki suka pukul sarang tebuan.",
        "pa": "Hornets are dangerous!\n Back in the day, Aki used to hit their nests too.",
        "vi": "Ong vò vẽ rất nguy hiểm đấy!\n Hồi trước ông cũng từng chọc vào tổ của chúng đấy."
    },
    {
        "start": "00:17:51,840",
        "end": "00:17:56,000",
        "en": "Tapi kamu jangan sekali-kali main dekat sarang tebuan.",
        "pa": "However, don't you ever do that.",
        "vi": "Nhưng mà, các cháu đừng làm thế nhé."
    },
    {
        "start": "00:17:56,080",
        "end": "00:17:59,680",
        "en": "Kalau kena sengat, habis lebam.",
        "pa": "You might end up in the hospital.",
        "vi": "Không thì sẽ phải vào bệnh viện đấy."
    },
    {
        "start": "00:17:59,760",
        "end": "00:18:02,200",
        "en": "Aki pernah kena sengat dengan Tebuan?",
        "pa": "Have you ever been stung by one?",
        "vi": "Ông từng bị ong chích chưa ạ?"
    },
    {
        "start": "00:18:02,280",
        "end": "00:18:04,840",
        "en": "Tak, Aki hebat!",
        "pa": "Never! I'm too quick.",
