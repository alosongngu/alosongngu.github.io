[
    {
        "start": "00:00:30,120",
        "end": "00:00:33,360",
        "en": "- Hore! Aki dah balik! - Aki dah balik!",
        "pa": "-Yay, Aki's home! -Aki's home! Aki's home!",
        "vi": "- Ông về rồi! - Ông về rồi!"
    },
    {
        "start": "00:00:35,120",
        "end": "00:00:36,960",
        "en": "Aki!",
        "pa": "Aki!",
        "vi": "Ông ơi!"
    },
    {
        "start": "00:00:38,000",
        "end": "00:00:39,480",
        "en": "Ada aiskrim?",
        "pa": "Is there ice-cream for us?",
        "vi": "Ông có mua kem không ạ?"
    },
    {
        "start": "00:00:41,480",
        "end": "00:00:44,120",
        "en": "Pasal aiskrim, laju saja kau.",
        "pa": "Nothing gets you going like ice-cream, huh?",
        "vi": "Chỉ có kem mới làm các cháu vui như thế nhỉ?"
    },
    {
        "start": "00:00:44,200",
        "end": "00:00:46,360",
        "en": "Mestilah. Dah lama tunggu Aki.",
        "pa": "Of course! We've been waiting for ages.",
        "vi": "Dĩ nhiên rồi! Bọn cháu đợi lâu lắm rồi."
    },
    {
        "start": "00:00:50,880",
        "end": "00:00:53,600",
        "en": "- Terima kasih, Aki! - Terima kasih, Aki!",
        "pa": "-Thanks, Aki! -Thank you, Aki!",
        "vi": "- Cảm ơn ông! - Cảm ơn ông!"
    },
    {
        "start": "00:01:09,680",
        "end": "00:01:12,120",
        "en": "Sedapnya.",
        "pa": "So yummy!",
        "vi": "Ngon quá!"
    },
    {
        "start": "00:01:18,600",
        "end": "00:01:20,760",
        "en": "Abang, tunggu!",
        "pa": "Brother, wait for me!",
        "vi": "Anh à, đợi em với!"
    },
    {
        "start": "00:01:35,040",
        "end": "00:01:36,640",
        "en": "Ambil kau!",
        "pa": "Take this!",
        "vi": "Đỡ này!"
    },
    {
        "start": "00:01:38,040",
        "end": "00:01:39,080",
        "en": "Tercungap-cungap.",
        "pa": "So funny!",
        "vi": "Vui quá!"
    },
    {
        "start": "00:01:39,160",
        "end": "00:01:42,520",
        "en": "- Kenapa, abang? - Dah lemaslah.",
        "pa": "Too bad you can't swim!",
        "vi": "Tiếc là bọn mày không biết bơi!"
    },
    {
        "start": "00:01:43,280",
        "end": "00:01:44,840",
        "en": "Abang, jangan!",
        "pa": "Brother, stop that!",
        "vi": "Anh à, dừng lại đi!"
    },
    {
        "start": "00:01:44,920",
        "end": "00:01:49,200",
        "en": "- Kasihan, semut! - Buat apa nak kasihan, semut saja.",
        "pa": "-Poor ants. -Why? They're just ants.",
        "vi": "- Tội nghiệp lũ kiến. - Tội gì chứ? Chỉ là kiến thôi mà."
    },
    {
        "start": "00:01:51,240",
        "end": "00:01:52,520",
        "en": "Abang!",
        "pa": "Brother!",
        "vi": "Anh!"
    },
    {
        "start": "00:02:00,480",
        "end": "00:02:02,120",
        "en": "- Kenapa, abang? - Aduh, sakit!",
        "pa": "-What happened, Brother? -That hurts!",
        "vi": "- Anh bị sao thế? - Nhức quá!"
    },
    {
        "start": "00:02:02,200",
        "end": "00:02:04,760",
        "en": "- Aduh, tolong! - Aris, Ara!",
        "pa": "-Ow! Help me! -Aris, Ara?",
        "vi": "- Cứu tôi với! - Aris, Ara?"
    },
    {
        "start": "00:02:05,640",
        "end": "00:02:07,680",
        "en": "Kenapa bising-bising?",
        "pa": "-What's all the shouting about? -Aki, help!",
        "vi": "- Cháu la hét gì thế? - Ông à, cứu cháu!"
    },
    {
        "start": "00:02:08,320",
        "end": "00:02:10,960",
        "en": "Abang, dia kena gigit semut.",
        "pa": "Brother got bitten by ants.",
        "vi": "Anh bị kiến cắn ạ."
    },
    {
        "start": "00:02:12,240",
        "end": "00:02:13,800",
        "en": "Itu pun nak bising.",
        "pa": "That's it?",
        "vi": "Chỉ vậy thôi à?"
    },
    {
        "start": "00:02:13,880",
        "end": "00:02:15,760",
        "en": "Dah, masuk.",
        "pa": "Come, get inside.",
        "vi": "Nào, vào trong đi."
    },
    {
        "start": "00:02:19,120",
        "end": "00:02:21,960",
        "en": "Pergi duduk di meja, nanti Aki datang.",
        "pa": "Sit down, I'll be a moment.",
        "vi": "Ngồi đi, ông quay lại ngay."
    },
    {
        "start": "00:02:22,040",
        "end": "00:02:23,960",
        "en": "Abang kacau semut Aki.",
        "pa": "Brother disturbed the ants just now!",
        "vi": "Anh vừa chọc phá bầy kiến đấy ông!"
    },
    {
        "start": "00:02:24,040",
        "end": "00:02:26,720",
        "en": "Yakah? Abang kau memang.",
        "pa": "He did? Your brother is always up to mischief.",
        "vi": "Thế à? Anh của cháu lúc nào cũng nghịch ngợm."
    },
    {
        "start": "00:02:30,600",
        "end": "00:02:33,080",
        "en": "Adoi, sakitnya!",
        "pa": "It hurts so bad!",
        "vi": "Nhức quá đi!"
    },
    {
        "start": "00:02:34,160",
        "end": "00:02:35,200",
        "en": "Padan muka.",
        "pa": "Serves you right!",
        "vi": "Đáng đời anh!"
    },
    {
        "start": "00:02:35,280",
        "end": "00:02:37,480",
        "en": "Tergaru-garu.",
        "pa": "You keep scratching yourself.",
        "vi": "Anh cứ ngồi gãi đi."
    },
    {
        "start": "00:02:38,240",
        "end": "00:02:41,080",
        "en": "Jangan garu-garu, nanti luka.",
        "pa": "Stop scratching. You'll hurt yourself.",
        "vi": "Đừng gãi nữa. Cháu chỉ càng ngứa thêm thôi."
    },
    {
        "start": "00:02:41,160",
        "end": "00:02:43,680",
        "en": "Itulah, siapa suruh abang kacau dia?",
        "pa": "-You shouldn't have disturbed them. -I'm so hurt.",
        "vi": "- Lẽ ra anh không nên trêu lũ kiến. - Nhức quá."
    },
    {
        "start": "00:02:43,760",
        "end": "00:02:45,040",
        "en": "Mari sini.",
        "pa": "Come here.",
        "vi": "Lại đây nào."
    },
    {
        "start": "00:02:45,760",
        "end": "00:02:49,160",
        "en": "- Kau memang tak serik-serik. - Abang memang begitu, Aki.",
        "pa": "-You never learn, do you? -Aris is always like this.",
        "vi": "- Cháu vẫn chưa chừa à? - Anh lúc nào cũng vậy."
    },
    {
        "start": "00:02:49,240",
        "end": "00:02:51,840",
        "en": "Dulu kena gigit kerengga, sekarang semut pula.",
        "pa": "Last time it was fire ants. Now this.",
        "vi": "Lần trước là kiến lửa. Giờ thì thế này."
    },
    {
        "start": "00:02:52,880",
        "end": "00:02:55,280",
        "en": "Buat apa kau kacau?",
        "pa": "Why would you do that to the ants?",
        "vi": "Sao cháu lại chọc lũ kiến như vậy?"
    },
    {
        "start": "00:02:55,360",
        "end": "00:02:56,400",
        "en": "Mestilah kena gigit.",
        "pa": "Of course they'll take revenge.",
        "vi": "Nó sẽ trả đũa đấy."
    },
    {
        "start": "00:02:56,480",
        "end": "00:02:59,600",
        "en": "Aki, semut pun tahu nak balas balik?",
        "pa": "Aki, ants can take revenge, too?",
        "vi": "Ông à, kiến cũng biết trả đũa ạ?"
    },
    {
        "start": "00:03:00,600",
        "end": "00:03:01,800",
        "en": "Tahu.",
        "pa": "Of course.",
        "vi": "Dĩ nhiên."
    },
    {
        "start": "00:03:02,680",
        "end": "00:03:04,160",
        "en": "Nak dengar cerita?",
        "pa": "There's a story about it.",
        "vi": "Có câu chuyện về nó đấy."
    },
    {
        "start": "00:03:06,160",
        "end": "00:03:09,160",
        "en": "Pada zaman dahulu…",
        "pa": "Once upon a time…",
        "vi": "Ngày xửa ngày xưa…"
    },
    {
        "start": "00:04:02,160",
        "end": "00:04:03,600",
        "en": "Itu apa?",
        "pa": "What was that?",
        "vi": "Cái gì thế kia?"
    },
    {
        "start": "00:04:05,400",
        "end": "00:04:06,880",
        "en": "Rambutan!",
        "pa": "A rambutan!",
        "vi": "Một quả chôm chôm!"
    },
    {
        "start": "00:04:13,120",
        "end": "00:04:14,640",
        "en": "Besarnya.",
        "pa": "So big!",
        "vi": "To quá!"
    },
    {
        "start": "00:04:16,560",
        "end": "00:04:18,560",
        "en": "Cukup untuk makan beberapa bulan.",
        "pa": "This can last me a whole month!",
        "vi": "Đủ cho mình ăn cả tháng luôn!"
    },
    {
        "start": "00:04:52,680",
        "end": "00:04:54,200",
        "en": "Tolong!",
        "pa": "Help!",
        "vi": "Cứu tôi với!"
    },
    {
        "start": "00:04:54,280",
        "end": "00:04:55,680",
        "en": "Tolong!",
        "pa": "Help me!\n Help!",
        "vi": "Cứu tôi!\n Cứu!"
    },
    {
        "start": "00:05:34,280",
        "end": "00:05:37,280",
        "en": "- Selamat. - Kau tak apa-apa?",
        "pa": "-That was close. -Are you okay?",
        "vi": "- Suýt chết. - Cậu không sao chứ?"
    },
    {
        "start": "00:05:41,280",
        "end": "00:05:43,360",
        "en": "Tak. Terima kasih, Merpati.",
        "pa": "Yeah. Thanks, Merpati!",
        "vi": "Không. Cảm ơn nhé, Merpati!"
    },
    {
        "start": "00:05:43,440",
        "end": "00:05:46,240",
        "en": "Kalau kau tiada, mesti aku sudah mati lemas.",
        "pa": "If it weren't for you, I would've drowned for sure!",
        "vi": "Nếu không có chị, tôi đã bị chết đuối rồi!"
    },
    {
        "start": "00:05:46,320",
        "end": "00:05:49,920",
        "en": "Aku akan ingat jasa kau, sampai bila-bila.",
        "pa": "I owe you big time.",
        "vi": "Tôi nợ chị nhiều lắm."
    },
    {
        "start": "00:05:50,760",
        "end": "00:05:53,480",
        "en": "Tak apa, hari ini aku tolong kau.",
        "pa": "No problem. Anyone would do it.",
        "vi": "Không có chi. Ai cũng sẽ làm vậy mà."
    },
    {
        "start": "00:05:53,560",
        "end": "00:05:56,400",
        "en": "Mungkin esok lusa, kau tolong aku pula.",
        "pa": "I'm sure you would do the same, too.",
        "vi": "Tôi dám chắc cậu cũng sẽ làm vậy thôi."
    },
    {
        "start": "00:05:56,480",
        "end": "00:05:58,840",
        "en": "Baiklah semut, aku pergi dulu.",
        "pa": "I'll be going now, Semut.",
        "vi": "Tôi đi đây, Semut."
    },
    {
        "start": "00:05:58,920",
        "end": "00:06:01,160",
        "en": "Hati-hati jalan balik.",
        "pa": "Be careful out here, okay?",
        "vi": "Nhớ cẩn thận nhé?"
    },
    {
        "start": "00:06:03,360",
        "end": "00:06:05,120",
        "en": "Selamat jalan!",
        "pa": "Fly safe!",
        "vi": "Thượng lộ bình an!"
    },
    {
        "start": "00:06:06,200",
        "end": "00:06:08,000",
        "en": "Nasib baik ada Merpati!",
        "pa": "Thank goodness for Merpati.",
        "vi": "May nhờ có Merpati."
    },
    {
        "start": "00:06:16,520",
        "end": "00:06:17,800",
        "en": "Aduh!",
        "pa": "Ow!",
        "vi": ""
    },
    {
        "start": "00:06:21,080",
        "end": "00:06:22,360",
        "en": "Pemburu!",
        "pa": "The Hunter?",
        "vi": "Pemburu?"
    },
    {
        "start": "00:06:28,480",
        "end": "00:06:30,440",
        "en": "Pemburu nak panah…",
        "pa": "What's he shooting at?",
        "vi": "Hắn ta bắn gì thế nhỉ?"
    },
    {
        "start": "00:06:33,160",
        "end": "00:06:34,080",
        "en": "Merpati!",
        "pa": "Merpati!",
        "vi": "Merpati!"
    },
    {
        "start": "00:06:51,440",
        "end": "00:06:52,480",
        "en": "Ambil kau!",
        "pa": "Take this!",
        "vi": "Nhận lấy này!"
    },
    {
        "start": "00:06:53,040",
        "end": "00:06:53,960",
        "en": "Aduh!",
        "pa": "Ow!",
        "vi": ""
    },
    {
        "start": "00:07:01,240",
        "end": "00:07:04,080",
        "en": "Selamat jalan, Merpati. Hati-hati tahu.",
        "pa": "Bye, Merpati! Be safe!",
        "vi": "Tạm biệt Merpati! Bảo trọng nhé!"
    },
    {
        "start": "00:07:04,880",
        "end": "00:07:07,840",
        "en": "Buat baik, dibalas baik.",
        "pa": "One good deed deserves another.",
        "vi": "Ở hiền gặp lành."
    },
    {
        "start": "00:07:09,360",
        "end": "00:07:11,320",
        "en": "Tak mengapa.",
        "pa": "Earthquake!",
        "vi": "Có động đất!"
    },
    {
        "start": "00:07:14,960",
        "end": "00:07:16,240",
        "en": "Apa yang berlaku?",
        "pa": "What was that?",
        "vi": "Cái gì thế nhỉ?"
    },
    {
        "start": "00:07:18,240",
        "end": "00:07:19,840",
        "en": "Lari!",
        "pa": "Run!",
        "vi": "Chạy đi!"
    },
    {
        "start": "00:07:31,800",
        "end": "00:07:33,160",
        "en": "Gajah?",
        "pa": "Elephants?",
        "vi": "Voi à?"
    },
    {
        "start": "00:07:34,280",
        "end": "00:07:36,240",
        "en": "Tidak!",
        "pa": "No!",
        "vi": "Không!"
    },
    {
        "start": "00:07:37,040",
        "end": "00:07:39,880",
        "en": "- Kau dengar tak? - Jangan tegur.",
        "pa": "-Did you hear that? -Ignore it.",
        "vi": "- Nghe thấy gì không? - Mặc kệ đi."
    },
    {
        "start": "00:08:00,600",
        "end": "00:08:02,240",
        "en": "Tolong…",
        "pa": "Help…",
        "vi": "Cứu tôi…"
    },
    {
        "start": "00:08:06,160",
        "end": "00:08:09,720",
        "en": "Menteri! Kenapa gajah-gajah tadi menceroboh kawasan kita?",
        "pa": "Minister! Why did the elephants trample our territory?",
        "vi": "Bộ Trưởng! Vì sao bầy voi lại xông vào lãnh thổ của ta?"
    },
    {
        "start": "00:08:11,120",
        "end": "00:08:12,080",
        "en": "Itu!",
        "pa": "That!",
        "vi": "Cái đó!"
    },
    {
        "start": "00:08:12,160",
        "end": "00:08:13,880",
        "en": "Tuanku, tengoklah.",
        "pa": "Look at that, sire!",
        "vi": "Nhìn kìa, bệ hạ!"
    },
    {
        "start": "00:08:18,400",
        "end": "00:08:22,040",
        "en": "Tapi ini kawasan kita, mereka tak boleh buat sesuka hati.",
        "pa": "But this is our home. They can't do what they did.",
        "vi": "Nhưng đây là nhà của chúng ta. Họ không thể làm vậy."
    },
    {
        "start": "00:08:22,120",
        "end": "00:08:24,000",
        "en": "Nak buat macam mana.",
        "pa": "What can we do? We're tiny ants.",
        "vi": "Ta có thể làm gì đây? Chúng ta chỉ là những con kiến bé tẹo."
    },
    {
        "start": "00:08:24,080",
        "end": "00:08:26,960",
        "en": "Kita kecil, mereka besar!",
        "pa": "They're big elephants.",
        "vi": "Họ là voi to lớn."
    },
    {
        "start": "00:08:27,040",
        "end": "00:08:29,560",
        "en": "Sekali pijak, semua penyek.",
        "pa": "They'll squish us without a second thought.",
        "vi": "Họ có thể giẫm ta bẹp dí không cần suy nghĩ."
    },
    {
        "start": "00:08:30,080",
        "end": "00:08:32,880",
        "en": "Tengoklah.",
        "pa": "See? See?",
        "vi": "Thấy không?"
    },
    {
        "start": "00:08:32,960",
        "end": "00:08:36,520",
        "en": "Tak apa. Beta akan pergi berunding dengan raja gajah.",
        "pa": "Don't worry. I'll talk to the King of Elephants.",
        "vi": "Đừng lo. Ta sẽ nói chuyện với Vua Voi."
    },
    {
        "start": "00:08:36,600",
        "end": "00:08:40,159",
        "en": "Supaya rakyatnya tidak mencari makanan di sini lagi.",
        "pa": "So they can look for food someplace else.",
        "vi": "Để họ đi kiếm ăn ở nơi khác."
    },
    {
        "start": "00:08:40,240",
        "end": "00:08:43,080",
        "en": "Tapi, dia nak dengarkah?",
        "pa": "But… will he listen?",
        "vi": "Nhưng… liệu ông ta có nghe không?"
    },
    {
        "start": "00:08:45,080",
        "end": "00:08:46,800",
        "en": "Serahkan pada beta.",
        "pa": "Leave that to me.",
        "vi": "Cứ để cho ta."
    },
    {
        "start": "00:09:06,080",
        "end": "00:09:07,960",
        "en": "Wahai Raja Gajah!",
        "pa": "Hey, King Gajah!",
        "vi": "Này, Vua Gajah!"
    },
    {
        "start": "00:09:15,360",
        "end": "00:09:17,840",
        "en": "Berhenti! Aku di sini.",
        "pa": "Stop! I'm right here!",
        "vi": "Dừng lại! Tôi ở ngay đây này!"
    },
    {
        "start": "00:09:17,920",
        "end": "00:09:20,280",
        "en": "Aku datang nak berbincang.",
        "pa": "I just want to talk to you!",
        "vi": "Tôi chỉ muốn nói chuyện với anh thôi!"
    },
    {
        "start": "00:09:27,280",
        "end": "00:09:28,960",
        "en": "Apa yang kau nak bincangkan?",
        "pa": "What is it?",
        "vi": "Cái gì đây?"
    },
    {
        "start": "00:09:29,040",
        "end": "00:09:33,000",
        "en": "Gajah! Aku minta supaya kau arahkan rakyat kau,",
        "pa": "Gajah, I'm asking you to take your herd",
        "vi": "Gajah, tôi nhờ anh bảo đàn của mình"
    },
    {
        "start": "00:09:33,080",
        "end": "00:09:36,640",
        "en": "jangan cari makan di kawasan tebu berhampiran sungai.",
        "pa": "to look for food away from the sugarcane by the river.",
        "vi": "đi tìm thức ăn ở xa vườn mía cạnh bờ sông."
    },
    {
        "start": "00:09:36,720",
        "end": "00:09:38,720",
        "en": "Hei, kawan-kawan. Dengar sini.",
        "pa": "Hey, guys, listen to this!",
        "vi": "Này anh em, nghe này!"
    },
    {
        "start": "00:09:38,800",
        "end": "00:09:42,560",
        "en": "Dia tak bagi kita makan di sungai itu lagi.",
        "pa": "He doesn't want us to eat by the river!",
        "vi": "Nó không muốn chúng ta kiếm ăn ở bờ sông."
    },
    {
        "start": "00:09:43,960",
        "end": "00:09:47,120",
        "en": "Kenapa? Tebu itu memang makanan kami.",
        "pa": "Why shouldn't we? The sugarcane is ours.",
        "vi": "Sao lại không? Vườn mía là của bọn ta."
    },
    {
        "start": "00:09:47,200",
        "end": "00:09:50,280",
        "en": "Di mana ada makanan, di situ kami pergi.",
        "pa": "If there's sugarcane, we'll be there!",
        "vi": "Ở đâu có mía, ở đó có bọn ta!"
    },
    {
        "start": "00:09:50,360",
        "end": "00:09:51,400",
        "en": "Betul tak, kawan-kawan?",
        "pa": "Isn't that right?",
        "vi": "Phải không?"
    },
    {
        "start": "00:09:52,560",
        "end": "00:09:54,720",
        "en": "Tapi, itu kawasan kami!",
        "pa": "But we live there!",
        "vi": "Nhưng chúng tôi sống ở đó!"
    },
    {
        "start": "00:09:54,800",
        "end": "00:09:56,320",
        "en": "Bila rakyat kau datang,",
        "pa": "When you and your herd stomp around,",
        "vi": "Khi cả đàn của anh đi đến,"
    },
    {
        "start": "00:09:56,400",
        "end": "00:09:58,400",
        "en": "habis semua rakyat aku mati dipijak!",
        "pa": "you kill many of my people!",
        "vi": "đã giẫm bẹp rất nhiều thần dân của tôi!"
    },
    {
        "start": "00:09:59,240",
        "end": "00:10:02,040",
        "en": "Siapa suruh kamu tak elak?",
        "pa": "Why didn't you run away?",
        "vi": "Sao các ngươi không chạy đi?"
    },
    {
        "start": "00:10:03,880",
        "end": "00:10:08,840",
        "en": "Gajah, kau betul-betul nak biarkan rakyat kau mencari makanan di kawasan itu?",
        "pa": "So you won't stop looking for food there?",
        "vi": "Vậy các anh sẽ không từ bỏ chỗ kiếm ăn đó?"
    },
    {
        "start": "00:10:10,280",
        "end": "00:10:14,880",
        "en": "Ya. Mereka bebas mencari makanan di mana-mana saja.",
        "pa": "No! We are free to look for food anywhere we want!",
        "vi": "Không! Chúng ta được phép tự do kiếm ăn chỗ nào tùy thích!"
    },
    {
        "start": "00:10:18,880",
        "end": "00:10:23,000",
        "en": "Semut! Aku ada cadangan yang lebih baik.",
        "pa": "Semut, I have a better suggestion.",
        "vi": "Kiến này, ta có ý này hay hơn."
    },
    {
        "start": "00:10:24,000",
        "end": "00:10:25,360",
        "en": "Yakah? Apa dia?",
        "pa": "Really? Tell me!",
        "vi": "Thật ư? Nói tôi nghe với!"
    },
    {
        "start": "00:10:25,440",
        "end": "00:10:27,680",
        "en": "Kita perang!",
        "pa": "We go to war!",
        "vi": "Chúng ta sẽ chiến đấu!"
    },
    {
        "start": "00:10:31,880",
        "end": "00:10:33,920",
        "en": "Kasihan semut.",
        "pa": "Those poor ants.",
        "vi": "Tội nghiệp đàn kiến."
    },
    {
        "start": "00:10:34,000",
        "end": "00:10:37,800",
        "en": "Gajah itu macam abang. Suka kacau semut.",
        "pa": "The elephants are like Brother. They won't leave the ants alone.",
        "vi": "Bầy voi giống như anh vậy. Không chịu để đàn kiến được yên."
    },
    {
        "start": "00:10:37,880",
        "end": "00:10:41,280",
        "en": "Semut nak perang dengan gajah? Mestilah kalah.",
        "pa": "So the elephants and ants will fight. The ants will lose, of course!",
        "vi": "Giờ voi và kiến sẽ đấu với nhau. Dĩ nhiên kiến sẽ thua rồi!"
    },
    {
        "start": "00:10:41,360",
        "end": "00:10:44,480",
        "en": "Sekali gajah pijak, habis penyek!",
        "pa": "The elephants only have to step on them.",
        "vi": "Bầy voi chỉ cần giẫm bẹp chúng là xong."
    },
    {
        "start": "00:10:45,360",
        "end": "00:10:47,880",
        "en": "- Belum lawan, belum tahu - Entah, abang!",
        "pa": "-The story isn't over yet. -Wait for the story to end.",
        "vi": "- Câu chuyện vẫn chưa hết. - Đợi nghe hết chuyện đã."
    },
    {
        "start": "00:10:47,960",
        "end": "00:10:51,840",
        "en": "Walaupun semut kecil, tapi dia ada kelebihannya.",
        "pa": "Sure the ants are small, but they have something special.",
        "vi": "Kiến tuy bé nhưng lại có một thứ rất đặc biệt."
    },
    {
        "start": "00:10:51,920",
        "end": "00:10:55,880",
        "en": "Kelebihan apa, Aki? Kekurangan adalah.",
        "pa": "What are you talking about, Aki? They're just ants.",
        "vi": "Ông nói gì vậy? Kiến chỉ là kiến mà thôi."
    },
    {
        "start": "00:10:56,680",
        "end": "00:10:58,320",
        "en": "Itulah kau, tak tahu.",
        "pa": "You're the one who doesn't know.",
        "vi": "Cháu không biết đâu."
    },
    {
        "start": "00:10:58,400",
        "end": "00:11:00,280",
        "en": "Dengar Aki sambung.",
        "pa": "Listen.",
        "vi": "Nghe này."
    },
    {
        "start": "00:11:00,920",
        "end": "00:11:04,440",
        "en": "Semut terkejut, lepas itu…",
        "pa": "The ant is shocked. And then…",
        "vi": "Kiến bị sốc. Rồi sau đó…"
    },
    {
        "start": "00:11:05,960",
        "end": "00:11:08,480",
        "en": "Perang? Baik kita pindah ke tempat lain.",
        "pa": "War? I say we move out!",
        "vi": "Chiến đấu? Tôi đề nghị chuyển nhà!"
    },
    {
        "start": "00:11:08,560",
        "end": "00:11:11,720",
        "en": "Mana boleh? Ini kawasan kita.",
        "pa": "No! This is our home.",
        "vi": "Không! Đây là nhà của chúng ta."
    },
    {
        "start": "00:11:11,800",
        "end": "00:11:13,120",
        "en": "Kita mesti lawan.",
        "pa": "We must defend it!",
        "vi": "Ta phải bảo vệ nó."
    },
    {
        "start": "00:11:13,200",
        "end": "00:11:14,560",
        "en": "Macam mana?",
        "pa": "How?",
        "vi": "Bằng cách nào?"
    },
    {
        "start": "00:11:15,080",
        "end": "00:11:17,720",
        "en": "Mereka besar, kita kecil.",
        "pa": "They're too big, and we're too small.",
        "vi": "Họ thì to, ta thì bé."
    },
    {
        "start": "00:11:17,800",
        "end": "00:11:19,560",
        "en": "Tak boleh menang punya.",
        "pa": "We can't beat them.",
        "vi": "Ta không thể đánh lại họ."
    },
    {
        "start": "00:11:23,040",
        "end": "00:11:24,120",
        "en": "Beta tahu.",
        "pa": "I know!",
        "vi": "Ta biết!"
    },
    {
        "start": "00:11:25,680",
        "end": "00:11:27,160",
        "en": "Tahu apa?",
        "pa": "What?",
        "vi": "Biết gì thế?"
    },
    {
        "start": "00:11:27,240",
        "end": "00:11:29,760",
        "en": "Dia mesti boleh selesaikan masalah kita.",
        "pa": "He can help us!",
        "vi": "Cậu ấy có thể giúp chúng ta!"
    },
    {
        "start": "00:11:31,280",
        "end": "00:11:34,640",
        "en": "Aku tahu macam mana nak selesaikan masalah ini.",
        "pa": "I know how to help you.",
        "vi": "Tôi biết cách giúp chị rồi."
    },
    {
        "start": "00:11:34,720",
        "end": "00:11:36,280",
        "en": "Itu macam suara Kancil.",
        "pa": "-That sounds like Kancil. -You are at the same height.",
        "vi": "- Nghe giống giọng Kancil. - Cả hai đều cao như nhau."
    },
    {
        "start": "00:11:38,040",
        "end": "00:11:40,280",
        "en": "- Begitulah. - Kancil!",
        "pa": "-That's it. -Kancil! Kancil!",
        "vi": "- Vậy nhé. - Kancil!"
    },
    {
        "start": "00:11:40,360",
        "end": "00:11:43,120",
        "en": "- Terima kasih, Kancil. Kau memang bijak. - Kancil!",
        "pa": "-Kancil! -Thanks Kancil. You're really smart.",
        "vi": "- Kancil! - Cảm ơn Kancil. Cậu thật thông minh."
    },
    {
        "start": "00:11:44,680",
        "end": "00:11:48,160",
        "en": "- Itu bunyi apa? - Jangan tegur.",
        "pa": "-What was that? -Ignore it.\n Hey!",
        "vi": "- Cái gì thế? - Kệ đi.\n Này!"
    },
    {
        "start": "00:11:58,720",
        "end": "00:12:01,480",
        "en": "Aduh!",
        "pa": "Ouch!",
        "vi": ""
    },
    {
        "start": "00:12:05,880",
        "end": "00:12:07,080",
        "en": "Sakitnya.",
        "pa": "That hurts!",
        "vi": "Đau quá!"
    },
    {
        "start": "00:12:08,440",
        "end": "00:12:11,720",
        "en": "Kau ingat kau saja sakit, aku lagi sakit.",
        "pa": "You think that hurts? It's worse for me.",
        "vi": "Thế mà cậu đã đau rồi à? Tôi còn đau hơn đây này."
    },
    {
        "start": "00:12:11,800",
        "end": "00:12:14,600",
        "en": "Aku panggil kau banyak kali, tapi kau tak dengar.",
        "pa": "I called you so many times, you didn't hear me!",
        "vi": "Tôi đã gọi cậu rất nhiều lần nhưng cậu không nghe!"
    },
    {
        "start": "00:12:16,240",
        "end": "00:12:19,600",
        "en": "Yakah? Kenapa kau cari aku?",
        "pa": "Really? What's up, Semut?",
        "vi": "Thật à? Có chuyện gì vậy, Semut?"
    },
    {
        "start": "00:12:19,680",
        "end": "00:12:21,600",
        "en": "Kancil, kau kena tolong aku.",
        "pa": "Kancil, you have to help me!",
        "vi": "Kancil, cậu phải giúp tôi!"
    },
    {
        "start": "00:12:25,560",
        "end": "00:12:29,800",
        "en": "Nak perang? Gajah besar, ada belalai yang panjang.",
        "pa": "War? Elephants are big. They have long trunks.",
        "vi": "Chiến tranh? Voi to lắm. Vòi của họ rất là dài."
    },
    {
        "start": "00:12:29,880",
        "end": "00:12:33,600",
        "en": "Kau pula halus! Mana boleh buat apa-apa.",
        "pa": "And you? So tiny. You don't stand a chance.",
        "vi": "Còn cậu? Quá nhỏ bé. Cậu không có cửa thắng đâu."
    },
    {
        "start": "00:12:37,000",
        "end": "00:12:38,480",
        "en": "Tapi…",
        "pa": "But…",
        "vi": "Nhưng…"
    },
    {
        "start": "00:12:41,120",
        "end": "00:12:44,920",
        "en": "Begitu! Aku tahu caranya.",
        "pa": "I see! I know how!",
        "vi": "Nghĩ ra rồi! Tôi biết cách rồi!"
    },
    {
        "start": "00:12:52,320",
        "end": "00:12:55,080",
        "en": "Semut, jangan risau.",
        "pa": "Semut, don't worry.",
        "vi": "Semut, đừng lo."
    },
    {
        "start": "00:12:55,160",
        "end": "00:12:59,480",
        "en": "Walaupun kau kecil, haiwan lain tak dengar suara kau,",
        "pa": "Even though you're small, and no one can hear you,",
        "vi": "Dù các cậu bé, nói không ai nghe,"
    },
    {
        "start": "00:12:59,560",
        "end": "00:13:02,320",
        "en": "selalu terpijak kau, selalu…",
        "pa": "and you always get stepped on…",
        "vi": "còn hay bị giẫm dưới chân nữa…"
    },
    {
        "start": "00:13:02,400",
        "end": "00:13:03,400",
        "en": "Dah habis cakap?",
        "pa": "Are you done?",
        "vi": "Cậu xong chưa?"
    },
    {
        "start": "00:13:05,840",
        "end": "00:13:08,440",
        "en": "Maaf semut, jangan marah.",
        "pa": "Sorry, Semut. Don't be mad.",
        "vi": "Xin lỗi nhé, Semut. Đừng giận mà."
    },
    {
        "start": "00:13:09,080",
        "end": "00:13:10,520",
        "en": "Begini.",
        "pa": "Listen.",
        "vi": "Nghe này."
    },
    {
        "start": "00:13:10,600",
        "end": "00:13:12,320",
        "en": "Pasukan kiri akan menyusur masuk.",
        "pa": "The soldiers on the left will come.",
        "vi": "Các chiến binh bên trái sẽ đến."
    },
    {
        "start": "00:13:12,400",
        "end": "00:13:14,880",
        "en": "Akhir sekali, askar-askar kita bergabung, serang ramai-ramai.",
        "pa": "Lastly, we will combine and attack in full force!",
        "vi": "Cuối cùng, ta sẽ hợp lại và tổng tiến công!"
    },
    {
        "start": "00:13:14,960",
        "end": "00:13:19,160",
        "en": "Bijak! Sang Kancil memang bijak!",
        "pa": "Clever! Kancil is very clever!",
        "vi": "Thông minh quá! Kancil thật mưu trí!"
    },
    {
        "start": "00:13:20,120",
        "end": "00:13:21,800",
        "en": "Sang Kancil saja yang bijak?",
        "pa": "Just Kancil?",
        "vi": "Chỉ có Kancil thôi à?"
    },
    {
        "start": "00:13:22,320",
        "end": "00:13:24,400",
        "en": "Raja Semut lagi bijak.",
        "pa": "You're clever, too, sire.",
        "vi": "Bệ hạ cũng mưu trí nữa."
    },
    {
        "start": "00:13:24,480",
        "end": "00:13:27,960",
        "en": "Pandai mengatur serangan yang hebat.",
        "pa": "You're good at strategizing.",
        "vi": "Ngài rất giỏi lên chiến lược."
    },
    {
        "start": "00:13:29,640",
        "end": "00:13:32,640",
        "en": "Siap kau, Gajah. Tunggu esok.",
        "pa": "You're in for a surprise, Gajah.",
        "vi": "Gajah à, anh sẽ bị bất ngờ lớn cho xem."
    },
    {
        "start": "00:13:40,480",
        "end": "00:13:42,320",
        "en": "Jangan gentar!",
        "pa": "Hold your ground!",
        "vi": "Ở yên vị trí!"
    },
    {
        "start": "00:13:42,920",
        "end": "00:13:44,360",
        "en": "Semua sedia?",
        "pa": "Everyone ready?",
        "vi": "Mọi người sẵn sàng chưa?"
    },
    {
        "start": "00:13:45,240",
        "end": "00:13:46,560",
        "en": "Sedia, tuan!",
        "pa": "Yes, sire!",
        "vi": "Sẵn sàng!"
    },
    {
        "start": "00:13:46,640",
        "end": "00:13:49,920",
        "en": "Bagus. Kalau tak sedia, balik!",
        "pa": "Good! Anyone want to back off now?",
        "vi": "Tốt! Giờ có ai muốn rút lui không?"
    },
    {
        "start": "00:13:53,720",
        "end": "00:13:56,720",
        "en": "Aku nak balik!",
        "pa": "I'm out!\n There's animals coming.",
        "vi": "Cho tôi ra!\n Một đàn thú đang đến đấy."
    },
    {
        "start": "00:14:17,440",
        "end": "00:14:20,840",
        "en": "Gajah, ini peluang terakhir!",
        "pa": "Gajah, this is your last chance!",
        "vi": "Gajah, đây là cơ hội cuối cùng của anh đấy!"
    },
    {
        "start": "00:14:20,920",
        "end": "00:14:22,960",
        "en": "Baik kau menyerah kalah kalau kau tak nak perang.",
        "pa": "Surrender, and we will do you no harm!",
        "vi": "Đầu hàng đi thì các anh sẽ không bị gì!"
    },
    {
        "start": "00:14:23,040",
        "end": "00:14:25,440",
        "en": "Tak nak perang?",
        "pa": "What? No war?",
        "vi": "Gì cơ? Không chiến đấu à?"
    },
    {
        "start": "00:14:26,840",
        "end": "00:14:29,400",
        "en": "Kita memang nak perang!",
        "pa": "But we want war!",
        "vi": "Nhưng bọn ta muốn chiến đấu!"
    },
    {
        "start": "00:14:30,840",
        "end": "00:14:32,840",
        "en": "Betul tak, kawan-kawan?",
        "pa": "Am I right?",
        "vi": "Đúng không?"
    },
    {
        "start": "00:14:32,920",
        "end": "00:14:35,440",
        "en": "Semut! Jangan banyak cakap.",
        "pa": "Save your breath, Semut.",
        "vi": "Giữ hơi đi, Semut."
    },
    {
        "start": "00:14:35,520",
        "end": "00:14:39,480",
        "en": "Tinggalkan kawasan ini sebelum kita penyek kamu semua.",
        "pa": "Leave now before we step on all of you!",
        "vi": "Hãy đi trước khi bọn ta giẫm bẹp các ngươi."
    },
    {
        "start": "00:14:40,000",
        "end": "00:14:41,280",
        "en": "Betul tak?",
        "pa": "Go!",
        "vi": "Đi đi!"
    },
    {
        "start": "00:14:43,400",
        "end": "00:14:44,920",
        "en": "Jangan harap!",
        "pa": "In your dreams!",
        "vi": "Anh mơ đi!"
    },
    {
        "start": "00:14:45,440",
        "end": "00:14:47,760",
        "en": "Serang!",
        "pa": "Attack!\n Attack!",
        "vi": "Tấn công!\n Tấn công!"
    },
    {
        "start": "00:15:10,240",
        "end": "00:15:12,520",
        "en": "",
        "pa": "Run!",
        "vi": "Chạy đi!"
    },
    {
        "start": "00:15:29,080",
        "end": "00:15:31,000",
        "en": "Menyerah sajalah, Raja Semut!",
        "pa": "Admit defeat, Semut!",
        "vi": "Đầu hàng đi, Semut!"
    },
    {
        "start": "00:15:34,120",
        "end": "00:15:35,480",
        "en": "Sekarang!",
        "pa": "Now!",
        "vi": "Lên nào!"
    },
    {
        "start": "00:15:39,960",
        "end": "00:15:44,520",
        "en": "Kulit kami keras. Gigitlah sekuat mana pun, takkan sakit.",
        "pa": "We have thick skin. We can't feel a thing, you know.",
        "vi": "Da bọn ta dày lắm, không xi nhê gì đâu."
    },
    {
        "start": "00:15:56,960",
        "end": "00:15:58,200",
        "en": "Tolong!",
        "pa": "Help!",
        "vi": "Cứu tôi với!"
    },
    {
        "start": "00:16:01,280",
        "end": "00:16:02,720",
        "en": "Ini apa?",
        "pa": "What is this?",
        "vi": "Cái gì thế này?"
    },
    {
        "start": "00:16:02,800",
        "end": "00:16:05,160",
        "en": "Kenapa? Bangun!",
        "pa": "What's wrong? Get up, get up!",
        "vi": "Có chuyện gì vậy? Đứng lên đi!"
    },
    {
        "start": "00:16:05,240",
        "end": "00:16:06,640",
        "en": "Tolong!",
        "pa": "Help!",
        "vi": "Cứu tôi với!"
    },
    {
        "start": "00:16:07,960",
        "end": "00:16:09,480",
        "en": "Tolong!",
        "pa": "Help!",
        "vi": "Cứu với!"
    },
    {
        "start": "00:16:10,120",
        "end": "00:16:12,320",
        "en": "Apa dah jadi?",
        "pa": "What's happening?\n Help me!",
        "vi": "Có chuyện gì vậy?\n Cứu tôi với!"
    },
    {
        "start": "00:16:22,320",
        "end": "00:16:23,200",
        "en": "Ini apa?",
        "pa": "What's going on?",
        "vi": "Cái gì thế này?"
    },
    {
        "start": "00:16:23,960",
        "end": "00:16:25,680",
        "en": "Keluar!",
        "pa": "Get out! Out!",
        "vi": "Tránh ra! Cút mau!"
    },
    {
        "start": "00:16:28,040",
        "end": "00:16:29,120",
        "en": "Adoi, sakitnya!",
        "pa": "No!\n That hurts!",
        "vi": "Không!\n Đau quá!"
    },
    {
        "start": "00:16:33,520",
        "end": "00:16:34,760",
        "en": "Rasakan!",
        "pa": "You see?",
        "vi": "Thấy chưa?"
    },
    {
        "start": "00:16:35,280",
        "end": "00:16:37,720",
        "en": "Memanglah kulit kau keras,",
        "pa": "Sure, your skin is tough!",
        "vi": "Da các anh đúng là dày thật!"
    },
    {
        "start": "00:16:37,800",
        "end": "00:16:40,760",
        "en": "tapi dalam belalai kau, lembut saja.",
        "pa": "But not the insides of your trunks!",
        "vi": "Nhưng vòi của các anh thì không!"
    },
    {
        "start": "00:16:41,880",
        "end": "00:16:45,520",
        "en": "Kami menyerah kalah, jangan gigit lagi.",
        "pa": "We surrender! Don't bite us anymore!",
        "vi": "Bọn ta đầu hàng! Đừng cắn nữa!"
    },
    {
        "start": "00:16:46,040",
        "end": "00:16:51,040",
        "en": "- Jom kawan-kawan. Lari! - Lari!",
        "pa": "-Come on friends. Run! -Run!",
        "vi": "- Nào các anh em. Chạy thôi! - Chạy!"
    },
    {
        "start": "00:16:51,120",
        "end": "00:16:54,760",
        "en": "Kita menang! Kita berjaya kalahkan Gajah!",
        "pa": "We did it! We defeated the elephants!",
        "vi": "Thành công rồi! Chúng ta đã hạ được đàn voi."
    },
    {
        "start": "00:16:56,200",
        "end": "00:17:01,400",
        "en": "Semut!",
        "pa": "Yay!\n Semut!",
        "vi": "Hoan hô!\n Semut!"
    },
    {
        "start": "00:17:10,280",
        "end": "00:17:13,400",
        "en": "Buat jahat, dibalas jahat.",
        "pa": "What goes around, comes around.",
        "vi": "Ác giả ác báo."
    },
    {
        "start": "00:17:18,720",
        "end": "00:17:23,960",
        "en": "Buat baik berpada-pada, buat jahat, jangan sesekali.",
        "pa": "So, you see? What goes around, comes around.",
        "vi": "Các cháu thấy chưa? Ác giả ác báo."
    },
    {
        "start": "00:17:24,920",
        "end": "00:17:26,560",
        "en": "Tak fahamlah, Aki.",
        "pa": "I don't get it, Aki.",
        "vi": "Cháu không hiểu ông à."
    },
    {
        "start": "00:17:28,000",
        "end": "00:17:32,880",
        "en": "Maksudnya, buat jahat berkali-kali, jangan buat sekali.",
        "pa": "That means… we must do bad things again and again.",
        "vi": "Câu đó nghĩa là… chúng ta nên làm điều xấu."
    },
    {
        "start": "00:17:35,840",
        "end": "00:17:36,880",
        "en": "Macam itu, Aki?",
        "pa": "Is that what it means, Aki?",
        "vi": "Có phải vậy không ông?"
    },
    {
        "start": "00:17:37,400",
        "end": "00:17:39,080",
        "en": "Apa yang kau cakapkan?",
        "pa": "What are you talking about?",
        "vi": "Cháu nói gì thế?"
    },
    {
        "start": "00:17:39,160",
        "end": "00:17:41,280",
        "en": "- Itu salah. - Abang!",
        "pa": "-That's not it. -Aris!",
        "vi": "- Không phải vậy. - Anh à!"
    },
    {
        "start": "00:17:41,360",
        "end": "00:17:43,840",
        "en": "Maksudnya, kita boleh buat baik,",
        "pa": "It means we must do good deeds.",
        "vi": "Câu đó nghĩa là hãy làm việc tốt."
    },
    {
        "start": "00:17:43,920",
        "end": "00:17:48,400",
        "en": "tapi biar bertempat. Kita tak boleh buat jahat.",
        "pa": "But we must never do bad things.",
        "vi": "Và đừng bao giờ làm điều xấu."
    },
    {
        "start": "00:17:48,480",
        "end": "00:17:52,040",
        "en": "Sekali, berkali-kali, jangan!",
        "pa": "Not once, not twice, not ever.",
        "vi": "Một lần cũng không được."
    },
    {
        "start": "00:17:52,120",
        "end": "00:17:55,320",
        "en": "Kalau tak, kena gigit oleh semut.",
        "pa": "If not, you'll get bitten by ants!",
        "vi": "Nếu không, ta sẽ bị kiến cắn đấy!"
    },
    {
        "start": "00:17:55,400",
        "end": "00:17:57,960",
        "en": "Macam, Abang. Betulkan, Kiki?",
        "pa": "Like you! Right, Kiki?",
        "vi": "Giống anh vậy! Phải không, Kiki?"
    },
    {
        "start": "00:17:59,440",
        "end": "00:18:00,280",
        "en": "Mana Kiki?",
        "pa": "Where's Kiki?",
        "vi": "Kiki đâu rồi?"
    },
    {
        "start": "00:18:01,280",
        "end": "00:18:05,640",
        "en": "Kiki! Kiki di mana?",
        "pa": "Kiki, where are you? Kiki!",
        "vi": "Kiki, em đâu rồi? Kiki!"
    },
    {
        "start": "00:18:06,960",
        "end": "00:18:11,720",
        "en": "- Abang, Kiki! - Kiki dah hilang. Abang tak tahu.",
        "pa": "-Where's Kiki? -Kiki's gone! Don't look at me.",
        "vi": "- Kiki đâu rồi? - Kiki mất rồi! Đừng nhìn anh."
    },
    {
        "start": "00:18:11,800",
        "end": "00:18:15,120",
        "en": "Aris, jangan kacau adik.",
        "pa": "Stop that, Aris.\n Stop teasing your sister. Go help her.",
        "vi": "Thôi đi, Aris.\n Đừng trêu em cháu nữa. Tìm giúp em đi."
    },
    {
        "start": "00:18:15,640",
        "end": "00:18:16,840",
        "en": "Pergi cari cepat!",
        "pa": "",
        "vi": ""
    },
    {
        "start": "00:18:16,920",
        "end": "00:18:20,920",
        "en": "Kiki!",
        "pa": "Kiki. Kiki!",
        "vi": "Kiki!"
    },
    {
        "start": "00:18:21,000",
        "end": "00:18:23,920",
        "en": "Ara, dah jumpa Kiki?",
        "pa": "Ara, have you found Kiki?",
        "vi": "Ara, cháu tìm thấy Kiki chưa?"
    },
    {
        "start": "00:18:24,000",
        "end": "00:18:28,720",
        "en": "Dah, tapi kena gigit oleh semut. Habis comot.",
        "pa": "Yeah, she got bitten by ants.",
        "vi": "Rồi ạ, nó bị kiến cắn ông à."
    },
    {
        "start": "00:18:29,240",
        "end": "00:18:32,640",
        "en": "Aki, kenapa semut suka hurung gula?",
        "pa": "Aki, why do ants like sugar?",
        "vi": "Ông ơi, sao kiến lại thích đường vậy?"
    },
    {
        "start": "00:18:32,720",
        "end": "00:18:37,160",
        "en": "Bukan gula saja. Dia memang suka hurung benda manis.",
        "pa": "Not just sugar. They like all sweet stuff.",
        "vi": "Không chỉ đường đâu. Cái gì ngọt là kiến thích."
    },
    {
        "start": "00:18:37,240",
        "end": "00:18:39,560",
        "en": "Yalah, tapi kenapa?",
        "pa": "Yeah, but why?",
        "vi": "Nhưng tại sao ạ?"
    },
    {
        "start": "00:18:39,640",
        "end": "00:18:41,800",
        "en": "Sebab, sedap!",
        "pa": "Because… it's yummy.",
        "vi": "Bởi vì… nó ngon."
    },
    {
        "start": "00:18:43,920",
        "end": "00:18:47,600",
        "en": "- Aki! - Itu semut saja.",
        "pa": "-Aki! -They're just ants.",
        "vi": "- Ông à! - Chúng là kiến mà."
    },
    {
        "start": "00:19:09,920",
        "end": "00:19:13,160",
        "en": "",
        "pa": "",
        "vi": "Biên dịch: Hoàng Nguyên"
    }
]