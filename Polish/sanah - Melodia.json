[
    {
        "start": "00:00:01,150",
        "end": "00:00:05,650",
        "en": "Musiałam uciec stąd na jakiś czas",
        "pa": "I had to run away from here for a while",
        "vi": "Em phải chạy trốn khỏi nơi này, một khoảng thời gian..."
    },
    {
        "start": "00:00:05,650",
        "end": "00:00:07,480",
        "en": "Wróciłam nie taka sama",
        "pa": "I came back not the same",
        "vi": "Em trở về, đã không còn như xưa..."
    },
    {
        "start": "00:00:07,480",
        "end": "00:00:10,670",
        "en": "Żebym nie czuła gdy zaboli mnie",
        "pa": "So I wouldn't feel the pain",
        "vi": "Để bản thân chẳng còn thấy đau..."
    },
    {
        "start": "00:00:10,670",
        "end": "00:00:13,990",
        "en": "Chcę dziś tańczyć do rana",
        "pa": "Tonight I want to dance till dawn",
        "vi": "Đêm nay, em muốn đắm chìm trong điệu nhạc, cho đến khi bình minh ló dạng..."
    },
    {
        "start": "00:00:17,160",
        "end": "00:00:21,170",
        "en": "Tylko tańczyć do rana",
        "pa": "Just dance till dawn",
        "vi": "Chỉ muốn mãi men theo điệu nhạc, cho đến khi bình minh ló dạng..."
    },
    {
        "start": "00:00:21,170",
        "end": "00:00:24,760",
        "en": "Nie...",
        "pa": "No...",
        "vi": "Chẳng thể nào..."
    },
    {
        "start": "00:00:24,760",
        "end": "00:00:28,020",
        "en": "Nie powiedział nikt, że to będzie proste",
        "pa": "Nobody said it would be easy",
        "vi": "Chẳng ai nói với em rằng, mọi thứ sẽ dễ dàng..."
    },
    {
        "start": "00:00:28,020",
        "end": "00:00:30,710",
        "en": "Nie uprzedził nikt, że wyleję łzy",
        "pa": "No one warned me that I would cry",
        "vi": "Chẳng ai nói trước rằng, em sẽ phải rơi lệ..."
    },
    {
        "start": "00:00:30,710",
        "end": "00:00:32,450",
        "en": "Czy za dużo ich?",
        "pa": "Are they too much?",
        "vi": "Phải chăng, nước mắt đã rơi quá nhiều?"
    },
    {
        "start": "00:00:32,450",
        "end": "00:00:33,850",
        "en": "Trochę to żałosne",
        "pa": "It's a little pathetic",
        "vi": "Thật đáng thương hại..."
    },
    {
        "start": "00:00:33,850",
        "end": "00:00:37,190",
        "en": "Co zostało mi?",
        "pa": "What's left for me?",
        "vi": "Giờ đây, em còn lại gì?"
    },
    {
        "start": "00:00:37,190",
        "end": "00:00:39,850",
        "en": "Została mi tylko melodia",
        "pa": "Only the melody is left for me",
        "vi": "Chỉ còn lại giai điệu này bên em..."
    },
    {
        "start": "00:00:39,850",
        "end": "00:00:41,990",
        "en": "Zagłusza płacz",
        "pa": "Drowning out the crying",
        "vi": "Xoa dịu đi những giọt nước mắt..."
    },
    {
        "start": "00:00:41,990",
        "end": "00:00:44,320",
        "en": "Mówię sobie: \"tańcz, tańcz, tańcz\"",
        "pa": "I tell myself: \"dance, dance, dance\"",
        "vi": "Tự nhủ với bản thân: \"Hãy cứ nhảy đi, nhảy đi, nhảy đi...\""
    },
    {
        "start": "00:00:44,320",
        "end": "00:00:47,180",
        "en": "Ale nie wiem jak",
        "pa": "But I don't know how",
        "vi": "Nhưng em nào biết phải làm sao..."
    },
    {
        "start": "00:00:47,180",
        "end": "00:00:48,830",
        "en": "Oooo",
        "pa": "Oooo",
        "vi": "Oooo..."
    },
    {
        "start": "00:00:48,830",
        "end": "00:00:51,979",
        "en": "Pociesza mnie tylko melodia",
        "pa": "Only the melody comforts me",
        "vi": "Chỉ có giai điệu này an ủi em..."
    },
    {
        "start": "00:00:51,979",
        "end": "00:00:53,850",
        "en": "Tłumi mój żal",
        "pa": "It stifles my grief",
        "vi": "Xoa dịu đi nỗi đau trong tim..."
